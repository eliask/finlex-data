@type":"sfl:SectionVersion","amendedBy":"sfsd:2015/1041/alkup","hasPart":[{"@id":"sfsd:1991/592/luku/16/pykala/65/momentti/1/ajantasa/20160101","@type":"sfl:SubsectionVersion","cites":"sfsd:1996/586","isMemberOf":"sfsd:1991/592/luku/16/pykala/65/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/16/pykala/65/momentti/1/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1991/592/luku/16/pykala/65/momentti/1/ajantasa/20160101/fin/txt","content_fi":"Säteilyturvakeskuksen päätökseen saa hakea muutosta valittamalla hallinto-oikeuteen siten kuin hallintolainkäyttölaissa (586/1996) säädetään."}}},{"@id":"sfsd:1991/592/luku/16/pykala/65/momentti/2/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/592/luku/16/pykala/65/momentti/2","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/16/pykala/65/momentti/2/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1991/592/luku/16/pykala/65/momentti/2/ajantasa/20160101/fin/txt","content_fi":"Hallinto-oikeuden päätökseen 20 §:ssä tarkoitetussa turvallisuusluvan peruuttamista koskevassa asiassa sekä 55 ja 56 §:ssä tarkoitetussa asiassa saa hakea muutosta valittamalla siten kuin hallintolainkäyttölaissa säädetään. Hallinto-oikeuden muuhun päätökseen saa hakea muutosta valittamalla vain, jos korkein hallinto-oikeus myöntää valitusluvan."}}}],"isMemberOf":"sfsd:1991/592/luku/16/pykala/65","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/16/pykala/65/ajantasa/20160101/fin","title_fi":"Muutoksenhaku"}},{"@id":"sfsd:1991/592/luku/16/pykala/66/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1991/592/luku/16/pykala/66/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/592/luku/16/pykala/66/momentti/1","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/16/pykala/66/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/592/luku/16/pykala/66/momentti/1/alkup/fin/txt","content_fi":"Tämän lain 54 ja 55 §:n nojalla tehtävässä päätöksessä voidaan määrätä, että päätöstä on noudatettava muutoksenhausta huolimatta, jollei valitusviranomainen toisin määrää."}}},{"@id":"sfsd:1991/592/luku/16/pykala/66/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/592/luku/16/pykala/66/momentti/2","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/16/pykala/66/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1991/592/luku/16/pykala/66/momentti/2/alkup/fin/txt","content_fi":"Jos 1 momentissa tarkoitettu määräys on annettu, valitusviranomaisen on käsiteltävä muutoksenhaku kiireellisenä."}}}],"isMemberOf":"sfsd:1991/592/luku/16/pykala/66","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/16/pykala/66/alkup/fin","title_fi":"Täytäntöönpano"}}],"isMemberOf":"sfsd:1991/592/luku/16","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/16/ajantasa/20160101/fin","title_fi":"Muutoksenhaku"}},{"@id":"sfsd:1991/592/luku/17/ajantasa/20180815","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1991/592/luku/17/pykala/67/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1991/592/luku/17/pykala/67/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/592/luku/17/pykala/67/momentti/1","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/17/pykala/67/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/592/luku/17/pykala/67/momentti/1/alkup/fin/txt","content_fi":"Silloinkin, kun työ- tai muuhun elinympäristöön pääsee niin suuria määriä radioaktiivisia aineita, että säteilyturvallisuudesta huolehtiminen edellyttää erityisiä toimenpiteitä (normaalista poikkeava säteilytilanne), on mahdollisuuksien mukaan otettava huomioon 2§:ssä säädetyt periaatteet huolehdittaessa säteilyaltistuksen rajoittamisesta."}}},{"@id":"sfsd:1991/592/luku/17/pykala/67/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/592/luku/17/pykala/67/momentti/2","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/17/pykala/67/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1991/592/luku/17/pykala/67/momentti/2/alkup/fin/txt","content_fi":"Sisäasiainministeriö antaa yleiset määräykset ja ohjeet toimenpiteiden suunnittelemiseksi ja yhteensovittamiseksi normaalista poikkeavien säteilytilanteiden varalta."}}}],"isMemberOf":"sfsd:1991/592/luku/17/pykala/67","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/17/pykala/67/alkup/fin","title_fi":"Normaalista poikkeava säteilytilanne"}},{"@id":"sfsd:1991/592/luku/17/pykala/68/ajantasa/20180815","@type":"sfl:SectionVersion","amendedBy":"sfsd:2018/657/alkup","hasPart":{"@id":"sfsd:1991/592/luku/17/pykala/68/momentti/1/ajantasa/20180815","@type":"sfl:SubsectionVersion","cites":"sfsd:1999/621","isMemberOf":"sfsd:1991/592/luku/17/pykala/68/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/17/pykala/68/momentti/1/ajantasa/20180815/fin","hasFormat":{"@id":"sfsd:1991/592/luku/17/pykala/68/momentti/1/ajantasa/20180815/fin/txt","content_fi":"Viranomaisten toiminnan julkisuudesta annetussa laissa (621/1999) säädetyn salassapitovelvollisuuden estämättä saa tämän lain noudattamista valvottaessa sekä säteilyturvaneuvottelukunnan jäsenen ja sihteerin tehtävässä saatuja tietoja yksityisen ja yhteisön taloudellisesta asemasta sekä liikesalaisuudesta sekä yksityisen henkilökohtaisista oloista luovuttaa tämän lain 57 §:n mukaisesti sekä syyttäjä- ja poliisiviranomaiselle rikoksen selvittämiseksi."}}},"isMemberOf":"sfsd:1991/592/luku/17/pykala/68","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/17/pykala/68/ajantasa/20180815/fin","title_fi":"Salassa pidettävien tietojen luovuttaminen"}},{"@id":"sfsd:1991/592/luku/17/pykala/69/ajantasa/19950101","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1991/592/luku/17/pykala/69/momentti/1/ajantasa/19950101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:1994/1334/alkup","cites":"sfsd:1992/150","isMemberOf":"sfsd:1991/592/luku/17/pykala/69/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/17/pykala/69/momentti/1/ajantasa/19950101/fin","hasFormat":{"@id":"sfsd:1991/592/luku/17/pykala/69/momentti/1/ajantasa/19950101/fin/txt","content_fi":"Turvallisuusluvasta ja muista tämän lain nojalla annetuista viranomaisen päätöksistä sekä tässä laissa tarkoitettuun valvontaan kuuluvista suoritteista samoin kuin näiden päätösten ja suoritteiden valmistelun edellyttämistä toimenpiteistä voidaan periä maksuja sen mukaan kuin asianomaisen ministeriön päätöksellä päätetään. Maksuista on voimassa, mitä valtion maksuperustelaissa (150/92) on säädetty.\n(22.12.1994/1334)"}}},{"@id":"sfsd:1991/592/luku/17/pykala/69/momentti/2/ajantasa/19950101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:1994/1334/alkup","isMemberOf":"sfsd:1991/592/luku/17/pykala/69/momentti/2","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/17/pykala/69/momentti/2/ajantasa/19950101/fin","hasFormat":{"@id":"sfsd:1991/592/luku/17/pykala/69/momentti/2/ajantasa/19950101/fin/txt","content_fi":"Säteilytoiminnan valvonnasta aiheutuvista kustannuksista voidaan päättää, että ne peritään joko osaksi tai kokonaan vuotuisina maksuina toiminnan harjoittajilta.\n(22.12.1994/1334)"}}},{"@id":"sfsd:1991/592/luku/17/pykala/69/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/592/luku/17/pykala/69/momentti/3","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/17/pykala/69/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1991/592/luku/17/pykala/69/momentti/3/alkup/fin/txt","content_fi":"Tässä laissa säädetyt maksut ja korvaukset saadaan ulosottaa ilman tuomiota tai päätöstä siinä järjestyksessä kuin verojen ja maksujen perimisestä ulosottotoimin on säädetty."}}}],"isMemberOf":"sfsd:1991/592/luku/17/pykala/69","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/17/pykala/69/ajantasa/19950101/fin","title_fi":"Maksut"}},{"@id":"sfsd:1991/592/luku/17/pykala/70/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1991/592/luku/17/pykala/70/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/592/luku/17/pykala/70/momentti/1","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/17/pykala/70/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/592/luku/17/pykala/70/momentti/1/alkup/fin/txt","content_fi":"Tarkempia säännöksiä tämän lain täytäntöönpanosta sekä viranomaisten tehtäväjaosta ja yhteistyöstä annetaan asetuksella."}}},{"@id":"sfsd:1991/592/luku/17/pykala/70/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/592/luku/17/pykala/70/momentti/2","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/17/pykala/70/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1991/592/luku/17/pykala/70/momentti/2/alkup/fin/txt","content_fi":"Säteilyturvakeskuksen tulee antaa tämän lain mukaisen turvallisuustason toteuttamista koskevia yleisiä ohjeita."}}}],"isMemberOf":"sfsd:1991/592/luku/17/pykala/70","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/17/pykala/70/alkup/fin","title_fi":"Tarkemmat säännökset ja säteilyturvakeskuksen ohjeet"}},{"@id":"sfsd:1991/592/luku/17/pykala/71/ajantasa/20150701","@type":"sfl:SectionVersion","amendedBy":"sfsd:2015/677/alkup","hasPart":{"@id":"sfsd:1991/592/luku/17/pykala/71/momentti/1/ajantasa/20150701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/592/luku/17/pykala/71/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/17/pykala/71/momentti/1/ajantasa/20150701/fin","hasFormat":{"@id":"sfsd:1991/592/luku/17/pykala/71/momentti/1/ajantasa/20150701/fin/txt","content_fi":"Säteilyturvakeskuksen on ennen 2 §:n 2 momentin, 11 §:n 3 momentin, 23 §:n 3 momentin, 31 c §:n 2 momentin, 32 §:n 2 momentin, 32 a §:n 4 momentin, 36 §:n 3 momentin, 39 b §:n 3 momentin, 40 §:n 2 momentin, 45 §:n 3 momentin ja 50 §:n 4 momentissa tarkoitettujen määräysten antamista kuultava 20 §:ssä tarkoitettuja luvanhaltijoita, 7 §:ssä tarkoitettua neuvottelukuntaa ja sosiaali- ja terveysministeriötä, työ- ja elinkeinoministeriötä sekä tarvittavassa laajuudessa muita viranomaisia."}}},"isMemberOf":"sfsd:1991/592/luku/17/pykala/71","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/17/pykala/71/ajantasa/20150701/fin","title_fi":"Kuuleminen Säteilyturvakeskuksen antamista määräyksistä"}},{"@id":"sfsd:1991/592/luku/17/pykala/72/ajantasa/19970523","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1991/592/luku/17/pykala/72","repealedBy":"sfsd:1997/490/alkup","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/17/pykala/72/ajantasa/19970523/fin","hasFormat":{"@id":"sfsd:1991/592/luku/17/pykala/72/ajantasa/19970523/fin/txt","content_fi":"72 §(23.5.1997/490)\n\n72 § on kumottu L:lla 23.5.1997/490."}}}],"isMemberOf":"sfsd:1991/592/luku/17","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/17/ajantasa/20180815/fin","title_fi":"Erinäisiä säännöksiä"}},{"@id":"sfsd:1991/592/luku/18/ajantasa/19950101","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1991/592/luku/18/pykala/73/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1991/592/luku/18/pykala/73/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/592/luku/18/pykala/73/momentti/1","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/18/pykala/73/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/592/luku/18/pykala/73/momentti/1/alkup/fin/txt","content_fi":"Tämä laki tulee voimaan 1 päivänä tammikuuta 1992."}}},{"@id":"sfsd:1991/592/luku/18/pykala/73/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/592/luku/18/pykala/73/momentti/2","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/18/pykala/73/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1991/592/luku/18/pykala/73/momentti/2/alkup/fin/txt","content_fi":"Tällä lailla kumotaan 26 päivänä huhtikuuta 1957 annettu säteilysuojauslaki (174/57) siihen myöhemmin tehtyine muutoksineen."}}},{"@id":"sfsd:1991/592/luku/18/pykala/73/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/592/luku/18/pykala/73/momentti/3","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/18/pykala/73/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1991/592/luku/18/pykala/73/momentti/3/alkup/fin/txt","content_fi":"Ennen lain voimaantuloa voidaan ryhtyä lain täytäntöönpanemiseksi tarpeellisiin toimenpiteisiin."}}}],"isMemberOf":"sfsd:1991/592/luku/18/pykala/73","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/18/pykala/73/alkup/fin","title_fi":"Voimaantulo"}},{"@id":"sfsd:1991/592/luku/18/pykala/74/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1991/592/luku/18/pykala/74/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/592/luku/18/pykala/74/momentti/1","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/18/pykala/74/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/592/luku/18/pykala/74/momentti/1/alkup/fin/txt","content_fi":"Säteilysuojauslain nojalla myönnetty turvallisuuslupa on edelleen voimassa toistaiseksi. Luvanhaltijan on kuitenkin vuoden kuluessa tämän lain voimaantulosta ilmoitettava säteilyturvakeskukselle toiminnan jatkumisesta. Jos ilmoitusta ei tehdä määräajassa, lupa katsotaan rauenneeksi."}}},{"@id":"sfsd:1991/592/luku/18/pykala/74/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/592/luku/18/pykala/74/momentti/2","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/18/pykala/74/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1991/592/luku/18/pykala/74/momentti/2/alkup/fin/txt","content_fi":"Jos säteilysuojauslain nojalla myönnetyssä turvallisuusluvassa tarkoitettu toiminta ei täytä tässä laissa säädettyjä vaatimuksia, säteilyturvakeskuksen tulee viimeistään vuoden kuluessa 1 momentissa tarkoitetun ilmoituksen saapumisesta kehottaa luvanhaltijaa hakemaan uutta turvallisuuslupaa. Jos luvanhaltija ei noudata kehotusta neljän kuukauden kuluessa, turvallisuuslupa katsotaan rauenneeksi."}}},{"@id":"sfsd:1991/592/luku/18/pykala/74/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/592/luku/18/pykala/74/momentti/3","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/18/pykala/74/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1991/592/luku/18/pykala/74/momentti/3/alkup/fin/txt","content_fi":"Muissa kuin 1 ja 2 momentissa tarkoitetuissa tapauksissa on toiminnalle, joka tämän lain mukaan edellyttää turvallisuuslupaa ja jonka harjoittaminen on aloitettu ennen tämän lain voimaantuloa, haettava turvallisuuslupaa vuoden kuluessa tämän lain voimaantulosta."}}}],"isMemberOf":"sfsd:1991/592/luku/18/pykala/74","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/18/pykala/74/alkup/fin","title_fi":"Siirtymäsäännökset"}},{"@id":"sfsd:1991/592/luku/18/pykala/75/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1991/592/luku/18/pykala/75/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/592/luku/18/pykala/75/momentti/1","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/18/pykala/75/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/592/luku/18/pykala/75/momentti/1/alkup/fin/txt","content_fi":"Joka on hyväksytty säteilysuojausasetuksen (328/57) 5 §:ssä tarkoitetuksi johtajaksi tai hoitajaksi, on tämän lain voimaan tullessa oikeutettu edelleen jatkamaan tehtävässään."}}},"isMemberOf":"sfsd:1991/592/luku/18/pykala/75","version":"sfl:Original","versionDate":"2018-08-15"},{"@id":"sfsd:1991/592/luku/18/pykala/76/ajantasa/19950101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1991/592/luku/18/pykala/76","repealedBy":"sfsd:1994/1334/alkup","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/18/pykala/76/ajantasa/19950101/fin","hasFormat":{"@id":"sfsd:1991/592/luku/18/pykala/76/ajantasa/19950101/fin/txt","content_fi":"76 §(22.12.1994/1334)\n\n76 § on kumottu L:lla 22.12.1994/1334."}}},{"@id":"sfsd:1991/592/luku/18/pykala/77/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1991/592/luku/18/pykala/77/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/592/luku/18/pykala/77/momentti/1","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/18/pykala/77/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/592/luku/18/pykala/77/momentti/1/alkup/fin/txt","content_fi":"Kaivoslaissa tarkoitetun kaivostyön harjoittajan ja kauemmin kuin kolme kuukautta kestävän, maan alla tai suljetussa tilassa suoritettavan louhintatyön harjoittajan tulee kolmen kuukauden kuluessa tämän lain voimaantulosta tehdä säteilyturvakeskukselle kirjallinen ilmoitus toiminnastaan."}}},"isMemberOf":"sfsd:1991/592/luku/18/pykala/77","version":"sfl:Original","versionDate":"2018-08-15"},{"@id":"sfsd:1991/592/luku/18/pykala/78/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1991/592/luku/18/pykala/78/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/592/luku/18/pykala/78/momentti/1","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/18/pykala/78/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/592/luku/18/pykala/78/momentti/1/alkup/fin/txt","content_fi":"Tämän lain voimaan tullessa säteilyturvakeskuksessa tai muussa viranomaisessa vireillä oleva säteilysuojauslaissa tarkoitettu asia käsitellään tämän lain säännösten mukaisesti."}}},"isMemberOf":"sfsd:1991/592/luku/18/pykala/78","version":"sfl:Original","versionDate":"2018-08-15"}],"isMemberOf":"sfsd:1991/592/luku/18","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/luku/18/ajantasa/19950101/fin","title_fi":"Voimaantulo- ja siirtymäsäännökset"}}],"isAbout":["http://data.finlex.fi/voc/finlex/kw5102","http://data.finlex.fi/voc/finlex/kw3446","http://data.finlex.fi/voc/finlex/kw4159"],"isMemberOf":"sfsd:1991/592","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1991/592/ajantasa/20180815/fin","title_fi":"Säteilylaki"}},"@context":{"languageVersion":{"@id":"eli:is_realized_by","@type":"@id"},"temporalVersions":{"@id":"eli:has_member","@type":"@id"},"temporalVersion":{"@id":"eli:has_member","@type":"@id"},"hasFormat":{"@id":"eli:is_embodied_by","@type":"@id"},"idLocal":"eli:id_local","dateDocument":{"@id":"eli:date_document","@type":"xsd:date"},"hasPart":{"@id":"eli:has_part","@type":"@id"},"isAbout":{"@id":"eli:is_about","@type":"@id"},"isMemberOf":{"@id":"eli:is_member_of","@type":"@id"},"version":{"@id":"eli:version","@type":"@id"},"versionDate":{"@id":"eli:version_date","@type":"xsd:date"},"title_fi":{"@id":"eli:title","@language":"fi"},"content_fi":{"@id":"sfl:text","@language":"fi"},"amendedBy":{"@id":"eli:amended_by","@type":"@id"},"cites":{"@id":"eli:cites","@type":"@id"},"repealedBy":{"@id":"eli:repealed_by