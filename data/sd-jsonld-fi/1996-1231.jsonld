rOf":"sfsd:1996/1231/luku/5/pykala/26a/momentti/2/kohta/5","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/5/pykala/26a/momentti/2/kohta/5/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/5/pykala/26a/momentti/2/kohta/5/ajantasa/20160101/fin/txt","content_fi":"5) lomittajan hoitamat tehtävät;"}}},{"@id":"sfsd:1996/1231/luku/5/pykala/26a/momentti/2/kohta/6/ajantasa/20160101","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/1231/luku/5/pykala/26a/momentti/2/kohta/6","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/5/pykala/26a/momentti/2/kohta/6/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/5/pykala/26a/momentti/2/kohta/6/ajantasa/20160101/fin/txt","content_fi":"6) lomituksen järjestämisestä sovittu tuntihinta;"}}},{"@id":"sfsd:1996/1231/luku/5/pykala/26a/momentti/2/kohta/7/ajantasa/20160101","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/1231/luku/5/pykala/26a/momentti/2/kohta/7","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/5/pykala/26a/momentti/2/kohta/7/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/5/pykala/26a/momentti/2/kohta/7/ajantasa/20160101/fin/txt","content_fi":"7) muut korvauksen maksamiseksi tarvittavat tiedot."}}}],"isMemberOf":"sfsd:1996/1231/luku/5/pykala/26a/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01"},{"@id":"sfsd:1996/1231/luku/5/pykala/26a/momentti/3/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/5/pykala/26a/momentti/3","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/5/pykala/26a/momentti/3/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/5/pykala/26a/momentti/3/ajantasa/20160101/fin/txt","content_fi":"Hakemukseen on liitettävä palvelujen tuottajan vahvistus 2 momentin 1–6 kohdassa tarkoitettujen tietojen oikeellisuudesta."}}},{"@id":"sfsd:1996/1231/luku/5/pykala/26a/momentti/4/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/5/pykala/26a/momentti/4","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/5/pykala/26a/momentti/4/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/5/pykala/26a/momentti/4/ajantasa/20160101/fin/txt","content_fi":"Paikallisyksikön on maksettava korvaus palvelujen tuottajan ilmoittamalle Euroopan unionin jäsenvaltiossa sijaitsevassa pankissa olevalle tilille."}}}],"isMemberOf":"sfsd:1996/1231/luku/5/pykala/26a","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/5/pykala/26a/ajantasa/20160101/fin","title_fi":"Korvauksen hakeminen ja maksaminen"}}],"isMemberOf":"sfsd:1996/1231/luku/5","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/5/ajantasa/20160101/fin","title_fi":"Maatalousyrittäjän itse järjestämä lomitus"}},{"@id":"sfsd:1996/1231/luku/6/ajantasa/20180101","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1996/1231/luku/6/pykala/27/ajantasa/20180101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2015/1663/alkup","hasPart":[{"@id":"sfsd:1996/1231/luku/6/pykala/27/momentti/1/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/27/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/27/momentti/1/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/27/momentti/1/ajantasa/20160101/fin/txt","content_fi":"Sijaisavusta peritään palvelun saajalta tuntimaksu, joka ei saa ylittää palvelun tuottamisesta aiheutuneita kustannuksia. Tuntimaksu on 3 euroa lomitustunnilta lisättynä määrällä, joka saadaan jakamalla sijaisavun saajan maatalousyrittäjän eläkelain 14 §:n mukainen työtulo euroina 5 400:lla, jollei jäljempänä tässä laissa toisin säädetä. Maksu määrätään sen työtulon mukaan, joka sijaisavun saajalla oli voimassa sijaisavun alkamisvuoden ensimmäisenä päivänä. Jollei tällaista työtuloa ole, maksu määrätään sijaisavun saajan viimeksi voimassa olleen indeksillä korotetun työtulon mukaan. Jos sijaisavun saajalla ei ole ollut maatalousyrittäjän eläkelain 10 §:n mukaista vakuutusta, maksun perusteena on mainitussa laissa säädettyjä perusteita noudattaen arvioitu alkavan yrittäjän vuotuinen työtulo."}}},{"@id":"sfsd:1996/1231/luku/6/pykala/27/momentti/2/ajantasa/20180101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2017/992/alkup","isMemberOf":"sfsd:1996/1231/luku/6/pykala/27/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/27/momentti/2/ajantasa/20180101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/27/momentti/2/ajantasa/20180101/fin/txt","content_fi":"Työtulon perusteella 1 momentin mukaan määrättyä tuntimaksua alennetaan 4,59 prosentilla vuonna 2018 pidetyssä sijaisavussa.\n(19.12.2017/992)"}}}],"isMemberOf":"sfsd:1996/1231/luku/6/pykala/27","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/27/ajantasa/20180101/fin","title_fi":"Sijaisavusta perittävät maksut"}},{"@id":"sfsd:1996/1231/luku/6/pykala/27a/ajantasa/20160101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2015/1663/alkup","hasPart":{"@id":"sfsd:1996/1231/luku/6/pykala/27a/momentti/1/ajantasa/20160101","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1996/1231/luku/6/pykala/27a/momentti/1/johdanto/ajantasa/20160101","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/27a/momentti/1/johdanto","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/27a/momentti/1/johdanto/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/27a/momentti/1/johdanto/ajantasa/20160101/fin/txt","content_fi":"Sijaisavusta peritään 27 §:n mukainen maksu 50 prosentilla korotettuna:"}}},{"@id":"sfsd:1996/1231/luku/6/pykala/27a/momentti/1/kohta/1/ajantasa/20160101","@type":"sfl:ParagraphVersion","cites":"sfsd:2006/1272","isMemberOf":"sfsd:1996/1231/luku/6/pykala/27a/momentti/1/kohta/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/27a/momentti/1/kohta/1/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/27a/momentti/1/kohta/1/ajantasa/20160101/fin/txt","content_fi":"1) kun sijaisavun saaja on sijaisavun alkaessa oikeutettu palkkaetuuksiin työ- tai virkasuhteen perusteella tai kun hänellä on voimassa yrittäjän eläkelaissa (1272/2006) tarkoitettu vakuutus;"}}},{"@id":"sfsd:1996/1231/luku/6/pykala/27a/momentti/1/kohta/2/ajantasa/20160101","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/27a/momentti/1/kohta/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/27a/momentti/1/kohta/2/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/27a/momentti/1/kohta/2/ajantasa/20160101/fin/txt","content_fi":"2) kun sijaisapua annetaan 7 d §:n nojalla."}}}],"isMemberOf":"sfsd:1996/1231/luku/6/pykala/27a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01"},"isMemberOf":"sfsd:1996/1231/luku/6/pykala/27a","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/27a/ajantasa/20160101/fin","title_fi":"Korotettu sijaisapumaksu"}},{"@id":"sfsd:1996/1231/luku/6/pykala/27b/ajantasa/20180101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2015/1663/alkup","hasPart":[{"@id":"sfsd:1996/1231/luku/6/pykala/27b/momentti/1/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/27b/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/27b/momentti/1/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/27b/momentti/1/ajantasa/20160101/fin/txt","content_fi":"Sijaisavun saajalta peritään 7 i §:n perusteella annetusta sijaisavusta 13,50 euroa tunnilta."}}},{"@id":"sfsd:1996/1231/luku/6/pykala/27b/momentti/2/ajantasa/20180101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2017/992/alkup","isMemberOf":"sfsd:1996/1231/luku/6/pykala/27b/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/27b/momentti/2/ajantasa/20180101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/27b/momentti/2/ajantasa/20180101/fin/txt","content_fi":"Sen estämättä, mitä 1 momentissa säädetään, sijaisavun saajalta peritään 7 i §:n perusteella vuonna 2018 pidetystä sijaisavusta 12 euroa tunnilta. Sen estämättä, mitä 47 b §:n 2 momentissa säädetään, tässä momentissa mainittu rahamäärä vastaa vuoden 2018 palkkakertoimen tasoa.\n(19.12.2017/992)"}}}],"isMemberOf":"sfsd:1996/1231/luku/6/pykala/27b","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/27b/ajantasa/20180101/fin","title_fi":"Yritystoiminnan järjestelyjä varten annettavasta sijaisavusta perittävä maksu"}},{"@id":"sfsd:1996/1231/luku/6/pykala/27c/ajantasa/20020101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2001/1135/alkup","hasPart":[{"@id":"sfsd:1996/1231/luku/6/pykala/27c/momentti/1/ajantasa/20020101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/27c/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/27c/momentti/1/ajantasa/20020101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/27c/momentti/1/ajantasa/20020101/fin/txt","content_fi":"Jos sijaisavun perusteena on raskaus ja synnytys, sijaisavusta peritään sairausvakuutuslaissa tarkoitetun äitiysrahakauden ajalta edellä tässä laissa tarkoitettu maksu 50 prosentilla alennettuna."}}},{"@id":"sfsd:1996/1231/luku/6/pykala/27c/momentti/2/ajantasa/20020101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/27c/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/27c/momentti/2/ajantasa/20020101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/27c/momentti/2/ajantasa/20020101/fin/txt","content_fi":"Sijaisavusta määrättyä maksua voidaan alentaa, jos sijaisavun saajan ja hänen perheensä toimeentuloedellytykset huomioon ottaen siihen on syytä."}}}],"isMemberOf":"sfsd:1996/1231/luku/6/pykala/27c","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/27c/ajantasa/20020101/fin","title_fi":"Alennettu sijaisapumaksu"}},{"@id":"sfsd:1996/1231/luku/6/pykala/28/ajantasa/20160101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2015/1663/alkup","hasPart":[{"@id":"sfsd:1996/1231/luku/6/pykala/28/momentti/1/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/28/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/28/momentti/1/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/28/momentti/1/ajantasa/20160101/fin/txt","content_fi":"Maatalousyrittäjältä peritään 16 b §:n 1 momentissa ja 24 §:n 2 momentissa tarkoitetusta lomittaja-avusta 13,75 euroa tunnilta."}}},{"@id":"sfsd:1996/1231/luku/6/pykala/28/momentti/2/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/28/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/28/momentti/2/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/28/momentti/2/ajantasa/20160101/fin/txt","content_fi":"Maatalousyrittäjältä peritään 16 b §:n 2 momentissa tarkoitetusta lomittaja-avusta 35 euroa tunnilta."}}}],"isMemberOf":"sfsd:1996/1231/luku/6/pykala/28","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/28/ajantasa/20160101/fin","title_fi":"Maksullisesta lomittaja-avusta perittävät maksut"}},{"@id":"sfsd:1996/1231/luku/6/pykala/28/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2015/1663/alkup","hasPart":[{"@id":"sfsd:1996/1231/luku/6/pykala/28/momentti/1/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/28/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/28/momentti/1/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/28/momentti/1/ajantasa/20160101/fin/txt","content_fi":"Maatalousyrittäjältä peritään 16 b §:n 1 momentissa ja 24 §:n 2 momentissa tarkoitetusta lomittaja-avusta 13,75 euroa tunnilta."}}},{"@id":"sfsd:1996/1231/luku/6/pykala/28/momentti/2/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/28/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/28/momentti/2/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/28/momentti/2/ajantasa/20160101/fin/txt","content_fi":"Maatalousyrittäjältä peritään 16 b §:n 2 momentissa tarkoitetusta lomittaja-avusta 35 euroa tunnilta."}}},{"@id":"sfsd:1996/1231/luku/6/pykala/28/momentti/3/ajantasa/20170101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2016/1345/alkup","isMemberOf":"sfsd:1996/1231/luku/6/pykala/28/momentti/3","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/28/momentti/3/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/28/momentti/3/ajantasa/20170101/fin/txt","content_fi":"Sen estämättä, mitä 1 momentissa säädetään, maatalousyrittäjältä peritään 16 b §:n 1 momentissa ja 24 §:n 2 momentissa tarkoitetusta vuonna 2017 pidetystä lomittaja-avusta 12 euroa tunnilta. Sen estämättä, mitä 47 b §:n 2 momentissa säädetään, tässä momentissa mainittu rahamäärä vastaa vuoden 2017 palkkakertoimen tasoa.\n(21.12.2016/1345)"}}}],"isMemberOf":"sfsd:1996/1231/luku/6/pykala/28","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/28/ajantasa/20170101/fin","title_fi":"Maksullisesta lomittaja-avusta perittävät maksut"}},{"@id":"sfsd:1996/1231/luku/6/pykala/28a/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2015/1663/alkup","hasPart":[{"@id":"sfsd:1996/1231/luku/6/pykala/28a/momentti/1/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/28a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/28a/momentti/1/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/28a/momentti/1/ajantasa/20160101/fin/txt","content_fi":"Maatalousyrittäjältä peritään paikallisyksikön järjestämästä valvontakäynnistä 33 euroa käyntikerralta."}}},{"@id":"sfsd:1996/1231/luku/6/pykala/28a/momentti/2/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/28a/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/28a/momentti/2/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/28a/momentti/2/ajantasa/20160101/fin/txt","content_fi":"Maatalousyrittäjältä peritään paikallisyksikön järjestämästä varallaolosta 5 euroa tunnilta. Jos varallaolo aiheuttaa käynnin maatalousyrityksessä, käynnistä peritään 33 euroa käyntikerralta."}}},{"@id":"sfsd:1996/1231/luku/6/pykala/28a/momentti/3/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/28a/momentti/3","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/28a/momentti/3/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/28a/momentti/3/ajantasa/20160101/fin/txt","content_fi":"Edellä 1 tai 2 momentissa tarkoitetulla käynnillä voidaan huolehtia vain tehtävistä, jotka ovat käyntihetkellä eläinsuojelullisista syistä välttämättömiä. Jos lomittajan on eläinsuojelullisista syistä työskenneltävä maatalousyrityksessä pitempään kuin tunnin, maatalousyrittäjältä peritään yhden tunnin ylittävältä ajalta 33 euroa tunnilta."}}},{"@id":"sfsd:1996/1231/luku/6/pykala/28a/momentti/4/ajantasa/20170101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2016/1345/alkup","isMemberOf":"sfsd:1996/1231/luku/6/pykala/28a/momentti/4","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/28a/momentti/4/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/28a/momentti/4/ajantasa/20170101/fin/txt","content_fi":"Sen estämättä, mitä 2 momentin ensimmäisessä virkkeessä säädetään, maatalousyrittäjältä peritään paikallisyksikön vuonna 2017 järjestämästä varallaolosta 1,50 euroa tunnilta. Sen estämättä, mitä 47 b §:n 2 momentissa säädetään, tässä momentissa mainittu rahamäärä vastaa vuoden 2017 palkkakertoimen tasoa.\n(21.12.2016/1345)"}}}],"isMemberOf":"sfsd:1996/1231/luku/6/pykala/28a","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/28a/ajantasa/20170101/fin","title_fi":"Valvontakäynneistä ja varallaolosta perittävät maksut"}},{"@id":"sfsd:1996/1231/luku/6/pykala/28a/ajantasa/20160101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2015/1663/alkup","hasPart":[{"@id":"sfsd:1996/1231/luku/6/pykala/28a/momentti/1/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/28a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/28a/momentti/1/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/28a/momentti/1/ajantasa/20160101/fin/txt","content_fi":"Maatalousyrittäjältä peritään paikallisyksikön järjestämästä valvontakäynnistä 33 euroa käyntikerralta."}}},{"@id":"sfsd:1996/1231/luku/6/pykala/28a/momentti/2/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/28a/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/28a/momentti/2/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/28a/momentti/2/ajantasa/20160101/fin/txt","content_fi":"Maatalousyrittäjältä peritään paikallisyksikön järjestämästä varallaolosta 5 euroa tunnilta. Jos varallaolo aiheuttaa käynnin maatalousyrityksessä, käynnistä peritään 33 euroa käyntikerralta."}}},{"@id":"sfsd:1996/1231/luku/6/pykala/28a/momentti/3/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/28a/momentti/3","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/28a/momentti/3/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/28a/momentti/3/ajantasa/20160101/fin/txt","content_fi":"Edellä 1 tai 2 momentissa tarkoitetulla käynnillä voidaan huolehtia vain tehtävistä, jotka ovat käyntihetkellä eläinsuojelullisista syistä välttämättömiä. Jos lomittajan on eläinsuojelullisista syistä työskenneltävä maatalousyrityksessä pitempään kuin tunnin, maatalousyrittäjältä peritään yhden tunnin ylittävältä ajalta 33 euroa tunnilta."}}}],"isMemberOf":"sfsd:1996/1231/luku/6/pykala/28a","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/28a/ajantasa/20160101/fin","title_fi":"Valvontakäynneistä ja varallaolosta perittävät maksut"}},{"@id":"sfsd:1996/1231/luku/6/pykala/29/ajantasa/20160101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2015/1663/alkup","hasPart":[{"@id":"sfsd:1996/1231/luku/6/pykala/29/momentti/1/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/29/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/29/momentti/1/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/29/momentti/1/ajantasa/20160101/fin/txt","content_fi":"Jos vuosilomaa, sijaisapua tai 16 b §:n 1 momentissa tarkoitettua maksullista lomittaja-apua on annettu perusteettomasti tai itse järjestetystä lomituksesta on maksettu korvausta perusteettomasti, palvelun saajan on korvattava paikallisyksikölle siitä aiheutuneet kustannukset. Paikallisyksikön järjestämässä lomituksessa paikallisyksikölle aiheutuneena kustannuksena pidetään 33 euroa tunnilta vähennettynä palvelun saajalta palvelusta perityllä maksulla. Varallaolosta aiheutuneena kustannuksena pidetään kuitenkin viittä euroa tunnilta vähennettynä palvelun saajalta palvelusta perityllä maksulla."}}},{"@id":"sfsd:1996/1231/luku/6/pykala/29/momentti/2/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/29/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/29/momentti/2/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/29/momentti/2/ajantasa/20160101/fin/txt","content_fi":"Jos 11 a §:ssä tarkoitettu ehdollinen päätös raukeaa, palvelun saajan on korvattava päätöksen nojalla annetuista lomituspalveluista paikallisyksikölle aiheutuneet kustannukset 1 momentissa säädetyin perustein."}}},{"@id":"sfsd:1996/1231/luku/6/pykala/29/momentti/3/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/29/momentti/3","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/29/momentti/3/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/29/momentti/3/ajantasa/20160101/fin/txt","content_fi":"Takaisinperinnästä voidaan luopua osittain tai kokonaan, jos se katsotaan kohtuulliseksi eikä perusteeton etu ole aiheutunut palvelun saajan taikka hänen edustajansa vilpillisestä menettelystä. Takaisinperinnästä voidaan luopua myös silloin, kun takaisin perittävä määrä on vähäinen."}}},{"@id":"sfsd:1996/1231/luku/6/pykala/29/momentti/4/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/29/momentti/4","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/29/momentti/4/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/29/momentti/4/ajantasa/20160101/fin/txt","content_fi":"Takaisinperintä on kuitenkin toteutettava täysimääräisenä 2 momentissa tarkoitetussa tilanteessa taikka jos takaisinperintä johtuu siitä, ettei annettu palvelu tai maksettu korvaus ole täyttänyt niitä maa- ja metsätalousalan valtiontukisääntöjä, joiden perusteella lomitusjärjestelmä on hyväksytty ja jos Euroopan unionin lainsäädäntö tätä edellyttää."}}},{"@id":"sfsd:1996/1231/luku/6/pykala/29/momentti/5/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/29/momentti/5","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/29/momentti/5/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/29/momentti/5/ajantasa/20160101/fin/txt","content_fi":"Päätös takaisinperinnästä on tehtävä viiden vuoden kuluessa perusteettoman lomituspalvelun antamisesta tai perusteettoman korvauksen maksamisesta, jollei Euroopan unionin lainsäädännöstä muuta johdu."}}}],"isMemberOf":"sfsd:1996/1231/luku/6/pykala/29","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/29/ajantasa/20160101/fin","title_fi":"Perusteettomasti annetun palvelun korvaaminen ja perusteettomasti maksetun korvauksen palauttaminen"}},{"@id":"sfsd:1996/1231/luku/6/pykala/30/ajantasa/20160101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2015/1663/alkup","hasPart":[{"@id":"sfsd:1996/1231/luku/6/pykala/30/momentti/1/ajantasa/20160101","@type":"sfl:SubsectionVersion","cites":["sfsd:1982/633/pykala/4","sfsd:1982/633/pykala/4a"],"isMemberOf":"sfsd:1996/1231/luku/6/pykala/30/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/30/momentti/1/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/30/momentti/1/ajantasa/20160101/fin/txt","content_fi":"Jos sijaisavusta, maksullisesta lomittaja-avusta tai valvontakäynneistä ja varallaolosta määrättyä maksua ei ole maksettu viimeistään eräpäivänä, saadaan viivästyskorkoa periä eräpäivästä lukien korkolain (633/1982) 4 a §:n 1 momentissa säädetyin perustein. Jos 29 §:ssä tarkoitettua korvausta ei ole maksettu viimeistään eräpäivänä, saadaan viivästyskorkoa periä eräpäivästä lukien korkolain 4 §:n 1 momentissa säädetyin perustein. Viivästyskoron perusteena oleva eräpäivä voi olla aikaisintaan kahden viikon kuluttua maksu- tai takaisinperintäpäätöksen antamisesta."}}},{"@id":"sfsd:1996/1231/luku/6/pykala/30/momentti/2/ajantasa/20160101","@type":"sfl:SubsectionVersion","cites":"sfsd:2007/706","isMemberOf":"sfsd:1996/1231/luku/6/pykala/30/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/30/momentti/2/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/30/momentti/2/ajantasa/20160101/fin/txt","content_fi":"Edellä 1 momentissa tarkoitettu maksu ja korvaus viivästyskorkoineen ovat suoraan ulosottokelpoisia. Niiden perimisestä säädetään verojen ja maksujen täytäntöönpanosta annetussa laissa (706/2007)."}}}],"isMemberOf":"sfsd:1996/1231/luku/6/pykala/30","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/30/ajantasa/20160101/fin","title_fi":"Viivästyskorko ja maksuvelvoitteen täytäntöönpano"}},{"@id":"sfsd:1996/1231/luku/6/pykala/30a/ajantasa/20160101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2015/1663/alkup","hasPart":{"@id":"sfsd:1996/1231/luku/6/pykala/30a/momentti/1/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/6/pykala/30a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/30a/momentti/1/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/6/pykala/30a/momentti/1/ajantasa/20160101/fin/txt","content_fi":"Yrittäjä on vastuussa samassa maatalousyrityksessä lomituspalveluja saaneen henkilön tämän lain mukaisista maksuista ja korvauksista niin kuin omasta velastaan. Tässä tarkoitettua vastuuta ei kuitenkaan ole sellaisella yrittäjällä, joka ei harjoita yrittäjätoimintaa omistus- tai vuokraoikeuden perusteella hallitsemassaan yrityksessä. Yhtymänä, avoimena yhtiönä, kommandiittiyhtiönä, osakeyhtiönä tai osuuskuntana toimiva maatalousyritys vastaa osakkaittensa maksuista ja korvauksista niin kuin omasta velastaan."}}},"isMemberOf":"sfsd:1996/1231/luku/6/pykala/30a","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/pykala/30a/ajantasa/20160101/fin","title_fi":"Yrittäjän vastuu maksuista ja korvauksista"}}],"isMemberOf":"sfsd:1996/1231/luku/6","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/6/ajantasa/20180101/fin","title_fi":"Palveluista perittävät maksut ja korvaukset"}},{"@id":"sfsd:1996/1231/luku/7/ajantasa/20160101","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1996/1231/luku/7/pykala/31/ajantasa/20040701","@type":"sfl:SectionVersion","amendedBy":"sfsd:2004/458/alkup","hasPart":[{"@id":"sfsd:1996/1231/luku/7/pykala/31/momentti/1/ajantasa/20040701","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1996/1231/luku/7/pykala/31/momentti/1/johdanto/ajantasa/20040701","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1996/1231/luku/7/pykala/31/momentti/1/johdanto","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/7/pykala/31/momentti/1/johdanto/ajantasa/20040701/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/7/pykala/31/momentti/1/johdanto/ajantasa/20040701/fin/txt","content_fi":"Valtion varoista suoritetaan eläkelaitokselle vuosittain laskennallisin perustein määräytyvä kohtuullinen korvaus:"}}},{"@id":"sfsd:1996/1231/luku/7/pykala/31/momentti/1/kohta/1/ajantasa/20040701","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/1231/luku/7/pykala/31/momentti/1/kohta/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/7/pykala/31/momentti/1/kohta/1/ajantasa/20040701/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/7/pykala/31/momentti/1/kohta/1/ajantasa/20040701/fin/txt","content_fi":"1) tämän lain mukaisten tehtävien hoitamisesta eläkelaitokselle aiheutuviin hallintokustannuksiin;"}}},{"@id":"sfsd:1996/1231/luku/7/pykala/31/momentti/1/kohta/2/ajantasa/20040701","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/1231/luku/7/pykala/31/momentti/1/kohta/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/7/pykala/31/momentti/1/kohta/2/ajantasa/20040701/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/7/pykala/31/momentti/1/kohta/2/ajantasa/20040701/fin/txt","content_fi":"2) niihin lisäkustannuksiin, joita eläkelaitokselle aiheutuu toimeksiantosopimuksen 12 §:n 3 momentin mukaisesta sopimusehdosta siltä osin kuin kunnalle 33 §:n nojalla maksettava korvaus on riittämätön; sekä"}}},{"@id":"sfsd:1996/1231/luku/7/pykala/31/momentti/1/kohta/3/ajantasa/20040701","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/1231/luku/7/pykala/31/momentti/1/kohta/3","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/7/pykala/31/momentti/1/kohta/3/ajantasa/20040701/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/7/pykala/31/momentti/1/kohta/3/ajantasa/20040701/fin/txt","content_fi":"3) niihin eläkelaitokselle aiheutuviin lisäkustannuksiin, jotka johtuvat paikallishallinnon järjestämisestä eläkelaitoksen oman paikallisyksikön avulla, siltä osin kuin eläkelaitokselle 33 §:n nojalla maksettava korvaus on riittämätön; lisäkorvausta voidaan maksaa vain, jos hallintotehtävien asianmukaiseen hoitamiseen vaikuttavat olosuhteet eläkelaitoksen ylläpitämän paikallisyksikön alueella poikkeavat ilmeisesti ja merkittävästi muiden paikallisyksikköjen vastaavista olosuhteista, minkä vuoksi hallintotehtävistä aiheutuvat kustannukset ovat tavanomaista korkeammat; lisäedellytyksenä on, että 33 §:ssä säädettyä suurempi korvaus harkitaan välttämättömäksi, jotta tämän lain mukaiset maatalousyrittäjän oikeudet lomituspalveluihin voidaan toteuttaa."}}}],"isMemberOf":"sfsd:1996/1231/luku/7/pykala/31/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01"},{"@id":"sfsd:1996/1231/luku/7/pykala/31/momentti/2/ajantasa/20040701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/7/pykala/31/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/7/pykala/31/momentti/2/ajantasa/20040701/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/7/pykala/31/momentti/2/ajantasa/20040701/fin/txt","content_fi":"Asianomainen ministeriö päättää vuosittain eläkelaitoksen esityksestä 1 momentissa tarkoitettujen korvausten määristä sekä 1 momentin 2 ja 3 kohdassa tarkoitettujen korvausten kohdentumisesta."}}}],"isMemberOf":"sfsd:1996/1231/luku/7/pykala/31","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/7/pykala/31/ajantasa/20040701/fin","title_fi":"Eläkelaitoksen hallintokustannukset"}},{"@id":"sfsd:1996/1231/luku/7/pykala/32/ajantasa/20110101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/1161/alkup","hasPart":[{"@id":"sfsd:1996/1231/luku/7/pykala/32/momentti/1/ajantasa/20110101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/7/pykala/32/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/7/pykala/32/momentti/1/ajantasa/20110101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/7/pykala/32/momentti/1/ajantasa/20110101/fin/txt","content_fi":"Valtion varoista maksetaan vuosittain kohtuullinen korvaus niistä kustannuksista, jotka toimeksiantosopimuksen tehneille kunnille ja eläkelaitokselle aiheutuvat tämän lain mukaisten paikallishallintotehtävien hoitamisesta. Valtioneuvosto vahvistaa vuosittain syyskuun loppuun mennessä eläkelaitoksen esityksestä korvauksen kokonaismäärän seuraavaa kalenterivuotta varten."}}},{"@id":"sfsd:1996/1231/luku/7/pykala/32/momentti/2/ajantasa/20110101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/7/pykala/32/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/7/pykala/32/momentti/2/ajantasa/20110101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/7/pykala/32/momentti/2/ajantasa/20110101/fin/txt","content_fi":"Korvauksen maksamisesta, jaksottamisesta ja maksamisajankohdasta säädetään valtioneuvoston asetuksella."}}}],"isMemberOf":"sfsd:1996/1231/luku/7/pykala/32","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/7/pykala/32/ajantasa/20110101/fin","title_fi":"Paikallishallintokustannukset"}},{"@id":"sfsd:1996/1231/luku/7/pykala/33/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1996/1231/luku/7/pykala/33/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/7/pykala/33/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/7/pykala/33/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/7/pykala/33/momentti/1/alkup/fin/txt","content_fi":"Paikallishallintokustannuksiin tarkoitetun korvauksen suuruus paikallisyksikköä kohti määräytyy paikallisyksikön toimialueella sijaitsevissa kunnissa edellistä varainhoitovuotta edeltäneen vuoden aikana lomitettujen päivien määrän perusteella."}}},"isMemberOf":"sfsd:1996/1231/luku/7/pykala/33","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/7/pykala/33/alkup/fin","title_fi":"Paikallishallintokustannusten korvauksen määräytyminen"}},{"@id":"sfsd:1996/1231/luku/7/pykala/34/ajantasa/20040701","@type":"sfl:SectionVersion","amendedBy":"sfsd:2004/458/alkup","hasPart":{"@id":"sfsd:1996/1231/luku/7/pykala/34/momentti/1/ajantasa/20040701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/7/pykala/34/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/7/pykala/34/momentti/1/ajantasa/20040701/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/7/pykala/34/momentti/1/ajantasa/20040701/fin/txt","content_fi":"Eläkelaitos maksaa toimeksiantosopimuksen tehneelle kunnalle varainhoitovuoden alusta kuukausittain, viimeistään kunkin kuukauden 11 päivänä, kahdennentoista osan siitä korvauksesta, johon kunnalla on oikeus 33 §:n ja toimeksiantosopimuksen perusteella."}}},"isMemberOf":"sfsd:1996/1231/luku/7/pykala/34","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/7/pykala/34/ajantasa/20040701/fin","title_fi":"Paikallishallintokustannusten korvaaminen toimeksiantosopimuksen tehneelle kunnalle"}},{"@id":"sfsd:1996/1231/luku/7/pykala/35/ajantasa/20160101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2015/1663/alkup","hasPart":[{"@id":"sfsd:1996/1231/luku/7/pykala/35/momentti/1/ajantasa/20160101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/1231/luku/7/pykala/35/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/7/pykala/35/momentti/1/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/7/pykala/35/momentti/1/ajantasa/20160101/fin/txt","content_fi":"Valtion varoista suoritetaan eläkelaitokselle tämän lain mukaisista lomituspalveluista aiheutuvat, toiminnan asianmukaisen hoitamisen kannalta tarpeellisina pidettävät kustannukset vähennettyinä 36 §:ssä mainituilla erillä."}}},{"@id":"sfsd:1996/1231/luku/7/pykala/35/momentti/2/ajantasa/20160101","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1996/1231/luku/7/pykala/35/momentti/2/johdanto/ajantasa/20160101","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1996/1231/luku/7/pykala/35/momentti/2/johdanto","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/7/pykala/35/momentti/2/johdanto/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/7/pykala/35/momentti/2/johdanto/ajantasa/20160101/fin/txt","content_fi":"Edellä 1 momentissa tarkoitettuina kustannuksina korvataan:"}}},{"@id":"sfsd:1996/1231/luku/7/pykala/35/momentti/2/kohta/1/ajantasa/20160101","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/1231/luku/7/pykala/35/momentti/2/kohta/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/1231/luku/7/pykala/35/momentti/2/kohta/1/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1996/1231/luku/7/pykala/35/momentti/2/kohta/1/ajantasa/20160101/fin/txt","content_fi":"1) 15 §:n 2 momentin 1 kohdassa ja 3 momentissa tarkoitettujen maatalouslomittajien palkkauksesta ja muista eduista aiheutuvat kustannukset enintään määrään, jota on pidettävä toiminnan tarkoituksenmukaisen ja taloudellisen hoitamisen kannalta tarpeellisena ja jota kulloinkin voimassa oleva lomittajia koskeva työehtosopimus ja muut työnantajaa velvoittavat sopimukset edellyttävät;"}}},{"@id":"sfsd:1996/1231/luku/7/pykala/35/momentti/2/kohta/2/ajantasa/20160101","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/1231/luku/7/pykala/35/momentti/2/kohta/2","vers