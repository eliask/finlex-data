{"@id":"sfsd:2013/401","@type":["http://data.finlex.fi/schema/sfl/NewStatute"],"idLocal":["401/2013"],"inForce":["http://data.europa.eu/eli/ontology#InForce-inForce"],"temporalVersions":["http://data.finlex.fi/eli/sd/2013/401/alkup"],"typeDocument":["http://data.finlex.fi/schema/sfl/Act"],"temporalVersion":{"@id":"sfsd:2013/401/alkup","@type":"sfl:StatuteVersion","dateDocument":"2013-06-07","datePublication":"2013-06-12","firstDateEntryInForce":"2014-01-01","hasPart":[{"@id":"sfsd:2013/401/johtolause/alkup","@type":"sfl:EnactingClauseVersion","isMemberOf":"sfsd:2013/401/johtolause","version":"sfl:Original","versionDate":"2013-06-07","languageVersion":{"@id":"sfsd:2013/401/johtolause/alkup/fin","hasFormat":{"@id":"sfsd:2013/401/johtolause/alkup/fin/txt","content_fi":"Eduskunnan päätöksen mukaisesti säädetään:"}}},{"@id":"sfsd:2013/401/pykala/1/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2013/401/pykala/1/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2013/401/pykala/1/momentti/1","version":"sfl:Original","versionDate":"2013-06-07","languageVersion":{"@id":"sfsd:2013/401/pykala/1/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2013/401/pykala/1/momentti/1/alkup/fin/txt","content_fi":"Saimaan saaristoluonnon, saimaannorpan elinympäristön ja Etelä-Suomen metsä- ja kallioluonnon suojelemiseksi ja hoitamiseksi sekä luonnonharrastusta, virkistystä ja ympäristöntutkimusta varten perustetaan luonnonsuojelulain (1096/1996) mukainen Koloveden kansallispuisto."}}},"isMemberOf":"sfsd:2013/401/pykala/1","version":"sfl:Original","versionDate":"2013-06-07","languageVersion":{"@id":"sfsd:2013/401/pykala/1/alkup/fin","title_fi":"Perustamistarkoitus"}},{"@id":"sfsd:2013/401/liite/1/alkup","@type":"sfl:AttachmentVersion","isMemberOf":"sfsd:2013/401/liite/1","version":"sfl:Original","versionDate":"2013-06-07","languageVersion":{"@id":"sfsd:2013/401/liite/1/alkup/fin","hasFormat":{"@id":"sfsd:2013/401/liite/1/alkup/fin/txt","content_fi":""}}},{"@id":"s