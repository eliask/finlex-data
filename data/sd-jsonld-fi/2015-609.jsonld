{"@id":"sfsd:2015/609","@type":["http://data.finlex.fi/schema/sfl/NewStatute"],"idLocal":["609/2015"],"inForce":["http://data.europa.eu/eli/ontology#InForce-inForce"],"temporalVersions":["http://data.finlex.fi/eli/sd/2015/609/alkup"],"typeDocument":["http://data.finlex.fi/schema/sfl/Act"],"temporalVersion":{"@id":"sfsd:2015/609/alkup","@type":"sfl:StatuteVersion","dateDocument":"2015-05-22","datePublication":"2015-05-25","firstDateEntryInForce":"2015-06-01","hasPart":[{"@id":"sfsd:2015/609/johtolause/alkup","@type":"sfl:EnactingClauseVersion","isMemberOf":"sfsd:2015/609/johtolause","version":"sfl:Original","versionDate":"2015-05-22","languageVersion":{"@id":"sfsd:2015/609/johtolause/alkup/fin","hasFormat":{"@id":"sfsd:2015/609/johtolause/alkup/fin/txt","content_fi":"Eduskunnan päätöksen mukaisesti säädetään:"}}},{"@id":"sfsd:2015/609/pykala/1/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2015/609/pykala/1/momentti/1/alkup","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:2015/609/pykala/1/momentti/1/johdanto/alkup","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:2015/609/pykala/1/momentti/1/johdanto","version":"sfl:Original","versionDate":"2015-05-22","languageVersion":{"@id":"sfsd:2015/609/pykala/1/momentti/1/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:2015/609/pykala/1/momentti/1/johdanto/alkup/fin/txt","content_fi":"Tämän lain tarkoituksena on:"}}},{"@id":"sfsd:2015/609/pykala/1/momentti/1/kohta/1/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2015/609/pykala/1/momentti/1/kohta/1","version":"sfl:Original","versionDate":"2015-05-22","languageVersion":{"@id":"sfsd:2015/609/pykala/1/momentti/1/kohta/1/alkup/fin","hasFormat":{"@id":"sfsd:2015/609/pykala/1/momentti/1/kohta/1/alkup/fin/txt","content_fi":"1) vahvistaa puitteet Suomen ilmastopolitiikan suunnittelulle ja sen toteutumisen seurannalle;"}}},{"@id":"sfsd:2015/609/pykala/1/momentti/1/kohta/2/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2015/609/pykala/1/momentti/1/k