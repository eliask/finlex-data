type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1997/48/luku/1/pykala/3a/momentti/1/johdanto/ajantasa/20080101","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1997/48/luku/1/pykala/3a/momentti/1/johdanto","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/3a/momentti/1/johdanto/ajantasa/20080101/fin","hasFormat":{"@id":"sfsd:1997/48/luku/1/pykala/3a/momentti/1/johdanto/ajantasa/20080101/fin/txt","content_fi":"Koulumatkatukea ei myönnetä opiskelijalle, joka:"}}},{"@id":"sfsd:1997/48/luku/1/pykala/3a/momentti/1/kohta/1/ajantasa/20130801","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1997/48/luku/1/pykala/3a/momentti/1/kohta/1","repealedBy":"sfsd:2012/1079/alkup","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/3a/momentti/1/kohta/1/ajantasa/20130801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/1/pykala/3a/momentti/1/kohta/1/ajantasa/20130801/fin/txt","content_fi":"1 kohta on kumottu L:lla 28.12.2012/1079."}}},{"@id":"sfsd:1997/48/luku/1/pykala/3a/momentti/1/kohta/2/ajantasa/20080101","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1997/48/luku/1/pykala/3a/momentti/1/kohta/2","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/3a/momentti/1/kohta/2/ajantasa/20080101/fin","hasFormat":{"@id":"sfsd:1997/48/luku/1/pykala/3a/momentti/1/kohta/2/ajantasa/20080101/fin/txt","content_fi":"2) voisi asua maksutta koulutuksen järjestäjän ylläpitämässä asunnossa, jollei opiskelija erityisistä syistä voi ottaa asuntoa vastaan; taikka"}}},{"@id":"sfsd:1997/48/luku/1/pykala/3a/momentti/1/kohta/3/ajantasa/20080101","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1997/48/luku/1/pykala/3a/momentti/1/kohta/3","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/3a/momentti/1/kohta/3/ajantasa/20080101/fin","hasFormat":{"@id":"sfsd:1997/48/luku/1/pykala/3a/momentti/1/kohta/3/ajantasa/20080101/fin/txt","content_fi":"3) saa tukea koulumatkasta aiheutuviin kustannuksiin muun lain nojalla."}}}],"isMemberOf":"sfsd:1997/48/luku/1/pykala/3a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-01"},{"@id":"sfsd:1997/48/luku/1/pykala/3a/momentti/2/ajantasa/20080101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/48/luku/1/pykala/3a/momentti/2","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/3a/momentti/2/ajantasa/20080101/fin","hasFormat":{"@id":"sfsd:1997/48/luku/1/pykala/3a/momentti/2/ajantasa/20080101/fin/txt","content_fi":"Valtioneuvoston asetuksella voidaan säätää terveydellisistä tai perheoloihin liittyvistä tai muista vastaavista erityisistä syistä, joiden perusteella opiskelijan ei katsota voivan ottaa koulutuksen järjestäjän ylläpitämää asuntoa vastaan."}}}],"isMemberOf":"sfsd:1997/48/luku/1/pykala/3a","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/3a/ajantasa/20130801/fin","title_fi":"Koulumatkatuen myöntämisen rajoitukset"}},{"@id":"sfsd:1997/48/luku/1/pykala/4/ajantasa/20180701","@type":"sfl:SectionVersion","amendedBy":"sfsd:2015/422/alkup","hasPart":[{"@id":"sfsd:1997/48/luku/1/pykala/4/momentti/1/ajantasa/20150801","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/48/luku/1/pykala/4/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/4/momentti/1/ajantasa/20150801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/1/pykala/4/momentti/1/ajantasa/20150801/fin/txt","content_fi":"Opiskelijan käyttäessä koulumatkaansa joukkoliikennettä matkakustannukset korvataan käytetyn kulkuneuvon halvimman opiskelijalle kuukautta vastaavalle ajalle tarkoitetun lipputuotteen hinnan mukaan."}}},{"@id":"sfsd:1997/48/luku/1/pykala/4/momentti/2/ajantasa/20180701","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2017/325/alkup","cites":"sfsd:2017/320","isMemberOf":"sfsd:1997/48/luku/1/pykala/4/momentti/2","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/4/momentti/2/ajantasa/20180701/fin","hasFormat":{"@id":"sfsd:1997/48/luku/1/pykala/4/momentti/2/ajantasa/20180701/fin/txt","content_fi":"Opiskelijan käyttäessä koulumatkaansa liikenteen palveluista annetun lain (320/2017) IV osan 1 luvun 4 §:ssä tarkoitettujen toimivaltaisten viranomaisten yhteistä lippu- ja maksujärjestelmää käyttävää joukkoliikennettä matkakustannukset määräytyvät sen estämättä, mitä tämän pykälän 1 momentissa ja tämän lain 3 §:n 2 momentin 2 kohdassa säädetään, sen matkan pituuden mukaan, jonka opiskelija kulkee tätä kuljetusta käyttäen.\n(24.5.2017/325)"}}},{"@id":"sfsd:1997/48/luku/1/pykala/4/momentti/3/ajantasa/20150801","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/48/luku/1/pykala/4/momentti/3","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/4/momentti/3/ajantasa/20150801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/1/pykala/4/momentti/3/ajantasa/20150801/fin/txt","content_fi":"Opiskelijan käyttäessä koulumatkaansa koulutuksen järjestäjän järjestämää koulukuljetusta matkakustannukset määräytyvät sen matkan pituuden mukaan, jonka opiskelija kulkee tätä kuljetusta käyttäen. Jos yhdensuuntainen koulumatka koostuu kahdesta tai useammasta matkaosuudesta, matkakustannukset voidaan laskea erikseen kultakin matkaosuudelta."}}},{"@id":"sfsd:1997/48/luku/1/pykala/4/momentti/4/ajantasa/20150801","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/48/luku/1/pykala/4/momentti/4","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/4/momentti/4/ajantasa/20150801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/1/pykala/4/momentti/4/ajantasa/20150801/fin/txt","content_fi":"Kun koulumatkatuki myönnetään 4 a §:n mukaisesti oman matkustustavan perusteella, matkakustannukset määräytyvät opiskelijan kodin ja oppilaitoksen välisen matkan pituuden mukaan. Matkakustannuksia ei kuitenkaan oteta huomioon siltä osin kuin ne aiheutuvat yli 100 kilometrin yhdensuuntaisesta koulumatkasta."}}},{"@id":"sfsd:1997/48/luku/1/pykala/4/momentti/5/ajantasa/20150801","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/48/luku/1/pykala/4/momentti/5","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/4/momentti/5/ajantasa/20150801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/1/pykala/4/momentti/5/ajantasa/20150801/fin/txt","content_fi":"Edellä 2–4 momentissa tarkoitettujen koulumatkakustannusten laskentaperusteista säädetään opetus- ja kulttuuriministeriön asetuksella. Kuukausittaiset matkakustannukset lasketaan 21,5 päivän mukaan."}}},{"@id":"sfsd:1997/48/luku/1/pykala/4/momentti/6/ajantasa/20150801","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/48/luku/1/pykala/4/momentti/6","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/4/momentti/6/ajantasa/20150801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/1/pykala/4/momentti/6/ajantasa/20150801/fin/txt","content_fi":"Alle viiden kilometrin erillisestä matkaosuudesta aiheutuvia kustannuksia ei korvata erikseen. Matkaosuuksista aiheutuvat kustannukset korvataan kuitenkin silloin, jos yhdensuuntaiseen vähintään kymmenen kilometrin koulumatkaan sisältyy erillisiä alle viiden kilometrin matkaosuuksia, jotka ovat yhteensä vähintään viisi kilometriä."}}}],"isMemberOf":"sfsd:1997/48/luku/1/pykala/4","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/4/ajantasa/20180701/fin","title_fi":"Matkakustannusten määräytyminen ja laskeminen"}},{"@id":"sfsd:1997/48/luku/1/pykala/4a/ajantasa/20110801","@type":"sfl:SectionVersion","amendedBy":"sfsd:2011/53/alkup","hasPart":{"@id":"sfsd:1997/48/luku/1/pykala/4a/momentti/1/ajantasa/20110801","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1997/48/luku/1/pykala/4a/momentti/1/johdanto/ajantasa/20110801","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1997/48/luku/1/pykala/4a/momentti/1/johdanto","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/4a/momentti/1/johdanto/ajantasa/20110801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/1/pykala/4a/momentti/1/johdanto/ajantasa/20110801/fin/txt","content_fi":"Opiskelijalle, joka käyttää muuta kuin joukkoliikennettä tai koulutuksen järjestäjän järjestämää koulukuljetusta, voidaan myöntää koulumatkatuki oman matkustustavan perusteella, jos:"}}},{"@id":"sfsd:1997/48/luku/1/pykala/4a/momentti/1/kohta/1/ajantasa/20110801","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1997/48/luku/1/pykala/4a/momentti/1/kohta/1","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/4a/momentti/1/kohta/1/ajantasa/20110801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/1/pykala/4a/momentti/1/kohta/1/ajantasa/20110801/fin/txt","content_fi":"1) opiskelijan koulumatkasta on joukkoliikenteen tai koulutuksen järjestäjän järjestämän koulukuljetuksen ulkopuolella yli viisi kilometriä yhdensuuntaista matkaa kohden;"}}},{"@id":"sfsd:1997/48/luku/1/pykala/4a/momentti/1/kohta/2/ajantasa/20110801","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1997/48/luku/1/pykala/4a/momentti/1/kohta/2","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/4a/momentti/1/kohta/2/ajantasa/20110801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/1/pykala/4a/momentti/1/kohta/2/ajantasa/20110801/fin/txt","content_fi":"2) opiskelijan päivittäinen koulumatka odotuksineen kestää keskimäärin yli kolme tuntia päivää kohden kuukaudessa; taikka"}}},{"@id":"sfsd:1997/48/luku/1/pykala/4a/momentti/1/kohta/3/ajantasa/20110801","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1997/48/luku/1/pykala/4a/momentti/1/kohta/3","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/4a/momentti/1/kohta/3/ajantasa/20110801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/1/pykala/4a/momentti/1/kohta/3/ajantasa/20110801/fin/txt","content_fi":"3) opiskelijalla on mahdollisuus käyttää joukkoliikennettä tai koulutuksen järjestäjän järjestämää koulukuljetusta kuukauden aikana keskimäärin enintään kuudella yhdensuuntaisella koulumatkalla viikossa."}}}],"isMemberOf":"sfsd:1997/48/luku/1/pykala/4a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-01"},"isMemberOf":"sfsd:1997/48/luku/1/pykala/4a","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/4a/ajantasa/20110801/fin","title_fi":"Oman matkustustavan käyttäminen"}},{"@id":"sfsd:1997/48/luku/1/pykala/5/ajantasa/20150801","@type":"sfl:SectionVersion","amendedBy":"sfsd:2004/346/alkup","hasPart":[{"@id":"sfsd:1997/48/luku/1/pykala/5/momentti/1/ajantasa/20040801","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2004/592/alkup","isMemberOf":"sfsd:1997/48/luku/1/pykala/5/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/5/momentti/1/ajantasa/20040801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/1/pykala/5/momentti/1/ajantasa/20040801/fin/txt","content_fi":"Koulumatkatukena korvataan 54 euroa kuukaudessa ylittävät koulumatkan kustannukset lisättynä 11 eurolla. Opiskelija maksaa laskennallisista koulumatkakustannuksista tällöin kuukausittain enintään 43 euroa, jos 4 §:stä tai tämän pykälän 3 momentista ei muuta johdu (opiskelijan maksuosuus).\n(24.6.2004/592)"}}},{"@id":"sfsd:1997/48/luku/1/pykala/5/momentti/2/ajantasa/20040801","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/48/luku/1/pykala/5/momentti/2","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/5/momentti/2/ajantasa/20040801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/1/pykala/5/momentti/2/ajantasa/20040801/fin/txt","content_fi":"Opiskelijan kulkiessa koulumatkansa useammalla kuin yhdellä matkustustavalla opiskelijan maksuosuus vähennetään joukkoliikenteen matkakustannuksista. Jos joukkoliikenteen matkakustannuksia ei ole, opiskelijan maksuosuus vähennetään koulutuksen järjestäjän järjestämästä kuljetuksesta aiheutuvista kustannuksista."}}},{"@id":"sfsd:1997/48/luku/1/pykala/5/momentti/3/ajantasa/20150801","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2015/422/alkup","isMemberOf":"sfsd:1997/48/luku/1/pykala/5/momentti/3","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/5/momentti/3/ajantasa/20150801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/1/pykala/5/momentti/3/ajantasa/20150801/fin/txt","content_fi":"Tuki on 70 prosenttia 1 momentin mukaisesta tuesta, jos tuki myönnetään 4 a §:n mukaisesti oman matkustustavan perusteella tai jos matkakustannukset aiheutuvat 4 §:n 6 momentissa tarkoitetuista useista erillisistä yhteensä vähintään viiden kilometrin matkaosuuksista.\n(10.4.2015/422)"}}}],"isMemberOf":"sfsd:1997/48/luku/1/pykala/5","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/pykala/5/ajantasa/20150801/fin","title_fi":"Koulumatkatuen määrä ja opiskelijan maksuosuus"}}],"isMemberOf":"sfsd:1997/48/luku/1","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/1/ajantasa/20180801/fin","title_fi":"Yleiset säännökset"}},{"@id":"sfsd:1997/48/luku/2/ajantasa/20150801","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1997/48/luku/2/pykala/6/ajantasa/20040801","@type":"sfl:SectionVersion","amendedBy":"sfsd:2004/346/alkup","hasPart":[{"@id":"sfsd:1997/48/luku/2/pykala/6/momentti/1/ajantasa/20040801","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/48/luku/2/pykala/6/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/2/pykala/6/momentti/1/ajantasa/20040801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/2/pykala/6/momentti/1/ajantasa/20040801/fin/txt","content_fi":"Koulumatkatukea haetaan Kansaneläkelaitokselta. Hakemus jätetään oppilaitokseen."}}},{"@id":"sfsd:1997/48/luku/2/pykala/6/momentti/2/ajantasa/20040801","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/48/luku/2/pykala/6/momentti/2","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/2/pykala/6/momentti/2/ajantasa/20040801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/2/pykala/6/momentti/2/ajantasa/20040801/fin/txt","content_fi":"Oppilaitoksen tulee liittää hakemukseen selvitys opiskelijan koulumatkan pituudesta sekä opiskelijan mahdollisuudesta käyttää joukkoliikennettä tai koulutuksen järjestäjän järjestämää koulukuljetusta."}}},{"@id":"sfsd:1997/48/luku/2/pykala/6/momentti/3/ajantasa/20040801","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/48/luku/2/pykala/6/momentti/3","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/2/pykala/6/momentti/3/ajantasa/20040801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/2/pykala/6/momentti/3/ajantasa/20040801/fin/txt","content_fi":"Oppilaitoksen antama selvitys koulumatkasta on Kansaneläkelaitosta sitova, jollei ole perusteltua syytä olettaa, että tiedot selvityksessä ovat virheellisiä. Jos on perusteltua syytä olettaa, että selvityksessä on virheellisiä tietoja, Kansaneläkelaitos voi poiketa selvityksestä päätöstä tehdessään."}}}],"isMemberOf":"sfsd:1997/48/luku/2/pykala/6","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/2/pykala/6/ajantasa/20040801/fin","title_fi":"Hakeminen"}},{"@id":"sfsd:1997/48/luku/2/pykala/7/ajantasa/20150801","@type":"sfl:SectionVersion","amendedBy":"sfsd:2004/346/alkup","hasPart":[{"@id":"sfsd:1997/48/luku/2/pykala/7/momentti/1/ajantasa/20130801","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2012/1079/alkup","isMemberOf":"sfsd:1997/48/luku/2/pykala/7/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/2/pykala/7/momentti/1/ajantasa/20130801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/2/pykala/7/momentti/1/ajantasa/20130801/fin/txt","content_fi":"Koulumatkatuen myöntää Kansaneläkelaitos. Kalenterikuukausi oikeuttaa koulumatkatukeen, jos siinä on vähintään 18 koulumatkatukeen oikeuttavaan aikaan kuuluvaa päivää.\n(28.12.2012/1079)"}}},{"@id":"sfsd:1997/48/luku/2/pykala/7/momentti/2/ajantasa/20040801","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/48/luku/2/pykala/7/momentti/2","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/2/pykala/7/momentti/2/ajantasa/20040801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/2/pykala/7/momentti/2/ajantasa/20040801/fin/txt","content_fi":"Koulumatkatuki myönnetään enintään lukuvuodeksi kerrallaan. Koulumatkatuki myönnetään aikaisintaan hakemuksen jättämistä edeltävän kuukauden alusta. Koulumatkatuki voidaan myöntää seuraavan tai edeltävän lukuvuoden tuen myöntämisen yhteydessä, jos opinnot alkavat tai päättyvät kesä-, heinä- tai elokuun aikana."}}},{"@id":"sfsd:1997/48/luku/2/pykala/7/momentti/3/ajantasa/20150801","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2015/422/alkup","isMemberOf":"sfsd:1997/48/luku/2/pykala/7/momentti/3","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/2/pykala/7/momentti/3/ajantasa/20150801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/2/pykala/7/momentti/3/ajantasa/20150801/fin/txt","content_fi":"Koulumatkatuki maksetaan kuukausittain opiskelijan matkustustavan mukaan joko koulutuksen järjestäjän, matkalipun myyjän, joukkoliikenteen lippujärjestelmän ylläpitäjän tai opiskelijan ilmoittamalle Euroopan unionissa sijaitsevalle tilille.\n(10.4.2015/422)"}}},{"@id":"sfsd:1997/48/luku/2/pykala/7/momentti/4/ajantasa/20150801","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2015/422/alkup","isMemberOf":"sfsd:1997/48/luku/2/pykala/7/momentti/4","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/2/pykala/7/momentti/4/ajantasa/20150801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/2/pykala/7/momentti/4/ajantasa/20150801/fin/txt","content_fi":"Koulumatkatuen laskuttamisesta ja liikaa maksetun koulumatkatuen palauttamisesta säädetään tarkemmin valtioneuvoston asetuksella.\n(10.4.2015/422)"}}},{"@id":"sfsd:1997/48/luku/2/pykala/7/momentti/5/ajantasa/20110801","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2011/53/alkup","isMemberOf":"sfsd:1997/48/luku/2/pykala/7/momentti/5","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/2/pykala/7/momentti/5/ajantasa/20110801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/2/pykala/7/momentti/5/ajantasa/20110801/fin/txt","content_fi":"Joukkoliikennettä käyttävä opiskelija voi ostaa koulumatkatuella tuetun lipputuotteen aikaisintaan 14 päivää ennen tukeen oikeuttavien opintojen alkamispäivää ja viimeistään 14 päivää ennen tukeen oikeuttavien opintojen päättymispäivää.\n(21.1.2011/53)"}}}],"isMemberOf":"sfsd:1997/48/luku/2/pykala/7","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/2/pykala/7/ajantasa/20150801/fin","title_fi":"Myöntäminen ja maksaminen"}},{"@id":"sfsd:1997/48/luku/2/pykala/8/ajantasa/20110801","@type":"sfl:SectionVersion","amendedBy":"sfsd:2011/53/alkup","hasPart":{"@id":"sfsd:1997/48/luku/2/pykala/8/momentti/1/ajantasa/20110801","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/48/luku/2/pykala/8/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/2/pykala/8/momentti/1/ajantasa/20110801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/2/pykala/8/momentti/1/ajantasa/20110801/fin/txt","content_fi":"Kun koulumatkatukea on maksettu aiheetta tai määrältään liikaa joukkoliikennettä käyttävälle opiskelijalle, takaisinperittävä määrä voi olla enintään muille kuin koulumatkatukeen oikeutetuille opiskelijoille myytävän kuukautta vastaavalle ajalle tarkoitetun halvimman lipputuotteen hinnan ja opiskelijan maksuosuuden välinen erotus."}}},"isMemberOf":"sfsd:1997/48/luku/2/pykala/8","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/2/pykala/8/ajantasa/20110801/fin","title_fi":"Koulumatkatuen takaisinperintä joukkoliikennettä käytettäessä"}}],"isMemberOf":"sfsd:1997/48/luku/2","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/2/ajantasa/20150801/fin","title_fi":"Hakeminen ja maksaminen"}},{"@id":"sfsd:1997/48/luku/3/ajantasa/20130801","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1997/48/luku/3/pykala/9/ajantasa/20110701","@type":"sfl:SectionVersion","amendedBy":"sfsd:2001/344/alkup","hasPart":[{"@id":"sfsd:1997/48/luku/3/pykala/9/momentti/1/ajantasa/20110701","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2011/696/alkup","cites":"sfsd:1996/586","isMemberOf":"sfsd:1997/48/luku/3/pykala/9/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/3/pykala/9/momentti/1/ajantasa/20110701/fin","hasFormat":{"@id":"sfsd:1997/48/luku/3/pykala/9/momentti/1/ajantasa/20110701/fin/txt","content_fi":"Koulumatkatukea koskevaan Kansaneläkelaitoksen päätökseen saa hakea muutosta valittamalla opintotuen muutoksenhakulautakuntaan. Lautakunnan päätökseen saa hakea muutosta valittamalla vakuutusoikeuteen siten kuin hallintolainkäyttölaissa (586/1996) säädetään. Lautakunnan lainvoimaisen päätöksen poistamista koskevassa asiassa antamaan päätökseen ei kuitenkaan saa hakea muutosta valittamalla.\n(17.6.2011/696)"}}},{"@id":"sfsd:1997/48/luku/3/pykala/9/momentti/2/ajantasa/20040101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2003/1323/alkup","isMemberOf":"sfsd:1997/48/luku/3/pykala/9/momentti/2","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/3/pykala/9/momentti/2/ajantasa/20040101/fin","hasFormat":{"@id":"sfsd:1997/48/luku/3/pykala/9/momentti/2/ajantasa/20040101/fin/txt","content_fi":"Valituskirjelmä on toimitettava Kansaneläkelaitokselle 30 päivän kuluessa siitä, kun valittaja on saanut päätöksestä tiedon.\n(30.12.2003/1323)"}}},{"@id":"sfsd:1997/48/luku/3/pykala/9/momentti/3/ajantasa/20030501","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2003/136/alkup","isMemberOf":"sfsd:1997/48/luku/3/pykala/9/momentti/3","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/3/pykala/9/momentti/3/ajantasa/20030501/fin","hasFormat":{"@id":"sfsd:1997/48/luku/3/pykala/9/momentti/3/ajantasa/20030501/fin/txt","content_fi":"Jos valitus on saapunut 2 momentissa säädetyn määräajan jälkeen, muutoksenhakulautakunta tai vakuutusoikeus voi tästä huolimatta ottaa valituksen tutkittavakseen, jos myöhästymiseen on ollut painavia syitä.\n(21.2.2003/136)"}}},{"@id":"sfsd:1997/48/luku/3/pykala/9/momentti/4/ajantasa/20010501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/48/luku/3/pykala/9/momentti/4","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/3/pykala/9/momentti/4/ajantasa/20010501/fin","hasFormat":{"@id":"sfsd:1997/48/luku/3/pykala/9/momentti/4/ajantasa/20010501/fin/txt","content_fi":"Kansaneläkelaitoksen päätöstä on muutoksenhausta huolimatta noudatettava, kunnes asia on lainvoimaisella päätöksellä ratkaistu."}}}],"isMemberOf":"sfsd:1997/48/luku/3/pykala/9","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/3/pykala/9/ajantasa/20110701/fin","title_fi":"Muutoksenhaku"}},{"@id":"sfsd:1997/48/luku/3/pykala/10/ajantasa/20130801","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1997/48/luku/3/pykala/10","repealedBy":"sfsd:2012/1079/alkup","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/3/pykala/10/ajantasa/20130801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/3/pykala/10/ajantasa/20130801/fin/txt","content_fi":"10 §(28.12.2012/1079)\n10 § on kumottu L:lla 28.12.2012/1079."}}}],"isMemberOf":"sfsd:1997/48/luku/3","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/3/ajantasa/20130801/fin","title_fi":"Muutoksenhaku ja virheen korjaaminen"}},{"@id":"sfsd:1997/48/luku/4/ajantasa/20150801","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1997/48/luku/4/pykala/11/ajantasa/20150801","@type":"sfl:SectionVersion","amendedBy":"sfsd:2015/422/alkup","hasPart":{"@id":"sfsd:1997/48/luku/4/pykala/11/momentti/1/ajantasa/20150801","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/48/luku/4/pykala/11/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/4/pykala/11/momentti/1/ajantasa/20150801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/4/pykala/11/momentti/1/ajantasa/20150801/fin/txt","content_fi":"Kansaneläkelaitos voi sopia joukkoliikenteen matkalippujen myyjien ja 4 §:n 2 momentissa tarkoitetun lippujärjestelmän ylläpitäjän kanssa ehdoista, joilla matkalippuja myydään opiskelijoille koulumatkatuen määrällä alennettuun hintaan. Sopimusmenettelystä säädetään tarkemmin valtioneuvoston asetuksella."}}},"isMemberOf":"sfsd:1997/48/luku/4/pykala/11","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/4/pykala/11/ajantasa/20150801/fin","title_fi":"Sopimus koulumatkatukeen oikeuttavien matkalippujen myymisestä"}},{"@id":"sfsd:1997/48/luku/4/pykala/12/ajantasa/20150801","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1997/48/luku/4/pykala/12/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/48/luku/4/pykala/12/momentti/1","version":"sfl:Original","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/4/pykala/12/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1997/48/luku/4/pykala/12/momentti/1/alkup/fin/txt","content_fi":"Opiskelija on velvollinen ilmoittamaan kansaneläkelaitokselle koulumatkatuen myöntämiseksi tarvittavat tiedot sekä koulumatkatukeen vaikuttavista muutoksista. Oppilaitos on velvollinen ilmoittamaan kansaneläkelaitokselle sellaiset tietoonsa tulleet seikat, jotka voivat johtaa koulumatkatuen tarkistamiseen tai lakkauttamiseen."}}},{"@id":"sfsd:1997/48/luku/4/pykala/12/momentti/2/ajantasa/20150801","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2015/422/alkup","isMemberOf":"sfsd:1997/48/luku/4/pykala/12/momentti/2","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/4/pykala/12/momentti/2/ajantasa/20150801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/4/pykala/12/momentti/2/ajantasa/20150801/fin/txt","content_fi":"Opiskelija on velvollinen ilmoittamaan henkilötunnuksensa matkalipun myyjälle ostaessaan matkalipun, joka oikeuttaa koulumatkatukeen.\n(10.4.2015/422)"}}}],"isMemberOf":"sfsd:1997/48/luku/4/pykala/12","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/4/pykala/12/ajantasa/20150801/fin","title_fi":"Tietojenantovelvollisuus"}},{"@id":"sfsd:1997/48/luku/4/pykala/13/ajantasa/19990101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1998/637/alkup","hasPart":{"@id":"sfsd:1997/48/luku/4/pykala/13/momentti/1/ajantasa/19990101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/48/luku/4/pykala/13/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/4/pykala/13/momentti/1/ajantasa/19990101/fin","hasFormat":{"@id":"sfsd:1997/48/luku/4/pykala/13/momentti/1/ajantasa/19990101/fin/txt","content_fi":"Tämän lain toimeenpanosta kansaneläkelaitokselle aiheutuvat menot luetaan kansaneläkelaitoksen toimintamenoiksi ja koulutuksen järjestäjälle 6 ja 12 §:ssä säädetyistä tehtävistä aiheutuvat menot kyseisen koulutuksen järjestäjän menoiksi."}}},"isMemberOf":"sfsd:1997/48/luku/4/pykala/13","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/4/pykala/13/ajantasa/19990101/fin","title_fi":"Toimintamenot"}},{"@id":"sfsd:1997/48/luku/4/pykala/14/ajantasa/20130801","@type":"sfl:SectionVersion","amendedBy":"sfsd:2012/1079/alkup","hasPart":{"@id":"sfsd:1997/48/luku/4/pykala/14/momentti/1/ajantasa/20130801","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/48/luku/4/pykala/14/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/4/pykala/14/momentti/1/ajantasa/20130801/fin","hasFormat":{"@id":"sfsd:1997/48/luku/4/pykala/14/momentti/1/ajantasa/20130801/fin/txt","content_fi":"Jollei tässä laissa toisin säädetä, koulumatkatukea koskevassa asiassa noudatetaan, mitä opintotukilaissa säädetään opintotuen toimeenpanosta, tarkistamisesta ja keskeyttämisestä, takaisinperinnästä ja takaisinperintäsaatavan vanhentumisesta, itseoikaisusta, päätöksen poistamisesta, asiavirheen korjaamisesta, tietojen saamisesta, käytöstä ja luovuttamisesta, ulosmittaus- ja siirtokiellosta sekä rahoituksesta."}}},"isMemberOf":"sfsd:1997/48/luku/4/pykala/14","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/4/pykala/14/ajantasa/20130801/fin","title_fi":"Viittaussäännös"}},{"@id":"sfsd:1997/48/luku/4/pykala/15/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1997/48/luku/4/pykala/15/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/48/luku/4/pykala/15/momentti/1","version":"sfl:Original","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/4/pykala/15/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1997/48/luku/4/pykala/15/momentti/1/alkup/fin/txt","content_fi":"Tarkemmat säännökset tämän lain täytäntöönpanosta annetaan asetuksella."}}},"isMemberOf":"sfsd:1997/48/luku/4/pykala/15","version":"sfl:Original","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/4/pykala/15/alkup/fin","title_fi":"Tarkemmat säännökset"}}],"isMemberOf":"sfsd:1997/48/luku/4","version":"sfl:Consolidated","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/4/ajantasa/20150801/fin","title_fi":"Erinäiset säännökset"}},{"@id":"sfsd:1997/48/luku/5/alkup","@type":"sfl:ChapterVersion","hasPart":{"@id":"sfsd:1997/48/luku/5/pykala/16/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1997/48/luku/5/pykala/16/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/48/luku/5/pykala/16/momentti/1","version":"sfl:Original","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/5/pykala/16/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1997/48/luku/5/pykala/16/momentti/1/alkup/fin/txt","content_fi":"Tämä laki tulee voimaan 1 päivänä heinäkuuta 1997."}}},{"@id":"sfsd:1997/48/luku/5/pykala/16/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/48/luku/5/pykala/16/momentti/2","version":"sfl:Original","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/5/pykala/16/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1997/48/luku/5/pykala/16/momentti/2/alkup/fin/txt","content_fi":"Ennen lain voimaantuloa voidaan ryhtyä sen täytäntöönpanon edellyttämiin toimenpiteisiin."}}}],"isMemberOf":"sfsd:1997/48/luku/5/pykala/16","version":"sfl:Original","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/5/pykala/16/alkup/fin","title_fi":"Voimaantulo"}},"isMemberOf":"sfsd:1997/48/luku/5","version":"sfl:Original","versionDate":"2018-08-01","languageVersion":{"@id":"sfsd:1997/48/luku/5/alkup/fin","title_fi":"Voimaantulo"}}],"isAbout":["http://data.finlex.fi/voc/finlex/kw4296","http://data.finlex.fi/voc/finlex/kw2471","http://data.finlex.fi/voc/finlex/kw1086","http://data.finlex.fi/voc/finlex/kw2141","http://data.finlex.fi/