25/alkup","isMemberOf":"sfsd:1995/974/luku/6/pykala/45/momentti/2","version":"sfl:Consolidated","versionDate":"2016-01-01","languageVersion":{"@id":"sfsd:1995/974/luku/6/pykala/45/momentti/2/ajantasa/19960101/fin","hasFormat":{"@id":"sfsd:1995/974/luku/6/pykala/45/momentti/2/ajantasa/19960101/fin/txt","content_fi":"Saamelaisvaltuuskunnan virat ja henkilöstö siirtyvät lain voimaan tullessa saamelaiskäräjien viroiksi ja henkilöstöksi.\n(22.12.1995/1725)"}}}],"isMemberOf":"sfsd:1995/974/luku/6/pykala/45","version":"sfl:Consolidated","versionDate":"2016-01-01","languageVersion":{"@id":"sfsd:1995/974/luku/6/pykala/45/ajantasa/19960101/fin","title_fi":"Siirtymäsäännökset"}}],"isMemberOf":"sfsd:1995/974/luku/6","relatedTo":"http://data.finlex.fi/eli/he/1994/248","version":"sfl:Consolidated","versionDate":"2016-01-01","languageVersion":{"@id":"sfsd:1995/974/luku/6/ajantasa/20030101/fin","title_fi":"Erinäisiä säännöksiä"}}],"isAbout":"http://data.finlex.fi/voc/finlex/kw3454","isMemberOf":"sfsd:1995/974","version":"sfl:Consolidated","versionDate":"2016-01-01","languageVersion":{"@id":"sfsd:1995/974/ajantasa/20160101/fin","title_fi":"Laki saamelaiskäräjistä"}},"@context":{"languageVersion":{"@id":"eli:is_realized_by","@type":"@id"},"temporalVersions":{"@id":"eli:has_member","@type":"@id"},"temporalVersion":{"@id":"eli:has_member","@type":"@id"},"hasFormat":{"@id":"eli:is_embodied_by","@type":"@id"},"idLocal":"eli:id_local","dateDocument":{"@id":"eli:date_document","@type":"xsd:date"},"hasPart":{"@id":"eli:has_part","@type":"@id"},"isAbout":{"@id":"eli:is_about","@type":"@id"},"isMemberOf":{"@id":"eli:is_member_of","@type":"@id"},"version":{"@id":"eli:version","@type":"@id"},"versionDate":{"@id":"eli:version_date","@type":"xsd:date"},"title_fi":{"@id":"eli:title","@language":"fi"},"content_fi":{"@id":"sfl:text","@language":"fi"},"amendedBy":{"@id":"eli:amended_by","@type":"@id"},"cites":{"@id":"eli:cites","@type":"@id"},"repealedBy":{"@id":"eli:repealed_by","@type":"@id"},"rela