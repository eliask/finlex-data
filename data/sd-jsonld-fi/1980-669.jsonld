fsd:1980/669/pykala/36a/momentti/1/kohta/17/ajantasa/20071213/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/36a/momentti/1/kohta/17/ajantasa/20071213/fin/txt","content_fi":"17) päivä, jolloin patentin rajoittaminen on hyväksytty."}}}],"isMemberOf":"sfsd:1980/669/pykala/36a/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/36a/momentti/2/ajantasa/20071213","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/36a/momentti/2","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/36a/momentti/2/ajantasa/20071213/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/36a/momentti/2/ajantasa/20071213/fin/txt","content_fi":"Edellä 36 §:n 2 momentissa tarkoitetun patenttijulkaisun tulee sisältää tämän pykälän 1 momentissa mainitut tiedot lukuun ottamatta sen 17 kohdassa mainittua tietoa ja tieto siitä, milloin tuomioistuin on tehnyt lainvoimaisen päätöksen patentin rajoittamisesta."}}}],"isMemberOf":"sfsd:1980/669/pykala/36a","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/valiotsikko/13/alkup","@type":"sfl:SubheadingVersion","isMemberOf":"sfsd:1980/669/valiotsikko/13","version":"sfl:Original","versionDate":"2013-09-01","followedBy":"sfsd:1980/669/pykala/37/ajantasa/20130901","languageVersion":{"@id":"sfsd:1980/669/valiotsikko/13/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/valiotsikko/13/alkup/fin/txt","content_fi":"Väliotsikko on kumottu A:lla 18.7.2013/580.\n\n(18.7.2013/580)"}}},{"@id":"sfsd:1980/669/pykala/37/ajantasa/20130901","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1980/669/pykala/37","repealedBy":"sfsd:2013/580/alkup","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/37/ajantasa/20130901/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/37/ajantasa/20130901/fin/txt","content_fi":"37 §(18.7.2013/580)\n37 § on kumottu A:lla 18.7.2013/580."}}},{"@id":"sfsd:1980/669/pykala/37a/ajantasa/19960301","@type":"sfl:SectionVersion","amendedBy":"sfsd:1996/104/alkup","hasPart":{"@id":"sfsd:1980/669/pykala/37a/momentti/1/ajantasa/19960301","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/37a/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/37a/momentti/1/ajantasa/19960301/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/37a/momentti/1/ajantasa/19960301/fin/txt","content_fi":"Patenttirekisteri sisältää tiedot patenttiviraston myöntämistä patenteista ja eurooppapatenteista, joilla on oikeusvaikutus Suomessa."}}},"isMemberOf":"sfsd:1980/669/pykala/37a","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/valiotsikko/14/alkup","@type":"sfl:SubheadingVersion","isMemberOf":"sfsd:1980/669/valiotsikko/14","version":"sfl:Original","versionDate":"2013-09-01","followedBy":"sfsd:1980/669/pykala/38/ajantasa/19970401","languageVersion":{"@id":"sfsd:1980/669/valiotsikko/14/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/valiotsikko/14/alkup/fin/txt","content_fi":"Patenttirekisteri"}}},{"@id":"sfsd:1980/669/pykala/38/ajantasa/19970401","@type":"sfl:SectionVersion","amendedBy":"sfsd:1997/246/alkup","hasPart":{"@id":"sfsd:1980/669/pykala/38/momentti/1/ajantasa/19970401","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/38/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/38/momentti/1/ajantasa/19970401/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/38/momentti/1/ajantasa/19970401/fin/txt","content_fi":"Kun patentti on Suomessa myönnetty, se on merkittävä patenttirekisteriin. Rekisteriin merkitään tällöin ne tiedot, jotka 31 §:n mukaan on ilmoitettava patenttijulkaisussa, viitejulkaisuja lukuun ottamatta."}}},"isMemberOf":"sfsd:1980/669/pykala/38","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/38a/ajantasa/20071213","@type":"sfl:SectionVersion","amendedBy":"sfsd:1996/104/alkup","hasPart":[{"@id":"sfsd:1980/669/pykala/38a/momentti/1/ajantasa/19960301","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/38a/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/38a/momentti/1/ajantasa/19960301/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/38a/momentti/1/ajantasa/19960301/fin/txt","content_fi":"Eurooppapatentti merkitään rekisteriin, kun Euroopan patenttivirasto on kuuluttanut patentin myöntämisestä ja kun hakija on antanut käännöksen ja suorittanut maksun patenttilain 70 h §:n 1 momentin mukaisesti."}}},{"@id":"sfsd:1980/669/pykala/38a/momentti/2/ajantasa/19970401","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1980/669/pykala/38a/momentti/2/johdanto/ajantasa/19960301","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1980/669/pykala/38a/momentti/2/johdanto","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/38a/momentti/2/johdanto/ajantasa/19960301/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/38a/momentti/2/johdanto/ajantasa/19960301/fin/txt","content_fi":"Rekisteriin merkitään tällöin:"}}},{"@id":"sfsd:1980/669/pykala/38a/momentti/2/kohta/1/ajantasa/19960301","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1980/669/pykala/38a/momentti/2/kohta/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/38a/momentti/2/kohta/1/ajantasa/19960301/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/38a/momentti/2/kohta/1/ajantasa/19960301/fin/txt","content_fi":"1) se päivä, jolloin Euroopan patenttivirasto on kuuluttanut patentin myöntämispäätöksestään;"}}},{"@id":"sfsd:1980/669/pykala/38a/momentti/2/kohta/2/ajantasa/19960301","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1980/669/pykala/38a/momentti/2/kohta/2","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/38a/momentti/2/kohta/2/ajantasa/19960301/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/38a/momentti/2/kohta/2/ajantasa/19960301/fin/txt","content_fi":"2) päivä, jolloin patenttilain 70 h §:n 1 momentin mukainen käännös ja maksu ovat saapuneet sekä milloin käännöksestä on kuulutettu;"}}},{"@id":"sfsd:1980/669/pykala/38a/momentti/2/kohta/3/ajantasa/19960301","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1980/669/pykala/38a/momentti/2/kohta/3","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/38a/momentti/2/kohta/3/ajantasa/19960301/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/38a/momentti/2/kohta/3/ajantasa/19960301/fin/txt","content_fi":"3) päivä, josta patenttiaika alkaa; sekä"}}},{"@id":"sfsd:1980/669/pykala/38a/momentti/2/kohta/4/ajantasa/19970401","@type":"sfl:ParagraphVersion","amendedBy":"sfsd:1997/246/alkup","isMemberOf":"sfsd:1980/669/pykala/38a/momentti/2/kohta/4","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/38a/momentti/2/kohta/4/ajantasa/19970401/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/38a/momentti/2/kohta/4/ajantasa/19970401/fin/txt","content_fi":"4) tiedot, jotka vastaavat niitä, jotka ilmenevät 31 §:n 2 momentin kohdista 1–5, 10 ja 13.\n(21.3.1997/246)"}}}],"isMemberOf":"sfsd:1980/669/pykala/38a/momentti/2","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/38a/momentti/3/ajantasa/20071213","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2007/1118/alkup","isMemberOf":"sfsd:1980/669/pykala/38a/momentti/3","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/38a/momentti/3/ajantasa/20071213/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/38a/momentti/3/ajantasa/20071213/fin/txt","content_fi":"Jos Euroopan patenttivirasto on lopullisesti päättänyt, että eurooppapatentti kumotaan, pidetään voimassa muutetussa muodossa tai lakkautetaan, merkitään tämä rekisteriin. Jos patentinhaltija on niissä tapauksissa, joissa patentti ylläpidetään muutetussa muodossa, antanut käännöksen ja suorittanut patenttilain 70 h §:n 1 momentin mukaisen maksun, tehdään tästä merkintä rekisteriin.\n(29.11.2007/1118)"}}},{"@id":"sfsd:1980/669/pykala/38a/momentti/4/ajantasa/19960301","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/38a/momentti/4","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/38a/momentti/4/ajantasa/19960301/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/38a/momentti/4/ajantasa/19960301/fin/txt","content_fi":"Jos 1 tai 3 momentissa mainitun käännöksen korjaus annetaan, rekisteriin merkitään, milloin tämä on tapahtunut sekä milloin korjauksesta on kuulutettu."}}},{"@id":"sfsd:1980/669/pykala/38a/momentti/5/ajantasa/20071213","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2007/1118/alkup","hasPart":[{"@id":"sfsd:1980/669/pykala/38a/momentti/5/johdanto/ajantasa/19960301","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1980/669/pykala/38a/momentti/5/johdanto","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/38a/momentti/5/johdanto/ajantasa/19960301/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/38a/momentti/5/johdanto/ajantasa/19960301/fin/txt","content_fi":"Eurooppapatenttia koskeva Suomessa voimaansaatettu rajoittaminen merkitään rekisteriin, kun Euroopan patenttivirasto on kuuluttanut eurooppapatentin rajoittamisesta, rajoitetun patentin käännös on annettu ja maksu suoritettu. Rekisteriin merkitään tällöin:"}}},{"@id":"sfsd:1980/669/pykala/38a/momentti/5/kohta/1/ajantasa/19960301","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1980/669/pykala/38a/momentti/5/kohta/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/38a/momentti/5/kohta/1/ajantasa/19960301/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/38a/momentti/5/kohta/1/ajantasa/19960301/fin/txt","content_fi":"1) päivä, jolloin Euroopan patenttivirasto on kuuluttanut eurooppapatentin rajoittamisesta; ja"}}},{"@id":"sfsd:1980/669/pykala/38a/momentti/5/kohta/2/ajantasa/19960301","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1980/669/pykala/38a/momentti/5/kohta/2","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/38a/momentti/5/kohta/2/ajantasa/19960301/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/38a/momentti/5/kohta/2/ajantasa/19960301/fin/txt","content_fi":"2) päivä, jolloin patenttilain 70 t §:n 2 momentin mukainen käännös ja maksu ovat saapuneet sekä päivä, jolloin käännöksestä on kuulutettu."}}}],"isMemberOf":"sfsd:1980/669/pykala/38a/momentti/5","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/38a/momentti/6/ajantasa/20071213","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2007/1118/alkup","isMemberOf":"sfsd:1980/669/pykala/38a/momentti/6","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/38a/momentti/6/ajantasa/20071213/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/38a/momentti/6/ajantasa/20071213/fin/txt","content_fi":"Jos eurooppapatentti kumotaan patenttilain 70 t §:n 3 momentin mukaisesti, merkitään tästä tieto rekisteriin.\n(29.11.2007/1118)"}}}],"isMemberOf":"sfsd:1980/669/pykala/38a","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/38b/ajantasa/19970401","@type":"sfl:SectionVersion","amendedBy":"sfsd:1997/246/alkup","hasPart":[{"@id":"sfsd:1980/669/pykala/38b/momentti/1/ajantasa/19970401","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1980/669/pykala/38b/momentti/1/johdanto/ajantasa/19970401","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1980/669/pykala/38b/momentti/1/johdanto","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/38b/momentti/1/johdanto/ajantasa/19970401/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/38b/momentti/1/johdanto/ajantasa/19970401/fin/txt","content_fi":"Kun myönnettyä patenttia vastaan on tehty väite, on siitä tehtävä merkintä patenttirekisteriin. Merkinnän tulee sisältää seuraavat tiedot:"}}},{"@id":"sfsd:1980/669/pykala/38b/momentti/1/kohta/1/ajantasa/19970401","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1980/669/pykala/38b/momentti/1/kohta/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/38b/momentti/1/kohta/1/ajantasa/19970401/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/38b/momentti/1/kohta/1/ajantasa/19970401/fin/txt","content_fi":"1) väitteentekijän nimi ja osoite;"}}},{"@id":"sfsd:1980/669/pykala/38b/momentti/1/kohta/2/ajantasa/19970401","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1980/669/pykala/38b/momentti/1/kohta/2","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/38b/momentti/1/kohta/2/ajantasa/19970401/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/38b/momentti/1/kohta/2/ajantasa/19970401/fin/txt","content_fi":"2) jos väitteentekijää edustaa asiamies, tämän nimi, kotipaikka ja osoite; sekä"}}},{"@id":"sfsd:1980/669/pykala/38b/momentti/1/kohta/3/ajantasa/19970401","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1980/669/pykala/38b/momentti/1/kohta/3","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/38b/momentti/1/kohta/3/ajantasa/19970401/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/38b/momentti/1/kohta/3/ajantasa/19970401/fin/txt","content_fi":"3) väitteen tekemispäivä."}}}],"isMemberOf":"sfsd:1980/669/pykala/38b/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/38b/momentti/2/ajantasa/19970401","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/38b/momentti/2","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/38b/momentti/2/ajantasa/19970401/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/38b/momentti/2/ajantasa/19970401/fin/txt","content_fi":"Väitteen johdosta tehdystä lainvoiman saaneesta päätöksestä on tehtävä merkintä patenttirekisteriin. Rekisteriin merkitään tällöin päätöksen päivämäärä ja sen pääasiallinen sisältö."}}}],"isMemberOf":"sfsd:1980/669/pykala/38b","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/39/ajantasa/20111101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2011/1097/alkup","hasPart":{"@id":"sfsd:1980/669/pykala/39/momentti/1/ajantasa/20111101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/39/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/39/momentti/1/ajantasa/20111101/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/39/momentti/1/ajantasa/20111101/fin/txt","content_fi":"Patenttilain 25 §:n 5 momentissa tarkoitetun, väitteen johdosta tehtyä päätöstä koskevan kuulutuksen tulee sisältää patentin rekisterinumero ja luokat, keksinnön nimitys, patentinhaltijan nimi sekä päätöksen päivämäärä."}}},"isMemberOf":"sfsd:1980/669/pykala/39","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/40/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1980/669/pykala/40/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/40/momentti/1","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/40/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/40/momentti/1/alkup/fin/txt","content_fi":"Kun vuosimaksu on suoritettu tai maksun suoritusajan pidennystä on myönnetty, on siitä tehtävä merkintä rekisteriin."}}},{"@id":"sfsd:1980/669/pykala/40/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/40/momentti/2","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/40/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/40/momentti/2/alkup/fin/txt","content_fi":"Jos patentti patenttilain 51 §:n perusteella on rauennut, on rekisteriin merkittävä se päivä, josta patentti on lakannut olemasta voimassa."}}},{"@id":"sfsd:1980/669/pykala/40/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/40/momentti/3","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/40/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/40/momentti/3/alkup/fin/txt","content_fi":"Jos patenttilain 71 a §:n mukaan on pyydetty, että vuosimaksu katsottaisiin määräajassa suoritetuksi, on siitä viipymättä tehtävä merkintä rekisteriin. Asiassa annetusta lopullisesta päätöksestä on myös tehtävä merkintä."}}}],"isMemberOf":"sfsd:1980/669/pykala/40","version":"sfl:Original","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/41/ajantasa/20130901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2013/580/alkup","hasPart":[{"@id":"sfsd:1980/669/pykala/41/momentti/1/ajantasa/20130901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/41/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/41/momentti/1/ajantasa/20130901/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/41/momentti/1/ajantasa/20130901/fin/txt","content_fi":"Kun patenttivirastolle on ilmoitettu patentin mitättömäksi julistamista, patentin siirtämistä tai pakkoluvan myöntämistä koskevan kanteen nostamisesta, siitä tehdään merkintä patenttirekisteriin."}}},{"@id":"sfsd:1980/669/pykala/41/momentti/2/ajantasa/20130901","@type":"sfl:SubsectionVersion","cites":"sfsd:2013/100/luku/4/pykala/23","isMemberOf":"sfsd:1980/669/pykala/41/momentti/2","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/41/momentti/2/ajantasa/20130901/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/41/momentti/2/ajantasa/20130901/fin/txt","content_fi":"Kun patenttivirastolle on toimitettu jäljennös asian ratkaisusta oikeudenkäynnistä markkinaoikeudessa annetun lain (100/2013) 4 luvun 23 §:n tai patenttilain 66 d §:n mukaisesti, siitä tehdään merkintä patenttirekisteriin. Ratkaisun saatua lainvoiman, sen pääasiallisesta sisällöstä on lisäksi tehtävä merkintä rekisteriin."}}},{"@id":"sfsd:1980/669/pykala/41/momentti/3/ajantasa/20130901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/41/momentti/3","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/41/momentti/3/ajantasa/20130901/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/41/momentti/3/ajantasa/20130901/fin/txt","content_fi":"Jos patenttia on rajoitettu patenttivirastossa tai tuomioistuimessa taikka se on lakkautettu patenttivirastossa, siitä on tehtävä merkintä patenttirekisteriin. Jos patenttia on rajoitettu, rekisteriin merkitään päivä, jolloin se on tehty sekä luokat. Jos patentti on lakkautettu, rekisteriin merkitään lakkauttamispäivä."}}}],"isMemberOf":"sfsd:1980/669/pykala/41","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/42/ajantasa/20000715","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1980/669/pykala/42/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/42/momentti/1","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/42/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/42/momentti/1/alkup/fin/txt","content_fi":"Patenttilain 44 §:ssä tarkoitetun, patentin siirrosta tahi käyttöluvan tai panttioikeuden myöntämisestä tehtävän merkinnän tulee sisältää oikeudenhaltijan nimi, kotipaikka ja osoite sekä siirron tai luovutuksen päivämäärä. Käyttöluvasta on pyynnöstä merkittävä, onko patentinhaltijan oikeus myöntää muita käyttölupia rajoitettu. Jollei kysymystä siirron tahi käyttölupa- tai panttioikeuden myöntämisen merkitsemisestä välittömästi voida ratkaista, on rekisteriin kuitenkin merkittävä, että tätä on pyydetty."}}},{"@id":"sfsd:1980/669/pykala/42/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/42/momentti/2","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/42/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/42/momentti/2/alkup/fin/txt","content_fi":"Jos patentti on ulosmitattu, on siitä tehtävä merkintä rekisteriin, kun ulosmittauksesta on ilmoitettu."}}},{"@id":"sfsd:1980/669/pykala/42/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/42/momentti/3","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/42/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/42/momentti/3/alkup/fin/txt","content_fi":"Ilmoitus asiamiestä koskevasta muutoksesta merkitään rekisteriin."}}},{"@id":"sfsd:1980/669/pykala/42/momentti/4/ajantasa/20000715","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2000/674/alkup","isMemberOf":"sfsd:1980/669/pykala/42/momentti/4","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/42/momentti/4/ajantasa/20000715/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/42/momentti/4/ajantasa/20000715/fin/txt","content_fi":"Jos patenttiviranomainen on saanut 17 b §:n 3 momentin mukaisen ilmoituksen talletuksen siirrosta tai 17 c §:n 3 momentin mukaisen uudesta biologisen materiaalin talletuksesta annetun kuitin, siirrosta tai uudesta talletuksesta on tehtävä merkintä rekisteriin.\n(13.7.2000/674)"}}}],"isMemberOf":"sfsd:1980/669/pykala/42","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/valiotsikko/15/alkup","@type":"sfl:SubheadingVersion","isMemberOf":"sfsd:1980/669/valiotsikko/15","version":"sfl:Original","versionDate":"2013-09-01","followedBy":["sfsd:1980/669/pykala/42a/ajantasa/20081001","sfsd:1980/669/pykala/43/alkup"],"languageVersion":{"@id":"sfsd:1980/669/valiotsikko/15/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/valiotsikko/15/alkup/fin/txt","content_fi":"Patenttiviraston kuulutukset"}}},{"@id":"sfsd:1980/669/pykala/42a/ajantasa/20081001","@type":"sfl:SectionVersion","amendedBy":"sfsd:2008/603/alkup","hasPart":[{"@id":"sfsd:1980/669/pykala/42a/momentti/1/ajantasa/20081001","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/42a/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/42a/momentti/1/ajantasa/20081001/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/42a/momentti/1/ajantasa/20081001/fin/txt","content_fi":"Patenttiviraston päätökseen, joka annetaan automaattisessa tietojenkäsittelyssä aikaansaatuna asiakirjana, allekirjoitus voidaan merkitä koneellisesti."}}},{"@id":"sfsd:1980/669/pykala/42a/momentti/2/ajantasa/20081001","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/42a/momentti/2","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/42a/momentti/2/ajantasa/20081001/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/42a/momentti/2/ajantasa/20081001/fin/txt","content_fi":"Patenttirekisteristä ja patenttiviraston hakemuksia koskevasta päiväkirjasta annettava automaattisen tietojenkäsittelyn avulla valmistettu asiakirja voidaan allekirjoittaa koneellisesti. Tästä on oltava maininta asiakirjassa."}}}],"isMemberOf":"sfsd:1980/669/pykala/42a","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/43/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1980/669/pykala/43/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/43/momentti/1","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/43/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/43/momentti/1/alkup/fin/txt","content_fi":"Kuulutukset patenttiasioissa julkaistaan patenttiviraston toimittamassa julkaisussa."}}},"isMemberOf":"sfsd:1980/669/pykala/43","version":"sfl:Original","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/valiotsikko/16/alkup","@type":"sfl:SubheadingVersion","isMemberOf":"sfsd:1980/669/valiotsikko/16","version":"sfl:Original","versionDate":"2013-09-01","followedBy":"sfsd:1980/669/pykala/44/ajantasa/20071213","languageVersion":{"@id":"sfsd:1980/669/valiotsikko/16/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/valiotsikko/16/alkup/fin/txt","content_fi":"Erityinen kannevalta"}}},{"@id":"sfsd:1980/669/pykala/44/ajantasa/20071213","@type":"sfl:SectionVersion","amendedBy":"sfsd:2007/1118/alkup","hasPart":{"@id":"sfsd:1980/669/pykala/44/momentti/1/ajantasa/20071213","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/44/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/44/momentti/1/ajantasa/20071213/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/44/momentti/1/ajantasa/20071213/fin/txt","content_fi":"Patenttilain 52 §:n 4 momentissa tarkoitettu viranomainen on virallinen syyttäjä, jollei valtioneuvosto erityisestä syystä määrää muuta viranomaista ajamaan kannetta."}}},"isMemberOf":"sfsd:1980/669/pykala/44","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/valiotsikko/17/alkup","@type":"sfl:SubheadingVersion","isMemberOf":"sfsd:1980/669/valiotsikko/17","version":"sfl:Original","versionDate":"2013-09-01","followedBy":"sfsd:1980/669/pykala/45/ajantasa/20060306","languageVersion":{"@id":"sfsd:1980/669/valiotsikko/17/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/valiotsikko/17/alkup/fin/txt","content_fi":"Kansainvälisiä patenttihakemuksia vastaanottava viranomainen"}}},{"@id":"sfsd:1980/669/pykala/45/ajantasa/20060306","@type":"sfl:SectionVersion","amendedBy":"sfsd:2006/144/alkup","hasPart":{"@id":"sfsd:1980/669/pykala/45/momentti/1/ajantasa/20060306","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/45/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/45/momentti/1/ajantasa/20060306/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/45/momentti/1/ajantasa/20060306/fin/txt","content_fi":"Patenttivirasto on kansainvälisiä patenttihakemuksia vastaanottava viranomainen, kun hakija tai joku hakijoista on Suomen kansalainen, luonnollinen henkilö, jolla on kotipaikka Suomessa, Suomen lain mukaan muodostettu oikeushenkilö taikka joku, joka harjoittaa liikettä Suomessa. Tällainen hakija voi tehdä kansainvälisen patenttihakemuksen myös Euroopan patenttivirastoon tai Maailman henkisen omaisuuden järjestön kansainväliselle toimistolle."}}},"isMemberOf":"sfsd:1980/669/pykala/45","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/46/ajantasa/20071213","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1980/669/pykala/46/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/46/momentti/1","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/46/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/46/momentti/1/alkup/fin/txt","content_fi":"Vastaanottavana viranomaisena patenttivirasto vastaanottaa, tarkastaa ja toimittaa edelleen kansainväliset patenttihakemukset patenttiyhteistyösopimuksen ja sen sovellutussääntöjen mukaisesti."}}},{"@id":"sfsd:1980/669/pykala/46/momentti/2/ajantasa/20071213","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2007/1118/alkup","hasPart":[{"@id":"sfsd:1980/669/pykala/46/momentti/2/johdanto/alkup","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1980/669/pykala/46/momentti/2/johdanto","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/46/momentti/2/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/46/momentti/2/johdanto/alkup/fin/txt","content_fi":"Kun patenttivirasto toimii kansainvälisiä patenttihakemuksia vastaanottavana viranomaisena, hakijan on suoritettava:"}}},{"@id":"sfsd:1980/669/pykala/46/momentti/2/kohta/1/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1980/669/pykala/46/momentti/2/kohta/1","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/46/momentti/2/kohta/1/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/46/momentti/2/kohta/1/alkup/fin/txt","content_fi":"1) patenttiyhteistyösopimuksen sovellutussäännössä 15.1 tarkoitettu kansainvälinen hakemusmaksu kuukauden kuluessa hakemuksen vastaanottamisesta;"}}},{"@id":"sfsd:1980/669/pykala/46/momentti/2/kohta/2/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1980/669/pykala/46/momentti/2/kohta/2","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/46/momentti/2/kohta/2/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/46/momentti/2/kohta/2/alkup/fin/txt","content_fi":"2) patenttiyhteistyösopimuksen sovellutussäännössä 16.1 tarkoitettu uutuustutkimusmaksu kuukauden kuluessa hakemuksen vastaanottamisesta;"}}},{"@id":"sfsd:1980/669/pykala/46/momentti/2/kohta/3/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1980/669/pykala/46/momentti/2/kohta/3","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/46/momentti/2/kohta/3/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/46/momentti/2/kohta/3/alkup/fin/txt","content_fi":"3) patenttiyhteistyösopimuksen sovellutussäännössä 14.1 tarkoitettu lähettämismaksu kuukauden kuluessa hakemuksen vastaanottamisesta; sekä"}}},{"@id":"sfsd:1980/669/pykala/46/momentti/2/kohta/4/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1980/669/pykala/46/momentti/2/kohta/4","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/46/momentti/2/kohta/4/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/46/momentti/2/kohta/4/alkup/fin/txt","content_fi":"4) patenttiyhteistyösopimuksen sovellutussäännössä 26bis.3 tarkoitettu etuoikeuden palauttamista koskeva maksu kahden kuukauden kuluessa etuoikeusvuoden päättymisestä."}}}],"isMemberOf":"sfsd:1980/669/pykala/46/momentti/2","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/46/momentti/3/ajantasa/20060306","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2006/144/alkup","isMemberOf":"sfsd:1980/669/pykala/46/momentti/3","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/46/momentti/3/ajantasa/20060306/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/46/momentti/3/ajantasa/20060306/fin/txt","content_fi":"Jos kansainvälistä hakemusmaksua korotetaan sen jälkeen kun hakemus on vastaanotettu mutta ennen kuin maksu suoritetaan, voidaan maksu suorittaa korottamattomana kuukauden kuluessa hakemuksen vastaanottamisesta.\n(16.2.2006/144)"}}},{"@id":"sfsd:1980/669/pykala/46/momentti/4/ajantasa/20071213","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2007/1118/alkup","isMemberOf":"sfsd:1980/669/pykala/46/momentti/4","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/46/momentti/4/ajantasa/20071213/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/46/momentti/4/ajantasa/20071213/fin/txt","content_fi":"Jollei 2 momentin 1–3 kohdassa tarkoitettua maksua ole suoritettu säädetyssä ajassa tai se on määräajan päättyessä suoritettu riittämättömänä, tulee patenttiviraston kehottaa hakijaa suorittamaan puuttuva maksu kuukauden kuluessa kehotuksen antamisesta.\n(29.11.2007/1118)"}}}],"isMemberOf":"sfsd:1980/669/pykala/46","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/47/ajantasa/20060306","@type":"sfl:SectionVersion","amendedBy":"sfsd:2006/144/alkup","hasPart":{"@id":"sfsd:1980/669/pykala/47/momentti/1/ajantasa/20060306","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/47/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/47/momentti/1/ajantasa/20060306/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/47/momentti/1/ajantasa/20060306/fin/txt","content_fi":"Patenttivirasto huolehtii hakijan pyynnöstä erikseen vahvistettua maksua vastaan siitä, että virastoon aikaisemmin jätetyn hakemuksen jäljennös lähetetään sovellutussäännön 17.1b mukaisesti Maailman henkisen omaisuuden järjestön kansainväliselle toimistolle. Maksu on suoritettava samassa ajassa kuin mainitun säännön mukainen pyyntö on esitettävä."}}},"isMemberOf":"sfsd:1980/669/pykala/47","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/48/ajantasa/20060306","@type":"sfl:SectionVersion","amendedBy":"sfsd:2006/144/alkup","hasPart":{"@id":"sfsd:1980/669/pykala/48/momentti/1/ajantasa/20060306","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/48/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/48/momentti/1/ajantasa/20060306/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/48/momentti/1/ajantasa/20060306/fin/txt","content_fi":"Patenttivirastoon tehtävä kansainvälinen patenttihakemus toimitetaan yhtenä kappaleena. Se on laadittava suomeksi, ruotsiksi tai englanniksi taikka muulla patenttiviraston hyväksymällä kielellä. Kansainvälisen hakemuksen anomuksen tulee olla englanninkielinen, vaikka itse hakemus on laadittu suomeksi tai ruotsiksi."}}},"isMemberOf":"sfsd:1980/669/pykala/48","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/49/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1980/669/pykala/49/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/49/momentti/1","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/49/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/49/momentti/1/alkup/fin/txt","content_fi":"Patenttivirastoon annetuista kansainvälisistä patenttihakemuksista pidetään erillistä päiväkirjaa. Päiväkirja ei ole julkinen."}}},"isMemberOf":"sfsd:1980/669/pykala/49","version":"sfl:Original","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/50/ajantasa/20000715","@type":"sfl:SectionVersion","amendedBy":"sfsd:2000/674/alkup","hasPart":{"@id":"sfsd:1980/669/pykala/50/momentti/1/ajantasa/20000715","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/50/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/50/momentti/1/ajantasa/20000715/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/50/momentti/1/ajantasa/20000715/fin/txt","content_fi":"Patentinhakijalla, jolla ei ole kotipaikkaa Suomessa, tulee olla Euroopan talousalueella asuva asiamies, joka on oikeutettu edustamaan häntä patenttivirastossa hakemusta koskevissa asioissa."}}},"isMemberOf":"sfsd:1980/669/pykala/50","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/51/ajantasa/19850901","@type":"sfl:SectionVersion","amendedBy":"sfsd:1985/505/alkup","hasPart":{"@id":"sfsd:1980/669/pykala/51/momentti/1/ajantasa/19850901","@type":"sfl:SubsectionVersion","cites":"sfsd:1967/551","isMemberOf":"sfsd:1980/669/pykala/51/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/51/momentti/1/ajantasa/19850901/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/51/momentti/1/ajantasa/19850901/fin/txt","content_fi":"Jollei maanpuolustukselle merkityksellisistä keksinnöistä annetun lain (551/67) mukaan ole estettä sille, patenttiviraston on patenttiyhteistyösopimuksen ja sen sovellutussääntöjen mukaisesti toimitettava vastaanottamansa kansainvälinen hakemus edelleen maailman henkisen omaisuuden järjestön kansainväliseen toimistoon."}}},"isMemberOf":"sfsd:1980/669/pykala/51","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/valiotsikko/18/alkup","@type":"sfl:SubheadingVersion","isMemberOf":"sfsd:1980/669/valiotsikko/18","version":"sfl:Original","versionDate":"2013-09-01","followedBy":"sfsd:1980/669/pykala/51a/ajantasa/20060306","languageVersion":{"@id":"sfsd:1980/669/valiotsikko/18/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/valiotsikko/18/alkup/fin/txt","content_fi":"Kansainvälisen hakemuksen välittäminen kansainväliselle toimistolle"}}},{"@id":"sfsd:1980/669/pykala/51a/ajantasa/20060306","@type":"sfl:SectionVersion","amendedBy":"sfsd:1997/246/alkup","hasPart":[{"@id":"sfsd:1980/669/pykala/51a/momentti/1/ajantasa/19970401","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/51a/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/51a/momentti/1/ajantasa/19970401/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/51a/momentti/1/ajantasa/19970401/fin/txt","content_fi":"Milloin patenttivirasto ei ole toimivaltainen vastaanottamaan hakijan, jolla on kotipaikka sopimusvaltiossa tai joka on sopimusvaltion kansalainen, sille tekemää kansainvälistä hakemusta, lähettää patenttivirasto viipymättä hakemuksen edelleen kansainväliselle toimistolle patenttiyhteistyösopimuksen ja sen sovellutussääntöjen mukaisesti."}}},{"@id":"sfsd:1980/669/pykala/51a/momentti/2/ajantasa/20060306","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2006/144/alkup","isMemberOf":"sfsd:1980/669/pykala/51a/momentti/2","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/51a/momentti/2/ajantasa/20060306/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/51a/momentti/2/ajantasa/20060306/fin/txt","content_fi":"Hakijan on tällöin suoritettava kuukauden kuluessa kansainvälisen hakemuksen vastaanottamisesta patenttivirastolle lähettämismaksu, jota tarkoitetaan patenttiyhteistyösopimuksen sovellutussäännössä 19.4. Tällä tavoin kansainvälisen toimiston puolesta vastaanotettu kansainvälinen hakemus katsotaan vastaanotetuksi sinä päivänä, jona kansainvälinen hakemus on tehty patenttivirastoon.\n(16.2.2006/144)"}}}],"isMemberOf":"sfsd:1980/669/pykala/51a","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/valiotsikko/19/alkup","@type":"sfl:SubheadingVersion","isMemberOf":"sfsd:1980/669/valiotsikko/19","version":"sfl:Original","versionDate":"2013-09-01","followedBy":"sfsd:1980/669/pykala/52/ajantasa/20130901","languageVersion":{"@id":"sfsd:1980/669/valiotsikko/19/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/valiotsikko/19/alkup/fin/txt","content_fi":"Kansainvälisen patenttihakemuksen jatkaminen \n\n(14.6.1985/505)"}}},{"@id":"sfsd:1980/669/pykala/52/ajantasa/20130901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2013/580/alkup","hasPart":{"@id":"sfsd:1980/669/pykala/52/momentti/1/ajantasa/20130901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/52/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/52/momentti/1/ajantasa/20130901/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/52/momentti/1/ajantasa/20130901/fin/txt","content_fi":"Jos kansainvälinen patenttihakemus on tehty muulla kuin patenttilain 8 §:n 5 momentin mukaisella kielellä, vaaditaan hakemuksesta käännös jatkettaessa hakemusta patenttilain 31 §:n mukaan ja esitettäessä patenttilain 38 §:n 1 momentin mukainen tutkimuspyyntö. Tällöin on noudatettava tämän asetuksen 3 §:n käännöstä koskevia säännöksiä."}}},"isMemberOf":"sfsd:1980/669/pykala/52","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/52a/ajantasa/19850901","@type":"sfl:SectionVersion","amendedBy":"sfsd:1985/505/alkup","hasPart":{"@id":"sfsd:1980/669/pykala/52a/momentti/1/ajantasa/19850901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/52a/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/52a/momentti/1/ajantasa/19850901/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/52a/momentti/1/ajantasa/19850901/fin/txt","content_fi":"Jos hakija on tehnyt kansainväliselle hakemukselle kaiken, mitä patenttilain 31 §:n mukaan vaaditaan hakemuksen jatkamiseen, mutta patenttivirasto ei ole saanut kansainväliseltä toimistolta ilmoitusta hakemuksen saapumisesta toimistoon, patenttiviraston on ilmoitettava tästä kansainväliselle toimistolle."}}},"isMemberOf":"sfsd:1980/669/pykala/52a","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/52b/ajantasa/20050101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2004/1200/alkup","hasPart":{"@id":"sfsd:1980/669/pykala/52b/momentti/1/ajantasa/20050101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/52b/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/52b/momentti/1/ajantasa/20050101/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/52b/momentti/1/ajantasa/20050101/fin/txt","content_fi":"Patenttilain 34 §:ssä tarkoitettu määräaika on neljä kuukautta patenttilain 31 §:n 1 momentissa kansainvälisen patenttihakemuksen jatkamiselle säädetyn määräajan päättymisestä."}}},"isMemberOf":"sfsd:1980/669/pykala/52b","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/52c/ajantasa/19850901","@type":"sfl:SectionVersion","amendedBy":"sfsd:1985/505/alkup","hasPart":[{"@id":"sfsd:1980/669/pykala/52c/momentti/1/ajantasa/19850901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/52c/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/52c/momentti/1/ajantasa/19850901/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/52c/momentti/1/ajantasa/19850901/fin/txt","content_fi":"Patenttilain 38 §:n 1 momentin mukainen tutkimusta koskeva pyyntö on esitettävä kahden kuukauden kuluessa siitä, kun vastaanottava viranomainen tai kansainvälinen toimisto on lähettänyt hakijalle tiedon sanotussa momentissa tarkoitetusta päätöksestä."}}},{"@id":"sfsd:1980/669/pykala/52c/momentti/2/ajantasa/19850901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/52c/momentti/2","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/52c/momentti/2/ajantasa/19850901/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/52c/momentti/2/ajantasa/19850901/fin/txt","content_fi":"Jos hakija näyttää, että hän on vastaanottanut 1 momentissa tarkoitetun tiedotuksen myöhemmin kuin seitsemän päivää tiedotuksen päiväyksen jälkeen, pidennetään määräaikaa yhtä monella päivällä kuin on kulunut tiedotuksen päiväyksestä siihen, kun hakija vastaanotti tiedotuksen. Määräajasta vähennetään kuitenkin edellä mainitut seitsemän päivää."}}}],"isMemberOf":"sfsd:1980/669/pykala/52c","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/valiotsikko/20/alkup","@type":"sfl:SubheadingVersion","isMemberOf":"sfsd:1980/669/valiotsikko/20","version":"sfl:Original","versionDate":"2013-09-01","followedBy":"sfsd:1980/669/pykala/52d/ajantasa/20111101","languageVersion":{"@id":"sfsd:1980/669/valiotsikko/20/alkup/fin","hasFormat":{"@id":"sfsd:1980/669/valiotsikko/20/alkup/fin/txt","content_fi":"Lisäsuojatodistus\n\n(21.3.1997/246)"}}},{"@id":"sfsd:1980/669/pykala/52d/ajantasa/20111101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2011/1097/alkup","hasPart":[{"@id":"sfsd:1980/669/pykala/52d/momentti/1/ajantasa/20111101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/52d/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/52d/momentti/1/ajantasa/20111101/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/52d/momentti/1/ajantasa/20111101/fin/txt","content_fi":"Lisäsuojatodistuksesta säädetään patenttilain 70 a §:ssä."}}},{"@id":"sfsd:1980/669/pykala/52d/momentti/2/ajantasa/20111101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/52d/momentti/2","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/52d/momentti/2/ajantasa/20111101/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/52d/momentti/2/ajantasa/20111101/fin/txt","content_fi":"Lisäsuojatodistusasetuksilla tarkoitetaan lääkkeiden lisäsuojatodistuksesta annettua Euroopan parlamentin ja neuvoston asetusta (EY) N:o 469/2009 ja kasvinsuojeluaineiden lisäsuojatodistuksen käyttöön ottamisesta annettua Euroopan parlamentin ja neuvoston asetusta (EY) N:o 1610/96."}}}],"isMemberOf":"sfsd:1980/669/pykala/52d","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/52e/ajantasa/20111101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2011/1097/alkup","hasPart":[{"@id":"sfsd:1980/669/pykala/52e/momentti/1/ajantasa/20111101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/52e/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/52e/momentti/1/ajantasa/20111101/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/52e/momentti/1/ajantasa/20111101/fin/txt","content_fi":"Lisäsuojatodistusta ja lääkkeiden lisäsuojatodistuksen voimassaoloajan jatkamista koskevat hakemukset on tehtävä kirjallisesti."}}},{"@id":"sfsd:1980/669/pykala/52e/momentti/2/ajantasa/20111101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/52e/momentti/2","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/52e/momentti/2/ajantasa/20111101/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/52e/momentti/2/ajantasa/20111101/fin/txt","content_fi":"Hakemuskirjan on oltava hakijan tai hänen asiamiehensä allekirjoittama, ja sen tulee sisältää asianomaisen lisäsuojatodistusasetuksen 8 artiklassa säädetyt tiedot. Jos useat yhdessä hakevat lisäsuojatodistusta, hakemuskirjassa tulee lisäksi olla ilmoitus siitä, kuka heistä on oikeutettu tai, jos hakijat käyttävät asiamiestä, kenet he yhteisesti ovat valtuuttaneet kaikkien puolesta ottamaan vastaan patenttiviraston ilmoitukset."}}},{"@id":"sfsd:1980/669/pykala/52e/momentti/3/ajantasa/20111101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/52e/momentti/3","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/52e/momentti/3/ajantasa/20111101/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/52e/momentti/3/ajantasa/20111101/fin/txt","content_fi":"Hakijan on lisäksi annettava patenttivirastolle tuotetta koskevia lisätietoja, milloin tämä on välttämätöntä hakemuksen käsittelyn kannalta."}}}],"isMemberOf":"sfsd:1980/669/pykala/52e","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/52f/ajantasa/19940701","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/595/alkup","hasPart":{"@id":"sfsd:1980/669/pykala/52f/momentti/1/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1980/669/pykala/52f/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/52f/momentti/1/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/52f/momentti/1/ajantasa/19940701/fin/txt","content_fi":"Hakemus on tehtävä suomen tai ruotsin kielellä voimassa olevan kielilainsäädännön säännösten mukaan. Jos hakemuksen liitteenä oleva asiakirja on laadittu muulla kuin edellä mainitulla kielellä, virasto voi vaatia käännöstä suomeksi tai ruotsiksi."}}},"isMemberOf":"sfsd:1980/669/pykala/52f","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1980/669/pykala/52g/ajantasa/20111101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/595/alkup","hasPart":[{"@id":"sfsd:1980/669/pykala/52g/momentti/1/ajantasa/20111101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:1997/246/alkup","hasPart":[{"@id":"sfsd:1980/669/pykala/52g/momentti/1/johdanto/ajantasa/20111101","@type":"sfl:PreambleVersion","amendedBy":"sfsd:2011/1097/alkup","isMemberOf":"sfsd:1980/669/pykala/52g/momentti/1/johdanto","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/52g/momentti/1/johdanto/ajantasa/20111101/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/52g/momentti/1/johdanto/ajantasa/20111101/fin/txt","content_fi":"Patenttivirasto pitää päiväkirjaa saapuneista hakemuksista. Päiväkirjaan merkitään kustakin hakemuksesta asianomaisen lisäsuojatodistusasetuksen 9 artiklan 2 kohdassa lueteltujen tietojen lisäksi: (27.10.2011/1097)"}}},{"@id":"sfsd:1980/669/pykala/52g/momentti/1/kohta/1/ajantasa/19940701","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1980/669/pykala/52g/momentti/1/kohta/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/52g/momentti/1/kohta/1/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/52g/momentti/1/kohta/1/ajantasa/19940701/fin/txt","content_fi":"1) hakemuksen päiväkirjanumero ja tekemispäivä;"}}},{"@id":"sfsd:1980/669/pykala/52g/momentti/1/kohta/2/ajantasa/19940701","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1980/669/pykala/52g/momentti/1/kohta/2","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/52g/momentti/1/kohta/2/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/52g/momentti/1/kohta/2/ajantasa/19940701/fin/txt","content_fi":"2) jos hakijaa edustaa asiamies, tämän nimi, kotipaikka ja osoite;"}}},{"@id":"sfsd:1980/669/pykala/52g/momentti/1/kohta/3/ajantasa/19940701","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1980/669/pykala/52g/momentti/1/kohta/3","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1980/669/pykala/52g/momentti/1/kohta/3/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1980/669/pykala/52g/momentti/1/kohta/3/ajantasa/19940701/fin/txt","content_fi":"3) asiassa saapuneet kirjelmät ja suoritetut maksut; sekä"}}},{"@id":"sfsd:1980/669/pykala/52g/momentti/1/kohta/4/ajanta