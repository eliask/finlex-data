onVersion","hasPart":[{"@id":"sfsd:1940/378/luku/3/pykala/20/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/3/pykala/20/momentti/1","version":"sfl:Original","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/3/pykala/20/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1940/378/luku/3/pykala/20/momentti/1/alkup/fin/txt","content_fi":"Velvollisuus suorittaa lahjaveroa alkaa, kun lahjansaaja on saanut lahjan haltuunsa. Milloin alaikäinen on vanhemmiltaan saanut veronalaisen lahjan, katsotaan verovelvollisuuden alkavan silloin, kun lahjoitus on tehty."}}},{"@id":"sfsd:1940/378/luku/3/pykala/20/momentti/2/ajantasa/19960101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:1995/1392/alkup","isMemberOf":"sfsd:1940/378/luku/3/pykala/20/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/3/pykala/20/momentti/2/ajantasa/19960101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/3/pykala/20/momentti/2/ajantasa/19960101/fin/txt","content_fi":"Jos lahjansaaja on kolmen vuoden kuluessa ennen verovelvollisuuden alkamista saanut samalta antajalta yhden tai useampia verotettavia lahjoja, on nekin veroa määrättäessä otettava lukuun. Verosta on kuitenkin vähennettävä se lahjavero, joka aikaisemmista lahjoista on jo suoritettu.\n(8.12.1995/1392)"}}},{"@id":"sfsd:1940/378/luku/3/pykala/20/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/3/pykala/20/momentti/3","version":"sfl:Original","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/3/pykala/20/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1940/378/luku/3/pykala/20/momentti/3/alkup/fin/txt","content_fi":"Eri antajilta saadut lahjat on erikseen verotettava."}}}],"isMemberOf":"sfsd:1940/378/luku/3/pykala/20","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/3/pykala/21/ajantasa/20080101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2007/1063/alkup","hasPart":{"@id":"sfsd:1940/378/luku/3/pykala/21/momentti/1/ajantasa/20080101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/3/pykala/21/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/3/pykala/21/momentti/1/ajantasa/20080101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/3/pykala/21/momentti/1/ajantasa/20080101/fin/txt","content_fi":"Mitä 9–11 ja 15 §:ssä säädetään perinnöstä ja testamentista, koskee myös lahjaa."}}},"isMemberOf":"sfsd:1940/378/luku/3/pykala/21","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/3/pykala/21a/ajantasa/20050101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2004/729/alkup","hasPart":{"@id":"sfsd:1940/378/luku/3/pykala/21a/momentti/1/ajantasa/20050101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/3/pykala/21a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/3/pykala/21a/momentti/1/ajantasa/20050101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/3/pykala/21a/momentti/1/ajantasa/20050101/fin/txt","content_fi":"Jos tuloverotuksessa on sovellettu tuloverolain 47 §:n 1 momentin säännöstä, jonka perusteella omaisuuden hankintameno on laskettu lahjoittajan hankintamenosta, omaisuuden arvosta vähennetään tuloverotuksessa luovutusvoitosta maksuunpantu vero. Omaisuuden arvosta vähennetään kuitenkin enintään se veron määrä, joka luovutusvoitosta olisi määrätty, jos luovutushintana olisi ollut lahjaverotuksessa käytetty verotusarvo."}}},"isMemberOf":"sfsd:1940/378/luku/3/pykala/21a","version":"sfl:Consolidated","versionDate":"2018-05-01"}],"isMemberOf":"sfsd:1940/378/luku/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/3/ajantasa/20170101/fin","title_fi":"Lahjaveron suorittamisvelvollisuus ja määrä"}},{"@id":"sfsd:1940/378/luku/4/ajantasa/20180501","@type":"sfl:ChapterVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/22/ajantasa/20100901","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/22","repealedBy":"sfsd:2010/523/alkup","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/22/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/22/ajantasa/20100901/fin/txt","content_fi":"22–24 §\n\n\n22–24 § on kumottu L:lla 11.6.2010/523."}}},{"@id":"sfsd:1940/378/luku/4/pykala/24/ajantasa/20100901","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/24","repealedBy":"sfsd:2010/523/alkup","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/24/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/24/ajantasa/20100901/fin/txt","content_fi":"24 §(11.6.2010/523)\n\n24 § on kumottu L:lla 11.6.2010/523."}}},{"@id":"sfsd:1940/378/luku/4/pykala/24/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/783/alkup","hasPart":{"@id":"sfsd:1940/378/luku/4/pykala/24/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/24/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/24/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/24/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Verovuosi on kalenterivuosi, jona perinnönjättäjä on kuollut, lahjansaaja on saanut lahjan haltuunsa tai verovelvollisuus on muutoin alkanut."}}},"isMemberOf":"sfsd:1940/378/luku/4/pykala/24","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/25/ajantasa/19940701","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/25/momentti/1/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/25/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/25/momentti/1/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/25/momentti/1/ajantasa/19940701/fin/txt","content_fi":"Perintöverotus toimitetaan perinnönjättäjän jäämistöstä laaditun perukirjan ja, jos perinnönjako on jo toimitettu, lisäksi jakokirjan perusteella."}}},{"@id":"sfsd:1940/378/luku/4/pykala/25/momentti/2/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/25/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/25/momentti/2/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/25/momentti/2/ajantasa/19940701/fin/txt","content_fi":"Edellä 22 §:n 2 momentissa mainituissa tapauksissa on verotusta toimitettaessa otettava huomioon myös edellisen haltijan tai osuutta rasittavan oikeuden haltijan jäämistöstä laadittu perukirja."}}},{"@id":"sfsd:1940/378/luku/4/pykala/25/momentti/3/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/25/momentti/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/25/momentti/3/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/25/momentti/3/ajantasa/19940701/fin/txt","content_fi":"Verohallinto voi kuolinpesän osakkaan ennen verotuksen toimittamista tekemästä hakemuksesta lykätä perintöverotuksen toimittamisen määräajaksi, kuitenkin enintään vuodeksi perukirjan Verohallinnolle jättämiselle säädetyn ajan päättymisestä, jos hakemuksen tueksi esitetään painavia syitä. Tällaisena syynä on muun muassa pidettävä perinnönjaon toimittamista pesässä, johon sisältyvän maatilan tai muun yrityksen toimintaa jatketaan kuolinpesän tai sen osakkaiden lukuun, kun kysymyksessä ei ole kuolinpesän kokonaisvaroihin nähden vähäinen varallisuuserä."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/25","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/26/ajantasa/19960101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1995/1561/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/26/momentti/1/ajantasa/19960101","@type":"sfl:SubsectionVersion","cites":"sfsd:1965/40/luku/20","isMemberOf":"sfsd:1940/378/luku/4/pykala/26/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/26/momentti/1/ajantasa/19960101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/26/momentti/1/ajantasa/19960101/fin/txt","content_fi":"Perintöverotuksen toimittamista varten on perintökaaren 20 luvussa tarkoitetun pesän ilmoittajan annettava kehotuksetta perinnönjättäjän jälkeen laadittu perukirja. Edellä 22 §:n 2 momentissa tarkoitetuissa tapauksissa on annettava myös edellisen haltijan tai osuutta rasittavan oikeuden haltijan jäämistöstä laadittu perukirja."}}},{"@id":"sfsd:1940/378/luku/4/pykala/26/momentti/2/ajantasa/19960101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/26/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/26/momentti/2/ajantasa/19960101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/26/momentti/2/ajantasa/19960101/fin/txt","content_fi":"Verovelvollisen on annettava veroilmoitus, jos perinnönjättäjän jälkeen ei ole laadittava perukirjaa tai jos velvollisuus suorittaa perintöveroa muissa kuin 22 §:n 2 momentissa tarkoitetuissa tapauksissa alkaa myöhemmin kuin 5 §:ssä säädetään."}}},{"@id":"sfsd:1940/378/luku/4/pykala/26/momentti/3/ajantasa/19960101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/26/momentti/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/26/momentti/3/ajantasa/19960101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/26/momentti/3/ajantasa/19960101/fin/txt","content_fi":"Jos verovelvollinen ei asu Suomessa, velvollinen antamaan veroilmoituksen on se, jolla on kuolinpesä hallussaan."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/26","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/27/ajantasa/19940701","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/27/momentti/1/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/27/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/27/momentti/1/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/27/momentti/1/ajantasa/19940701/fin/txt","content_fi":"Lahjaverotuksen toimittamista varten on lahjansaajan annettava kehotuksetta lahjaveroilmoitus."}}},{"@id":"sfsd:1940/378/luku/4/pykala/27/momentti/2/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/27/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/27/momentti/2/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/27/momentti/2/ajantasa/19940701/fin/txt","content_fi":"Jos lahjasta ei ole 19 §:n nojalla suoritettava veroa, veroilmoitusta ei ole annettava, ellei sitä erikseen vaadita."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/27","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/28/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/28/momentti/1/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/28/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/28/momentti/1/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/28/momentti/1/ajantasa/19940701/fin/txt","content_fi":"Perukirjassa on mainittava, kuka pesänhoitajana ottaa vastaan perintöverotusta koskevia ilmoituksia ja tiedusteluja."}}},{"@id":"sfsd:1940/378/luku/4/pykala/28/momentti/2/ajantasa/19960101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:1995/1392/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/28/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/28/momentti/2/ajantasa/19960101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/28/momentti/2/ajantasa/19960101/fin/txt","content_fi":"Perukirjassa on myös mainittava, onko perilliselle tai testamentinsaajalle annettu sellaista omaisuutta, joka 16 §:n mukaan on otettava lukuun perintöveroa määrättäessä, ja tällaisen omaisuuden laatu ja arvo. Perukirjassa on myös mainittava 7 a §:ssä tarkoitettujen etuuksien saajat ja määrät.\n(8.12.1995/1392)"}}},{"@id":"sfsd:1940/378/luku/4/pykala/28/momentti/3/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/523/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/28/momentti/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/28/momentti/3/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/28/momentti/3/ajantasa/20100901/fin/txt","content_fi":"Verohallinto antaa tarkemmat määräykset siitä, mitä asiakirjoja verotuksen toimittamista varten on annettava. Verohallinto voi myös määrätä, että perintö- tai lahjaverotusta varten annettavat tiedot tulee antaa Verohallinnon vahvistamaa lomaketta käyttäen.\n(11.6.2010/523)"}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/28","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/28a/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/523/alkup","hasPart":{"@id":"sfsd:1940/378/luku/4/pykala/28a/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/28a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/28a/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/28a/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Vakuutusyhtiön tai muun etuuden maksajan on ilmoitettava maksamistaan 7 a tai 18 a §:ssä tarkoitetuista etuuksista Verohallinnolle. Verohallinto antaa tarkemmat määräykset ilmoituksen ajankohdasta, sisällöstä ja tavasta."}}},"isMemberOf":"sfsd:1940/378/luku/4/pykala/28a","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/29/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/523/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/29/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/29/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/29/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/29/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Perukirja on annettava kuukauden kuluessa perunkirjoituksesta Verohallinnolle."}}},{"@id":"sfsd:1940/378/luku/4/pykala/29/momentti/2/ajantasa/20100901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/29/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/29/momentti/2/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/29/momentti/2/ajantasa/20100901/fin/txt","content_fi":"Verohallinto voi ilmoittaa käräjäoikeudelle perunkirjoituksen toimittamisen laiminlyönnistä."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/29","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/30/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/523/alkup","hasPart":{"@id":"sfsd:1940/378/luku/4/pykala/30/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/30/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/30/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/30/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Veroilmoitus on annettava Verohallinnolle kolmen kuukauden kuluessa verovelvollisuuden alkamisesta."}}},"isMemberOf":"sfsd:1940/378/luku/4/pykala/30","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/31/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/523/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/31/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Verohallinto toimittaa perintö- ja lahjaverotuksen. Se myös oikaisee havaitsemansa 40 tai 41 §:ssä tarkoitetun virheen.\n(11.6.2010/523)"}}},{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/2/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/523/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/31/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/2/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/2/ajantasa/20100901/fin/txt","content_fi":"Verotus katsotaan toimitetuksi päivänä, jona Verohallinto on käsitellyt asian. Verotuksen toimittamispäivän tulee ilmetä verotusasiakirjoista.\n(11.6.2010/523)"}}},{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/3/ajantasa/19991230","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/31/momentti/3","repealedBy":"sfsd:1999/1346/alkup","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/3/ajantasa/19991230/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/3/ajantasa/19991230/fin/txt","content_fi":"3 momentti on kumottu L:lla 30.12.1999/1346."}}},{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/4/ajantasa/20020101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2001/909/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/31/momentti/4","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/4/ajantasa/20020101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/4/ajantasa/20020101/fin/txt","content_fi":"Verotettavan perintöosuuden tai lahjan arvo vahvistetaan täysin sadoin euroin siten, että ylimenevät eurot jätetään ottamatta huomioon.\n(26.10.2001/909)"}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/31","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/31/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/783/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/31/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Verohallinto toimittaa perintö- ja lahjaverotuksen. Se myös oikaisee havaitsemansa 40 tai 41 §:ssä tarkoitetun virheen."}}},{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/2/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/31/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/2/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/2/ajantasa/20170101/fin/txt","content_fi":"Perintö- ja lahjaverotus on toimitettava tai oikaisu on suoritettava kolmen vuoden kuluessa verovuoden päättymisestä. Verotus katsotaan toimitetuksi ja verotuksen oikaisu suoritetuksi määräajassa, jos Verohallinnon päätös siitä on tehty ennen tässä laissa säädettyjen määräaikojen päättymistä."}}},{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/3/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/31/momentti/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/3/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/3/ajantasa/20170101/fin/txt","content_fi":"Verohallinto voi kuolinpesän osakkaan tai lahjansaajan ennen verotuksen toimittamista tekemästä hakemuksesta lykätä perintö- tai lahjaverotuksen toimittamisen määräajaksi, kuitenkin niin, että verotus on toimitettava 10 vuoden kuluessa verovuoden päättymisestä."}}},{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/4/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/31/momentti/4","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/4/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/31/momentti/4/ajantasa/20170101/fin/txt","content_fi":"Verotettavan perintöosuuden tai lahjan arvo vahvistetaan täysin sadoin euroin siten, että ylimenevät eurot jätetään ottamatta huomioon."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/31","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/32/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/523/alkup","hasPart":{"@id":"sfsd:1940/378/luku/4/pykala/32/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/32/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/32/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/32/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Kuolinpesän osakas, erityisjälkisäädöksen saaja ja eloonjäänyt puoliso, silloinkin kun hän ei ole kuolinpesän osakas, sekä näiden lailliset edustajat, perunkirjoituksen toimittajat ja se, joka on ilmoittanut pesän tai hoitaa sitä, samoin kuin lahjanantaja ja -saaja ovat velvolliset Verohallinnon kehotuksesta antamaan kaikki pesää tai lahjaa koskevat tiedot ja selvitykset sekä esittämään tarkastettaviksi kaikki pesää tai lahjaa koskevat asiakirjat."}}},"isMemberOf":"sfsd:1940/378/luku/4/pykala/32","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/33/ajantasa/20180501","@type":"sfl:SectionVersion","amendedBy":"sfsd:2018/17/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/33/momentti/1/ajantasa/20180501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/33/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/33/momentti/1/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/33/momentti/1/ajantasa/20180501/fin/txt","content_fi":"Verotus toimitetaan perukirjan, veroilmoituksen, lahjaveroilmoituksen, sivulliselta tiedonantovelvolliselta saatujen tietojen ja asiassa saadun muun selvityksen perusteella ottaen huomioon tasapuolisesti valtion ja verovelvollisen etu."}}},{"@id":"sfsd:1940/378/luku/4/pykala/33/momentti/2/ajantasa/20180501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/33/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/33/momentti/2/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/33/momentti/2/ajantasa/20180501/fin/txt","content_fi":"Verotusta toimittaessaan Verohallinnon on tutkittava saamansa tiedot ja selvitykset tavalla, joka asian laatu, laajuus, verovelvollisten yhdenmukainen kohtelu ja verovalvonnan tarpeet huomioon ottaen on perusteltua."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/33","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/33/ajantasa/19940701","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":{"@id":"sfsd:1940/378/luku/4/pykala/33/momentti/1/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/33/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/33/momentti/1/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/33/momentti/1/ajantasa/19940701/fin/txt","content_fi":"Verotus on toimitettava huolellisesti ja ottaen huomioon sekä valtion että verovelvollisen edun."}}},"isMemberOf":"sfsd:1940/378/luku/4/pykala/33","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/33a/ajantasa/20180501","@type":"sfl:SectionVersion","amendedBy":"sfsd:1995/1392/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/33a/momentti/1/ajantasa/19960101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/33a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/33a/momentti/1/ajantasa/19960101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/33a/momentti/1/ajantasa/19960101/fin/txt","content_fi":"Jos perinnönjakosopimukselle, luovutustoimelle tai muulle perintö- tai lahjaverotukseen vaikuttavalle oikeustoimelle on annettu sellainen oikeudellinen sisältö tai muoto, joka ei vastaa asian varsinaista luonnetta tai tarkoitusta, tai jos on ryhdytty muuhun toimenpiteeseen ilmeisesti siinä tarkoituksessa, että perintö- tai lahjaverosta vapauduttaisiin, on perintö- tai lahjaverotuksessa meneteltävä asian varsinaisen luonteen tai tarkoituksenmukaisesti."}}},{"@id":"sfsd:1940/378/luku/4/pykala/33a/momentti/2/ajantasa/20180501","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2018/17/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/33a/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/33a/momentti/2/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/33a/momentti/2/ajantasa/20180501/fin/txt","content_fi":"Jos on ilmeistä, että perintö- tai lahjaverotusta toimitettaessa olisi meneteltävä 1 momentissa tarkoitetulla tavalla, on perintö- tai lahjaverotusta toimitettaessa huolellisesti tutkittava kaikki ne seikat, jotka voivat vaikuttaa asian arvostelemiseen, sekä annettava verovelvolliselle tilaisuus esittää selvitys havaituista seikoista. Ellei verovelvollinen tällöin esitä selvitystä siitä, että perintö- tai lahjaverotukseen vaikuttavalle oikeustoimelle annettu oikeudellinen sisältö tai muoto vastaa asian varsinaista luonnetta tai tarkoitusta taikka ettei muuhun toimenpiteeseen ole ryhdytty ilmeisesti siinä tarkoituksessa, että suoritettavasta verosta vapauduttaisiin, perintö- tai lahjaverotusta toimitettaessa on meneteltävä 1 momentissa tarkoitetulla tavalla.\n(12.1.2018/17)"}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/33a","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/34/ajantasa/20180501","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/34/momentti/1/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/34/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/34/momentti/1/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/34/momentti/1/ajantasa/19940701/fin/txt","content_fi":"Jos perukirjaa tai veroilmoitusta ei ole annettu tai sitä ei voida panna oikaistunakaan verotuksen perusteeksi, on verotus toimitettava arvioimalla (arvioverotus)."}}},{"@id":"sfsd:1940/378/luku/4/pykala/34/momentti/2/ajantasa/20180501","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2018/17/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/34/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/34/momentti/2/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/34/momentti/2/ajantasa/20180501/fin/txt","content_fi":"Verohallinnon on ennen arvioverotuksen toimittamista lähetettävä kehotus perukirjan tai veroilmoituksen antamiseksi tai selvityksen esittämiseksi. Kehotus sisältää tiedon arvion perusteella perintönä, testamentilla tai lahjana saatuun omaisuuteen lisättävästä varojen määrästä. Lisättävää määrää arvioitaessa otetaan huomioon tiedot perinnönjättäjän varallisuudesta, lahjana annetusta omaisuudesta tai muut vastaavat käytettävissä olevat tiedot.\n(12.1.2018/17)"}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/34","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/35/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/783/alkup","hasPart":{"@id":"sfsd:1940/378/luku/4/pykala/35/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/35/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/35/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/35/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Jos perintö- tai lahjaverotusta toimitettaessa harkitaan verotuksen toimittamista arvioimalla tai muun olennaisen poikkeaman tekemistä perukirjaan tai veroilmoitukseen, on 26 tai 27 §:ssä tarkoitetulle ilmoittamisvelvolliselle tai pesänhoitajalle, mikäli mahdollista, varattava tilaisuus antaa asiassa selvitys ennen verotuksen toimittamista."}}},"isMemberOf":"sfsd:1940/378/luku/4/pykala/35","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/35/ajantasa/19940701","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":{"@id":"sfsd:1940/378/luku/4/pykala/35/momentti/1/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/35/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/35/momentti/1/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/35/momentti/1/ajantasa/19940701/fin/txt","content_fi":"Jos perintö- tai lahjaverotusta toimitettaessa harkitaan verotuksen toimittamista arvioimalla tai muun olennaisen poikkeaman tekemistä perukirjaan tai veroilmoitukseen, on 26 tai 27 §:ssä tarkoitetulle ilmoittamisvelvolliselle tai pesänhoitajalle, mikäli mahdollista, varattava tilaisuus tulla asiassa kuulluksi ennen verotuksen toimittamista."}}},"isMemberOf":"sfsd:1940/378/luku/4/pykala/35","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/36/ajantasa/19940701","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/1/ajantasa/19940701","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/1/johdanto/ajantasa/19940701","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36/momentti/1/johdanto","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/1/johdanto/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/1/johdanto/ajantasa/19940701/fin/txt","content_fi":"Veroa on korotettava (veronkorotus):"}}},{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/1/kohta/1/ajantasa/19940701","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36/momentti/1/kohta/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/1/kohta/1/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/1/kohta/1/ajantasa/19940701/fin/txt","content_fi":"1) jos sellainen verovelvollinen, jonka velvollisuutena on antaa perukirja, veroilmoitus tai muu tieto tai asiakirja, on ilman hyväksyttävää syytä laiminlyönyt antaa perukirjan, veroilmoituksen tai muun säädetyn tiedon tai asiakirjan oikeassa ajassa taikka antanut ne olennaisesti vaillinaisina, enintään 20 prosentilla ja, jos hän säädetyn kehotuksen todistettavasti saatuaankin on ilman hyväksyttävää estettä jättänyt velvollisuutensa kokonaan tai osittain täyttämättä, vielä enintään 20 prosentilla; sekä"}}},{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/1/kohta/2/ajantasa/19940701","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36/momentti/1/kohta/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/1/kohta/2/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/1/kohta/2/ajantasa/19940701/fin/txt","content_fi":"2) jos verovelvollinen on tietensä tai törkeästä huolimattomuudesta antanut asiatiedoiltaan olennaisesti väärän perukirjan, veroilmoituksen tai muun säädetyn tiedon tai asiakirjan, enintään kaksinkertaiseksi."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/36/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/2/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/2/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/2/ajantasa/19940701/fin/txt","content_fi":"Veronkorotus voidaan määrätä sellaisellekin verovelvolliselle, joka ei ole ollut 26 §:n mukaan ilmoittamisvelvollinen mutta jonka on katsottava ilmeisesti olleen tietoinen perukirjan, veroilmoituksen tai muun tiedon virheellisyydestä."}}},{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/3/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36/momentti/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/3/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/3/ajantasa/19940701/fin/txt","content_fi":"Jos 1 momentissa tarkoitettu virheellisyys on koskenut vain tiettyä perinnön, testamentin tai lahjan osaa, on veronkorotus määrättävä vain tämän osan aiheuttamasta veron lisääntymisestä."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/36","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/36/ajantasa/20180501","@type":"sfl:SectionVersion","amendedBy":"sfsd:2018/17/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/1/ajantasa/20180501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/1/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/1/ajantasa/20180501/fin/txt","content_fi":"Verohallinto määrää verovelvolliselle veronkorotuksen, jos perukirja, veroilmoitus tai muu ilmoitus taikka muu säädetty tieto, asiakirja tai selvitys on annettu puutteellisena tai virheellisenä taikka on jätetty kokonaan antamatta. Veronkorotus voidaan määrätä myös sellaiselle ilmoittamisvelvolliselle, joka ei ole verovelvollinen."}}},{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/2/ajantasa/20180501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/2/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/2/ajantasa/20180501/fin/txt","content_fi":"Veronkorotusta ei kuitenkaan määrätä sen johdosta, että perukirjassa tai veroilmoituksessa on jätetty ilmoittamatta velka, vähennys tai muu verotuksessa verovelvollisen hyväksi vaikuttava tieto, ellei kyse ole 36 a §:n 2 momentissa tarkoitetusta tilanteesta."}}},{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/3/ajantasa/20180501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36/momentti/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/3/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/3/ajantasa/20180501/fin/txt","content_fi":"Veronkorotus jätetään määräämättä sellaiselle verovelvolliselle, joka ei ole ollut 26 §:n mukaan ilmoittamisvelvollinen ja jonka ei voida katsoa ilmeisesti olleen tietoinen perukirjan, veroilmoituksen tai muun tiedon virheellisyydestä."}}},{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/4/ajantasa/20180501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36/momentti/4","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/4/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36/momentti/4/ajantasa/20180501/fin/txt","content_fi":"Veronkorotus jätetään määräämättä, jos laiminlyönti on vähäinen tai laiminlyönnille on pätevä syy tai jos veronkorotuksen määrääminen olisi 36 a §:n 4 momentissa tarkoitetussa tilanteessa olosuhteet huomioon ottaen kohtuutonta."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/36","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/36a/ajantasa/20180501","@type":"sfl:SectionVersion","amendedBy":"sfsd:2018/17/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/1/ajantasa/20180501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/1/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/1/ajantasa/20180501/fin/txt","content_fi":"Veronkorotus on 10 prosenttia lisääntyneen veron määrästä."}}},{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/2/ajantasa/20180501","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/2/johdanto/ajantasa/20180501","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36a/momentti/2/johdanto","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/2/johdanto/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/2/johdanto/ajantasa/20180501/fin/txt","content_fi":"Veronkorotus on kuitenkin vähintään 15 prosenttia ja enintään 50 prosenttia lisääntyneen veron määrästä, jos:"}}},{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/2/kohta/1/ajantasa/20180501","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36a/momentti/2/kohta/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/2/kohta/1/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/2/kohta/1/ajantasa/20180501/fin/txt","content_fi":"1) 36 §:ssä tarkoitettu laiminlyönti on toistuvaa; tai"}}},{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/2/kohta/2/ajantasa/20180501","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36a/momentti/2/kohta/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/2/kohta/2/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/2/kohta/2/ajantasa/20180501/fin/txt","content_fi":"2) verovelvollisen toimet osoittavat ilmeistä piittaamattomuutta verotusta koskevista velvoitteista."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/36a/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/3/ajantasa/20180501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36a/momentti/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/3/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/3/ajantasa/20180501/fin/txt","content_fi":"Arvioverotuksessa veronkorotus on 25 prosenttia lisääntyneen veron määrästä."}}},{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/4/ajantasa/20180501","@type":"sfl:SubsectionVersion","cites":"sfsd:1995/1558/pykala/26","isMemberOf":"sfsd:1940/378/luku/4/pykala/36a/momentti/4","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/4/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/4/ajantasa/20180501/fin/txt","content_fi":"Jos kyse on verotusmenettelystä annetun lain (1558/1995) 26 §:n 2 momentissa tarkoitetulla tavalla tulkinnanvaraisesta tai epäselvästä asiasta tai 1 momentin mukainen veronkorotus on muusta erityisestä syystä kohtuuton, veronkorotus on 5 prosenttia lisääntyneen veron määrästä."}}},{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/5/ajantasa/20180501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36a/momentti/5","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/5/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/5/ajantasa/20180501/fin/txt","content_fi":"Veronkorotus on 2 prosenttia lisääntyneen veron määrästä, jos verovelvollinen oma-aloitteisesti vaatii virheen korjaamista myöhemmin kuin 60 päivän kuluttua perukirjan tai veroilmoituksen antamiselle säädetystä määräpäivästä ja on täyttänyt selvittämisvelvollisuutensa."}}},{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/6/ajantasa/20180501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36a/momentti/6","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/6/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/6/ajantasa/20180501/fin/txt","content_fi":"Luonnolliselle henkilölle määrättävä veronkorotus on kuitenkin vähintään 75 euroa ja muulle verovelvolliselle vähintään 150 euroa."}}},{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/7/ajantasa/20180501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36a/momentti/7","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/7/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36a/momentti/7/ajantasa/20180501/fin/txt","content_fi":"Veronkorotus määrätään I veroluokan asteikon perusteella lasketusta lisääntyneen veron määrästä."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/36a","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/36b/ajantasa/20180501","@type":"sfl:SectionVersion","amendedBy":"sfsd:2018/17/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/36b/momentti/1/ajantasa/20180501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36b/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36b/momentti/1/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36b/momentti/1/ajantasa/20180501/fin/txt","content_fi":"Verohallinto määrää ilmoittamisvelvolliselle veronkorotuksen sijasta myöhästymismaksun, jos ilmoittamisvelvollinen antaa perukirjan tai veroilmoituksen taikka korjaa oma-aloitteisesti muun 36 §:ssä tarkoitetun laiminlyönnin 60 päivän kuluessa perukirjan tai veroilmoituksen antamiselle säädetystä määräpäivästä."}}},{"@id":"sfsd:1940/378/luku/4/pykala/36b/momentti/2/ajantasa/20180501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36b/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36b/momentti/2/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36b/momentti/2/ajantasa/20180501/fin/txt","content_fi":"Myöhästymismaksua ei kuitenkaan määrätä sen johdosta, että perukirjassa tai veroilmoituksessa on jätetty ilmoittamatta velka, vähennys tai muu verotuksessa verovelvollisen hyväksi vaikuttava tieto, ellei kyse ole 36 a §:n 2 momentissa tarkoitetusta tilanteesta."}}},{"@id":"sfsd:1940/378/luku/4/pykala/36b/momentti/3/ajantasa/20180501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36b/momentti/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36b/momentti/3/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36b/momentti/3/ajantasa/20180501/fin/txt","content_fi":"Luonnolliselle henkilölle määrättävä myöhästymismaksu on 50 euroa perukirjalta tai veroilmoitukselta ja muulle ilmoittamisvelvolliselle 100 euroa perukirjalta tai veroilmoitukselta."}}},{"@id":"sfsd:1940/378/luku/4/pykala/36b/momentti/4/ajantasa/20180501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36b/momentti/4","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36b/momentti/4/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36b/momentti/4/ajantasa/20180501/fin/txt","content_fi":"Ilmoittamisvelvolliselle on varattava tilaisuus selvityksen antamiseen ennen myöhästymismaksun määräämistä, jos se on erityisestä syystä tarpeen."}}},{"@id":"sfsd:1940/378/luku/4/pykala/36b/momentti/5/ajantasa/20180501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/36b/momentti/5","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/36b/momentti/5/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/36b/momentti/5/ajantasa/20180501/fin/txt","content_fi":"Myöhästymismaksu jätetään määräämättä, jos perukirjan tai veroilmoituksen antaminen on viivästynyt verovelvollisesta riippumattomasta syystä, laiminlyönti on vähäinen tai laiminlyönnille on pätevä syy tai muu erityinen syy."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/36b","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/37/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/37/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/523/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/37/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/37/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/37/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Kun Verohallinto on toimittanut verotuksen, on sille, jolle on maksuunpantu perintö- tai lahjaveroa, lähetettävä verolippu ja annettava selvitys verotuksen perusteista sekä ohjeet siitä, miten verotukseen voidaan hakea muutosta.\n(11.6.2010/523)"}}},{"@id":"sfsd:1940/378/luku/4/pykala/37/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2016/783/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/37/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/37/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/37/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Kun Verohallinto on toimittanut verotuksen, on sille, jolle on maksuunpantu perintö- tai lahjaveroa, lähetettävä verotuspäätös ja annettava selvitys verotuksen perusteista sekä ohjeet siitä, miten verotukseen voidaan hakea muutosta.\n(9.9.2016/783)"}}},{"@id":"sfsd:1940/378/luku/4/pykala/37/momentti/2/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/37/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/37/momentti/2/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/37/momentti/2/ajantasa/19940701/fin/txt","content_fi":"Jos verovelvollisen osoitetta tai olinpaikkaa ei tiedetä, toimitetaan häntä koskevat 1 momentissa mainitut asiakirjat 28 §:n 1 momentissa tarkoitetulle pesänhoitajalle."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/37","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/38/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/523/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/38/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","cites":"sfsd:1965/40/luku/20/pykala/10","isMemberOf":"sfsd:1940/378/luku/4/pykala/38/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/38/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/38/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Jos perintöverotuksen toimittamisen jälkeen kuolinpesään ilmaantuu uusia varoja tai velkoja, on perintökaaren 20 luvun 10 §:ssä tarkoitettu täydennys- tai oikaisukirja annettava Verohallinnolle kuukauden kuluessa sen laatimisesta."}}},{"@id":"sfsd:1940/378/luku/4/pykala/38/momentti/2/ajantasa/20100901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/38/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/38/momentti/2/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/38/momentti/2/ajantasa/20100901/fin/txt","content_fi":"Kun perintöverotus toimitetaan veroilmoituksen nojalla, on uusista varoista ja veloista annettava Verohallinnolle veroilmoitus kolmen kuukauden kuluessa sen jälkeen, kun niitä on ilmaantunut pesään."}}},{"@id":"sfsd:1940/378/luku/4/pykala/38/momentti/3/ajantasa/20100901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/38/momentti/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/38/momentti/3/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/38/momentti/3/ajantasa/20100901/fin/txt","content_fi":"Jos perintö- tai lahjaverotuksen perusteet ovat verotuksen toimittamisen jälkeen muutoin muuttuneet, on se verovelvollinen, jonka perintö- tai lahjaveron määrä nousisi, kolmen kuukauden kuluessa sanotusta syystä tiedon saatuaan velvollinen ilmoittamaan asiasta Verohallinnolle, jollei joku muu osakas tai lahjansaaja jo ole ilmoittanut asiasta."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/38","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/39/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/39/momentti/1/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/39/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/39/momentti/1/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/39/momentti/1/ajantasa/19940701/fin/txt","content_fi":"Edellä 38 §:ssä tarkoitetuissa tapauksissa verotus voidaan toimittaa uudelleen 10 vuoden kuluessa perintö- tai lahjaverovelvollisuuden alkamisesta ja noudattaen muutoin soveltuvin osin, mitä 40 ja 41 §:ssä säädetään."}}},{"@id":"sfsd:1940/378/luku/4/pykala/39/momentti/2/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/523/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/39/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/39/momentti/2/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/39/momentti/2/ajantasa/20100901/fin/txt","content_fi":"Jos purkavin ehdoin saadun omaisuuden saaja ehdon täyttyessä joutuu luovuttamaan omaisuuden viiden vuoden kuluessa siitä, kun hän sen sai, on aikaisemmin maksettu vero palautettava 41 §:ssä säädetyssä järjestyksessä. Verovelvollisen on tehtävä tätä koskeva ilmoitus Verohallinnolle kuuden kuukauden kuluessa omaisuuden luovuttamisesta.\n(11.6.2010/523)"}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/39","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/39a/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:1996/540/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/39a/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/523/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/39a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/39a/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/39a/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Verohallinto voi kirjallisesta hakemuksesta antaa perintö- tai lahjaveroa koskevan ennakkoratkaisun.\n(11.6.2010/523)"}}},{"@id":"sfsd:1940/378/luku/4/pykala/39a/momentti/2/ajantasa/19970101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/39a/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/39a/momentti/2/ajantasa/19970101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/39a/momentti/2/ajantasa/19970101/fin/txt","content_fi":"Lahjaverosta ennakkoratkaisua voi hakea sekä luovuttaja että luovutuksensaaja. Perintöverosta ennakkoratkaisua voi hakea perinnönjättäjän kuoleman jälkeen kuolinpesän osakas, erityisjälkisäädöksen saaja ja eloonjäänyt puoliso, silloinkin kun tämä ei ole kuolinpesän osakas."}}},{"@id":"sfsd:1940/378/luku/4/pykala/39a/momentti/3/ajantasa/20100901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/39a/momentti/3","repealedBy":"sfsd:2010/523/alkup","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/39a/momentti/3/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/39a/momentti/3/ajantasa/20100901/fin/txt","content_fi":"3 momentti on kumottu L:lla 11.6.2010/523."}}},{"@id":"sfsd:1940/378/luku/4/pykala/39a/momentti/3/ajantasa/19970101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/39a/momentti/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/39a/momentti/3/ajantasa/19970101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/39a/momentti/3/ajantasa/19970101/fin/txt","content_fi":"3 momentti on kumottu L:lla 11.6.2010/523."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/39a","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/39b/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1996/540/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/39b/momentti/1/ajantasa/19970101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/39b/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/39b/momentti/1/ajantasa/19970101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/39b/momentti/1/ajantasa/19970101/fin/txt","content_fi":"Lainvoiman saanutta ennakkoratkaisua on verovelvollisen vaatimuksesta sitovana noudatettava siten kuin ennakkoratkaisussa määrätään."}}},{"@id":"sfsd:1940/378/luku/4/pykala/39b/momentti/2/ajantasa/20170101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2016/783/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/39b/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/39b/momentti/2/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/39b/momentti/2/ajantasa/20170101/fin/txt","content_fi":"Ennakkoratkaisua koskeva asia on käsiteltävä Verohallinnossa kiireellisenä.\n(9.9.2016/783)"}}},{"@id":"sfsd:1940/378/luku/4/pykala/39b/momentti/2/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/523/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/39b/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/39b/momentti/2/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/39b/momentti/2/ajantasa/20100901/fin/txt","content_fi":"Ennakkoratkaisua koskeva asia on käsiteltävä Verohallinnossa, hallinto-oikeudessa ja korkeimmassa hallinto-oikeudessa kiireellisenä.\n(11.6.2010/523)"}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/39b","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/40/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/783/alkup","hasPart":{"@id":"sfsd:1940/378/luku/4/pykala/40/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","cites":"sfsd:1995/1558/pykala/56","isMemberOf":"sfsd:1940/378/luku/4/pykala/40/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/40/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/40/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Jos verovelvollinen on jäänyt osaksi tai kokonaan verottamatta tai hänelle on muuten jäänyt määräämättä säädetty vero, Verohallinto oikaisee verotusta verovelvollisen vahingoksi siten kuin verotusmenettelystä annetun lain (1558/1995) 56 §:n 1, 3 ja 4 momentissa säädetään."}}},"isMemberOf":"sfsd:1940/378/luku/4/pykala/40","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/40/ajantasa/19940701","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/40/momentti/1/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/40/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/40/momentti/1/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/40/momentti/1/ajantasa/19940701/fin/txt","content_fi":"Jos verovelvolliselle laskuvirheen tai siihen verrattavan erehdyksen vuoksi taikka sen johdosta, että asiaa ei ole joltakin osalta tutkittu, on hänen sitä aiheuttamattaan jäänyt panematta säädetty vero tai osa siitä, on verotus oikaistava, jollei asiaa ole valitukseen annetulla päätöksellä ratkaistu (veronoikaisu verovelvollisen vahingoksi)."}}},{"@id":"sfsd:1940/378/luku/4/pykala/40/momentti/2/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/40/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/40/momentti/2/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/40/momentti/2/ajantasa/19940701/fin/txt","content_fi":"Jos verottamatta jääminen on tapahtunut sen takia, että verovelvollinen tai muu ilmoitusvelvollinen on antanut puutteellisen, erehdyttävän tai väärän perukirjan tai veroilmoituksen taikka muun tiedon tai asiakirjan, on veronoikaisua tehtäessä veroa lisäksi korotettava siten kuin 36 §:ssä säädetään."}}},{"@id":"sfsd:1940/378/luku/4/pykala/40/momentti/3/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/40/momentti/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/40/momentti/3/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/40/momentti/3/ajantasa/19940701/fin/txt","content_fi":"Veronoikaisu verovelvollisen vahingoksi voidaan tehdä perintö- tai lahjaverotuksen toimittamisvuotta seuraavien kahden vuoden kuluessa. Edellä 2 momentissa tarkoitetuissa tilanteissa oikaisu voidaan kuitenkin tehdä verotuksen toimittamisvuotta seuraavien viiden vuoden kuluessa."}}},{"@id":"sfsd:1940/378/luku/4/pykala/40/momentti/4/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/40/momentti/4","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/40/momentti/4/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/40/momentti/4/ajantasa/19940701/fin/txt","content_fi":"Ennen oikaisun tekemistä verovelvolliselle on, mikäli mahdollista, varattava tilaisuus tulla kuulluksi asiassa."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/40","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/41/ajantasa/19940701","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/41/momentti/1/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/41/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/41/momentti/1/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/41/momentti/1/ajantasa/19940701/fin/txt","content_fi":"Jos verovelvolliselle on virheen johdosta pantu liikaa veroa, on verotus oikaistava, jollei asiaa ole valitukseen annetulla päätöksellä ratkaistu (veronoikaisu verovelvollisen hyväksi)."}}},{"@id":"sfsd:1940/378/luku/4/pykala/41/momentti/2/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/41/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/41/momentti/2/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/41/momentti/2/ajantasa/19940701/fin/txt","content_fi":"Veronoikaisu verovelvollisen hyväksi voidaan tehdä perintö- tai lahjaverotuksen toimittamisvuotta seuraavien viiden vuoden kuluessa."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/41","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/41/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/783/alkup","hasPart":{"@id":"sfsd:1940/378/luku/4/pykala/41/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/41/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/41/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/41/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Jos verotuksessa on virhe, jonka johdosta verovelvolliselle on määrätty liikaa veroa, Verohallinto oikaisee verotusta verovelvollisen hyväksi siten kuin verotusmenettelystä annetun lain 55 §:n 1, 3 ja 4 momentissa säädetään."}}},"isMemberOf":"sfsd:1940/378/luku/4/pykala/41","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/41a/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/523/alkup","hasPart":{"@id":"sfsd:1940/378/luku/4/pykala/41a/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/41a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/41a/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/41a/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Veronoikaisun katsotaan tapahtuneen määräajassa, jos Verohallinnon päätös oikaisusta on tehty ennen määräajan päättymistä."}}},"isMemberOf":"sfsd:1940/378/luku/4/pykala/41a","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/41a/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/783/alkup","hasPart":{"@id":"sfsd:1940/378/luku/4/pykala/41a/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/41a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/41a/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/41a/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Verohallinto voi jatkaa 31 §:ssä säädettyä verotuksen toimittamisaikaa tai verotuksen oikaisuaikaa yhdellä vuodella siten kuin verotusmenettelystä annetun lain 56 a §:n 1, 2 ja 4 momentissa säädetään. Mitä verotusmenettelystä annetun lain 56 a §:n 1 momentin 2 kohdassa verovelvollisesta säädetään, sovelletaan myös tämän lain 26 §:ssä tarkoitettuun ilmoittamisvelvolliseen."}}},"isMemberOf":"sfsd:1940/378/luku/4/pykala/41a","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/41b/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/783/alkup","hasPart":{"@id":"sfsd:1940/378/luku/4/pykala/41b/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/41b/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/41b/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/41b/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Verohallinto voi toimittaa verotuksen tai oikaista verotusta verovelvollisen vahingoksi 31 §:ssä säädetyn määräajan estämättä kuuden vuoden kuluessa verovuoden päättymistä seuraavan vuoden alusta siten kuin verotusmenettelystä annetun lain 56 b §:n 1 momentin 3 kohdassa säädetään."}}},"isMemberOf":"sfsd:1940/378/luku/4/pykala/41b","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/41c/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/783/alkup","hasPart":{"@id":"sfsd:1940/378/luku/4/pykala/41c/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","cites":"sfsd:1889/39/luku/29","isMemberOf":"sfsd:1940/378/luku/4/pykala/41c/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/41c/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/41c/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Verohallinto voi toimittaa verotuksen tai oikaista verotusta verovelvollisen vahingoksi 31 §:ssä säädetyn määräajan estämättä vuoden kuluessa sen kalenterivuoden päättymisestä, jonka aikana syyttäjä on nostanut syytteen rikoslain (39/1889) 29 luvun 1–3 §:ssä tarkoitetussa rikosasiassa siten kuin verotusmenettelystä annetun lain 56 c §:ssä säädetään."}}},"isMemberOf":"sfsd:1940/378/luku/4/pykala/41c","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/42/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/42/momentti/1/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/42/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/42/momentti/1/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/42/momentti/1/ajantasa/19940701/fin/txt","content_fi":"Verotuksen oikaisulautakunta ratkaisee verotusta koskevat 43 §:ssä tarkoitetut oikaisuvaatimukset."}}},{"@id":"sfsd:1940/378/luku/4/pykala/42/momentti/2/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/523/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/42/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/42/momentti/2/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/42/momentti/2/ajantasa/20100901/fin/txt","content_fi":"Verohallinto voi kuitenkin ratkaista verovelvollisen tekemän 43 §:ssä tarkoitetun oikaisuvaatimuksen silloin, kun esitetty vaatimus hyväksytään.\n(11.6.2010/523)"}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/42","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/43/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/783/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/43/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Verovelvollinen, ennakkoratkaisun hakija sekä Veronsaajien oikeudenvalvontayksikkö saavat hakea muutosta verotukseen tai ennakkoratkaisuun siten kuin verotusmenettelystä annetussa laissa säädetään. Muutosta ei kuitenkaan saa hakea 31 §:n 3 momentissa tarkoitettuun verotuksen toimittamisen lykkäämistä koskevaan päätökseen."}}},{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/2/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/43/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/2/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/2/ajantasa/20170101/fin/txt","content_fi":"Muutoksenhakua koskevia määräaikoja laskettaessa verovuodella tarkoitetaan 24 §:ssä säädettyä verovuotta. Veronsaajien oikeudenvalvontayksikön on tehtävä oikaisuvaatimus 60 päivän kuluessa verotuksen toimittamis- tai oikaisupäivästä."}}},{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/3/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/43/momentti/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/3/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/3/ajantasa/20170101/fin/txt","content_fi":"Oikaisuvaatimuksen johdosta annettuun päätökseen ja ennakkoratkaisuun haetaan muutosta valittamalla siihen hallinto-oikeuteen, jonka tuomiopiirissä perinnönjättäjä kuollessaan asui tai lahjanantaja lahjaverovelvollisuuden alkaessa asui taikka varallisuuden luovuttaja asuu. Jos mikään hallinto-oikeus ei olisi toimivaltainen käsittelemään valitusta, muutosta haetaan Helsingin hallinto-oikeudelta."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/43","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/43/ajantasa/20110101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2008/242/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/1/ajantasa/20080501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/43/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/1/ajantasa/20080501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/1/ajantasa/20080501/fin/txt","content_fi":"Verovelvollinen tai Veronsaajien oikeudenvalvontayksikkö saa hakea muutosta verotuksen oikaisulautakunnalta kirjallisella oikaisuvaatimuksella, jollei asiaa ole valitukseen annetulla päätöksellä ratkaistu. Muutosta ei kuitenkaan saa hakea 25 §:n 3 momentissa tarkoitettuun päätökseen."}}},{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/2/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/523/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/43/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/2/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/2/ajantasa/20100901/fin/txt","content_fi":"Perintö- ja lahjaverotusta koskeva oikaisuvaatimus tehdään verotuksen oikaisulautakunnalle. Oikaisuvaatimus on toimitettava määräajassa Verohallinnolle.\n(11.6.2010/523)"}}},{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/3/ajantasa/20080501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/43/momentti/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/3/ajantasa/20080501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/3/ajantasa/20080501/fin/txt","content_fi":"Verovelvollisen on tehtävä oikaisuvaatimus viiden vuoden kuluessa perintö- tai lahjaverotuksen toimittamisvuotta seuraavan vuoden alusta. Veronsaajien oikeudenvalvontayksikön on tehtävä oikaisuvaatimus kuuden kuukauden kuluessa verotuksen toimittamispäivästä."}}},{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/4/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/523/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/43/momentti/4","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/4/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/4/ajantasa/20100901/fin/txt","content_fi":"Veronoikaisua ja muutoksenhakuviranomaisen uudelleen toimitettavaksi palauttamasta verotusta koskevasta Verohallinnon päätöksestä saadaan kuitenkin edellä säädetyn määräajan estämättä tehdä oikaisuvaatimus 60 päivän kuluessa päivästä, jona muutoksenhakuun oikeutettu on saanut päätöksestä tiedon. Päätös voidaan antaa tiedoksi saantitodistusmenettelyä käyttämättä, jolloin se katsotaan saadun tiedoksi seitsemäntenä päivänä sen jälkeen, kun päätös on annettu postin kuljetettavaksi, jollei muuta näytetä. Veronsaajien oikeudenvalvontayksikön määräaika lasketaan päätöksen tekemisestä.\n(11.6.2010/523)"}}},{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/5/ajantasa/20110101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/1412/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/43/momentti/5","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/5/ajantasa/20110101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/43/momentti/5/ajantasa/20110101/fin/txt","content_fi":"Oikaisuvaatimus on käsiteltävä ilman aiheetonta viivytystä.\n(30.12.2010/1412)"}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/43","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/44/ajantasa/20170101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/44","repealedBy":"sfsd:2016/783/alkup","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/44/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/44/ajantasa/20170101/fin/txt","content_fi":"44 §(9.9.2016/783)\n\n44 § on kumottu L:lla 9.9.2016/783, joka tulee voimaan 1.1.2017. Aiempi sanamuoto kuuluu:"}}},{"@id":"sfsd:1940/378/luku/4/pykala/44/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2008/242/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/44/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/523/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/44/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/44/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/44/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Edellä 43 §:stä säädetystä poiketen 39 a §:ssä tarkoitettuun ennakkoratkaisuun haetaan valittamalla muutosta siltä hallinto-oikeudelta, jonka tuomiopiirissä perinnönjättäjä kuollessaan asui tai varallisuuden luovuttaja asuu. Jos mikään hallinto-oikeus ei olisi toimivaltainen käsittelemään ennakkoratkaisusta tehtävää valitusta, muutosta haetaan Helsingin hallinto-oikeudelta. Valitusoikeus on ennakkoratkaisun hakijalla ja Veronsaajien oikeudenvalvontayksiköllä. Valituskirjelmä on toimitettava valitusajassa hallinto-oikeudelle tai Verohallinnolle. Verohallinnon on viipymättä lähetettävä sille toimitettu valituskirjelmä ja verotusta koskevat asiakirjat hallinto-oikeudelle.\n(11.6.2010/523)"}}},{"@id":"sfsd:1940/378/luku/4/pykala/44/momentti/2/ajantasa/20080501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/44/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/44/momentti/2/ajantasa/20080501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/44/momentti/2/ajantasa/20080501/fin/txt","content_fi":"Valitus on tehtävä 30 päivän kuluessa ennakkoratkaisun tiedoksisaannista. Veronsaajien oikeudenvalvontayksikön määräaika lasketaan päätöksen tekemisestä."}}},{"@id":"sfsd:1940/378/luku/4/pykala/44/momentti/3/ajantasa/20080501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/44/momentti/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/44/momentti/3/ajantasa/20080501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/44/momentti/3/ajantasa/20080501/fin/txt","content_fi":"Päätökseen, jolla ennakkoratkaisu on jätetty antamatta, ei saa hakea muutosta valittamalla."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/44","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/45/ajantasa/20170101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/45","repealedBy":"sfsd:2016/783/alkup","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/45/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/45/ajantasa/20170101/fin/txt","content_fi":"45 §(9.9.2016/783)\n\n45 § on kumottu L:lla 9.9.2016/783, joka tulee voimaan 1.1.2017. Aiempi sanamuoto kuuluu:"}}},{"@id":"sfsd:1940/378/luku/4/pykala/45/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2008/242/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/45/momentti/1/ajantasa/20080501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/45/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/45/momentti/1/ajantasa/20080501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/45/momentti/1/ajantasa/20080501/fin/txt","content_fi":"Oikaisuvaatimuksen johdosta annettuun päätökseen verovelvollinen tai Veronsaajien oikeudenvalvontayksikkö saa hakea valittamalla muutosta siltä hallinto-oikeudelta, jonka tuomiopiirissä perinnönjättäjä kuollessaan tai lahjanantaja lahjaverovelvollisuuden alkaessa asui. Jos mikään hallinto-oikeus ei olisi toimivaltainen käsittelemään valitusta, muutosta haetaan Helsingin hallinto-oikeudelta."}}},{"@id":"sfsd:1940/378/luku/4/pykala/45/momentti/2/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/523/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/45/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/45/momentti/2/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/45/momentti/2/ajantasa/20100901/fin/txt","content_fi":"Valituskirjelmä on toimitettava valitusajassa hallinto-oikeudelle tai Verohallinnolle. Verohallinnon on viipymättä lähetettävä sille toimitettu valituskirjelmä ja verotusta koskevat asiakirjat hallinto-oikeudelle.\n(11.6.2010/523)"}}},{"@id":"sfsd:1940/378/luku/4/pykala/45/momentti/3/ajantasa/20080501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/45/momentti/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/45/momentti/3/ajantasa/20080501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/45/momentti/3/ajantasa/20080501/fin/txt","content_fi":"Verovelvollisen valitusaika on viisi vuotta perintö- tai lahjaverotuksen toimittamisvuotta seuraavan vuoden alusta. Veronsaajien oikeudenvalvontayksikön valitusaika on kuusi kuukautta verotuksen toimittamispäivästä. Oikaisuvaatimukseen annetusta päätöksestä saadaan kuitenkin näiden määräaikojen estämättä valittaa 60 päivän aikana sen päivän jälkeen, jona muutoksenhakuun oikeutettu on saanut tiedon päätöksestä. Päätös voidaan antaa tiedoksi saantitodistusmenettelyä käyttämättä, jolloin se katsotaan saadun tiedoksi seitsemäntenä päivänä sen jälkeen, kun päätös on annettu postin kuljetettavaksi, jollei muuta näytetä. Veronsaajien oikeudenvalvontayksikön valitusaika lasketaan päätöksen tekemisestä."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/45","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/46/ajantasa/20080501","@type":"sfl:SectionVersion","amendedBy":"sfsd:2008/242/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/46/momentti/1/ajantasa/20080501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/46/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/46/momentti/1/ajantasa/20080501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/46/momentti/1/ajantasa/20080501/fin/txt","content_fi":"Valitusta käsittelevän hallintotuomioistuimen tulee varata verovelvollisen tekemästä valituksesta Veronsaajien oikeudenvalvontayksikölle tilaisuus vastineen ja tarvittaessa vastaselityksen antamiseen."}}},{"@id":"sfsd:1940/378/luku/4/pykala/46/momentti/2/ajantasa/20080501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/46/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/46/momentti/2/ajantasa/20080501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/46/momentti/2/ajantasa/20080501/fin/txt","content_fi":"Sen lisäksi, mitä hallintolainkäyttölain 34 §:n 2 momentissa säädetään asian ratkaisemisesta asianosaista kuulematta, hallintotuomioistuin voi ratkaista valituksen kuulematta Veronsaajien oikeudenvalvontayksikköä, jos veron määrä voi verovelvollisen valituksen johdosta muuttua enintään 3 000 euroa."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/46","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/46/ajantasa/20170101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/46","repealedBy":"sfsd:2016/783/alkup","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/46/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/46/ajantasa/20170101/fin/txt","content_fi":"46 §(9.9.2016/783)\n\n46 § on kumottu L:lla 9.9.2016/783, joka tulee voimaan 1.1.2017. Aiempi sanamuoto kuuluu:"}}},{"@id":"sfsd:1940/378/luku/4/pykala/47/ajantasa/20080501","@type":"sfl:SectionVersion","amendedBy":"sfsd:2008/242/alkup","hasPart":{"@id":"sfsd:1940/378/luku/4/pykala/47/momentti/1/ajantasa/20080501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/47/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/47/momentti/1/ajantasa/20080501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/47/momentti/1/ajantasa/20080501/fin/txt","content_fi":"Valitus, jonka verovelvollinen tai Veronsaajien oikeudenvalvontayksikkö tekee hallinto-oikeudelle ja joka olisi tullut osoittaa oikaisuvaatimuksena verotuksen oikaisulautakunnalle, käsitellään oikaisuvaatimuksena. Valituksen tutkimatta jättämisestä ei tällöin tehdä päätöstä."}}},"isMemberOf":"sfsd:1940/378/luku/4/pykala/47","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/47/ajantasa/20170101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/47","repealedBy":"sfsd:2016/783/alkup","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/47/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/47/ajantasa/20170101/fin/txt","content_fi":"47 §(9.9.2016/783)\n\n47 § on kumottu L:lla 9.9.2016/783, joka tulee voimaan 1.1.2017. Aiempi sanamuoto kuuluu:"}}},{"@id":"sfsd:1940/378/luku/4/pykala/48/ajantasa/20170101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/48","repealedBy":"sfsd:2016/783/alkup","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/48/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/48/ajantasa/20170101/fin/txt","content_fi":"48 §(9.9.2016/783)\n\n48 § on kumottu L:lla 9.9.2016/783, joka tulee voimaan 1.1.2017. Aiempi sanamuoto kuuluu:"}}},{"@id":"sfsd:1940/378/luku/4/pykala/48/ajantasa/20080501","@type":"sfl:SectionVersion","amendedBy":"sfsd:2008/242/alkup","hasPart":{"@id":"sfsd:1940/378/luku/4/pykala/48/momentti/1/ajantasa/20080501","@type":"sfl:SubsectionVersion","cites":"sfsd:1995/1558/pykala/70","isMemberOf":"sfsd:1940/378/luku/4/pykala/48/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/48/momentti/1/ajantasa/20080501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/48/momentti/1/ajantasa/20080501/fin/txt","content_fi":"Hallinto-oikeuden päätökseen saa hakea muutosta valittamalla korkeimpaan hallinto-oikeuteen, jos korkein hallinto-oikeus myöntää valitusluvan. Muutoksenhaussa noudatetaan tällöin, mitä verotusmenettelystä annetun lain (1558/1995) 70 ja 71 §:ssä säädetään. Valitusaika hallinto-oikeuden ennakkoratkaisua koskevasta päätöksestä on kuitenkin 30 päivää tiedoksisaamisesta. Valtion puolesta on valitusoikeus Veronsaajien oikeudenvalvontayksiköllä."}}},"isMemberOf":"sfsd:1940/378/luku/4/pykala/48","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/48a/ajantasa/20170101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/48a","repealedBy":"sfsd:2016/783/alkup","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/48a/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/48a/ajantasa/20170101/fin/txt","content_fi":"48 a §(9.9.2016/783)\n\n48 a § on kumottu L:lla 9.9.2016/783, joka tulee voimaan 1.1.2017. Aiempi sanamuoto kuuluu:"}}},{"@id":"sfsd:1940/378/luku/4/pykala/48a/ajantasa/20130101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2012/883/alkup","hasPart":{"@id":"sfsd:1940/378/luku/4/pykala/48a/momentti/1/ajantasa/20130101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/48a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/48a/momentti/1/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/48a/momentti/1/ajantasa/20130101/fin/txt","content_fi":"Ennakkopäätösvalitukseen sovelletaan, mitä verotusmenettelystä annetun lain 71 a–71 d sekä 71 f §:ssä säädetään."}}},"isMemberOf":"sfsd:1940/378/luku/4/pykala/48a","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/49/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/523/alkup","hasPart":{"@id":"sfsd:1940/378/luku/4/pykala/49/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/49/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/49/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/49/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Jos perintö- tai lahjaverotusta koskevassa asiassa, josta on tehty oikaisuvaatimus verotuksen oikaisulautakunnalle tai haettu muutosta hallinto-oikeudelta, asia joltakin osaltaan on jäänyt tutkimatta tai verotuksessa on sovellettu vääriä perusteita taikka verotus on muutoin toimitettu olennaisesti väärin, voidaan asia kokonaisuudessaankin palauttaa Verohallinnolle tai verotuksen oikaisulautakunnalle uuden verotuksen toimittamista tai päätöksen tekemistä varten."}}},"isMemberOf":"sfsd:1940/378/luku/4/pykala/49","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/49/ajantasa/20170101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/49","repealedBy":"sfsd:2016/783/alkup","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/49/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/49/ajantasa/20170101/fin/txt","content_fi":"49 §(9.9.2016/783)\n\n49 § on kumottu L:lla 9.9.2016/783, joka tulee voimaan 1.1.2017. Aiempi sanamuoto kuuluu:"}}},{"@id":"sfsd:1940/378/luku/4/pykala/50/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/50/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/523/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/50/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/50/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/50/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Jos verotusta on muutettu toisen verovelvollisen verotukseen vaikuttavalla tavalla, Verohallinto oikaisee veronoikaisuin myös tämän verovelvollisen verotusta muutosta vastaavasti (seurannaismuutos), jollei oikaisun tekeminen ole erityisestä syystä kohtuutonta. Seurannaismuutos voidaan tehdä, vaikka veronoikaisun edellytykset puuttuvat.\n(11.6.2010/523)"}}},{"@id":"sfsd:1940/378/luku/4/pykala/50/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2016/783/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/50/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/50/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/50/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Jos verotusta on muutettu toisen verovelvollisen verotukseen vaikuttavalla tavalla, Verohallinto oikaisee verotuksen oikaisuna myös tämän verovelvollisen verotusta muutosta vastaavasti (seurannaismuutos), jollei oikaisun tekeminen ole erityisestä syystä kohtuutonta. Seurannaismuutos voidaan tehdä silloinkin, kun muuta kuin tämän lain mukaista verotusta on muutettu suoritettavan veron määrään vaikuttavalla tavalla. Seurannaismuutos voidaan tehdä, vaikka verotuksen oikaisun edellytykset puuttuvat.\n(9.9.2016/783)"}}},{"@id":"sfsd:1940/378/luku/4/pykala/50/momentti/2/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/4/pykala/50/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/50/momentti/2/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/50/momentti/2/ajantasa/19940701/fin/txt","content_fi":"Verovelvolliselle on, mikäli mahdollista, ennen oikaisun tekemistä varattava tilaisuus tulla asiassa kuulluksi, jollei se ole ilmeisen tarpeetonta."}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/50","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/4/pykala/51/ajantasa/20180501","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/4/pykala/51/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/523/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/51/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/51/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/51/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Jos viranomainen on oikaisuvaatimukseen tai valitukseen antamallaan päätöksellä määrännyt jonkun verovelvolliseksi tai muuttanut verotusta koskevaa päätöstä, on muutoksen tehneen viranomaisen vahvistettava muutoksen peruste. Jäljennös korkeimman hallinto-oikeuden tai hallinto-oikeuden edellä tarkoitetusta päätöksestä on lähetettävä hallinto-oikeudelle ja Verohallinnolle. Verohallinto oikaisee verotuksen edellä tarkoitetun päätöksen perusteella noudattaen soveltuvin osin, mitä veronoikaisusta säädetään.\n(11.6.2010/523)"}}},{"@id":"sfsd:1940/378/luku/4/pykala/51/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2016/783/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/51/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/51/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/51/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Muutoksenhakuviranomaisen tai Verohallinnon päätöksen perusteella uudelleen toimitettava verotus, seurannaismuutos, 38 §:ssä tarkoitettu muutos ja muutoksenhaun johdosta tehtävät muut muutokset verotukseen toteutetaan ja niihin haetaan muutosta siten kuin verotusmenettelystä annetun lain 78 §:ssä säädetään. \n(9.9.2016/783)"}}},{"@id":"sfsd:1940/378/luku/4/pykala/51/momentti/2/ajantasa/20180501","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2018/17/alkup","cites":"sfsd:2018/11","isMemberOf":"sfsd:1940/378/luku/4/pykala/51/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/51/momentti/2/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/51/momentti/2/ajantasa/20180501/fin/txt","content_fi":"Jos vero on päätöksen perusteella poistettu tai sitä on alennettu, on asianomaiselle palautettava liikaa maksettu määrä. Palautettavalle määrälle maksetaan veronkantolaissa (11/2018) säädetty hyvityskorko veron maksupäivää seuraavasta päivästä siihen päivään, jona palautettava määrä veloitetaan Verohallinnon tililtä tai käytetään veron suoritukseksi.\n(12.1.2018/17)"}}},{"@id":"sfsd:1940/378/luku/4/pykala/51/momentti/3/ajantasa/20170101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2016/783/alkup","isMemberOf":"sfsd:1940/378/luku/4/pykala/51/momentti/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/pykala/51/momentti/3/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/4/pykala/51/momentti/3/ajantasa/20170101/fin/txt","content_fi":"Jos joku on määrätty verovelvolliseksi tai veroa on päätöksen perusteella korotettu, verovelvolliselle on annettava verotuspäätös ja häneltä on perittävä maksamaton vero sekä sille edellä 2 momentin nojalla maksettu korko.\n(9.9.2016/783)"}}}],"isMemberOf":"sfsd:1940/378/luku/4/pykala/51","version":"sfl:Consolidated","versionDate":"2018-05-01"}],"isMemberOf":"sfsd:1940/378/luku/4","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/4/ajantasa/20180501/fin","title_fi":"Verotus, ennakkoratkaisu ja muutoksenhaku (26.7.1996/540)"}},{"@id":"sfsd:1940/378/luku/5/ajantasa/20180501","@type":"sfl:ChapterVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/5/pykala/52/ajantasa/20180501","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/5/pykala/52/momentti/1/ajantasa/19960101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:1995/1561/alkup","cites":"sfsd:1978/611","isMemberOf":"sfsd:1940/378/luku/5/pykala/52/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/52/momentti/1/ajantasa/19960101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/52/momentti/1/ajantasa/19960101/fin/txt","content_fi":"Perintö- tai lahjaveron kanto, perintä ja palauttaminen toimitetaan siten kuin veronkantolaissa (611/78) säädetään taikka sen nojalla säädetään tai määrätään, ellei jäljempänä toisin säädetä.\n(18.12.1995/1561)"}}},{"@id":"sfsd:1940/378/luku/5/pykala/52/momentti/1/ajantasa/20180501","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2018/17/alkup","isMemberOf":"sfsd:1940/378/luku/5/pykala/52/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/52/momentti/1/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/52/momentti/1/ajantasa/20180501/fin/txt","content_fi":"Perintö- tai lahjaveron kanto, perintä ja palauttaminen toimitetaan siten kuin veronkantolaissa säädetään taikka sen nojalla säädetään tai määrätään, ellei jäljempänä toisin säädetä.\n(12.1.2018/17)"}}},{"@id":"sfsd:1940/378/luku/5/pykala/52/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2016/783/alkup","cites":"sfsd:2016/769","isMemberOf":"sfsd:1940/378/luku/5/pykala/52/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/52/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/52/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Perintö- tai lahjaveron kanto,  perintä ja palauttaminen toimitetaan siten kuin veronkantolaissa (769/2016) säädetään taikka sen nojalla säädetään tai määrätään, ellei jäljempänä toisin säädetä. \n(9.9.2016/783)"}}},{"@id":"sfsd:1940/378/luku/5/pykala/52/momentti/2/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/5/pykala/52/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/52/momentti/2/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/52/momentti/2/ajantasa/19940701/fin/txt","content_fi":"Vero on maksettava, vaikka verotus ei olisikaan saanut lainvoimaa."}}},{"@id":"sfsd:1940/378/luku/5/pykala/52/momentti/3/ajantasa/20170101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2016/1320/alkup","cites":"sfsd:1999/1346","isMemberOf":"sfsd:1940/378/luku/5/pykala/52/momentti/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/52/momentti/3/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/52/momentti/3/ajantasa/20170101/fin/txt","content_fi":"Maksamatta olevaa perintöveroa, sen korotusta ja viivästysseuraamusta ei kuitenkaan peritä ulosottoteitse eikä niiden perusteella haeta verovelvollisen asettamista konkurssiin eikä tietoa niistä merkitä verotustietojen julkisuudesta ja salassapidosta annetussa laissa (1346/1999) säädettyyn verovelkarekisteriin ennen kuin kaksi vuotta on kulunut veron ensimmäisestä eräpäivästä.\n(21.12.2016/1320)"}}}],"isMemberOf":"sfsd:1940/378/luku/5/pykala/52","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/5/pykala/52a/ajantasa/20171101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1940/378/luku/5/pykala/52a","repealedBy":"sfsd:2017/64/alkup","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/52a/ajantasa/20171101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/52a/ajantasa/20171101/fin/txt","content_fi":"52 a §(27.1.2017/64)\n\n52 a § on kumottu L:lla 27.1.2017/64, joka tuli voimaan 1.11.2017. Aiempi sanamuoto kuuluu:"}}},{"@id":"sfsd:1940/378/luku/5/pykala/52a/ajantasa/20160101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2015/1548/alkup","hasPart":{"@id":"sfsd:1940/378/luku/5/pykala/52a/momentti/1/ajantasa/20160101","@type":"sfl:SubsectionVersion","cites":"sfsd:1995/1556","isMemberOf":"sfsd:1940/378/luku/5/pykala/52a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/52a/momentti/1/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/52a/momentti/1/ajantasa/20160101/fin/txt","content_fi":"Eräpäivän jälkeen maksamatta olevalle verolle on suoritettava viivekorkoa siten kuin veronlisäyksestä ja viivekorosta annetussa laissa (1556/1995) säädetään, kuitenkin siten, että viivekorolla ei ole mainitun lain 4 §:ssä tarkoitettua kolmen euron vähimmäismäärää."}}},"isMemberOf":"sfsd:1940/378/luku/5/pykala/52a","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/5/pykala/53/ajantasa/20130101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1940/378/luku/5/pykala/53","repealedBy":"sfsd:2012/883/alkup","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/53/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/53/ajantasa/20130101/fin/txt","content_fi":"53–54 §\n\n\n53–54 § on kumottu L:lla 21.12.2012/883."}}},{"@id":"sfsd:1940/378/luku/5/pykala/55/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/1110/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/1/johdanto/ajantasa/20100901","@type":"sfl:PreambleVersion","amendedBy":"sfsd:2010/523/alkup","isMemberOf":"sfsd:1940/378/luku/5/pykala/55/momentti/1/johdanto","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/1/johdanto/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/1/johdanto/ajantasa/20100901/fin/txt","content_fi":"Perintö- tai lahjaverosta jätetään verovelvollisen Verohallinnolle ennen verotuksen toimittamista tekemästä pyynnöstä osa maksuunpanematta, jos:\n(11.6.2010/523)"}}},{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/1/kohta/1/ajantasa/19940701","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1940/378/luku/5/pykala/55/momentti/1/kohta/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/1/kohta/1/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/1/kohta/1/ajantasa/19940701/fin/txt","content_fi":"1) veronalaiseen perintöön tai lahjaan sisältyy maatila, muu yritys tai osa niistä;"}}},{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/1/kohta/2/ajantasa/19940701","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1940/378/luku/5/pykala/55/momentti/1/kohta/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/1/kohta/2/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/1/kohta/2/ajantasa/19940701/fin/txt","content_fi":"2) verovelvollinen jatkaa perintönä tai lahjana saaduilla varoilla maatalouden tai maa- ja metsätalouden harjoittamista tai muuta yritystoimintaa perintönä tai lahjana saadulla maatilalla tai näin saadussa yrityksessä; ja"}}},{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/1/kohta/3/ajantasa/20020101","@type":"sfl:ParagraphVersion","amendedBy":"sfsd:2001/909/alkup","isMemberOf":"sfsd:1940/378/luku/5/pykala/55/momentti/1/kohta/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/1/kohta/3/ajantasa/20020101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/1/kohta/3/ajantasa/20020101/fin/txt","content_fi":"3) edellä 1 kohdassa tarkoitetusta maatilasta, muusta yrityksestä tai niiden osasta määrätyn perintö- tai lahjaveron suhteellinen osa koko verosta on suurempi kuin 850 euroa.\n(26.10.2001/909)"}}}],"isMemberOf":"sfsd:1940/378/luku/5/pykala/55/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/2/ajantasa/20060101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2005/1144/alkup","cites":"sfsd:2005/1142","isMemberOf":"sfsd:1940/378/luku/5/pykala/55/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/2/ajantasa/20060101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/2/ajantasa/20060101/fin/txt","content_fi":"Maksuunpanematta jätettävän veron osan laskemiseksi vähennetään tämän lain mukaan määrätystä verosta vero, joka perintöosuudesta tai lahjasta olisi määrättävä, jos maatilan maatalousmaa ja sitä vastaava CAP-maatilatukioikeus, rakennukset, rakennelmat, koneet, kalusto ja laitteet sekä niihin kohdistuvat velat samoin kuin muuhun yritykseen kuin maatilaan kuuluva yritysvarallisuus arvostettaisiin määrään, joka vastaa 40 prosenttia verovelvollisuuden alkamista edeltäneeltä vuodelta toimitettavassa tai toimitetussa tuloverotuksessa noudatettavien tai noudatettujen, varojen arvostamisesta verotuksessa annetun lain (1142/2005) 3 ja 4 luvun mukaisten perusteiden mukaisesta määrästä. Osakeyhtiön yritysvarallisuus arvostetaan määrään, joka vastaa 40 prosenttia mainitun lain 4 ja 5 §:n mukaisten perusteiden mukaan lasketusta määrästä.\n(22.12.2005/1144)"}}},{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/3/ajantasa/20060101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2005/1144/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/3/johdanto/ajantasa/19940701","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1940/378/luku/5/pykala/55/momentti/3/johdanto","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/3/johdanto/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/3/johdanto/ajantasa/19940701/fin/txt","content_fi":"Varat, joita ei lueta nettovarallisuuteen, arvostetaan seuraavasti:"}}},{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/3/kohta/1/ajantasa/19940701","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1940/378/luku/5/pykala/55/momentti/3/kohta/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/3/kohta/1/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/3/kohta/1/ajantasa/19940701/fin/txt","content_fi":"1) maatilaan kuuluva metsä 40 prosenttiin sen varojen arvostamisesta verotuksessa annetun lain 7 §:n mukaisesta arvosta;"}}},{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/3/kohta/2/ajantasa/19940701","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1940/378/luku/5/pykala/55/momentti/3/kohta/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/3/kohta/2/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/3/kohta/2/ajantasa/19940701/fin/txt","content_fi":"2) maatilan asuinrakennus ja metsätalouden tuotantorakennuksen rakennuspaikka 40 prosenttiin varojen arvostamisesta verotuksessa annetun lain 5 luvun mukaisten perusteiden mukaisesta määrästä;"}}},{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/3/kohta/3/ajantasa/19940701","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1940/378/luku/5/pykala/55/momentti/3/kohta/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/3/kohta/3/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/3/kohta/3/ajantasa/19940701/fin/txt","content_fi":"3) metsätalouden tuotantorakennukset, koneet ja laitteet 40 prosenttiin niiden verotuksessa poistamatta olevasta hankintamenosta; ja"}}},{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/3/kohta/4/ajantasa/19940701","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1940/378/luku/5/pykala/55/momentti/3/kohta/4","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/3/kohta/4/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/3/kohta/4/ajantasa/19940701/fin/txt","content_fi":"4) muut varat 40 prosenttiin niiden käyvästä arvosta."}}}],"isMemberOf":"sfsd:1940/378/luku/5/pykala/55/momentti/3","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/4/ajantasa/20060101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2005/1144/alkup","isMemberOf":"sfsd:1940/378/luku/5/pykala/55/momentti/4","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/4/ajantasa/20060101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/4/ajantasa/20060101/fin/txt","content_fi":"Edellä 2 momentissa tarkoitettu erotus tai, jos se on suurempi kuin 1 momentin 3 kohdassa tarkoitetun perintö- ja lahjaveron 850 euron ylimenevä osa, viimeksi mainittu määrä jätetään maksuunpanematta.\n(22.12.2005/1144)"}}},{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/5/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/5/pykala/55/momentti/5","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/5/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/5/ajantasa/19940701/fin/txt","content_fi":"Jos maatilan, muun yrityksen tai sen osan luovutus 1 momentin 2 kohdassa tarkoitetuissa tapauksissa on osittain vastikkeellinen ja vastike on enemmän kuin 50 prosenttia käyvästä arvosta, maatilaan, muuhun yritykseen tai sen osaan kohdistuva lahjavero jätetään kokonaan maksuunpanematta."}}},{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/6/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/5/pykala/55/momentti/6","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/6/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/6/ajantasa/19940701/fin/txt","content_fi":"Jos verovelvollinen luovuttaa pääosan maatilasta, muusta yrityksestä tai sellaisen osasta, josta on myönnetty 2 tai 3 momentissa tarkoitettu huojennus, ennen kuin viisi vuotta on kulunut perintö- tai lahjaverotuksen toimittamispäivästä, verovelvollisen maksettavaksi pannaan tämän pykälän nojalla maksuunpanematta jätetty vero korotettuna 20 prosentilla."}}},{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/7/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/523/alkup","isMemberOf":"sfsd:1940/378/luku/5/pykala/55/momentti/7","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/7/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/7/ajantasa/20100901/fin/txt","content_fi":"Verovelvollisen on tehtävä 6 momentissa tarkoitettua luovutusta koskeva ilmoitus Verohallinnolle kolmen kuukauden kuluessa omaisuuden luovuttamisesta. Maksuunpanossa noudatetaan soveltuvin osin, mitä 40 §:ssä säädetään.\n(11.6.2010/523)"}}},{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/8/ajantasa/20050801","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2005/559/alkup","isMemberOf":"sfsd:1940/378/luku/5/pykala/55/momentti/8","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/8/ajantasa/20050801/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/55/momentti/8/ajantasa/20050801/fin/txt","content_fi":"Edellä 2 momenttia sovellettaessa maatalousmaa ja samalla luovutettava maatalousmaata vastaava CAP-maatilatukioikeus arvostetaan yhdessä määrään, joka vastaa 40 prosenttia maatalousmaan varallisuusverotuksessa noudatetusta arvosta.\n(15.7.2005/559)"}}}],"isMemberOf":"sfsd:1940/378/luku/5/pykala/55","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/5/pykala/56/ajantasa/20150601","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/5/pykala/56/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/523/alkup","isMemberOf":"sfsd:1940/378/luku/5/pykala/56/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/56/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/56/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Jos maatilasta, muusta yrityksestä tai niiden osasta määrätyn perintö- tai lahjaveron suhteellinen osa koko verosta on 1 700 euroa tai enemmän ja jos 55 §:n 1 momentin 1 ja 2 kohdassa mainitut edellytykset ovat olemassa, tämän veron osan maksuaikaa pidennetään verovelvollisen ennen verotuksen toimittamista Verohallinnolle tekemästä pyynnöstä.\n(11.6.2010/523)"}}},{"@id":"sfsd:1940/378/luku/5/pykala/56/momentti/2/ajantasa/20150601","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2015/514/alkup","isMemberOf":"sfsd:1940/378/luku/5/pykala/56/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/56/momentti/2/ajantasa/20150601/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/56/momentti/2/ajantasa/20150601/fin/txt","content_fi":"Vero, jonka maksamiseen on myönnetty pidennystä 1 momentin mukaan, kannetaan yhtä suurina, vähintään 850 euron määräisinä vuotuiserinä enintään kymmenen vuoden aikana. Tarkempia säännöksiä veron kantamisesta erissä annetaan valtiovarainministeriön asetuksella. Pidennetyltä maksuajalta ei peritä korkoa.\n(24.4.2015/514)"}}},{"@id":"sfsd:1940/378/luku/5/pykala/56/momentti/2/ajantasa/20020101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2001/909/alkup","isMemberOf":"sfsd:1940/378/luku/5/pykala/56/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/56/momentti/2/ajantasa/20020101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/56/momentti/2/ajantasa/20020101/fin/txt","content_fi":"Vero, jonka maksamiseen on myönnetty pidennystä 1 momentin mukaan, kannetaan yhtä suurina, vähintään 850 euron suuruisina vuotuiserinä enintään viiden vuoden aikana sen mukaan kuin valtiovarainministeriön asetuksella tarkemmin säädetään. Pidennetyltä maksuajalta ei peritä korkoa.\n(26.10.2001/909)"}}},{"@id":"sfsd:1940/378/luku/5/pykala/56/momentti/3/ajantasa/20150601","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2015/514/alkup","cites":"sfsd:2007/706/pykala/20","isMemberOf":"sfsd:1940/378/luku/5/pykala/56/momentti/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/56/momentti/3/ajantasa/20150601/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/56/momentti/3/ajantasa/20150601/fin/txt","content_fi":"Useamman kuin viiden vuoden aikana kannettava vero vanhentuu verojen ja maksujen täytäntöönpanosta annetun lain (706/2007) 20 §:stä poiketen kymmenen vuoden kuluttua laskettuna sitä seuranneen vuoden alusta, jona vero on määrätty.\n(24.4.2015/514)"}}},{"@id":"sfsd:1940/378/luku/5/pykala/56/momentti/4/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/523/alkup","isMemberOf":"sfsd:1940/378/luku/5/pykala/56/momentti/4","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/56/momentti/4/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/56/momentti/4/ajantasa/20100901/fin/txt","content_fi":"Jos verovelvollinen luovuttaa pääasiallisen osan edellä tarkoitetusta maatilasta, muusta yrityksestä tai sellaisen osasta, Verohallinto määrää kaikki vielä tällöin maksamatta olevat maksuerät kannettaviksi seuraavan kantoerän yhteydessä.\n(11.6.2010/523)"}}}],"isMemberOf":"sfsd:1940/378/luku/5/pykala/56","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/5/pykala/57/ajantasa/20010101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2000/1084/alkup","hasPart":{"@id":"sfsd:1940/378/luku/5/pykala/57/momentti/1/ajantasa/20010101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/5/pykala/57/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/pykala/57/momentti/1/ajantasa/20010101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/5/pykala/57/momentti/1/ajantasa/20010101/fin/txt","content_fi":"Edellä 55 ja 56 §:ssä tarkoitetaan maatilan tai muun yrityksen osalla myös vähintään yhtä kymmenesosaa maatilan tai yrityksen omistamiseen oikeuttavista osakkeista tai osuuksista."}}},"isMemberOf":"sfsd:1940/378/luku/5/pykala/57","version":"sfl:Consolidated","versionDate":"2018-05-01"}],"isMemberOf":"sfsd:1940/378/luku/5","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/5/ajantasa/20180501/fin","title_fi":"Veronkanto, maksuajan pidentäminen ja veronhuojennus"}},{"@id":"sfsd:1940/378/luku/6/ajantasa/20180501","@type":"sfl:ChapterVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/6/pykala/58/ajantasa/19940701","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":{"@id":"sfsd:1940/378/luku/6/pykala/58/momentti/1/ajantasa/19940701","@type":"sfl:SubsectionVersion","cites":"sfsd:1889/39/luku/29","isMemberOf":"sfsd:1940/378/luku/6/pykala/58/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/6/pykala/58/momentti/1/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/6/pykala/58/momentti/1/ajantasa/19940701/fin/txt","content_fi":"Rangaistus perintö- ja lahjaveron lainvastaisesta välttämisestä ja sen yrittämisestä säädetään rikoslain 29 luvun 1–3 §:ssä."}}},"isMemberOf":"sfsd:1940/378/luku/6/pykala/58","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/6/pykala/59/ajantasa/20180501","@type":"sfl:SectionVersion","amendedBy":"sfsd:2018/17/alkup","hasPart":{"@id":"sfsd:1940/378/luku/6/pykala/59/momentti/1/ajantasa/20180501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/6/pykala/59/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/6/pykala/59/momentti/1/ajantasa/20180501/fin","hasFormat":{"@id":"sfsd:1940/378/luku/6/pykala/59/momentti/1/ajantasa/20180501/fin/txt","content_fi":"Jos tässä laissa ei toisin säädetä, noudatetaan, mitä verotusmenettelystä annetun lain 2 ja 3 luvussa, 26, 26 a–26 d ja 51 §:ssä sekä 11 luvussa säädetään."}}},"isMemberOf":"sfsd:1940/378/luku/6/pykala/59","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/6/pykala/59/ajantasa/20040101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2003/1068/alkup","hasPart":{"@id":"sfsd:1940/378/luku/6/pykala/59/momentti/1/ajantasa/20040101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/6/pykala/59/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/6/pykala/59/momentti/1/ajantasa/20040101/fin","hasFormat":{"@id":"sfsd:1940/378/luku/6/pykala/59/momentti/1/ajantasa/20040101/fin/txt","content_fi":"Mikäli tässä laissa ei toisin säädetä, soveltuvin osin noudatetaan, mitä verotusmenettelystä annetun lain 2 ja 3 luvuissa, 4 luvun 26 a–26 d ja 51 §:ssä sekä 11 luvussa säädetään."}}},"isMemberOf":"sfsd:1940/378/luku/6/pykala/59","version":"sfl:Consolidated","versionDate":"2018-05-01"},{"@id":"sfsd:1940/378/luku/6/pykala/60/ajantasa/19940701","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":{"@id":"sfsd:1940/378/luku/6/pykala/60/momentti/1/ajantasa/19940701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1940/378/luku/6/pykala/60/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/6/pykala/60/momentti/1/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/6/pykala/60/momentti/1/ajantasa/19940701/fin/txt","content_fi":"Tarkemmat säännökset tämän lain täytäntöönpanosta annetaan tarvittaessa asetuksella."}}},"isMemberOf":"sfsd:1940/378/luku/6/pykala/60","version":"sfl:Consolidated","versionDate":"2018-05-01"}],"isMemberOf":"sfsd:1940/378/luku/6","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/6/ajantasa/20180501/fin","title_fi":"Erityisiä säännöksiä"}},{"@id":"sfsd:1940/378/luku/7/ajantasa/19940701","@type":"sfl:ChapterVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":{"@id":"sfsd:1940/378/luku/7/pykala/61/ajantasa/19940701","@type":"sfl:SectionVersion","amendedBy":"sfsd:1994/318/alkup","hasPart":[{"@id":"sfsd:1940/378/luku/7/pykala/61/momentti/1/ajantasa/19940701","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:1994/318/alkup","isMemberOf":"sfsd:1940/378/luku/7/pykala/61/momentti/1","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/7/pykala/61/momentti/1/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/7/pykala/61/momentti/1/ajantasa/19940701/fin/txt","content_fi":"Tämä laki tulee voimaan 1 päivänä elokuuta 1940, ja sillä kumotaan 30 päivänä joulukuuta 1921 annettu perintö- ja lahjaverolaki. (29.4.1994/318)"}}},{"@id":"sfsd:1940/378/luku/7/pykala/61/momentti/2/ajantasa/19940701","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:1994/318/alkup","isMemberOf":"sfsd:1940/378/luku/7/pykala/61/momentti/2","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/7/pykala/61/momentti/2/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/7/pykala/61/momentti/2/ajantasa/19940701/fin/txt","content_fi":"Tätä lakia sovelletaan niihinkin tapauksiin, joissa verovelvollisuus on alkanut ennen tämän lain voimaantuloa, mutta joita ei ole vielä lopullisesti ratkaistu, paitsi että 12 §:n 2 momentin säännöstä on sovellettava 7 päivänä lokakuuta 1939 tai sen jälkeen; kuitenkin kaikkiin tapauksiin, joissa verovelvollisuus on alkanut niin, että veron määrä, paitsi 12 §:n 2 momentissa mainituissa tapauksissa, on laskettava niiden säännösten mukaan, jotka olivat voimassa verovelvollisuuden alkaessa. (29.4.1994/318)"}}},{"@id":"sfsd:1940/378/luku/7/pykala/61/momentti/3/ajantasa/19940701","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:1994/318/alkup","isMemberOf":"sfsd:1940/378/luku/7/pykala/61/momentti/3","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/7/pykala/61/momentti/3/ajantasa/19940701/fin","hasFormat":{"@id":"sfsd:1940/378/luku/7/pykala/61/momentti/3/ajantasa/19940701/fin/txt","content_fi":"Jos perintö- tai lahjaveroasia on tammikuun 1 päivän 1938 jälkeen ratkaistu lainvoiman saaneella päätöksellä, on verovelvollinen oikeutettu asianomaiselle verotuslautakunnalle tehtävällä kirjallisella ilmoituksella saattamaan asian uuden käsittelyn alaiseksi, jolloin tätä lakia on sovellettava myös veron määrään nähden. Jos verovelvollinen näin on joutunut suorittamaan enemmän veroa, kuin mitä hänelle tämän lain mukaan on tuleva, suoritettakoon hänelle valtionvaroista takaisin liikaa maksamansa määrä sekä sille korkoa sen mukaisesti, kuin 55 §:ssä on säädetty. (29.4.1994/318)"}}}],"isMemberOf":"sfsd:1940/378/luku/7/pykala/61","version":"sfl:Consolidated","versionDate":"2018-05-01"},"isMemberOf":"sfsd:1940/378/luku/7","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/luku/7/ajantasa/19940701/fin","title_fi":"Voimaantulo"}}],"isAbout":["http://data.finlex.fi/voc/finlex/kw2627","http://data.finlex.fi/voc/finlex/kw2295","http://data.finlex.fi/voc/finlex/kw3876","http://data.finlex.fi/voc/finlex/kw4376","http://data.finlex.fi/voc/finlex/kw2103","http://data.finlex.fi/voc/finlex/kw4593","http://data.finlex.fi/voc/finlex/kw1154","http://data.finlex.fi/voc/finlex/kw4394","http://data.finlex.fi/voc/finlex/kw3069","http://data.finlex.fi/voc/finlex/kw4463","http://data.finlex.fi/voc/finlex/kw1700","http://data.finlex.fi/voc/finlex/kw1764"],"isMemberOf":"sfsd:1940/378","version":"sfl:Consolidated","versionDate":"2018-05-01","languageVersion":{"@id":"sfsd:1940/378/ajantasa/20180501/fin","title_fi":"Perintö- ja lahjaverolaki"}},"@context":{"languageVersion":{"@id":"eli:is_realized_by","@type":"@id"},"temporalVersions":{"@id":"eli:has_member","@type":"@id"},"temporalVersion":{"@id":"eli:has_member","@type":"@id"},"hasFormat":{"@id":"eli:is_embodied_by","@type":"@id"},"idLocal":"eli:id_local","dateDocument":{"@id":"eli:date_document","@type":"xsd:date"},"hasPart":{"@id":"eli:has_part","@type":"@id"},"isAbout":{"@id":"eli:is_about","@type":"@id"},"isMemberOf":{"@id":"eli:is_member_of","@type":"@id"},"version":{"@id":"eli:version","@type":"@id"},"versionDate":{"@id":"eli:version_date","@type":"xsd:date"},"title_fi":{"@id":"eli:title","@language":"fi"},"content_fi":{"@id":"sfl:text","@language":"fi"},"amendedBy":{"@id":"eli:amended_by","@type":"@id"},"cites":{"@id":"eli:cites","@type":"@id"},"repealedBy":{"@id":"eli:repealed_by","@type":"@id"},"inForce":{"@id":"eli:in_force","@type":"@id"},"typeDocument":{"@id":"eli:type_document","@type":"@id"},"dcterms":"http://purl.org/dc/terms/","xsd":"http://www.w3.org/2001/XMLSchema#","common":"http://data.finlex.fi/common/","rdf":"http://www.w3.org/1999/02/22-rdf-syntax-ns#","sfcl":"http://data.finlex.fi/schema/scfl/","sfl":"http://data.finlex.fi/schema/sfl/","sfecli":"http://data.finlex.fi/ecli/","sfsd":"http://data.finlex.fi/eli/sd/","skos":"http://www.w3.org/2004/02/skos/core#","text":"http://jena.apache.org/text#","eli":"http://data.europa.eu/eli/ontology#"}}