rsion":{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/2/alkup/fin","hasFormat":{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/2/alkup/fin/txt","content_fi":"2) vakuutussopimus ei koske henkivakuutusta eikä vastuuvakuutusta eikä 5 kohdasta muuta johdu;"}}},{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/3/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/3","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/3/alkup/fin","hasFormat":{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/3/alkup/fin/txt","content_fi":"3) vakuutusedustusta harjoitetaan henkilön pääasiallisen muun liike- tai ammattitoiminnan ohella;"}}},{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/4/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/4","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/4/alkup/fin","hasFormat":{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/4/alkup/fin/txt","content_fi":"4) vuotuinen vakuutusmaksu on enintään 500 euroa ja vakuutussopimus on voimassa enintään viisi vuotta; sekä"}}},{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/5/alkup","@type":"sfl:ParagraphVersion","hasPart":[{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/5/alakohta/v/alkup","@type":"sfl:SubparagraphVersion","isMemberOf":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/5/alakohta/v","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/5/alakohta/v/alkup/fin","hasFormat":{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/5/alakohta/v/alkup/fin/txt","content_fi":"5) vakuutusedustus koskee vakuutusta, joka liittyy täydentävänä elinkeinonharjoittajan myymään tavaraan tai palveluun ja joka kattaa:"}}},{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/5/alakohta/a/alkup","@type":"sfl:SubparagraphVersion","isMemberOf":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/5/alakohta/a","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/5/alakohta/a/alkup/fin","hasFormat":{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/5/alakohta/a/alkup/fin/txt","content_fi":"a) tavaran viallisuuteen, katoamiseen tai vahingoittumiseen liittyvän riskin; taikka"}}},{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/5/alakohta/b/alkup","@type":"sfl:SubparagraphVersion","isMemberOf":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/5/alakohta/b","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/5/alakohta/b/alkup/fin","hasFormat":{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/5/alakohta/b/alkup/fin/txt","content_fi":"b) varattuun matkaan liittyvät riskit, kun mahdollinen henkivakuutusturva ja vastuuvakuutusturva ovat liitännäisiä pääasialliseen vakuutusturvaan nähden."}}},{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/5/johdanto/alkup","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/5/johdanto","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/5/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/5/johdanto/alkup/fin/txt","content_fi":"5) vakuutusedustus koskee vakuutusta, joka liittyy täydentävänä elinkeinonharjoittajan myymään tavaraan tai palveluun ja joka kattaa:"}}}],"isMemberOf":"sfsd:2005/570/luku/1/pykala/2/momentti/1/kohta/5","version":"sfl:Original","versionDate":"2018-08-15"}],"isMemberOf":"sfsd:2005/570/luku/1/pykala/2/momentti/1","version":"sfl:Original","versionDate":"2018-08-15"},{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/2/ajantasa/20090101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2008/897/alkup","cites":"sfsd:2008/878/luku/5","isMemberOf":"sfsd:2005/570/luku/1/pykala/2/momentti/2","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/2/ajantasa/20090101/fin","hasFormat":{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/2/ajantasa/20090101/fin/txt","content_fi":"Jälleenvakuutusta koskevaan vakuutusedustukseen ei sovelleta 19–23, 25 ja 26 §:ää, 28 §:n 2 ja 3 momenttia, 29 §:ää, 40 §:n 2–4 momenttia ja 41 §:ää eikä Finanssivalvonnasta annetun lain (878/2008) 5 luvun säännöksiä. Tätä lakia ei sovelleta Euroopan talousalueen ulkopuolella sijaitsevasta valtiosta käsin Suomessa harjoitettavaan vakuutusedustukseen, jos toiminta koskee ainoastaan jälleenvakuutusta ja toimintaa valvotaan kyseisessä maassa vähintään tämän lain vaatimuksia vastaavalla tai niihin rinnastettavalla tavalla.\n(19.12.2008/897)"}}},{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2005/570/luku/1/pykala/2/momentti/3","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:2005/570/luku/1/pykala/2/momentti/3/alkup/fin/txt","content_fi":"Tämän lain 6―13, 28―31 ja 41 §:ää ei sovelleta vakuutusedustajaan, joka on rekisteröity muussa Euroopan talousalueeseen kuuluvassa valtiossa kuin Suomessa."}}}],"isMemberOf":"sfsd:2005/570/luku/1/pykala/2","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/1/pykala/2/ajantasa/20090101/fin","title_fi":"Soveltamisalan rajoitukset"}},{"@id":"sfsd:2005/570/luku/1/pykala/3/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2005/570/luku/1/pykala/3/momentti/1/alkup","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:2005/570/luku/1/pykala/3/momentti/1/johdanto/alkup","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:2005/570/luku/1/pykala/3/momentti/1/johdanto","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/1/pykala/3/momentti/1/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:2005/570/luku/1/pykala/3/momentti/1/johdanto/alkup/fin/txt","content_fi":"Tässä laissa tarkoitetaan:"}}},{"@id":"sfsd:2005/570/luku/1/pykala/3/momentti/1/kohta/1/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2005/570/luku/1/pykala/3/momentti/1/kohta/1","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/1/pykala/3/momentti/1/kohta/1/alkup/fin","hasFormat":{"@id":"sfsd:2005/570/luku/1/pykala/3/momentti/1/kohta/1/alkup/fin/txt","content_fi":"1) asiamiehellä luonnollista henkilöä tai oikeushenkilöä, joka harjoittaa vakuutusedustusta vakuutuksenantajan lukuun ja vastuulla;"}}},{"@id":"sfsd:2005/570/luku/1/pykala/3/momentti/1/kohta/2/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2005/570/luku/1/pykala/3/momentti/1/kohta/2","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/1/pykala/3/momentti/1/kohta/2/alkup/fin","hasFormat":{"@id":"sfsd:2005/570/luku/1/pykala/3/momentti/1/kohta/2/alkup/fin/txt","content_fi":"2) vakuutusmeklarilla luonnollista henkilöä tai oikeushenkilöä, joka harjoittaa vakuutusedustusta muun asiakkaan kuin ensivakuutusta harjoittavan vakuutuksenantajan kanssa tekemänsä toimeksiantosopimuksen perusteella;"}}},{"@id":"sfsd:2005/570/luku/1/pykala/3/momentti/1/kohta/3/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2005/570/luku/1/pykala/3/momentti/1/kohta/3","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/1/pykala/3/momentti/1/kohta/3/alkup/fin","hasFormat":{"@id":"sfsd:2005/570/luku/1/pykala/3/momentti/1/kohta/3/alkup/fin/txt","content_fi":"3) vakuutusedustajalla vakuutusmeklaria ja asiamiestä."}}}],"isMemberOf":"sfsd:2005/570/luku/1/pykala/3/momentti/1","version":"sfl:Original","versionDate":"2018-08-15"},"isMemberOf":"sfsd:2005/570/luku/1/pykala/3","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/1/pykala/3/alkup/fin","title_fi":"Määritelmät"}},{"@id":"sfsd:2005/570/luku/1/pykala/4/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2005/570/luku/1/pykala/4/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2005/570/luku/1/pykala/4/momentti/1","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/1/pykala/4/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2005/570/luku/1/pykala/4/momentti/1/alkup/fin/txt","content_fi":"Sen lisäksi, mitä tässä laissa säädetään, asiamiehiin sovelletaan, mitä vakuutussopimuslain (543/1994) 5 §:n 1 ja 2 momentissa ja 9 §:ssä säädetään."}}},"isMemberOf":"sfsd:2005/570/luku/1/pykala/4","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/1/pykala/4/alkup/fin","title_fi":"Asiamiehiä koskevat säännökset"}}],"isMemberOf":"sfsd:2005/570/luku/1","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/1/ajantasa/20090101/fin","title_fi":"Yleiset säännökset"}},{"@id":"sfsd:2005/570/luku/2/ajantasa/20160101","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:2005/570/luku/2/pykala/5/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2005/570/luku/2/pykala/5/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2005/570/luku/2/pykala/5/momentti/1","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/2/pykala/5/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2005/570/luku/2/pykala/5/momentti/1/alkup/fin/txt","content_fi":"Vakuutusedustusta saa harjoittaa Suomessa vain sellainen vakuutusedustaja, joka on merkitty Vakuutusvalvontaviraston pitämään rekisteriin (vakuutusedustajarekisteri) tai joka on rekisteröity muussa Euroopan talousalueeseen kuuluvassa valtiossa kuin Suomessa."}}},"isMemberOf":"sfsd:2005/570/luku/2/pykala/5","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/2/pykala/5/alkup/fin","title_fi":"Oikeus vakuutusedustuksen harjoittamiseen"}},{"@id":"sfsd:2005/570/luku/2/pykala/6/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2005/570/luku/2/pykala/6/momentti/1/alkup","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:2005/570/luku/2/pykala/6/momentti/1/johdanto/alkup","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:2005/570/luku/2/pykala/6/momentti/1/johdanto","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/2/pykala/6/momentti/1/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:2005/570/luku/2/pykala/6/momentti/1/johdanto/alkup/fin/txt","content_fi":"Vakuutusvalvontaviraston on pyynnöstä rekisteröitävä asiamieheksi luonnollinen henkilö, jos:"}}},{"@id":"sfsd:2005/570/luku/2/pykala/6/momentti/1/kohta/1/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2005/570/luku/2/pykala/6/momentti/1/kohta/1","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/2/pykala/6/momentti/1/kohta/1/alkup/fin","hasFormat":{"@id":"sfsd:2005/570/luku/2/pykala/6/momentti/1/kohta/1/alkup/fin/txt","content_fi":"1) hänen asuinpaikkansa on Suomessa;"}}},{"@id":"sfsd:2005/570/luku/2/pykala/6/momentti/1/kohta/2/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2005/570/luku/2/pykala/6/momentti/1/kohta/2","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2005/570/luku/2/pykala/6/momentti/1/kohta/2/alkup/fin","hasFormat":{"@id":"sfsd:2005/570/luku/2/pykala/6/momentti/1/kohta/2/alkup/fin/txt","content_fi":"2) hän on täysi-ikäinen, ei ole konkurssissa ja hänen toimintakelpoisuuttaan ei ole rajoitettu; sekä"}}},{"@id":"sfsd:2005/570/luku/2/pykala/6/momentti/1/kohta/3/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2005/570/luku/2/pykala/6/momentti/1/kohta/3","version":"sfl:Original","versi