{"@id":"sfsd:2013/658","@type":["http://data.finlex.fi/schema/sfl/NewStatute"],"idLocal":["658/2013"],"inForce":["http://data.europa.eu/eli/ontology#InForce-inForce"],"temporalVersions":["http://data.finlex.fi/eli/sd/2013/658/alkup","http://data.finlex.fi/eli/sd/2013/658/ajantasa/20160101"],"typeDocument":["http://data.finlex.fi/schema/sfl/Act"],"temporalVersion":{"@id":"sfsd:2013/658/ajantasa/20160101","@type":"sfl:StatuteVersion","dateDocument":"2013-08-30","hasPart":[{"@id":"sfsd:2013/658/johtolause/alkup","@type":"sfl:EnactingClauseVersion","isMemberOf":"sfsd:2013/658/johtolause","version":"sfl:Original","versionDate":"2016-01-01","languageVersion":{"@id":"sfsd:2013/658/johtolause/alkup/fin","hasFormat":{"@id":"sfsd:2013/658/johtolause/alkup/fin/txt","content_fi":"Eduskunnan päätöksen mukaisesti säädetään:"}}},{"@id":"sfsd:2013/658/pykala/1/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2013/658/pykala/1/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2013/658/pykala/1/momentti/1","version":"sfl:Original","versionDate":"2016-01-01","languageVersion":{"@id":"sfsd:2013/658/pykala/1/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2013/658/pykala/1/momentti/1/alkup/fin/txt","content_fi":"Tämän lain tavoitteena on torjua harmaata taloutta vähentämällä elinkeinonharjoittajan mahdollisuutta jättää käteismaksuna saatuja tuloja merkitsemättä kirjanpitoon ja edistämällä ostajan mahdollisuutta havaita tällaista myyntiä."}}},"isMemberOf":"sfsd:2013/658/pykala/1","version":"sfl:Original","versionDate":"2016-01-01","languageVersion":{"@id":"sfsd:2013/658/pykala/1/alkup/fin","title_fi":"Lain tavoite"}},{"@id":"sfsd:2013/658/pykala/2/ajantasa/20160101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2015/516/alkup","hasPart":{"@id":"sfsd:2013/658/pykala/2/momentti/1/ajantasa/20160101","@type":"sfl:SubsectionVersion","cites":"sfsd:1968/360","isMemberOf":"sfsd:2013/658/pykala/2/momentti/1","version":"sfl:Consolidated","versionDate":"2016-01-01","lan