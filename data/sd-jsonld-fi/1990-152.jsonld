d":"sfsd:1990/152/luku/1/pykala/2/momentti/1/kohta/6/ajantasa/20091203/fin","hasFormat":{"@id":"sfsd:1990/152/luku/1/pykala/2/momentti/1/kohta/6/ajantasa/20091203/fin/txt","content_fi":"6) lääkäri- ja hammaslääkäripalveluja ja muuta terveyden- ja sairaanhoitoa sekä näihin rinnastettavia palveluja;"}}},{"@id":"sfsd:1990/152/luku/1/pykala/2/momentti/1/kohta/7/ajantasa/20091203","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1990/152/luku/1/pykala/2/momentti/1/kohta/7","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/1/pykala/2/momentti/1/kohta/7/ajantasa/20091203/fin","hasFormat":{"@id":"sfsd:1990/152/luku/1/pykala/2/momentti/1/kohta/7/ajantasa/20091203/fin/txt","content_fi":"7) hierontaa; sekä"}}},{"@id":"sfsd:1990/152/luku/1/pykala/2/momentti/1/kohta/8/ajantasa/20091203","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1990/152/luku/1/pykala/2/momentti/1/kohta/8","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/1/pykala/2/momentti/1/kohta/8/ajantasa/20091203/fin","hasFormat":{"@id":"sfsd:1990/152/luku/1/pykala/2/momentti/1/kohta/8/ajantasa/20091203/fin/txt","content_fi":"8) sairaankuljetuspalveluja."}}}],"isMemberOf":"sfsd:1990/152/luku/1/pykala/2/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-15"},{"@id":"sfsd:1990/152/luku/1/pykala/2/momentti/2/ajantasa/20091203","@type":"sfl:SubsectionVersion","cites":"sfsd:2001/1383","isMemberOf":"sfsd:1990/152/luku/1/pykala/2/momentti/2","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/1/pykala/2/momentti/2/ajantasa/20091203/fin","hasFormat":{"@id":"sfsd:1990/152/luku/1/pykala/2/momentti/2/ajantasa/20091203/fin/txt","content_fi":"Palvelujen tuottajalla tarkoitetaan sellaista yksittäistä henkilöä taikka yhtiötä, osuuskuntaa, yhdistystä tai muuta yhteisöä taikka säätiötä, joka ylläpitää terveydenhuollon palveluja tuottavaa yksikköä, sekä sairaankuljetuspalveluja tarjoavaa itsenäistä ammatinharjoittajaa. Palvelujen tuottajana ei pidetä muuta itsenäistä ammatinharjoittajaa tai työnantajaa, joka järjestää itse työterveyshuoltolaissa (1383/2001) tarkoitetut työterveyshuoltopalvelut."}}},{"@id":"sfsd:1990/152/luku/1/pykala/2/momentti/3/ajantasa/20091203","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1990/152/luku/1/pykala/2/momentti/3","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/1/pykala/2/momentti/3/ajantasa/20091203/fin","hasFormat":{"@id":"sfsd:1990/152/luku/1/pykala/2/momentti/3/ajantasa/20091203/fin/txt","content_fi":"Itsenäisellä ammatinharjoittajalla tarkoitetaan sellaista terveydenhuollon ammattihenkilöistä annetun lain 2 §:n 1 momentissa tarkoitettua terveydenhuollon ammattihenkilöä, joka harjoittaa itsenäisesti ammattiaan."}}}],"isMemberOf":"sfsd:1990/152/luku/1/pykala/2","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/1/pykala/2/ajantasa/20091203/fin","title_fi":"Määritelmät"}},{"@id":"sfsd:1990/152/luku/1/pykala/2a/ajantasa/20100101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2009/1549/alkup","hasPart":{"@id":"sfsd:1990/152/luku/1/pykala/2a/momentti/1/ajantasa/20100101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1990/152/luku/1/pykala/2a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/1/pykala/2a/momentti/1/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:1990/152/luku/1/pykala/2a/momentti/1/ajantasa/20100101/fin/txt","content_fi":"Tämän lain mukaisia lupaviranomaisia ovat aluehallintovirastot ja Sosiaali- ja terveysalan lupa- ja valvontavirasto. Lupaviranomaisten välisestä toimivallan jaosta säädetään valtioneuvoston asetuksella."}}},"isMemberOf":"sfsd:1990/152/luku/1/pykala/2a","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/1/pykala/2a/ajantasa/20100101/fin","title_fi":"Lupaviranomainen"}}],"isMemberOf":"sfsd:1990/152/luku/1","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/1/ajantasa/20100101/fin","title_fi":"Yleiset säännökset"}},{"@id":"sfsd:1990/152/luku/2/ajantasa/20110501","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1990/152/luku/2/pykala/3/ajantasa/20091203","@type":"sfl:SectionVersion","amendedBy":"sfsd:2009/928/alkup","hasPart":[{"@id":"sfsd:1990/152/luku/2/pykala/3/momentti/1/ajantasa/20091203","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1990/152/luku/2/pykala/3/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/2/pykala/3/momentti/1/ajantasa/20091203/fin","hasFormat":{"@id":"sfsd:1990/152/luku/2/pykala/3/momentti/1/ajantasa/20091203/fin/txt","content_fi":"Terveydenhuollon palveluja annettaessa on oltava asianmukaiset tilat ja laitteet. Sairaankuljetuspalveluja annettaessa on oltava asianmukainen sairaankuljetusajoneuvo varusteineen. Lisäksi palvelujen tuottajalla tulee olla toiminnan edellyttämä, asianmukaisen koulutuksen saanut henkilökunta."}}},{"@id":"sfsd:1990/152/luku/2/pykala/3/momentti/2/ajantasa/20091203","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1990/152/luku/2/pykala/3/momentti/2","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/2/pykala/3/momentti/2/ajantasa/20091203/fin","hasFormat":{"@id":"sfsd:1990/152/luku/2/pykala/3/momentti/2/ajantasa/20091203/fin/txt","content_fi":"Toiminnan on oltava lääketieteellisesti asianmukaista ja siinä tulee ottaa huomioon potilasturvallisuus."}}}],"isMemberOf":"sfsd:1990/152/luku/2/pykala/3","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/2/pykala/3/ajantasa/20091203/fin","title_fi":"Toimintaedellytykset"}},{"@id":"sfsd:1990/152/luku/2/pykala/4/ajantasa/20091203","@type":"sfl:SectionVersion","amendedBy":"sfsd:2005/689/alkup","hasPart":[{"@id":"sfsd:1990/152/luku/2/pykala/4/momentti/1/ajantasa/20091203","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2009/928/alkup","isMemberOf":"sfsd:1990/152/luku/2/pykala/4/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/2/pykala/4/momentti/1/ajantasa/20091203/fin","hasFormat":{"@id":"sfsd:1990/152/luku/2/pykala/4/momentti/1/ajantasa/20091203/fin/txt","content_fi":"Palvelujen tuottajalla on oltava lupaviranomaisen myöntämä lupa terveydenhuollon palvelujen antamiseen. Luvasta tulee ilmetä palvelujen tuottajan 2 §:n mukainen palveluala. Lupaan voidaan liittää potilasturvallisuuden varmistamiseksi välttämättömiä ehtoja palvelujen määrästä, henkilöstöstä, tiloista, sairaankuljetusajoneuvosta ja sen varusteista, laitteista ja tarvikkeista sekä toimintatavoista.\n(20.11.2009/928)"}}},{"@id":"sfsd:1990/152/luku/2/pykala/4/momentti/2/ajantasa/20060101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1990/152/luku/2/pykala/4/momentti/2","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/2/pykala/4/momentti/2/ajantasa/20060101/fin","hasFormat":{"@id":"sfsd:1990/152/luku/2/pykala/4/momentti/2/ajantasa/20060101/fin/txt","content_fi":"Lupa myönnetään kirjallisen ja allekirjoitetun hakemuksen perusteella palvelujen tuottajalle, joka täyttää 3 §:ssä säädetyt edellytykset."}}},{"@id":"sfsd:1990/152/luku/2/pykala/4/momentti/3/ajantasa/20091203","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1990/152/luku/2/pykala/4/momentti/3/johdanto/ajantasa/20060101","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1990/152/luku/2/pykala/4/momentti/3/johdanto","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/2/pykala/4/momentti/3/johdanto/ajantasa/20060101/fin","hasFormat":{"@id":"sfsd:1990/152/luku/2/pykala/4/momentti/3/johdanto/ajantasa/20060101/fin/txt","content_fi":"Lupahakemuksessa tulee olla seuraavat tiedot 