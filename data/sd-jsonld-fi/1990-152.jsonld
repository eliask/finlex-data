rsion","isMemberOf":"sfsd:1990/152/luku/6/pykala/25/momentti/2","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/6/pykala/25/momentti/2/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:1990/152/luku/6/pykala/25/momentti/2/ajantasa/20100101/fin/txt","content_fi":"Muutosta ei saa hakea valittamalla 9 §:n 2 momentissa tarkoitettuun päätökseen."}}}],"isMemberOf":"sfsd:1990/152/luku/6/pykala/25","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/6/pykala/25/ajantasa/20100101/fin","title_fi":"Muutoksenhaku"}},{"@id":"sfsd:1990/152/luku/6/pykala/26/ajantasa/20091203","@type":"sfl:SectionVersion","amendedBy":"sfsd:2009/928/alkup","hasPart":[{"@id":"sfsd:1990/152/luku/6/pykala/26/momentti/1/ajantasa/20091203","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1990/152/luku/6/pykala/26/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/6/pykala/26/momentti/1/ajantasa/20091203/fin","hasFormat":{"@id":"sfsd:1990/152/luku/6/pykala/26/momentti/1/ajantasa/20091203/fin/txt","content_fi":"Vastaavan johtajan hyväksymisen peruuttamista, toiminnan keskeyttämistä ja luvan peruuttamista samoin kuin toimintayksikön tai sen osan, sairaankuljetusajoneuvon tai sen varusteen sekä laitteen käytön kieltämistä koskeva päätös voidaan panna muutoksenhausta huolimatta heti täytäntöön, jos se on potilasturvallisuuden kannalta välttämätöntä."}}},{"@id":"sfsd:1990/152/luku/6/pykala/26/momentti/2/ajantasa/20091203","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1990/152/luku/6/pykala/26/momentti/2","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/6/pykala/26/momentti/2/ajantasa/20091203/fin","hasFormat":{"@id":"sfsd:1990/152/luku/6/pykala/26/momentti/2/ajantasa/20091203/fin/txt","content_fi":"Kun muutosta on haettu, muutoksenhakuviranomainen voi kieltää päätöksen täytäntöönpanon tai määrätä sen keskeytettäväksi."}}}],"isMemberOf":"sfsd:1990/152/luku/6/pykala/26","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/6/pykala/26/ajantasa/20091203/fin","title_fi":"Täytäntöönpano"}}],"isMemberOf":"sfsd:1990/152/luku/6","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/6/ajantasa/20100101/fin","title_fi":"Muutoksenhaku ja täytäntöönpano"}},{"@id":"sfsd:1990/152/luku/7/alkup","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1990/152/luku/7/pykala/27/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1990/152/luku/7/pykala/27/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1990/152/luku/7/pykala/27/momentti/1","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/7/pykala/27/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1990/152/luku/7/pykala/27/momentti/1/alkup/fin/txt","content_fi":"Tämä laki tulee voimaan 1 päivänä tammikuuta 1991."}}},{"@id":"sfsd:1990/152/luku/7/pykala/27/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1990/152/luku/7/pykala/27/momentti/2","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/7/pykala/27/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1990/152/luku/7/pykala/27/momentti/2/alkup/fin/txt","content_fi":"Tällä lailla kumotaan yksityisistä sairaanhoito- ja tutkimuslaitoksista 24 päivänä heinäkuuta 1964 annettu laki (426/64) siihen myöhemmin tehtyine muutoksineen."}}}],"isMemberOf":"sfsd:1990/152/luku/7/pykala/27","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/7/pykala/27/alkup/fin","title_fi":"Voimaantulo"}},{"@id":"sfsd:1990/152/luku/7/pykala/28/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1990/152/luku/7/pykala/28/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1990/152/luku/7/pykala/28/momentti/1","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/7/pykala/28/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1990/152/luku/7/pykala/28/momentti/1/alkup/fin/txt","content_fi":"Edellä 2§:n 2 momentissa tarkoitetun palvelujen tuottajan, joka harjoittaa tämän lain voimaan tullessa yksityisistä sairaanhoito- ja tutkimuslaitoksista annetun lain tai lääkitysvoimistelijan ja hierojan toimesta annetun lain nojalla tässä laissa tarkoitettua toimintaa, tulee tehdä ilmoitus toiminnan jatkumisesta asianomaiselle lääninhallitukselle yhden vuoden kuluessa tämän lain voimaantulosta."}}},{"@id":"sfsd:1990/152/luku/7/pykala/28/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1990/152/luku/7/pykala/28/momentti/2","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/7/pykala/28/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1990/152/luku/7/pykala/28/momentti/2/alkup/fin/txt","content_fi":"Ilmoituksen saatuaan lääninhallitus voi velvoittaa palvelujen tuottajan hakemaan tässä laissa tarkoitettua lupaa tai vastaavan johtajan hyväksymistä lääninhallituksen asettamaan määräaikaan mennessä. Lupaa tai hyväksymistä koskeva päätös on tehtävä kuuden kuukauden kuluessa siitä, kun ilmoitus on saapunut lääninhallitukselle."}}},{"@id":"sfsd:1990/152/luku/7/pykala/28/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1990/152/luku/7/pykala/28/momentti/3","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/7/pykala/28/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1990/152/luku/7/pykala/28/momentti/3/alkup/fin/txt","content_fi":"Mikäli toiminnan jatkumista koskevaa ilmoitusta ei 1 momentissa mainittuun määräaikaan mennessä ole tehty, lääninhallituksen tulee peruuttaa toiminnan harjoittamiseen myönnetty lupa."}}}],"isMemberOf":"sfsd:1990/152/luku/7/pykala/28","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/7/pykala/28/alkup/fin","title_fi":"Siirtymäsäännökset"}},{"@id":"sfsd:1990/152/luku/7/pykala/29/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1990/152/luku/7/pykala/29/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1990/152/luku/7/pykala/29/momentti/1","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/7/pykala/29/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1990/152/luku/7/pykala/29/momentti/1/alkup/fin/txt","content_fi":"Tarkemmat säännökset tämän lain täytäntöönpanosta annetaan asetuksella."}}},{"@id":"sfsd:1990/152/luku/7/pykala/29/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1990/152/luku/7/pykala/29/momentti/2","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/7/pykala/29/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1990/152/luku/7/pykala/29/momentti/2/alkup/fin/txt","content_fi":"Ennen tämän lain voimaantuloa voidaan ryhtyä sen täytäntöönpanon edellyttämiin toimenpiteisiin."}}}],"isMemberOf":"sfsd:1990/152/luku/7/pykala/29","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/7/pykala/29/alkup/fin","title_fi":"Tarkemmat säännökset"}}],"isMemberOf":"sfsd:1990/152/luku/7","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/luku/7/alkup/fin","title_fi":"Lain voimaantulo ja tarkemmat säännökset"}}],"isAbout":["http://data.finlex.fi/voc/finlex/kw3473","http://data.finlex.fi/voc/finlex/kw3844","http://data.finlex.fi/voc/finlex/kw3839","http://data.finlex.fi/voc/finlex/kw2190","http://data.finlex.fi/voc/finlex/kw3171","http://data.finlex.fi/voc/finlex/kw4583"],"isMemberOf":"sfsd:1990/152","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1990/152/ajantasa/20180815/fin","title_fi":"Laki yksityisestä terveydenhuollosta"}},"@context":{"languageVersion":{"@id":"eli:is_realized