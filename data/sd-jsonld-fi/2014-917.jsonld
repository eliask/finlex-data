eVersion":{"@id":"sfsd:2014/917/luku/29/pykala/243/momentti/4/alkup/fin","hasFormat":{"@id":"sfsd:2014/917/luku/29/pykala/243/momentti/4/alkup/fin/txt","content_fi":"Edellä 1 momentissa tarkoitetut laatuvaatimukset koskevat myös viestintäverkkoihin ja viestintäpalveluihin liittyviä merkittäviä liitännäistoimintoja ja liitännäispalveluja."}}},{"@id":"sfsd:2014/917/luku/29/pykala/243/momentti/5/ajantasa/20160620","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2016/456/alkup","isMemberOf":"sfsd:2014/917/luku/29/pykala/243/momentti/5","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2014/917/luku/29/pykala/243/momentti/5/ajantasa/20160620/fin","hasFormat":{"@id":"sfsd:2014/917/luku/29/pykala/243/momentti/5/ajantasa/20160620/fin/txt","content_fi":"Teleyrityksen on julkaistava ajan tasalla olevat tekniset eritelmät niistä yleisen viestintäverkon rajapinnoista, joihin telepäätelaitteet voidaan liittää. Eritelmistä on käytävä ilmi riittävällä tarkkuudella tiedot, joiden perusteella voidaan valmistaa telepäätelaitteita ja joiden avulla voidaan käyttää rajapinnan kautta käytettäviä palveluita.\n(17.6.2016/456)"}}}],"isMemberOf":"sfsd:2014/917/luku/29/pykala/243","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2014/917/luku/29/pykala/243/ajantasa/20160620/fin","title_fi":"Viestintäverkon ja viestintäpalvelun laatuvaatimukset"}},{"@id":"sfsd:2014/917/luku/29/pykala/244/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2014/917/luku/29/pykala/244/momentti/1/alkup","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:2014/917/luku/29/pykala/244/momentti/1/johdanto/alkup","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:2014/917/luku/29/pykala/244/momentti/1/johdanto","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:2014/917/luku/29/pykala/244/momentti/1/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:2014/917/luku/29/pykala/244/momentti/1/johdanto/alkup/fin/t