sionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/33d/momentti/2/kohta/2/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/5/pykala/33d/momentti/2/kohta/2/ajantasa/20170101/fin/txt","content_fi":"2) 16 §:n 2 momentissa säädetty veron perimisvelvollisuus tai 31 §:ssä säädetty veron tilitysvelvollisuus;"}}},{"@id":"sfsd:1996/931/luku/5/pykala/33d/momentti/2/kohta/3/ajantasa/20170101","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/931/luku/5/pykala/33d/momentti/2/kohta/3","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/33d/momentti/2/kohta/3/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/5/pykala/33d/momentti/2/kohta/3/ajantasa/20170101/fin/txt","content_fi":"3) 18 §:n 2 momentissa tarkoitetun arvopaperin luovutus ja luovutuksensaaja on Suomessa yleisesti verovelvollinen."}}}],"isMemberOf":"sfsd:1996/931/luku/5/pykala/33d/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01"}],"isMemberOf":"sfsd:1996/931/luku/5/pykala/33d","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/33d/ajantasa/20170101/fin","title_fi":"Pidennetty veron määräämisen ja oikaisun määräaika"}},{"@id":"sfsd:1996/931/luku/5/pykala/33e/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/785/alkup","hasPart":{"@id":"sfsd:1996/931/luku/5/pykala/33e/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/5/pykala/33e/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/33e/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/5/pykala/33e/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Vero voidaan määrätä tai päätöstä oikaista verosta vastuussa olevan vahingoksi 33 b §:ssä säädetyn määräajan estämättä vuoden kuluessa sen kalenterivuoden päättymisestä, jonka aikana syyttäjä on nostanut syytteen rikoslain (39/1889) 29 luvun 1–3 §:ssä tarkoitetussa rikosasiassa siten kuin oma-aloitteisten verojen verotusmenettelystä annetun lain 47 §:ssä säädetään."}}},"isMemberOf":"sfsd:1996/931/luku/5/pykala/33e","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/33e/ajantasa/20170101/fin","title_fi":"Rikosperusteinen veron määrääminen ja oikaisu"}},{"@id":"sfsd:1996/931/luku/5/pykala/34/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/526/alkup","hasPart":{"@id":"sfsd:1996/931/luku/5/pykala/34/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/5/pykala/34/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/34/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1996/931/luku/5/pykala/34/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Jos veron perimiseen velvollinen ei ole maksanut luovutuksensaajalta perimiään veroja 31 §:ssä säädetyssä ajassa, Verohallinto maksuunpanee veron korotuksineen ja viivästysseuraamuksineen tämän maksettavaksi viiden vuoden kuluessa sen vuoden päättymisestä, jonka kuluessa vero on peritty."}}},"isMemberOf":"sfsd:1996/931/luku/5/pykala/34","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/34/ajantasa/20100901/fin","title_fi":"Maksuunpano veron perimiseen velvolliselle"}},{"@id":"sfsd:1996/931/luku/5/pykala/34/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/785/alkup","hasPart":{"@id":"sfsd:1996/931/luku/5/pykala/34/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/5/pykala/34/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/34/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/5/pykala/34/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Vero on määrätty ja oikaisu tehty määräajassa, jos Verohallinnon päätös on tehty ennen tässä laissa säädettyjen määräaikojen päättymistä."}}},"isMemberOf":"sfsd:1996/931/luku/5/pykala/34","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/34/ajantasa/20170101/fin","title_fi":"Määräaika päätöksen tekemiselle"}},{"@id":"sfsd:1996/931/luku/5/pykala/34a/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/785/alkup","hasPart":{"@id":"sfsd:1996/931/luku/5/pykala/34a/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/5/pykala/34a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/34a/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/5/pykala/34a/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Verovelvollisen kuoltua vero määrätään kuolinpesälle tai oikaisu kohdistetaan kuolinpesään. Veroa ei määrätä, jos 33 b §:ssä säädetty määräaika on päättynyt."}}},"isMemberOf":"sfsd:1996/931/luku/5/pykala/34a","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/34a/ajantasa/20170101/fin","title_fi":"Kuolinpesään kohdistettu veron määrääminen ja oikaisu"}},{"@id":"sfsd:1996/931/luku/5/pykala/35/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/526/alkup","hasPart":{"@id":"sfsd:1996/931/luku/5/pykala/35/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/5/pykala/35/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/35/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1996/931/luku/5/pykala/35/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Jos veron suorittaminen tai perityn veron maksaminen on laiminlyöty tai veroa on ilmeisesti suoritettu liian vähän eikä verovelvollinen tai vastuuvelvollinen ole kehotuksesta huolimatta antanut maksuunpanoa varten tarvittavia tietoja, Verohallinnon on toimitettava 33 ja 34 §:n mukainen maksuunpano arvioimalla."}}},"isMemberOf":"sfsd:1996/931/luku/5/pykala/35","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/35/ajantasa/20100901/fin","title_fi":"Arviomaksuunpano"}},{"@id":"sfsd:1996/931/luku/5/pykala/36/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/1/alkup","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/1/johdanto/alkup","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1996/931/luku/5/pykala/36/momentti/1/johdanto","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/1/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/1/johdanto/alkup/fin/txt","content_fi":"Veroa voidaan maksuunpanon yhteydessä korottaa, jos:"}}},{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/1/kohta/1/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/931/luku/5/pykala/36/momentti/1/kohta/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/1/kohta/1/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/1/kohta/1/alkup/fin/txt","content_fi":"1) verovelvollinen tai vastuuvelvollinen on ilman hyväksyttävää syytä jättänyt veron osaksi tai kokonaan säädetyssä ajassa suorittamatta tai perimättä, eikä laiminlyöntiä ole pidettävä vähäisenä, enintään 20 prosentilla laiminlyödylle määrälle;"}}},{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/1/kohta/2/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/931/luku/5/pykala/36/momentti/1/kohta/2","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/1/kohta/2/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/1/kohta/2/alkup/fin/txt","content_fi":"2) verovelvollinen tai vastuuvelvollinen on laiminlyönyt ilmoituksen tai muun tiedon tai asiakirjan oikeassa ajassa antamisen tai on antanut sen puutteellisena eikä asianomainen ole noudattanut saamaansa kehotusta laiminlyönnin korjaamiseen, enintään 20 prosentilla;"}}},{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/1/kohta/3/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/931/luku/5/pykala/36/momentti/1/kohta/3","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/1/kohta/3/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/1/kohta/3/alkup/fin/txt","content_fi":"3) veron perimiseen velvollinen on osaksi tai kokonaan jättänyt perimänsä määrän säädetyssä ajassa maksamatta, eikä laiminlyöntiä ole pidettävä vähäisenä, enintään 30 prosentilla laiminlyödylle määrälle;"}}},{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/1/kohta/4/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/931/luku/5/pykala/36/momentti/1/kohta/4","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/1/kohta/4/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/1/kohta/4/alkup/fin/txt","content_fi":"4) verovelvollinen tai vastuuvelvollinen on tahallaan tai törkeästä huolimattomuuudesta antanut puutteellisen, erehdyttävän tai väärän tiedon tai asiakirjan ja menettely on omiaan aiheuttamaan sen, että verovelvollinen tai vastuuvelvollinen vapautuu veron suorittamisesta, vähintään 20 ja enintään 50 prosentilla laiminlyödylle määrälle."}}}],"isMemberOf":"sfsd:1996/931/luku/5/pykala/36/momentti/1","version":"sfl:Original","versionDate":"2018-01-01"},{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/5/pykala/36/momentti/2","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/2/alkup/fin/txt","content_fi":"Veronkorotus voidaan määrätä vain sille tämän lain mukaan verosta vastuussa olevalle, jonka laiminlyönnistä asiassa ilmenevät olosuhteet huomioon ottaen on kysymys."}}},{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/5/pykala/36/momentti/3","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/5/pykala/36/momentti/3/alkup/fin/txt","content_fi":"Jos veroa on korotettava 8 §:n mukaan, ei 1 kohdassa tarkoitettua korotusta määrätä."}}}],"isMemberOf":"sfsd:1996/931/luku/5/pykala/36","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/36/alkup/fin","title_fi":"Veronkorotus"}},{"@id":"sfsd:1996/931/luku/5/pykala/37/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1996/931/luku/5/pykala/37/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/5/pykala/37/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/37/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/5/pykala/37/momentti/1/alkup/fin/txt","content_fi":"Jos jollekin olosuhteelle tai toimenpiteelle on annettu sellainen sisältö tai muoto, joka ei vastaa asian varsinaista luonnetta tai tarkoitusta, tai jos on ryhdytty muuhun toimenpiteeseen ilmeisesti siinä tarkoituksessa, että verosta vapauduttaisiin, on verotuksessa maksuunpanoineen meneteltävä asian varsinaisen luonteen tai tarkoituksen mukaisesti."}}},"isMemberOf":"sfsd:1996/931/luku/5/pykala/37","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/37/alkup/fin","title_fi":"Veron kiertäminen"}},{"@id":"sfsd:1996/931/luku/5/pykala/38/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/526/alkup","hasPart":{"@id":"sfsd:1996/931/luku/5/pykala/38/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/5/pykala/38/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/38/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1996/931/luku/5/pykala/38/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Jos Veronsaajien oikeudenvalvontayksikkö katsoo, että verovelvollisen maksettavaksi olisi määrättävä veroa, veronlisäystä tai veronkorotusta, Verohallinnon on yksikön pyynnöstä tehtävä asiassa päätös."}}},"isMemberOf":"sfsd:1996/931/luku/5/pykala/38","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/pykala/38/ajantasa/20100901/fin","title_fi":"Päätös Veronsaajien oikeudenvalvontayksikön pyynnöstä"}}],"isMemberOf":"sfsd:1996/931/luku/5","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/5/ajantasa/20170101/fin","title_fi":"Valvonta ja maksuunpano"}},{"@id":"sfsd:1996/931/luku/6/ajantasa/20180101","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1996/931/luku/6/pykala/39/ajantasa/20170101","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1996/931/luku/6/pykala/39/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/526/alkup","isMemberOf":"sfsd:1996/931/luku/6/pykala/39/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/39/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/39/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Verohallinto voi kirjallisesta hakemuksesta antaa ennakkoratkaisun velvollisuudesta suorittaa veroa sekä veron määrästä.\n(11.6.2010/526)"}}},{"@id":"sfsd:1996/931/luku/6/pykala/39/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/39/momentti/2","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/39/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/39/momentti/2/alkup/fin/txt","content_fi":"Ennakkoratkaisun antaa se lääninverovirasto, jonka alueella hakijan kotikunta on, tai mikäli tällä ei ole kotikuntaa Suomessa, Uudenmaan lääninverovirasto."}}},{"@id":"sfsd:1996/931/luku/6/pykala/39/momentti/2/ajantasa/20100901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/39/momentti/2","repealedBy":"sfsd:2010/526/alkup","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/39/momentti/2/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/39/momentti/2/ajantasa/20100901/fin/txt","content_fi":"2 momentti on kumottu L:lla 11.6.2010/526, joka tuli voimaan 1.9.2010."}}},{"@id":"sfsd:1996/931/luku/6/pykala/39/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/39/momentti/3","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/39/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/39/momentti/3/alkup/fin/txt","content_fi":"Hakemuksessa on ilmoitettava yksilöitynä kysymys, josta ennakkoratkaisua haetaan, ja esitettävä asian ratkaisemiseksi tarvittava selvitys."}}},{"@id":"sfsd:1996/931/luku/6/pykala/39/momentti/4/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/39/momentti/4","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/39/momentti/4/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/39/momentti/4/alkup/fin/txt","content_fi":"Lainvoiman saanutta ennakkoratkaisua on hakijan vaatimuksesta sitovana noudatettava siten kuin ennakkoratkaisussa määrätään."}}},{"@id":"sfsd:1996/931/luku/6/pykala/39/momentti/5/ajantasa/20170101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2016/785/alkup","isMemberOf":"sfsd:1996/931/luku/6/pykala/39/momentti/5","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/39/momentti/5/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/39/momentti/5/ajantasa/20170101/fin/txt","content_fi":"Ennakkoratkaisua koskeva asia on käsiteltävä Verohallinnossa kiireellisenä.\n(9.9.2016/785)"}}},{"@id":"sfsd:1996/931/luku/6/pykala/39/momentti/5/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/526/alkup","isMemberOf":"sfsd:1996/931/luku/6/pykala/39/momentti/5","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/39/momentti/5/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/39/momentti/5/ajantasa/20100901/fin/txt","content_fi":"Ennakkoratkaisua koskeva asia on käsiteltävä Verohallinnossa, hallinto-oikeudessa ja korkeimmassa hallinto-oikeudessa kiireellisenä.\n(11.6.2010/526)"}}}],"isMemberOf":"sfsd:1996/931/luku/6/pykala/39","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/39/ajantasa/20170101/fin","title_fi":"Ennakkoratkaisu"}},{"@id":"sfsd:1996/931/luku/6/pykala/40/ajantasa/20170101","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1996/931/luku/6/pykala/40/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/526/alkup","isMemberOf":"sfsd:1996/931/luku/6/pykala/40/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/40/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/40/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Jos veroa on suoritettu aiheettomasti tai liikaa, Verohallinto palauttaa asianomaisen hakemuksesta liikaa suoritetun veron.\n(11.6.2010/526)"}}},{"@id":"sfsd:1996/931/luku/6/pykala/40/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/40/momentti/2","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/40/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/40/momentti/2/alkup/fin/txt","content_fi":"Suoritettu vero palautetaan hakemuksesta, jos luovutus on tuomioistuimen päätöksellä kumottu tai määrätty purkautumaan taikka se on purettu sopimuksella tilanteessa, jossa se olisi voinut luovutuskirjassa olevan ehdon tai sopimusrikkomuksen vuoksi tulla puretuksi tuomiolla. Sama on voimassa, jos kauppahintaa on vastaavassa tilanteessa alennettu."}}},{"@id":"sfsd:1996/931/luku/6/pykala/40/momentti/3/ajantasa/20170101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2016/785/alkup","isMemberOf":"sfsd:1996/931/luku/6/pykala/40/momentti/3","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/40/momentti/3/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/40/momentti/3/ajantasa/20170101/fin/txt","content_fi":"Hakemus on tehtävä kolmen vuoden kuluessa verovuoden päättymistä seuraavan vuoden alusta. Edellä 2 momentissa tarkoitetuissa tilanteissa määräaika on kuitenkin kymmenen vuotta verovuoden päättymistä seuraavan vuoden alusta.\n(9.9.2016/785)"}}}],"isMemberOf":"sfsd:1996/931/luku/6/pykala/40","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/40/ajantasa/20170101/fin","title_fi":"Aiheettomasti suoritettu vero"}},{"@id":"sfsd:1996/931/luku/6/pykala/41/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/526/alkup","hasPart":{"@id":"sfsd:1996/931/luku/6/pykala/41/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/41/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/41/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/41/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Jos luovutuksesta, josta on suoritettu varainsiirtoveroa, määrätään myös lahjaveroa, Verohallinto palauttaa asianomaisen hakemuksesta liikaa suoritetun varainsiirtoveron."}}},"isMemberOf":"sfsd:1996/931/luku/6/pykala/41","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/41/ajantasa/20100901/fin","title_fi":"Veron palauttaminen lahjaveron määräämisen johdosta"}},{"@id":"sfsd:1996/931/luku/6/pykala/42/ajantasa/20100901","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/42","repealedBy":"sfsd:2010/526/alkup","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/42/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/42/ajantasa/20100901/fin/txt","content_fi":"42 §(11.6.2010/526)\n\n42 § on kumottu L:lla 11.6.2010/526, joka tuli voimaan 1.9.2010."}}},{"@id":"sfsd:1996/931/luku/6/pykala/43/ajantasa/20170101","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1996/931/luku/6/pykala/43/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/526/alkup","isMemberOf":"sfsd:1996/931/luku/6/pykala/43/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Jos elinkeinotulon verottamisesta annetun lain 52 d §:ssä tarkoitetuissa tapauksissa siirtyvään toimintaan kuuluva kiinteistö tai arvopaperi on luovutettu toimintaa jatkamaan perustetulle yhteisölle, Verohallinto palauttaa suoritetun veron hakemuksesta. Jos hakemus on tehty ja edellä mainittujen edellytysten täyttymisestä on esitetty selvitys ennen veron suorittamista, Verohallinto voi päättää, että veroa ei ole suoritettava. Lainvoiman saanutta päätöstä on hakijan vaatimuksesta noudatettava lainhuutoa tai kirjaamista haettaessa.\n(11.6.2010/526)"}}},{"@id":"sfsd:1996/931/luku/6/pykala/43/momentti/2/ajantasa/20170101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2016/785/alkup","isMemberOf":"sfsd:1996/931/luku/6/pykala/43/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43/momentti/2/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43/momentti/2/ajantasa/20170101/fin/txt","content_fi":"Veron palauttamista koskeva hakemus on tehtävä kolmen vuoden kuluessa verovuoden päättymistä seuraavan vuoden alusta. Veroa ei palauteta enempää kuin luovutuksesta 7 ja 21 §:n mukaan säädetyssä ajassa suoritettuna olisi ollut maksettava.\n(9.9.2016/785)"}}}],"isMemberOf":"sfsd:1996/931/luku/6/pykala/43","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43/ajantasa/20170101/fin","title_fi":"Kiinteistön tai arvopaperin luovutus liiketoimintasiirrossa"}},{"@id":"sfsd:1996/931/luku/6/pykala/43a/ajantasa/20130101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2012/991/alkup","hasPart":[{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/1/ajantasa/20130101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/1/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/1/ajantasa/20130101/fin/txt","content_fi":"Jos kunta tai sen omistama yhtiö vuosina 2013–2016 luovuttaa omistamansa vuokratalokiinteistön, asumisoikeustalokiinteistön taikka vuokratalo- tai asumisoikeustaloyhtiön osakkeet yhden tai useamman kunnan omistamalle osakeyhtiölle tai sen omistamalle yhtiölle ja saa vastikkeeksi vastaanottavan yhtiön osakkeita, Verohallinto palauttaa suoritetun veron hakemuksesta."}}},{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/2/ajantasa/20130101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43a/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/2/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/2/ajantasa/20130101/fin/txt","content_fi":"Edellä 1 momentissa tarkoitettu verovapaus koskee luovutuksia, joissa kunta tai useampi kunta yhdessä omistaa suoraan tai välillisesti vähintään 90 prosenttia luovutettavasta vuokratalo- tai asumisoikeustalokiinteistöstä taikka vuokra-asunnon, vuokratalon taikka asumisoikeustalon hallintaan oikeuttavista osakkeista sekä vastaanottavan yhtiön osakkeista."}}},{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/3/ajantasa/20130101","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/3/johdanto/ajantasa/20130101","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43a/momentti/3/johdanto","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/3/johdanto/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/3/johdanto/ajantasa/20130101/fin/txt","content_fi":"Verovapaan luovutuksen kohteena voivat olla:"}}},{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/3/kohta/1/ajantasa/20130101","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43a/momentti/3/kohta/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/3/kohta/1/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/3/kohta/1/ajantasa/20130101/fin/txt","content_fi":"1) aravarajoituslaissa tarkoitettujen käyttö- ja luovutusrajoitusten alaiset vuokra-asunnot ja aravavuokratalot tai niiden hallintaan oikeuttavat osakkeet;"}}},{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/3/kohta/2/ajantasa/20130101","@type":"sfl:ParagraphVersion","cites":"sfsd:2001/604/pykala/17","isMemberOf":"sfsd:1996/931/luku/6/pykala/43a/momentti/3/kohta/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/3/kohta/2/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/3/kohta/2/ajantasa/20130101/fin/txt","content_fi":"2) vuokra-asuntolainojen ja asumisoikeustalolainojen korkotuesta annetun lain (604/2001) 17 §:ssä tarkoitetun käyttö- ja luovutusrajoitusajan alaiset vuokra-asunnot ja korkotukivuokratalot tai niiden hallintaan oikeuttavat osakkeet;"}}},{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/3/kohta/3/ajantasa/20130101","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43a/momentti/3/kohta/3","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/3/kohta/3/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/3/kohta/3/ajantasa/20130101/fin/txt","content_fi":"3) vuokra-asuntolainojen korkotuesta annetun lain (867/1980) 10 §:ssä tarkoitetun 20 vuoden käyttörajoitusajan alaiset asunnot tai niiden hallintaan oikeuttavat osakkeet;"}}},{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/3/kohta/4/ajantasa/20130101","@type":"sfl:ParagraphVersion","cites":"sfsd:1990/650","isMemberOf":"sfsd:1996/931/luku/6/pykala/43a/momentti/3/kohta/4","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/3/kohta/4/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/3/kohta/4/ajantasa/20130101/fin/txt","content_fi":"4) asumisoikeusasunnoista annetussa laissa (650/1990) tarkoitetut muut asunnot kuin vapaarahoitteiset asumisoikeusasunnot."}}}],"isMemberOf":"sfsd:1996/931/luku/6/pykala/43a/momentti/3","version":"sfl:Consolidated","versionDate":"2018-01-01"},{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/4/ajantasa/20130101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43a/momentti/4","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/4/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/4/ajantasa/20130101/fin/txt","content_fi":"Hakemus veron palauttamiseksi on tehtävä vuoden kuluessa veron suorittamisesta. Hakemukseen on liitettävä Valtiokonttorin antama todistus siitä, että luovutus koskee 3 momentissa tarkoitettua kohdetta."}}},{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/5/ajantasa/20130101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43a/momentti/5","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/5/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/5/ajantasa/20130101/fin/txt","content_fi":"Jos hakemus on tehty ja verovapaan luovutuksen edellytysten täyttymisestä on esitetty selvitys ennen veron suorittamista, Verohallinto voi päättää, että veroa ei ole suoritettava."}}},{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/6/ajantasa/20130101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43a/momentti/6","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/6/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43a/momentti/6/ajantasa/20130101/fin/txt","content_fi":"Veroa ei palauteta enempää kuin luovutuksesta 7 ja 21 §:n mukaan säädetyssä ajassa suoritettuna olisi tullut maksaa. Palautettavalle verolle ei makseta korkoa."}}}],"isMemberOf":"sfsd:1996/931/luku/6/pykala/43a","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43a/ajantasa/20130101/fin","title_fi":"Määräaikainen verovapaus kunnallisten vuokra-asuntojen omistusjärjestelyssä"}},{"@id":"sfsd:1996/931/luku/6/pykala/43b/ajantasa/20130101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2012/991/alkup","hasPart":[{"@id":"sfsd:1996/931/luku/6/pykala/43b/momentti/1/ajantasa/20130101","@type":"sfl:SubsectionVersion","cites":"sfsd:2007/169","isMemberOf":"sfsd:1996/931/luku/6/pykala/43b/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43b/momentti/1/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43b/momentti/1/ajantasa/20130101/fin/txt","content_fi":"Jos kunta, kuntayhtymä tai niiden omistama kiinteistön omistamista ja hallintaa harjoittava osakeyhtiö vuosina 2013–2016 luovuttaa vuosina 2007–2012 voimassa olleessa kunta- ja palvelurakenneuudistuksesta annetussa laissa (169/2007) tarkoitettujen velvoitteiden toteuttamiseksi omistamansa kiinteistön, joka on pääasiallisesti ollut välittömästi sosiaalihuolto-, terveyden- ja sairaanhoito-, koulu-, nuoriso-, kirjasto-, kulttuuri-, liikunta- tai palo- ja pelastustoimen taikka kunnan tai kuntayhtymän yleishallinnon käytössä yhden tai useamman kunnan tai kuntayhtymän omistamalle kiinteistön omistamista ja hallintaa harjoittavalle osakeyhtiölle sosiaali- tai terveydenhuollon käyttöön ja saa vastikkeeksi vastaanottavan yhtiön osakkeita, Verohallinto palauttaa suoritetun veron hakemuksesta."}}},{"@id":"sfsd:1996/931/luku/6/pykala/43b/momentti/2/ajantasa/20130101","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1996/931/luku/6/pykala/43b/momentti/2/johdanto/ajantasa/20130101","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43b/momentti/2/johdanto","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43b/momentti/2/johdanto/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43b/momentti/2/johdanto/ajantasa/20130101/fin/txt","content_fi":"Verovapaus koskee vastaavasti kunnan tai kuntayhtymän omistaman kiinteistön omistamista ja hallintaa harjoittavan yhtiön osakkeiden luovutusta vastaanottavan yhtiön osakkeita vastaan, jos:"}}},{"@id":"sfsd:1996/931/luku/6/pykala/43b/momentti/2/kohta/1/ajantasa/20130101","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43b/momentti/2/kohta/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43b/momentti/2/kohta/1/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43b/momentti/2/kohta/1/ajantasa/20130101/fin/txt","content_fi":"1) kiinteistöt ovat pääasiassa 1 momentissa tarkoitetussa käytössä; tai"}}},{"@id":"sfsd:1996/931/luku/6/pykala/43b/momentti/2/kohta/2/ajantasa/20130101","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43b/momentti/2/kohta/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43b/momentti/2/kohta/2/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43b/momentti/2/kohta/2/ajantasa/20130101/fin/txt","content_fi":"2) osakkeet oikeuttavat 1 momentissa tarkoitetussa käytössä olevan huoneiston hallintaan."}}}],"isMemberOf":"sfsd:1996/931/luku/6/pykala/43b/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01"},{"@id":"sfsd:1996/931/luku/6/pykala/43b/momentti/3/ajantasa/20130101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43b/momentti/3","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43b/momentti/3/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43b/momentti/3/ajantasa/20130101/fin/txt","content_fi":"Hakemus veron palauttamiseksi on tehtävä vuoden kuluessa veron suorittamisesta. Hakemukseen on liitettävä asianomaisten kuntien antama selvitys luovutuksen liittymisestä kunta- ja palvelurakenneuudistuksesta annetussa laissa tarkoitettujen velvoitteiden toteuttamiseen sekä kiinteistön käytöstä ennen luovutusta ja sen jälkeen. Hakemukseen on lisäksi liitettävä kunnan tilintarkastajan lausunto. Verohallinto voi tarvittaessa pyytää valtiovarainministeriöltä lausunnon luovutuksen liittymisestä kunta- ja palvelurakenneuudistuksesta annetussa laissa tarkoitettujen velvoitteiden toteuttamiseen."}}},{"@id":"sfsd:1996/931/luku/6/pykala/43b/momentti/4/ajantasa/20130101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43b/momentti/4","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43b/momentti/4/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43b/momentti/4/ajantasa/20130101/fin/txt","content_fi":"Jos hakemus on tehty ja verovapaan luovutuksen edellytysten täyttymisestä on esitetty selvitys ennen veron suorittamista, Verohallinto voi päättää, että veroa ei ole suoritettava."}}}],"isMemberOf":"sfsd:1996/931/luku/6/pykala/43b","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43b/ajantasa/20130101/fin","title_fi":"Määräaikainen verovapaus kuntien rakennejärjestelyissä"}},{"@id":"sfsd:1996/931/luku/6/pykala/43b/ajantasa/20070223","@type":"sfl:SectionVersion","amendedBy":"sfsd:2007/171/alkup","isMemberOf":"sfsd:1996/931/luku/6/pykala/43b","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43b/ajantasa/20070223/fin","title_fi":"Määräaikainen verovapaus kuntien rakennejärjestelyissä","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43b/ajantasa/20070223/fin/txt","content_fi":"43 b §(9.2.2007/171)\nMääräaikainen verovapaus kuntien rakennejärjestelyissä"}}},{"@id":"sfsd:1996/931/luku/6/pykala/43c/ajantasa/20180101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2017/879/alkup","hasPart":[{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/1/ajantasa/20180101","@type":"sfl:SubsectionVersion","cites":"sfsd:2014/932","isMemberOf":"sfsd:1996/931/luku/6/pykala/43c/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/1/ajantasa/20180101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/1/ajantasa/20180101/fin/txt","content_fi":"Verohallinto palauttaa suoritetun veron hakemuksesta, jos kunta, kuntayhtymä, yksityinen yhteisö tai säätiö vuosina 2018–2022 luovuttaa yksityiselle osakeyhtiölle sen osakkeita vastaan omistamansa kiinteistön, joka on pääasiassa luovuttajan harjoittaman ammattikorkeakoululaissa (932/2014) säädetyn toimiluvan tai ammatillisesta koulutuksesta annetussa laissa (531/2017) säädetyn järjestämisluvan mukaisen toiminnan käytössä."}}},{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/2/ajantasa/20180101","@type":"sfl:SubsectionVersion","cites":"sfsd:2009/1705/pykala/9","isMemberOf":"sfsd:1996/931/luku/6/pykala/43c/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/2/ajantasa/20180101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/2/ajantasa/20180101/fin/txt","content_fi":"Veron palauttamisen edellytyksenä on, että luovutettua kiinteistöä käytetään pääasiassa vastaanottavan yhtiön tai sen omistajan harjoittaman 1 momentissa tarkoitetun toimiluvan tai järjestämisluvan mukaiseen toimintaan. Lisäksi edellytyksenä on, että kiinteistössä järjestettävän toiminnan kustannukset rahoitetaan pääosin opetus- ja kulttuuritoimen rahoituksesta annetun lain (1705/2009) 9 §:n mukaisesti myönnettävällä rahoituksella tai että ammattikorkeakoulu käyttää tiloja pääasiassa ammattikorkeakoululain 4 §:ssä tarkoitettujen julkisesti rahoitettujen tehtäviensä täyttämiseen."}}},{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/3/ajantasa/20180101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43c/momentti/3","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/3/ajantasa/20180101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/3/ajantasa/20180101/fin/txt","content_fi":"Verovapaus koskee vastaavasti kunnan, kuntayhtymän, yksityisen yhteisön tai säätiön omistaman kiinteistön omistamista ja hallintaa harjoittavan yhtiön osakkeiden luovutusta vastaanottavalle yhtiölle, jos kiinteistö on pääasiassa 1 momentissa tarkoitetussa käytössä tai osakkeet oikeuttavat 1 momentissa tarkoitetussa käytössä olevan huoneiston hallintaan."}}},{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/4/ajantasa/20180101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43c/momentti/4","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/4/ajantasa/20180101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/4/ajantasa/20180101/fin/txt","content_fi":"Hakemus veron palauttamiseksi on tehtävä vuoden kuluessa veron suorittamisesta. Hakemukseen on liitettävä selvitys kiinteistön käytöstä ennen luovutusta ja sen jälkeen. Hakemukseen on liitettävä opetus- ja kulttuuriministeriön lausunto verovapauden edellytysten täyttymisestä."}}},{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/5/ajantasa/20180101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43c/momentti/5","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/5/ajantasa/20180101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/5/ajantasa/20180101/fin/txt","content_fi":"Jos hakemus on tehty ja verovapaan luovutuksen edellytysten täyttymisestä on esitetty selvitys ennen veron suorittamista, Verohallinto voi päättää, että veroa ei ole suoritettava."}}},{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/6/ajantasa/20180101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43c/momentti/6","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/6/ajantasa/20180101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/6/ajantasa/20180101/fin/txt","content_fi":"Palautettu tai suorittamatta jäänyt vero on suoritettava, jos kiinteistön tai huoneiston käyttötarkoitus muuttuu ennen kuin viisi vuotta on kulunut luovutuksesta tai jos kiinteistö taikka kiinteistön tai huoneiston hallintaan oikeuttavat osakkeet mainitun ajan kuluessa luovutetaan edelleen muulle kuin 1 momentissa tarkoitetulle taholle 2 momentissa tarkoitettuun käyttöön. Vero on suoritettava viimeistään kuuden kuukauden kuluttua luovutuksesta tai siitä ajankohdasta, jona kiinteistön tai huoneiston käyttötarkoitus on muuttunut."}}}],"isMemberOf":"sfsd:1996/931/luku/6/pykala/43c","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43c/ajantasa/20180101/fin","title_fi":"Määräaikainen verovapaus eräissä koulutuksen rakennejärjestelyissä"}},{"@id":"sfsd:1996/931/luku/6/pykala/43c/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2007/171/alkup","hasPart":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/5/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/526/alkup","isMemberOf":"sfsd:1996/931/luku/6/pykala/43c/momentti/5","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/5/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/5/ajantasa/20100901/fin/txt","content_fi":"Jos hakemus on tehty ja verovapaan luovutuksen edellytysten täyttymisestä on esitetty selvitys ennen veron suorittamista, Verohallinto voi päättää, että veroa ei ole suoritettava.\n(11.6.2010/526)"}}},"isMemberOf":"sfsd:1996/931/luku/6/pykala/43c","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43c/ajantasa/20100901/fin","title_fi":"Määräaikainen verovapaus eräissä koulutuksen rakennejärjestelyissä"}},{"@id":"sfsd:1996/931/luku/6/pykala/43c/ajantasa/20130101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2012/991/alkup","hasPart":[{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/1/ajantasa/20130101","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/1/johdanto/ajantasa/20130101","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43c/momentti/1/johdanto","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/1/johdanto/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/1/johdanto/ajantasa/20130101/fin/txt","content_fi":"Verohallinto palauttaa suoritetun veron hakemuksesta, jos:"}}},{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/1/kohta/1/ajantasa/20130101","@type":"sfl:ParagraphVersion","cites":["sfsd:2003/351","sfsd:1998/630","sfsd:1998/631"],"isMemberOf":"sfsd:1996/931/luku/6/pykala/43c/momentti/1/kohta/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/1/kohta/1/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/1/kohta/1/ajantasa/20130101/fin/txt","content_fi":"1) kunta, kuntayhtymä tai yksityinen yhteisö tai säätiö vuosina 2013–2016 luovuttaa omistamansa kiinteistön ammattikorkeakoululaissa (351/2003) säädetyn toimiluvan ja ammatillisesta koulutuksesta annetussa laissa (630/1998) sekä ammatillisesta aikuiskoulutuksesta annetussa laissa (631/1998) säädetyn järjestämisluvan mukaista toimintaa harjoittavalle yhdistykselle, säätiölle taikka yksityiselle osakeyhtiölle sen osakkeita vastaan; tai"}}},{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/1/kohta/2/ajantasa/20130101","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43c/momentti/1/kohta/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/1/kohta/2/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/1/kohta/2/ajantasa/20130101/fin/txt","content_fi":"2) kunta tai kuntayhtymä luovuttaa 43 b §:ssä tarkoitetussa koulutoimen käytössä olleen kiinteistön käytettäväksi tämän momentin 1 kohdan mukaisesti siinä tarkoitetulle saajalle."}}}],"isMemberOf":"sfsd:1996/931/luku/6/pykala/43c/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01"},{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/2/ajantasa/20130101","@type":"sfl:SubsectionVersion","cites":["sfsd:2009/1705/pykala/25","sfsd:2012/916"],"isMemberOf":"sfsd:1996/931/luku/6/pykala/43c/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/2/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/2/ajantasa/20130101/fin/txt","content_fi":"Veron palauttamisen edellytyksenä on, että luovutettua kiinteistöä käytetään pääasiassa toimintaan, johon vastaanottaja, sekä 1 momentin 1 kohdassa tarkoitetuissa tilanteissa myös luovuttaja, on saanut valtioneuvostolta 1 momentissa tarkoitetun toimiluvan tai järjestämisluvan. Lisäksi edellytyksenä on, että kiinteistössä järjestettävän toiminnan kustannukset voidaan lukea opetus- ja kulttuuritoimen rahoituksesta annetun lain (1705/2009) 25 ja 26 §:n mukaisesti valtionosuuden perusteena käytettäviin kustannuksiin tai että mainitun lain 25 §:n mukainen ammatillisen peruskoulutuksen järjestäjä käyttää kiinteistöä pääosin ammatillisesta aikuiskoulutuksesta annetussa laissa tarkoitettuun ammatilliseen lisäkoulutukseen tai julkisesta työvoima- ja yrityspalvelusta annetussa laissa (916/2012) tarkoitettuun työvoimakoulutukseen."}}},{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/3/ajantasa/20130101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43c/momentti/3","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/3/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/3/ajantasa/20130101/fin/txt","content_fi":"Verovapaus koskee vastaavasti kunnan, kuntayhtymän, yksityisen yhteisön tai säätiön omistaman kiinteistön omistamista ja hallintaa harjoittavan yhtiön osakkeiden luovutusta vastaanottavalle yhtiölle, yhdistykselle tai säätiölle, jos kiinteistöt ovat pääasiassa 2 momentissa tarkoitetussa käytössä tai osakkeet oikeuttavat 2 momentissa tarkoitetussa käytössä olevan huoneiston hallintaan."}}},{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/4/ajantasa/20130101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43c/momentti/4","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/4/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/4/ajantasa/20130101/fin/txt","content_fi":"Hakemus veron palauttamiseksi on tehtävä vuoden kuluessa veron suorittamisesta. Hakemukseen on liitettävä selvitys kiinteistön käytöstä ennen luovutusta ja sen jälkeen. Hakemukseen on liitettävä opetus- ja kulttuuriministeriön lausunto verovapauden edellytysten täyttymisestä."}}},{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/5/ajantasa/20130101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43c/momentti/5","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/5/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/5/ajantasa/20130101/fin/txt","content_fi":"Jos hakemus on tehty ja verovapaan luovutuksen edellytysten täyttymisestä on esitetty selvitys ennen veron suorittamista, Verohallinto voi päättää, että veroa ei ole suoritettava."}}},{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/6/ajantasa/20130101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/43c/momentti/6","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/6/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/43c/momentti/6/ajantasa/20130101/fin/txt","content_fi":"Palautettu tai suorittamatta jäänyt vero on suoritettava, jos kiinteistön tai huoneiston käyttötarkoitus muuttuu ennen kuin viisi vuotta on kulunut luovutuksesta tai jos kiinteistön tai huoneiston hallintaan oikeuttavat osakkeet mainitun ajan kuluessa luovutetaan edelleen muulle kuin 1 momentin 1 kohdassa tarkoitetulle taholle 2 momentissa tarkoitettuun käyttöön. Veroa ei ole kuitenkaan suoritettava käyttötarkoituksen muuttumisen perusteella silloin, kun omistajana on kunnan tai kuntayhtymän omistama osakeyhtiö ja kiinteistö tai huoneisto otetaan 43 b §:ssä tarkoitettuun käyttöön. Vero on suoritettava viimeistään kuuden kuukauden kuluessa luovutuksesta tai siitä ajankohdasta, jona kiinteistön tai huoneiston käyttötarkoitus on muuttunut."}}}],"isMemberOf":"sfsd:1996/931/luku/6/pykala/43c","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/43c/ajantasa/20130101/fin","title_fi":"Määräaikainen verovapaus eräissä koulutuksen rakennejärjestelyissä."}},{"@id":"sfsd:1996/931/luku/6/pykala/44/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/526/alkup","hasPart":[{"@id":"sfsd:1996/931/luku/6/pykala/44/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/44/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/44/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/44/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Jos 11 tai 25 §:ssä tarkoitetut verovapauden edellytykset ovat täyttyneet vasta sen jälkeen kun vero on suoritettu, Verohallinto maksaa hakemuksesta takaisin suoritetun veron. Veroa ei kuitenkaan palauteta enempää kuin se määrä, joka luovutuksesta 7 tai 21 §:ssä säädetyssä ajassa suoritettuna olisi ollut maksettava."}}},{"@id":"sfsd:1996/931/luku/6/pykala/44/momentti/2/ajantasa/20170101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2016/785/alkup","isMemberOf":"sfsd:1996/931/luku/6/pykala/44/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/44/momentti/2/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/44/momentti/2/ajantasa/20170101/fin/txt","content_fi":"Hakemus on tehtävä vuoden kuluessa siitä, kun verovapauden edellytykset ovat täyttyneet.\n(9.9.2016/785)"}}}],"isMemberOf":"sfsd:1996/931/luku/6/pykala/44","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/44/ajantasa/20170101/fin","title_fi":"Ensiasunnosta suoritetun veron palauttaminen"}},{"@id":"sfsd:1996/931/luku/6/pykala/45/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1996/931/luku/6/pykala/45/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/6/pykala/45/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/45/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/6/pykala/45/momentti/1/alkup/fin/txt","content_fi":"Hakijan on esitettävä selvitys veron palauttamisen edellytyksistä."}}},"isMemberOf":"sfsd:1996/931/luku/6/pykala/45","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/pykala/45/alkup/fin","title_fi":"Veron palauttamista haettaessa esitettävä selvitys"}}],"isMemberOf":"sfsd:1996/931/luku/6","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/6/ajantasa/20180101/fin","title_fi":"Ennakkoratkaisu ja veron palauttaminen"}},{"@id":"sfsd:1996/931/luku/7/ajantasa/20170101","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1996/931/luku/7/pykala/46/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/785/alkup","hasPart":[{"@id":"sfsd:1996/931/luku/7/pykala/46/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/7/pykala/46/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/pykala/46/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/7/pykala/46/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Verovelvollinen tai muu tämän lain mukaan verosta vastuussa oleva, ennakkoratkaisun hakija sekä Veronsaajien oikeudenvalvontayksikkö saavat hakea muutosta veron suorittamisvelvollisuutta tai suoritettavan veron määrää koskevaan päätökseen tai ennakkoratkaisuun siten kuin oma-aloitteisten verojen verotusmenettelystä annetussa laissa säädetään. Verovelvollisen tai muun tämän lain mukaan verosta vastuussa olevan oikaisuvaatimuksen määräaika lasketaan kuitenkin 32 a §:ssä tarkoitetun verovuoden päättymistä seuraavan vuoden alusta."}}},{"@id":"sfsd:1996/931/luku/7/pykala/46/momentti/2/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/7/pykala/46/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/pykala/46/momentti/2/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/7/pykala/46/momentti/2/ajantasa/20170101/fin/txt","content_fi":"Oikaisuvaatimuksen johdosta annettuun päätökseen ja ennakkoratkaisuun haetaan muutosta valittamalla siihen hallinto-oikeuteen, jonka tuomiopiirissä päätöksessä tarkoitettu kiinteistö sijaitsee tai jonka alueella on sen yhteisön kotipaikka, jonka liikkeeseen laskemia arvopapereita päätös koskee. Jos päätös koskee sekä kiinteistöä että arvopapereita, muutosta haetaan kiinteistön sijaintipaikan hallinto-oikeudelta. Jollei tätä perustetta voida käyttää, valitus tehdään Helsingin hallinto-oikeudelle."}}}],"isMemberOf":"sfsd:1996/931/luku/7/pykala/46","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/pykala/46/ajantasa/20170101/fin","title_fi":"Muutoksenhaku"}},{"@id":"sfsd:1996/931/luku/7/pykala/47/ajantasa/20130101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2012/876/alkup","hasPart":[{"@id":"sfsd:1996/931/luku/7/pykala/47/momentti/1/ajantasa/20130101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/7/pykala/47/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/pykala/47/momentti/1/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/7/pykala/47/momentti/1/ajantasa/20130101/fin/txt","content_fi":"Verohallinnon päätökseen haetaan muutosta valittamalla siihen hallinto-oikeuteen, jonka tuomiopiirissä päätöksessä tarkoitettu kiinteistö sijaitsee tai jonka alueella on sen yhteisön kotipaikka, jonka liikkeeseen laskemia arvopapereita päätös koskee. Jos päätös koskee sekä kiinteistöä että arvopapereita, muutosta haetaan kiinteistön sijaintipaikan hallinto-oikeudelta. Jollei tätä perustetta voida käyttää, valitus tehdään Helsingin hallinto-oikeudelle. Päätökseen, jolla ennakkoratkaisu on jätetty antamatta, ei saa hakea muutosta valittamalla. Valituskirjelmä on toimitettava valitusajassa Verohallinnolle tai hallinto-oikeuteen."}}},{"@id":"sfsd:1996/931/luku/7/pykala/47/momentti/2/ajantasa/20130101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/7/pykala/47/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/pykala/47/momentti/2/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/7/pykala/47/momentti/2/ajantasa/20130101/fin/txt","content_fi":"Valitusaika on 30 päivää päätöksen tiedoksisaannista. Päätös voidaan antaa tiedoksi saantitodistusmenettelyä käyttämättä, jolloin se katsotaan saadun tiedoksi seitsemäntenä päivänä sen jälkeen, kun päätös on annettu postin kuljetettavaksi, jollei muuta näytetä. Veronsaajien oikeudenvalvontayksikön valitusaika lasketaan päätöksen tekemisestä."}}},{"@id":"sfsd:1996/931/luku/7/pykala/47/momentti/3/ajantasa/20130101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/7/pykala/47/momentti/3","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/pykala/47/momentti/3/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/7/pykala/47/momentti/3/ajantasa/20130101/fin/txt","content_fi":"Jos Verohallinto oikaisee päätöstään 48 §:ssä tarkoitetulla tavalla verovelvollisen vaatimuksen mukaisesti, valitus raukeaa. Siltä osin kuin Verohallinto katsoo, että verovelvollisen valituksessaan esittämä vaatimus ei anna aihetta päätöksen oikaisemiseen, Verohallinnon on annettava valituksen johdosta lausuntonsa ja siirrettävä valitus tältä osin hallinto-oikeuden käsiteltäväksi."}}},{"@id":"sfsd:1996/931/luku/7/pykala/47/momentti/4/ajantasa/20130101","@type":"sfl:SubsectionVersion","cites":"sfsd:1996/586/pykala/34","isMemberOf":"sfsd:1996/931/luku/7/pykala/47/momentti/4","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/pykala/47/momentti/4/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/7/pykala/47/momentti/4/ajantasa/20130101/fin/txt","content_fi":"Hallinto-oikeuden on varattava valituksen ja lausunnon johdosta Veronsaajien oikeudenvalvontayksikön tekemästä valituksesta verovelvolliselle ja verovelvollisen tekemästä valituksesta Veronsaajien oikeudenvalvontayksikölle tilaisuus vastineen antamiseen sekä tarvittaessa muutoksenhakijalle tilaisuus vastaselityksen antamiseen. Sen lisäksi, mitä hallintolainkäyttölain (586/1996) 34 §:n 2 momentissa säädetään asian ratkaisemisesta asianosaista kuulematta, hallinto-oikeus voi kuitenkin ratkaista valituksen kuulematta Veronsaajien oikeudenvalvontayksikköä, jos veron määrä voi verovelvollisen vaatimuksesta muuttua enintään 6 000 euroa, eikä asia ole tulkinnanvarainen tai epäselvä."}}}],"isMemberOf":"sfsd:1996/931/luku/7/pykala/47","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/pykala/47/ajantasa/20130101/fin","title_fi":"Valitus hallinto-oikeuteen"}},{"@id":"sfsd:1996/931/luku/7/pykala/47/ajantasa/20170101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1996/931/luku/7/pykala/47","repealedBy":"sfsd:2016/785/alkup","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/pykala/47/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/7/pykala/47/ajantasa/20170101/fin/txt","content_fi":"47 §(9.9.2016/785)\n\n47 § on kumottu L:lla 9.9.2016/785, joka tulee voimaan 1.1.2017. Aiempi sanamuoto kuuluu:"}}},{"@id":"sfsd:1996/931/luku/7/pykala/48/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/526/alkup","hasPart":{"@id":"sfsd:1996/931/luku/7/pykala/48/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/7/pykala/48/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/pykala/48/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1996/931/luku/7/pykala/48/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Jos Verohallinto valituksen johdosta toteaa, että sen päätös on vero- tai vastuuvelvollisen vahingoksi virheellinen, Verohallinnon on oikaistava päätöksessä oleva virhe ja maksettava virheen johdosta liikaa maksettu tai palauttamatta jäänyt vero, jollei asiaa ole valitukseen annetulla päätöksellä ratkaistu."}}},"isMemberOf":"sfsd:1996/931/luku/7/pykala/48","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/pykala/48/ajantasa/20100901/fin","title_fi":"Oikaisu"}},{"@id":"sfsd:1996/931/luku/7/pykala/48/ajantasa/20170101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1996/931/luku/7/pykala/48","repealedBy":"sfsd:2016/785/alkup","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/pykala/48/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/7/pykala/48/ajantasa/20170101/fin/txt","content_fi":"48 §(9.9.2016/785)\n\n48 § on kumottu L:lla 9.9.2016/785, joka tulee voimaan 1.1.2017. Aiempi sanamuoto kuuluu:"}}},{"@id":"sfsd:1996/931/luku/7/pykala/49/ajantasa/20170101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1996/931/luku/7/pykala/49","repealedBy":"sfsd:2016/785/alkup","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/pykala/49/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/7/pykala/49/ajantasa/20170101/fin/txt","content_fi":"49 §(9.9.2016/785)\n\n49 § on kumottu L:lla 9.9.2016/785, joka tulee voimaan 1.1.2017. Aiempi sanamuoto kuuluu:"}}},{"@id":"sfsd:1996/931/luku/7/pykala/49/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/526/alkup","hasPart":{"@id":"sfsd:1996/931/luku/7/pykala/49/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/7/pykala/49/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/pykala/49/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1996/931/luku/7/pykala/49/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Hallinto-oikeuden päätökseen saa hakea muutosta valittamalla korkeimpaan hallinto-oikeuteen, jos korkein hallinto-oikeus myöntää valitusluvan. Muutoksenhaussa noudatetaan tällöin, mitä verotusmenettelystä annetun lain 70 ja 71 §:ssä säädetään. Valitusaika hallinto-oikeuden ennakkoratkaisua koskevasta päätöksestä on kuitenkin 30 päivää tiedoksisaamisesta."}}},"isMemberOf":"sfsd:1996/931/luku/7/pykala/49","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/pykala/49/ajantasa/20100901/fin","title_fi":"Valitus korkeimpaan hallinto-oikeuteen"}},{"@id":"sfsd:1996/931/luku/7/pykala/50/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/785/alkup","hasPart":{"@id":"sfsd:1996/931/luku/7/pykala/50/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/7/pykala/50/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/pykala/50/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/7/pykala/50/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Verohallinnon on muutoksenhaun johdosta annetun päätöksen perusteella palautettava vero korkoineen tai määrättävä vero viivästysseuraamuksineen."}}},"isMemberOf":"sfsd:1996/931/luku/7/pykala/50","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/pykala/50/ajantasa/20170101/fin","title_fi":"Veron palauttaminen tai määrääminen muutoksenhaun johdosta"}},{"@id":"sfsd:1996/931/luku/7/pykala/50/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/526/alkup","hasPart":{"@id":"sfsd:1996/931/luku/7/pykala/50/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/7/pykala/50/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/pykala/50/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1996/931/luku/7/pykala/50/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Verohallinnon on muutoksenhaun johdosta annetun lainvoimaisen päätöksen perusteella palautettava vero korkoineen tai maksuunpantava vero viivästysseuraamuksineen."}}},"isMemberOf":"sfsd:1996/931/luku/7/pykala/50","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/pykala/50/ajantasa/20100901/fin","title_fi":"Veron palauttaminen tai maksuunpano muutoksenhaun johdosta"}},{"@id":"sfsd:1996/931/luku/7/pykala/51/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1996/931/luku/7/pykala/51/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/7/pykala/51/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/pykala/51/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/7/pykala/51/momentti/1/alkup/fin/txt","content_fi":"Jos veroa palautetaan hakemuksen tai muutoksenhaun johdosta, palautettavalle verolle maksetaan veronkantoasetuksessa (903/78) säädetty korko veron maksupäivästä takaisinmaksupäivään."}}},"isMemberOf":"sfsd:1996/931/luku/7/pykala/51","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/pykala/51/alkup/fin","title_fi":"Palautettavalle määrälle suoritettava korko"}}],"isMemberOf":"sfsd:1996/931/luku/7","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/7/ajantasa/20170101/fin","title_fi":"Muutoksenhaku"}},{"@id":"sfsd:1996/931/luku/8/ajantasa/20170101","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1996/931/luku/8/pykala/52/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/785/alkup","hasPart":[{"@id":"sfsd:1996/931/luku/8/pykala/52/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/8/pykala/52/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/52/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/8/pykala/52/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Jos verosta vastuussa olevan suoritettavan veron määrä on Verohallinnon päätöksen tai muutoksenhaun johdosta muuttunut toisen verosta vastuussa olevan suoritettavan veron määrään vaikuttavalla tavalla, viimeksi mainittu vero määrätään muutosta vastaavaksi."}}},{"@id":"sfsd:1996/931/luku/8/pykala/52/momentti/2/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/8/pykala/52/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/52/momentti/2/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/8/pykala/52/momentti/2/ajantasa/20170101/fin/txt","content_fi":"Seurannaismuutos voidaan tehdä silloinkin, kun muuta kuin tämän lain mukaista verotusta on muutettu suoritettavan veron määrään vaikuttavalla tavalla."}}},{"@id":"sfsd:1996/931/luku/8/pykala/52/momentti/3/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/8/pykala/52/momentti/3","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/52/momentti/3/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/8/pykala/52/momentti/3/ajantasa/20170101/fin/txt","content_fi":"Seurannaismuutos voidaan tehdä, vaikka veron määräämiselle säädetty määräaika on päättynyt. Seurannaismuutos voidaan jättää tekemättä, jos muutoksen tekeminen on erityisestä syystä kohtuutonta."}}}],"isMemberOf":"sfsd:1996/931/luku/8/pykala/52","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/52/ajantasa/20170101/fin","title_fi":"Seurannaismuutos"}},{"@id":"sfsd:1996/931/luku/8/pykala/52/ajantasa/20130101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1996/931/luku/8/pykala/52","repealedBy":"sfsd:2012/876/alkup","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/52/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/8/pykala/52/ajantasa/20130101/fin/txt","content_fi":"52 §(21.12.2012/876)\n\n52 § on kumottu L:lla 21.12.2012/876."}}},{"@id":"sfsd:1996/931/luku/8/pykala/53/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1996/931/luku/8/pykala/53/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/8/pykala/53/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/53/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/8/pykala/53/momentti/1/alkup/fin/txt","content_fi":"Arvopaperikauppiaan on järjestettävä kirjanpitonsa sellaiseksi tai muutoin pidettävä sellaista kirjaa, että siitä luotettavasti saadaan selville varainsiirtoveron määräytymisen kannalta merkitykselliset seikat."}}},"isMemberOf":"sfsd:1996/931/luku/8/pykala/53","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/53/alkup/fin","title_fi":"Arvopaperikauppiaan kirjanpitovelvollisuus"}},{"@id":"sfsd:1996/931/luku/8/pykala/54/ajantasa/20131201","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1996/931/luku/8/pykala/54/momentti/1/ajantasa/20131201","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2013/786/alkup","cites":"sfsd:1889/39/luku/29","isMemberOf":"sfsd:1996/931/luku/8/pykala/54/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/54/momentti/1/ajantasa/20131201/fin","hasFormat":{"@id":"sfsd:1996/931/luku/8/pykala/54/momentti/1/ajantasa/20131201/fin/txt","content_fi":"Rangaistus veron lainvastaisesta välttämisestä ja sen yrittämisestä säädetään rikoslain (39/1889) 29 luvun 1–3 §:ssä.\n(8.11.2013/786)"}}},{"@id":"sfsd:1996/931/luku/8/pykala/54/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/8/pykala/54/momentti/2","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/54/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/8/pykala/54/momentti/2/alkup/fin/txt","content_fi":"Rangaistus varainsiirtoveroon kohdistuvasta verorikkomuksesta säädetään rikoslain 29 luvun 4 §:ssä."}}},{"@id":"sfsd:1996/931/luku/8/pykala/54/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/8/pykala/54/momentti/3","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/54/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/8/pykala/54/momentti/3/alkup/fin/txt","content_fi":"Jos 1 tai 2 momentissa tarkoitettu rikos on veron määrä ja muut olosuhteet huomioon ottaen vähäinen, voi veroviranomainen, ottaen huomioon myös mahdollisesti määrätyn veronkorotuksen, jättää ilmoittamatta asian syyttäjälle."}}}],"isMemberOf":"sfsd:1996/931/luku/8/pykala/54","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/54/ajantasa/20131201/fin","title_fi":"Veropetos ja verorikkomus"}},{"@id":"sfsd:1996/931/luku/8/pykala/55/ajantasa/20131201","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1996/931/luku/8/pykala/55/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/8/pykala/55/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/55/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/8/pykala/55/momentti/1/alkup/fin/txt","content_fi":"Joka viranomaisen kehotuksesta huolimatta jättää asianmukaisesti täyttämättä 29― 31 §:ssä säädetyn ilmoittamis- tai tietojenantovelvollisuuden, on tuomittava verotusta koskevan tiedonantovelvollisuuden laiminlyönnistä sakkoon."}}},{"@id":"sfsd:1996/931/luku/8/pykala/55/momentti/2/ajantasa/20131201","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2013/786/alkup","cites":"sfsd:1889/39/luku/29/pykala/11","isMemberOf":"sfsd:1996/931/luku/8/pykala/55/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/55/momentti/2/ajantasa/20131201/fin","hasFormat":{"@id":"sfsd:1996/931/luku/8/pykala/55/momentti/2/ajantasa/20131201/fin/txt","content_fi":"Edellä 1 momentissa tarkoitettujen rikkomusten ilmoittamatta jättämisestä, esitutkinnan toimittamatta jättämisestä, syytteen ajamatta jättämisestä ja rangaistuksen määräämättä jättämisestä säädetään rikoslain 29 luvun 11 §:ssä.\n(8.11.2013/786)"}}}],"isMemberOf":"sfsd:1996/931/luku/8/pykala/55","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/55/ajantasa/20131201/fin","title_fi":"Verotusta koskevan tiedonantovelvollisuuden laiminlyönti"}},{"@id":"sfsd:1996/931/luku/8/pykala/56/ajantasa/20100901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/526/alkup","hasPart":{"@id":"sfsd:1996/931/luku/8/pykala/56/momentti/1/ajantasa/20100901","@type":"sfl:SubsectionVersion","cites":"sfsd:2005/609","isMemberOf":"sfsd:1996/931/luku/8/pykala/56/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/56/momentti/1/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1996/931/luku/8/pykala/56/momentti/1/ajantasa/20100901/fin/txt","content_fi":"Veron palauttamisessa sekä Verohallinnolle maksettavan veron kannossa, perinnässä ja tilittämisessä noudatetaan veronkantolain (609/2005) säännöksiä, jollei tässä laissa tai sen nojalla toisin säädetä."}}},"isMemberOf":"sfsd:1996/931/luku/8/pykala/56","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/56/ajantasa/20100901/fin","title_fi":"Veronkantolain säännösten soveltaminen"}},{"@id":"sfsd:1996/931/luku/8/pykala/56/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/785/alkup","hasPart":{"@id":"sfsd:1996/931/luku/8/pykala/56/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","cites":"sfsd:2016/769","isMemberOf":"sfsd:1996/931/luku/8/pykala/56/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/56/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/8/pykala/56/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Veron palauttamisessa sekä Verohallinnolle maksettavan veron kannossa, perinnässä ja tilittämisessä noudatetaan veronkantolain (769/2016) säännöksiä, jollei tässä laissa tai sen nojalla toisin säädetä."}}},"isMemberOf":"sfsd:1996/931/luku/8/pykala/56","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/56/ajantasa/20170101/fin","title_fi":"Veronkantolain säännösten soveltaminen"}},{"@id":"sfsd:1996/931/luku/8/pykala/56a/ajantasa/20080501","@type":"sfl:SectionVersion","amendedBy":"sfsd:2008/244/alkup","hasPart":{"@id":"sfsd:1996/931/luku/8/pykala/56a/momentti/1/ajantasa/20080501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/8/pykala/56a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/56a/momentti/1/ajantasa/20080501/fin","hasFormat":{"@id":"sfsd:1996/931/luku/8/pykala/56a/momentti/1/ajantasa/20080501/fin/txt","content_fi":"Veronsaajien oikeudenvalvontayksikköä kuullaan ja päätös annetaan tiedoksi siten, että yksikölle varataan tilaisuus tutustua verotusasiakirjoihin, jollei 47 §:ssä toisin säädetä."}}},"isMemberOf":"sfsd:1996/931/luku/8/pykala/56a","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/56a/ajantasa/20080501/fin","title_fi":"Veronsaajien oikeudenvalvontayksikön kuuleminen ja päätöksen tiedoksianto"}},{"@id":"sfsd:1996/931/luku/8/pykala/56b/ajantasa/20160701","@type":"sfl:SectionVersion","amendedBy":"sfsd:2005/1085/alkup","hasPart":[{"@id":"sfsd:1996/931/luku/8/pykala/56b/momentti/1/ajantasa/20060101","@type":"sfl:SubsectionVersion","cites":"sfsd:2003/13","isMemberOf":"sfsd:1996/931/luku/8/pykala/56b/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/56b/momentti/1/ajantasa/20060101/fin","hasFormat":{"@id":"sfsd:1996/931/luku/8/pykala/56b/momentti/1/ajantasa/20060101/fin/txt","content_fi":"Tämän lain mukaan käsiteltäviin asioihin sovelletaan sähköisestä asioinnista viranomaistoiminnassa annettua lakia (13/2003)."}}},{"@id":"sfsd:1996/931/luku/8/pykala/56b/momentti/2/ajantasa/20160701","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2016/541/alkup","isMemberOf":"sfsd:1996/931/luku/8/pykala/56b/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/56b/momentti/2/ajantasa/20160701/fin","hasFormat":{"@id":"sfsd:1996/931/luku/8/pykala/56b/momentti/2/ajantasa/20160701/fin/txt","content_fi":"Ilmoitukset ja muut asiakirjat, jotka voidaan toimittaa veroviranomaiselle sähköisesti ja jotka on allekirjoitettava, on varmennettava sähköisellä allekirjoituksella tai muulla hyväksyttävällä tavalla. \n(29.6.2016/541)"}}},{"@id":"sfsd:1996/931/luku/8/pykala/56b/momentti/2/ajantasa/20090901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2009/622/alkup","cites":"sfsd:2009/617","isMemberOf":"sfsd:1996/931/luku/8/pykala/56b/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/56b/momentti/2/ajantasa/20090901/fin","hasFormat":{"@id":"sfsd:1996/931/luku/8/pykala/56b/momentti/2/ajantasa/20090901/fin/txt","content_fi":"Ilmoitukset ja muut asiakirjat, jotka voidaan toimittaa veroviranomaiselle sähköisesti ja jotka on allekirjoitettava, on varmennettava vahvasta sähköisestä tunnistamisesta ja sähköisistä allekirjoituksista annetussa laissa (617/2009) tarkoitetulla kehittyneellä sähköisellä allekirjoituksella tai muulla hyväksyttävällä tavalla.\n(7.8.2009/622)"}}},{"@id":"sfsd:1996/931/luku/8/pykala/56b/momentti/3/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/526/alkup","isMemberOf":"sfsd:1996/931/luku/8/pykala/56b/momentti/3","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/56b/momentti/3/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1996/931/luku/8/pykala/56b/momentti/3/ajantasa/20100901/fin/txt","content_fi":"Verohallinto antaa tarkemmat määräykset siitä, millä tavoin varmennettuina ilmoituksia ja muita asiakirjoja voidaan toimittaa sähköisesti.\n(11.6.2010/526)"}}}],"isMemberOf":"sfsd:1996/931/luku/8/pykala/56b","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/56b/ajantasa/20160701/fin","title_fi":"Sähköinen asiointi ja allekirjoittaminen"}},{"@id":"sfsd:1996/931/luku/8/pykala/57/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1996/931/luku/8/pykala/57/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/8/pykala/57/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/57/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/8/pykala/57/momentti/1/alkup/fin/txt","content_fi":"Tarkemmat säännökset tämän lain täytäntöönpanosta annetaan asetuksella."}}},"isMemberOf":"sfsd:1996/931/luku/8/pykala/57","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/pykala/57/alkup/fin","title_fi":"Tarkemmat säännökset"}}],"isMemberOf":"sfsd:1996/931/luku/8","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/8/ajantasa/20170101/fin","title_fi":"Erinäiset säännökset"}},{"@id":"sfsd:1996/931/luku/9/ajantasa/19991230","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1996/931/luku/9/pykala/58/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1996/931/luku/9/pykala/58/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/9/pykala/58/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/9/pykala/58/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/9/pykala/58/momentti/1/alkup/fin/txt","content_fi":"Tämä laki tulee voimaan 1 päivänä tammikuuta 1997."}}},{"@id":"sfsd:1996/931/luku/9/pykala/58/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/9/pykala/58/momentti/2","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/9/pykala/58/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/9/pykala/58/momentti/2/alkup/fin/txt","content_fi":"Lakia sovelletaan lain voimaantulon jälkeen tehtyihin luovutuksiin."}}},{"@id":"sfsd:1996/931/luku/9/pykala/58/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/9/pykala/58/momentti/3","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/9/pykala/58/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/9/pykala/58/momentti/3/alkup/fin/txt","content_fi":"Ennen lain voimaantuloa voidaan ryhtyä sen täytäntöönpanon edellyttämiin toimiin."}}}],"isMemberOf":"sfsd:1996/931/luku/9/pykala/58","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/9/pykala/58/alkup/fin","title_fi":"Voimaantulo"}},{"@id":"sfsd:1996/931/luku/9/pykala/59/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1996/931/luku/9/pykala/59/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/9/pykala/59/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/9/pykala/59/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/9/pykala/59/momentti/1/alkup/fin/txt","content_fi":"Heinäkuun 1 päivänä 1996 tai sen jälkeen mutta ennen tämän lain voimaantuloa tehtyihin luovutuksiin sovelletaan tämän lain verokantaa koskevia säännöksiä. Leimaverolain (662/43) 17 §:ssä tarkoitettuihin mainittuna ajankohtana tehtyihin luovutuksiin sovelletaan kuitenkin tämän lain voimaan tullessa voimassa olleita säännöksiä. Ennen tämän lain voimaantuloa liikaa suoritettu leimavero palautetaan lääninverovirastolle tehdystä hakemuksesta. Palautukselle ei makseta korkoa."}}},"isMemberOf":"sfsd:1996/931/luku/9/pykala/59","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/9/pykala/59/alkup/fin","title_fi":"Sovellettava verokanta"}},{"@id":"sfsd:1996/931/luku/9/pykala/60/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1996/931/luku/9/pykala/60/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/9/pykala/60/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/9/pykala/60/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/9/pykala/60/momentti/1/alkup/fin/txt","content_fi":"Mitä 9 §:ssä säädetään velvollisuudesta suorittaa varainsiirtoveroa myös aikaisemmista luovutuksista, koskee myös leimaveroa, joka luovutuksista olisi leimaverolain mukaan ollut suoritettava."}}},"isMemberOf":"sfsd:1996/931/luku/9/pykala/60","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/9/pykala/60/alkup/fin","title_fi":"Vastuu aikaisemmista luovutuksistasuoritettavasta verosta"}},{"@id":"sfsd:1996/931/luku/9/pykala/61/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1996/931/luku/9/pykala/61/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/9/pykala/61/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/9/pykala/61/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/9/pykala/61/momentti/1/alkup/fin/txt","content_fi":"Ennen lain voimaantuloa tehdystä määräalan luovutuksesta vero on suoritettava lainhuutoa haettaessa."}}},{"@id":"sfsd:1996/931/luku/9/pykala/61/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/9/pykala/61/momentti/2","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/9/pykala/61/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/9/pykala/61/momentti/2/alkup/fin/txt","content_fi":"Jos lainhuutoa ei ole maakaaren voimaanpanosta annetun lain (541/95) 6 §:n 3 momentin mukaan haettava, vero on suoritettava kahden vuoden kuluessa tämän lain voimaantulosta. Luovutuksensaajan on esitettävä selvitys veron suorittamisesta ja ilmoitettava 29 §:ssä säädetyt tiedot siten kuin mainitussa lainkohdassa säädetään."}}}],"isMemberOf":"sfsd:1996/931/luku/9/pykala/61","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/9/pykala/61/alkup/fin","title_fi":"Määräala ja määräalan erottamisoikeus"}},{"@id":"sfsd:1996/931/luku/9/pykala/62/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1996/931/luku/9/pykala/62/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/9/pykala/62/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/9/pykala/62/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/9/pykala/62/momentti/1/alkup/fin/txt","content_fi":"Leimavero ennen tämän lain voimaantuloa tehdystä leimaverolain 26 §:ssä tarkoitetun vuokraoikeuden siirrosta on suoritettava mainitussa lainkohdassa säädetyssä ajassa. Selvitys veron suorittamisesta on esitettävä kirjaamista haettaessa."}}},{"@id":"sfsd:1996/931/luku/9/pykala/62/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/9/pykala/62/momentti/2","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/9/pykala/62/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/9/pykala/62/momentti/2/alkup/fin/txt","content_fi":"Jos 1 momentissa tarkoitetun vuokraoikeuden siirto on ennen tämän lain voimaantuloa ilmoitettu kunnalliselle viranomaiselle, selvityksenä veron suorittamisesta voidaan esittää myös viranomaisen antama todistus ilmoituksen tekemisestä."}}}],"isMemberOf":"sfsd:1996/931/luku/9/pykala/62","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/9/pykala/62/alkup/fin","title_fi":"Vuokratonttia rakennuksineen koskeva siirto"}},{"@id":"sfsd:1996/931/luku/9/pykala/63/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1996/931/luku/9/pykala/63/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/9/pykala/63/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/9/pykala/63/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/9/pykala/63/momentti/1/alkup/fin/txt","content_fi":"Edellä 11 §:n 1  momentin 3 kohdassa ja 25 §:n 1 momentin 2 kohdassa tarkoitettujen aikaisempaa omistusta koskevien edellytysten katsotaan täyttyvän, jos luovutuksensaajalla ei vuoden 1989 jälkeen ole ollut omistuksessaan vähintään puolta asuinrakennuksesta tai asuinhuoneiston hallintaan oikeuttavista osakkeista tai osuuksista."}}},"isMemberOf":"sfsd:1996/931/luku/9/pykala/63","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/9/pykala/63/alkup/fin","title_fi":"Ensiasunnon verovapaus"}},{"@id":"sfsd:1996/931/luku/9/pykala/64/ajantasa/19991230","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1996/931/luku/9/pykala/64/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/9/pykala/64/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/9/pykala/64/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/9/pykala/64/momentti/1/alkup/fin/txt","content_fi":"Tämän lain veron suorittamista ja verotusmenettelyä koskevia 4―7 luvun säännöksiä sovelletaan myös ennen tämän lain voimaantuloa tehtyihin luovutuksiin."}}},{"@id":"sfsd:1996/931/luku/9/pykala/64/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/9/pykala/64/momentti/2","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/9/pykala/64/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/9/pykala/64/momentti/2/alkup/fin/txt","content_fi":"Ennen tämän lain voimaantuloa tehtyihin luovutuksiin sovelletaan kuitenkin leimaverolain maksuunpanoa, veron palauttamista ja muutoksenhakua koskevia säännöksiä, jos luovutuksesta on suoritettu tai olisi ollut suoritettava vero tai jos viranomaisen veron suorittamisvelvollisuutta tai määrää koskeva päätös taikka ennakkoratkaisua koskeva päätös on tehty ennen tämän lain voimaantuloa."}}},{"@id":"sfsd:1996/931/luku/9/pykala/64/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/9/pykala/64/momentti/3","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/9/pykala/64/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1996/931/luku/9/pykala/64/momentti/3/alkup/fin/txt","content_fi":"Ennen tämän lain voimaantuloa tehtyihin luovutuksiin ei sovelleta tämän lain veron korottamista koskevia säännöksiä."}}},{"@id":"sfsd:1996/931/luku/9/pykala/64/momentti/4/ajantasa/19991230","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/931/luku/9/pykala/64/momentti/4","repealedBy":"sfsd:1999/1346/alkup","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/9/pykala/64/momentti/4/ajantasa/19991230/fin","hasFormat":{"@id":"sfsd:1996/931/luku/9/pykala/64/momentti/4/ajantasa/19991230/fin/txt","content_fi":"4 momentti on kumottu L:lla 30.12.1999/1346."}}}],"isMemberOf":"sfsd:1996/931/luku/9/pykala/64","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1996/931/luku/9/pykala/64/ajantasa/19991230/fin","title_fi":"Veron maksaminen ja menettely"}},{"@id":"sfsd:1996/931/luku/9/pykala/65/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1996/931/luku/9/pykala/65/momentti