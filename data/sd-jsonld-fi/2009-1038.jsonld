uageVersion":{"@id":"sfsd:2009/1038/luku/5/pykala/18/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2009/1038/luku/5/pykala/18/momentti/2/alkup/fin/txt","content_fi":"Sitä, joka rikkoo 17 §:n nojalla määrättyä uhkasakolla tehostettua velvoitetta, ei voida tuomita rangaistukseen samasta teosta."}}}],"isMemberOf":"sfsd:2009/1038/luku/5/pykala/18","version":"sfl:Original","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2009/1038/luku/5/pykala/18/alkup/fin","title_fi":"Henkilöluettelorikkomus matkustaja-aluksella"}},{"@id":"sfsd:2009/1038/luku/5/pykala/19/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2009/1038/luku/5/pykala/19/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2009/1038/luku/5/pykala/19/momentti/1","version":"sfl:Original","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2009/1038/luku/5/pykala/19/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2009/1038/luku/5/pykala/19/momentti/1/alkup/fin/txt","content_fi":"Rangaistus henkilörekisteririkoksesta tuomitaan rikoslain (39/1889) 38 luvun 9 §:n mukaan ja tietomurrosta rikoslain 38 luvun 8 tai 8 a §:n mukaan. Rangaistus salassapitovelvollisuuden rikkomisesta tuomitaan rikoslain 38 luvun 1 tai 2 §:n mukaan."}}},"isMemberOf":"sfsd:2009/1038/luku/5/pykala/19","version":"sfl:Original","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2009/1038/luku/5/pykala/19/alkup/fin","title_fi":"Viittaukset rikoslakiin"}}],"isMemberOf":"sfsd:2009/1038/luku/5","version":"sfl:Original","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2009/1038/luku/5/alkup/fin","title_fi":"Valvonta, hallintopakkokeinot ja seuraamukset"}},{"@id":"sfsd:2009/1038/luku/6/ajantasa/20160101","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:2009/1038/luku/6/pykala/20/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2009/1038/luku/6/pykala/20/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2009/1038/luku/6/pykala/20/momentti/1","version":"sfl:Original","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2009/1038/luku/6/pykala/20/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2009/1038/luku/6/pykala/20/momentti/1/alkup/fin/txt","content_fi":"Liikenteen turvallisuusviraston on ilmoitettava henkilöluettelodirektiivin 9 artiklan 3 kohdan mukaisesti Euroopan yhteisöjen komissiolle luettelointivaatimusten vapautuksista, joita 5 §:n 2 momentin ja 6 §:n 3 momentin mukaan on muualla kuin Suomen sisävesialueella."}}},"isMemberOf":"sfsd:2009/1038/luku/6/pykala/20","version":"sfl:Original","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2009/1038/luku/6/pykala/20/alkup/fin","title_fi":"Vapautukset"}},{"@id":"sfsd:2009/1038/luku/6/pykala/21/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2009/1038/luku/6/pykala/21/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2009/1038/luku/6/pykala/21/momentti/1","version":"sfl:Original","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2009/1038/luku/6/pykala/21/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2009/1038/luku/6/pykala/21/momentti/1/alkup/fin/txt","content_fi":"Yhtiöllä ei ole 6 §:n mukaista henkilöiden kirjaamis- ja ilmoittamisvelvollisuutta sellaisen alueen säännöllisessä liikenteessä, jolle Euroopan yhteisöjen komissio on myöntänyt kokonaan tai osittain poikkeuksen henkilöluettelodirektiivin 5 artiklan 1 kohdan vaatimuksista direktiivin 9 artiklan 4 kohdassa säädetyin edellytyksin."}}},{"@id":"sfsd:2009/1038/luku/6/pykala/21/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2009/1038/luku/6/pykala/21/momentti/2","version":"sfl:Original","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2009/1038/luku/6/pykala/21/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2009/1038/luku/6/pykala/21/momentti/2/alkup/fin/txt","content_fi":"Edellä 1 momentissa tarkoitettua poikkeusta koskeva hakemus on osoitettava liikenne- ja viestintäministeriölle. Liikenne- ja viestintäministeriö hakee esitettyä poikkeusta Euroopan yhteisöjen komissiolta, jos henkilöluettelodirektiivin 9 artiklan 4 kohdassa säädetyt edellytykset täyttyvät. Liikenne- ja viestintäministeriön on ennen päätöksen tekemistä pyydettävä asiasta lausunto Liikenteen turvallisuusvirastolta, rajavartiolaitokselta sekä myös Ahvenanmaan maakunnan hallitukselta niissä tapauksissa, joissa poikkeuksella on merkitystä Ahvenanmaan maakunnan liikenteelle."}}},{"@id":"sfsd:2009/1038/luku/6/pykala/21/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2009/1038/luku/6/pykala/21/momentti/3","version":"sfl:Original","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2009/1038/luku/6/pykala/21/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:2009/1038/luku/6/pykala/21/momentti/3/alkup/fin/txt","content_fi":"Poikkeus voi koskea Suomen satamista liikennöiviä matkustaja-aluksia, jotka purjehtivat SOLAS-yleissopimuksen sopimuspuolena olevan muun kuin Euroopan yhteisön jäsenvaltion lipun alla vain, jos kyseinen valtio antaa siihen luvan."}}}],"isMemberOf":"sfsd:2009/1038/luku/6/pykala/21","version":"sfl:Original","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2009/1038/luku/6/pykala/21/alkup/fin","title_fi":"Poikkeukset"}},{"@id":"sfsd:2009/1038/luku/6/pykala/22/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2009/1038/luku/6/pykala/22/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2009/1038/luku/6/pykala/22/momentti/1","version":"sfl:Original","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2009/1038/luku/6/pykala/22/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2009/1038/luku/6/pykala/22/momentti/1/alkup/fin/txt","content_fi":"Liikenteen turvallisuusvirasto antaa tarkemmat määräykset 3 §:n 11 kohdassa tarkoitetuista suojaisista merialueista ja niiden rajoista."}}},"isMemberOf":"sfsd:2009/1038/luku/6/pykala/22","version":"sfl:Original","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2009/1038/luku/6/pykala/22/alkup/fin","title_fi":"Määräykset suojaisista merialueista ja niiden rajoista"}},{"@id":"sfsd:2009/1038/luku/6/pykala/23/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2009/1038/luku/6/pykala/23/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2009/1038/luku/6/pykala/23/momentti/1","version":"sfl:Original","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2009/1038/luku/6/pykala/23/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2009/1038/luku/6/pykala/23/momentti/1/alkup/fin/txt","content_fi":"Henkilöluettelointijärjestelmän hyväksymisestä peritään maksu siten kuin valtion maksuperustelaissa (150/1992) säädetään."}}},"isMemberOf":"sfsd:2009/1038/luku/6/pykala/23","version":"sfl:Original","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2009/1038/luku/6/pykala/23/alkup/fin","title_fi":"Henkilöluettelointijärjestelmän hyväksymisestä perittävä maksu"}},{"@id":"sfsd:2009/1038/luku/6/pykala/24/ajantasa/20160101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2015/993/alkup","hasPart":[{"@id":"sfsd:2009/1038/luku/6/pykala/24/momentti/1/ajantasa/20160101","@type":"sfl:SubsectionVersion","cites":"sfsd:1996/586","isMemberOf":"sfsd:2009/1038/luku/6/pykala/24/momentti/1","version":"sfl:Consolidated","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2009/1038/luku/6/pykala/24/momentti/1/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:2009/1038/luku/6/pykala/24/momentti/1/ajantasa/20160101/fin/txt","content_fi":"Tämän lain 17 §:n nojalla tehtyyn päätökseen saa hakea muutosta valittamalla hallinto-oikeuteen siten kuin hallintolainkäyttölaissa (586/1996) säädetään."}}},{"@id":"sfsd:2009/1038/luku/6/pykala/24/momentti/2/ajantasa/20160101","@type":"sfl:SubsectionVersion","cites":"sfsd:2003/434","isMemberOf":"sfsd:2009/1038/luku/6/pykala/24/momentti/2","version":"sfl:Consolidated","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2009/1038/luku/6/pykala/24/momentti/2/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:2009/1038/luku/6/pykala/24/momentti