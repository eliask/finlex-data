dBy":"sfsd:1998/869/alkup","hasPart":[{"@id":"sfsd:1993/666/pykala/31/momentti/1/ajantasa/19990101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/666/pykala/31/momentti/1","version":"sfl:Consolidated","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/31/momentti/1/ajantasa/19990101/fin","hasFormat":{"@id":"sfsd:1993/666/pykala/31/momentti/1/ajantasa/19990101/fin/txt","content_fi":"Suomessa metsästetyn kanadanhanhen, metsähanhen, heinätavin, allin, telkän, tukkakoskelon, isokoskelon, pyyn, teeren ja metson samoin kuin niiden tunnistettavien osien tai niistä valmistettujen tuotteiden kauppa on kielletty."}}},{"@id":"sfsd:1993/666/pykala/31/momentti/2/ajantasa/20010801","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2001/664/alkup","cites":"sfsd:1996/1096/pykala/44","isMemberOf":"sfsd:1993/666/pykala/31/momentti/2","version":"sfl:Consolidated","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/31/momentti/2/ajantasa/20010801/fin","hasFormat":{"@id":"sfsd:1993/666/pykala/31/momentti/2/ajantasa/20010801/fin/txt","content_fi":"Luonnonvaraisten eläinten ja kasvien suojelusta niiden kauppaa säätelemällä annetun neuvoston asetuksen (EY) N:o 338/97 täytäntöönpanosta säädetään luonnonsuojelulain (1096/1996) 44 §:ssä.\n(19.7.2001/664)"}}}],"isMemberOf":"sfsd:1993/666/pykala/31","version":"sfl:Consolidated","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/31/ajantasa/20010801/fin","title_fi":"Riistaeläinten kauppa"}},{"@id":"sfsd:1993/666/pykala/32/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1993/666/pykala/32/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/666/pykala/32/momentti/1","version":"sfl:Original","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/32/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/666/pykala/32/momentti/1/alkup/fin/txt","content_fi":"Metsästyslain 55 §:n 3 momentissa tarkoitettu korvaus koiran hoitamisesta määrätään siten, että se kattaa haltuunotetun koiran hoidosta aiheutuneet kustannukset täysimääräisesti. Tällaisina kustannuksina otetaan huomioon hoitamiseen käytetty työ, hoitamista varten hankitut tarvikkeet ja asianmukainen osuus hoitoon käytettyjen tilojen kustannuksista hoitoajalta."}}},"isMemberOf":"sfsd:1993/666/pykala/32","version":"sfl:Original","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/32/alkup/fin","title_fi":"Korvaus koiran haltuunotosta"}},{"@id":"sfsd:1993/666/pykala/33/ajantasa/20110301","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1993/666/pykala/33","repealedBy":"sfsd:2011/170/alkup","version":"sfl:Consolidated","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/33/ajantasa/20110301/fin","hasFormat":{"@id":"sfsd:1993/666/pykala/33/ajantasa/20110301/fin/txt","content_fi":"33-34 §\n33-34 § on kumottu A:lla 24.2.2011/170."}}},{"@id":"sfsd:1993/666/pykala/35/ajantasa/20000101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1993/666/pykala/35","repealedBy":"sfsd:1999/1238/alkup","version":"sfl:Consolidated","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/35/ajantasa/20000101/fin","hasFormat":{"@id":"sfsd:1993/666/pykala/35/ajantasa/20000101/fin/txt","content_fi":"35 §(23.12.1999/1238)\n35 § on kumottu A:lla 23.12.1999/1238."}}},{"@id":"sfsd:1993/666/pykala/36/ajantasa/20110301","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1993/666/pykala/36","repealedBy":"sfsd:2011/170/alkup","version":"sfl:Consolidated","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/36/ajantasa/20110301/fin","hasFormat":{"@id":"sfsd:1993/666/pykala/36/ajantasa/20110301/fin/txt","content_fi":"36-37 §\n36-37 § on kumottu A:lla 24.2.2011/170."}}},{"@id":"sfsd:1993/666/pykala/38/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1993/666/pykala/38/momentti/1/alkup","@type":"sfl:SubsectionVersion","cites":"sfsd:1993/616/pykala/2","isMemberOf":"sfsd:1993/666/pykala/38/momentti/1","version":"sfl:Original","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/38/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/666/pykala/38/momentti/1/alkup/fin/txt","content_fi":"Metsästyslain 46 §:n 3 momentissa tarkoitettu lupa voidaan myöntää ulkomaalaiselle, joka metsästää sanotussa momentissa tarkoitetulla alueella suomalaisen metsästäjän kanssa. Edellytyksenä on lisäksi, että asianomainen täyttää riistanhoitomaksusta ja pyyntilupamaksusta annetun lain (616/93) 2 §:ssä säädetyt edellytykset."}}},"isMemberOf":"sfsd:1993/666/pykala/38","version":"sfl:Original","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/38/alkup/fin","title_fi":"Metsästyslupa ulkomaalaiselle"}},{"@id":"sfsd:1993/666/pykala/39/ajantasa/20110301","@type":"sfl:SectionVersion","amendedBy":"sfsd:2011/170/alkup","hasPart":[{"@id":"sfsd:1993/666/pykala/39/momentti/1/ajantasa/20110301","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/666/pykala/39/momentti/1","version":"sfl:Consolidated","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/39/momentti/1/ajantasa/20110301/fin","hasFormat":{"@id":"sfsd:1993/666/pykala/39/momentti/1/ajantasa/20110301/fin/txt","content_fi":"Metsästyslain 83 c §:n 2 momentissa tarkoitettu ilmoitus on tehtävä suullisesti, puhelimitse tai muulla tavoin viimeistään riistaeläimen löytymistä seuraavana arkipäivänä. Jollei metsästysoikeuden haltijaa tavoiteta kahden vuorokauden aikana, metsästäjä saa pitää eläimen."}}},{"@id":"sfsd:1993/666/pykala/39/momentti/2/ajantasa/20110301","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/666/pykala/39/momentti/2","version":"sfl:Consolidated","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/39/momentti/2/ajantasa/20110301/fin","hasFormat":{"@id":"sfsd:1993/666/pykala/39/momentti/2/ajantasa/20110301/fin/txt","content_fi":"Jos metsästysoikeuden haltija ottaa metsästyslain 83 §:n 2 momentissa tarkoitetussa tapauksessa eläimen, hänen katsotaan käyttäneen metsästäjän sijasta sitä vastaavan pyyntiluvan."}}}],"isMemberOf":"sfsd:1993/666/pykala/39","version":"sfl:Consolidated","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/39/ajantasa/20110301/fin","title_fi":"Ilmoitus metsästysoikeuden haltijalle"}},{"@id":"sfsd:1993/666/pykala/40/ajantasa/20050801","@type":"sfl:SectionVersion","amendedBy":"sfsd:2005/440/alkup","hasPart":{"@id":"sfsd:1993/666/pykala/40/momentti/1/ajantasa/20050801","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/666/pykala/40/momentti/1","version":"sfl:Consolidated","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/40/momentti/1/ajantasa/20050801/fin","hasFormat":{"@id":"sfsd:1993/666/pykala/40/momentti/1/ajantasa/20050801/fin/txt","content_fi":"Metsään haavoittuneena jääneestä villisiasta, sudesta, karhusta, ahmasta tai ilveksestä on viipymättä ilmoitettava lähimmälle poliisille."}}},"isMemberOf":"sfsd:1993/666/pykala/40","version":"sfl:Consolidated","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/40/ajantasa/20050801/fin","title_fi":"Ilmoittaminen eläimen haavoittamisesta"}},{"@id":"sfsd:1993/666/pykala/41/ajantasa/20110301","@type":"sfl:SectionVersion","amendedBy":"sfsd:2011/170/alkup","hasPart":[{"@id":"sfsd:1993/666/pykala/41/momentti/1/ajantasa/20110301","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/666/pykala/41/momentti/1","version":"sfl:Consolidated","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/41/momentti/1/ajantasa/20110301/fin","hasFormat":{"@id":"sfsd:1993/666/pykala/41/momentti/1/ajantasa/20110301/fin/txt","content_fi":"Metsästyslain 40 §:ssä tarkoitettua lupaa on haettava kirjallisesti Suomen riistakeskukselta. Hakemuksessa on esitettävä ne syyt, joiden vuoksi lupaa haetaan, samoin kuin käytettävät pyyntimenetelmät. Luvan myöntämisen edellytyksenä on, että luvassa tarkoitetusta toimenpiteestä ei aiheudu merkittävää haittaa luonnonvaraisille riistaeläinkannoille."}}},{"@id":"sfsd:1993/666/pykala/41/momentti/2/ajantasa/20110301","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1993/666/pykala/41/momentti/2/johdanto/ajantasa/20110301","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1993/666/pykala/41/momentti/2/johdanto","version":"sfl:Consolidated","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/41/momentti/2/johdanto/ajantasa/20110301/fin","hasFormat":{"@id":"sfsd:1993/666/pykala/41/momentti/2/johdanto/ajantasa/20110301/fin/txt","content_fi":"Lupapäätöksessä on mainittava:"}}},{"@id":"sfsd:1993/666/pykala/41/momentti/2/kohta/1/ajantasa/20110301","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1993/666/pykala/41/momentti/2/kohta/1","version":"sfl:Consolidated","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/41/momentti/2/kohta/1/ajantasa/20110301/fin","hasFormat":{"@id":"sfsd:1993/666/pykala/41/momentti/2/kohta/1/ajantasa/20110301/fin/txt","content_fi":"1) mitkä pyyntitavat luvan nojalla sallitaan;"}}},{"@id":"sfsd:1993/666/pykala/41/momentti/2/kohta/2/ajantasa/20110301","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1993/666/pykala/41/momentti/2/kohta/2","version":"sfl:Consolidated","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/41/momentti/2/kohta/2/ajantasa/20110301/fin","hasFormat":{"@id":"sfsd:1993/666/pykala/41/momentti/2/kohta/2/ajantasa/20110301/fin/txt","content_fi":"2) kuinka monta eläintä hakija saa pyydystää tai ottaa; ja"}}},{"@id":"sfsd:1993/666/pykala/41/momentti/2/kohta/3/ajantasa/20110301","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1993/666/pykala/41/momentti/2/kohta/3","version":"sfl:Consolidated","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/41/momentti/2/kohta/3/ajantasa/20110301/fin","hasFormat":{"@id":"sfsd:1993/666/pykala/41/momentti/2/kohta/3/ajantasa/20110301/fin/txt","content_fi":"3) miten luvan saajan on ilmoitettava Suomen riistakeskukselle riistanhoidollisten toimenpiteiden vaikutuksista."}}}],"isMemberOf":"sfsd:1993/666/pykala/41/momentti/2","version":"sfl:Consolidated","versionDate":"2018-02-01"},{"@id":"sfsd:1993/666/pykala/41/momentti/3/ajantasa/20110301","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/666/pykala/41/momentti/3","version":"sfl:Consolidated","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/41/momentti/3/ajantasa/20110301/fin","hasFormat":{"@id":"sfsd:1993/666/pykala/41/momentti/3/ajantasa/20110301/fin/txt","content_fi":"Myönnetty lupa on voimassa vuoden kerrallaan, jollei lupapäätöksessä voimassaoloaikaa määrätä lyhyemmäksi."}}}],"isMemberOf":"sfsd:1993/666/pykala/41","version":"sfl:Consolidated","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/41/ajantasa/20110301/fin","title_fi":"Lupa eräisiin riistanhoidollisiin toimenpiteisiin"}},{"@id":"sfsd:1993/666/pykala/42/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1993/666/pykala/42/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/666/pykala/42/momentti/1","version":"sfl:Original","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/42/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/666/pykala/42/momentti/1/alkup/fin/txt","content_fi":"Jos metsästyslain 81 §:n 2 momentissa tarkoitetun pyyntilaitteen omistaja tiedetään ja jos pyyntilaite vähäisin korjauksin voidaan muuttaa asetetut vaatimukset täyttäväksi pyyntilaitteeksi tai jos sen osilla on käyttöarvoa, pyyntilaite palautetaan omistajalle. Muussa tapauksessa pyyntilaite hävitetään."}}},{"@id":"sfsd:1993/666/pykala/42/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/666/pykala/42/momentti/2","version":"sfl:Original","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/42/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/666/pykala/42/momentti/2/alkup/fin/txt","content_fi":"Jos 1 momentin mukaisesti palautettu pyyntilaite uudestaan otetaan talteen metsästyslain 81 §:n nojalla, pyyntilaite hävitetään."}}}],"isMemberOf":"sfsd:1993/666/pykala/42","version":"sfl:Original","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/42/alkup/fin","title_fi":"Laittoman pyyntivälineen käsittely"}},{"@id":"sfsd:1993/666/pykala/43/ajantasa/20110301","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1993/666/pykala/43","repealedBy":"sfsd:2011/170/alkup","version":"sfl:Consolidated","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/43/ajantasa/20110301/fin","hasFormat":{"@id":"sfsd:1993/666/pykala/43/ajantasa/20110301/fin/txt","content_fi":"43 §(24.2.2011/170)\n43 § on kumottu A:lla 24.2.2011/170."}}},{"@id":"sfsd:1993/666/pykala/44/ajantasa/20170107","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1993/666/pykala/44","repealedBy":"sfsd:2017/516/alkup","version":"sfl:Consolidated","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/44/ajantasa/20170107/fin","hasFormat":{"@id":"sfsd:1993/666/pykala/44/ajantasa/20170107/fin/txt","content_fi":"44–45 §\n\n\n44–45 § on kumottu A:lla 3.8.2017/516."}}},{"@id":"sfsd:1993/666/pykala/46/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1993/666/pykala/46/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/666/pykala/46/momentti/1","version":"sfl:Original","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/46/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/666/pykala/46/momentti/1/alkup/fin/txt","content_fi":"Tämä asetus tulee voimaan 1 päivänä elokuuta 1993."}}},"isMemberOf":"sfsd:1993/666/pykala/46","version":"sfl:Original","versionDate":"2018-02-01","languageVersion":{"@id":"sfsd:1993/666/pykala/46/alkup/fin","title_fi":"Voimaantulo"}},{"@id":"sfsd:1993/666/pykala/47/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1993/666/pykala/47/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/666/pykala/47/