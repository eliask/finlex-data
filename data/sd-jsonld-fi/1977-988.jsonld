,{"@id":"sfsd:1977/988/luku/2/pykala/4/momentti/2/ajantasa/19840101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/2/pykala/4/momentti/2","repealedBy":"sfsd:1983/548/alkup","version":"sfl:Consolidated","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/2/pykala/4/momentti/2/ajantasa/19840101/fin","hasFormat":{"@id":"sfsd:1977/988/luku/2/pykala/4/momentti/2/ajantasa/19840101/fin/txt","content_fi":"2 momentti on kumottu A:lla 17.6.1983/548."}}}],"isMemberOf":"sfsd:1977/988/luku/2/pykala/4","version":"sfl:Consolidated","versionDate":"1997-08-01"},{"@id":"sfsd:1977/988/luku/2/pykala/4/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1977/988/luku/2/pykala/4/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/2/pykala/4/momentti/1","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/2/pykala/4/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/2/pykala/4/momentti/1/alkup/fin/txt","content_fi":"Kehitysvammalain 34 §:ssä tarkoitetun erityishuolto-ohjelman tulee sisältää suunnitelma annettavasta erityishuollosta, sen toteuttamistavasta sekä siitä, milloin erityishuolto-ohjelma viimeistään on otettava tarkistettavaksi."}}},{"@id":"sfsd:1977/988/luku/2/pykala/4/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/2/pykala/4/momentti/2","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/2/pykala/4/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/2/pykala/4/momentti/2/alkup/fin/txt","content_fi":"2 momentti on kumottu A:lla 17.6.1983/548."}}}],"isMemberOf":"sfsd:1977/988/luku/2/pykala/4","version":"sfl:Original","versionDate":"1997-08-01"}],"isMemberOf":"sfsd:1977/988/luku/2","version":"sfl:Consolidated","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/2/ajantasa/19850201/fin","title_fi":"Erityishuollon järjestäminen"}},{"@id":"sfsd:1977/988/luku/3/ajantasa/19970801","@type":"sfl:ChapterVersion","amendedBy":"sfsd:1997/173/alkup","hasPart":{"@id":"sfsd:1977/988/luku/3/pykala/1/ajantasa/19970801","@type":"sfl:SectionVersion","amendedBy":"sfsd:1997/173/alkup","hasPart":{"@id":"sfsd:1977/988/luku/3/pykala/1/momentti/1/ajantasa/19970801","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/3/pykala/1/momentti/1","version":"sfl:Consolidated","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/3/pykala/1/momentti/1/ajantasa/19970801/fin","hasFormat":{"@id":"sfsd:1977/988/luku/3/pykala/1/momentti/1/ajantasa/19970801/fin/txt","content_fi":"3 LUKU on kumottu A:lla 14.2.1997/173."}}},"isMemberOf":"sfsd:1977/988/luku/3/pykala/1","version":"sfl:Consolidated","versionDate":"1997-08-01"},"isMemberOf":"sfsd:1977/988/luku/3","version":"sfl:Consolidated","versionDate":"1997-08-01"},{"@id":"sfsd:1977/988/luku/4/ajantasa/19850201","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1977/988/luku/4/pykala/12/ajantasa/19840101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1977/988/luku/4/pykala/12","repealedBy":"sfsd:1983/548/alkup","version":"sfl:Consolidated","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/4/pykala/12/ajantasa/19840101/fin","hasFormat":{"@id":"sfsd:1977/988/luku/4/pykala/12/ajantasa/19840101/fin/txt","content_fi":"12 §(17.6.1983/548)\n12 § on kumottu A:lla 17.6.1983/548."}}},{"@id":"sfsd:1977/988/luku/4/pykala/13/ajantasa/19850201","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1977/988/luku/4/pykala/13","repealedBy":"sfsd:1985/335/alkup","version":"sfl:Consolidated","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/4/pykala/13/ajantasa/19850201/fin","hasFormat":{"@id":"sfsd:1977/988/luku/4/pykala/13/ajantasa/19850201/fin/txt","content_fi":"13 §(19.4.1985/335)\n13 § on kumottu A:lla 19.4.1985/335."}}},{"@id":"sfsd:1977/988/luku/4/pykala/14/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1977/988/luku/4/pykala/14/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/4/pykala/14/momentti/1","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/4/pykala/14/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/4/pykala/14/momentti/1/alkup/fin/txt","content_fi":"Määrättäessä kehitysvammalain 73 §:n nojalla jäsenkuntien osuuksia otetaan perustamiskustannusosuuksina varainhoitovuosittain huomioon jäsenkunnille määrätyt maksuosuudet perustamiskustannuksiin vähennettynä määrillä, jotka kuntainliitto on niille suorittanut. Perustamiskustannusosuuksien vuotuinen arvonvähennys on 4 prosenttia. Jäsenkuntien perustamiskustannusosuudet muunnetaan osuuksien määräytymisajankohdan rahanarvoon korottamalla tai alentamalla niitä samassa suhteessa kuin tilastokeskuksen julkaisema rakennuskustannusindeksin kokonaiskustannusten indeksiluku on osuuksien määräämiskuukauden indeksilukuun verrattuna muuttunut sen varainhoitovuoden indeksilukujen arvosta, jonka aikana perustamiskustannusosuudet ovat aiheutuneet."}}},"isMemberOf":"sfsd:1977/988/luku/4/pykala/14","version":"sfl:Original","versionDate":"1997-08-01"}],"isMemberOf":"sfsd:1977/988/luku/4","version":"sfl:Consolidated","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/4/ajantasa/19850201/fin","title_fi":"Korvaukset ja maksuosuudet"}},{"@id":"sfsd:1977/988/luku/5/ajantasa/19791001","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1977/988/luku/5/pykala/15/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1977/988/luku/5/pykala/15/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/15/momentti/1","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/15/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/15/momentti/1/alkup/fin/txt","content_fi":"Sen lisäksi, mitä kehitysvammalaissa on säädetty, valtion erityishuollon toimintayksiköt voivat järjestää harjaantumis- ja muuta tarpeellista opetusta sekä ammatillista koulutusta."}}},"isMemberOf":"sfsd:1977/988/luku/5/pykala/15","version":"sfl:Original","versionDate":"1997-08-01"},{"@id":"sfsd:1977/988/luku/5/pykala/16/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1977/988/luku/5/pykala/16/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/16/momentti/1","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/16/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/16/momentti/1/alkup/fin/txt","content_fi":"Valtion erityishuollon toimintayksikköjä ovat Alavuden ja Perttulan ammatilliset koulutuskeskukset sekä Kuhankosken opetuskoti."}}},"isMemberOf":"sfsd:1977/988/luku/5/pykala/16","version":"sfl:Original","versionDate":"1997-08-01"},{"@id":"sfsd:1977/988/luku/5/pykala/17/ajantasa/19791001","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1977/988/luku/5/pykala/17/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/17/momentti/1","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/17/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/17/momentti/1/alkup/fin/txt","content_fi":"Valtion erityishuollon toimintayksiköllä tulee olla ohjesääntö, jonka vahvistaa sosiaalihallitus, pyydettyään siitä opetuksen osalta kehitysvammalain 3 §:n 2 momentissa tarkoitetun opetusviranomaisen lausunnon."}}},{"@id":"sfsd:1977/988/luku/5/pykala/17/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/17/momentti/2","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/17/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/17/momentti/2/alkup/fin/txt","content_fi":"Ohjesäännössä määrätään johtokunnan kokoontumisesta, päätösvallasta, sen ja johtavien viranhaltijoiden tehtävistä sekä laitoksen toiminnasta ottaen huomioon, mitä tässä asetuksessa säädetään."}}},{"@id":"sfsd:1977/988/luku/5/pykala/17/momentti/3/ajantasa/19791001","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:1979/730/alkup","isMemberOf":"sfsd:1977/988/luku/5/pykala/17/momentti/3","version":"sfl:Consolidated","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/17/momentti/3/ajantasa/19791001/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/17/momentti/3/ajantasa/19791001/fin/txt","content_fi":"Henkilöstön osallistumisesta sitä itseään koskevan päätöksenteon valmisteluun valtion erityishuollon toimintayksikössä määrätään tarkemmin virastodemokratiaohjesäännössä, jonka sosiaalihallitus vahvistaa.\n(21.9.1979/730)"}}}],"isMemberOf":"sfsd:1977/988/luku/5/pykala/17","version":"sfl:Consolidated","versionDate":"1997-08-01"},{"@id":"sfsd:1977/988/luku/5/pykala/18/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1977/988/luku/5/pykala/18/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/18/momentti/1","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/18/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/18/momentti/1/alkup/fin/txt","content_fi":"Toimintayksiköllä tulee olla johtokunta, jonka toimikausi on neljä kalenterivuotta. Johto kuntaan kuuluu seitsemän jäsentä, joista sosiaalihallitus määrää kuusi ja ammattikasvatushallitus yhden. Sosiaalihallitus määrää yhden jäsenistä toimimaan johtokunnan puheenjohtajana. Varapuheenjohtajan johtokunta valitsee keskuudestaan."}}},"isMemberOf":"sfsd:1977/988/luku/5/pykala/18","version":"sfl:Original","versionDate":"1997-08-01"},{"@id":"sfsd:1977/988/luku/5/pykala/19/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1977/988/luku/5/pykala/19/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/19/momentti/1","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/19/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/19/momentti/1/alkup/fin/txt","content_fi":"Valtion erityishuollon toimintayksikössä voi olla johtajan, apulaisjohtajan, opettajan ja ammattiopettajan virkoja sekä ylihoitajan, sosiaalihuoltajan, sairaanhoitajan, työnjohtajan, ohjaajan, alikonemestarin, ylikatsastajan, katsastajan, emännöitsijän, kanslistin, toimistoapulaisen, apuopettajan, yövartijan ja ompelijan toimia. Lisäksi erityishuollon toimintayksikössä voi olla ylimääräisiä toimenhaltijoita ja tilapäisiä toimihenkilöitä sekä työsopimussuhteessa olevaa henkilökuntaa."}}},"isMemberOf":"sfsd:1977/988/luku/5/pykala/19","version":"sfl:Original","versionDate":"1997-08-01"},{"@id":"sfsd:1977/988/luku/5/pykala/20/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/20/momentti/1","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/1/alkup/fin/txt","content_fi":"Kelpoisuusehtona valtion erityishuollon toimintayksikön virkoihin ja toimiin vaaditaan, että asianomainen on aikaisemmalla toiminnallaan osoittanut omaavansa sellaisen taidon ja kyvyn, jota viran tai toimen menestyksellinen hoitaminen edellyttää."}}},{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/alkup","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/johdanto/alkup","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/20/momentti/2/johdanto","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/johdanto/alkup/fin/txt","content_fi":"Sen lisäksi vaaditaan:"}}},{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/1/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/1","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/1/alkup/fin/txt","content_fi":"1) johtajalta ja apulaisjohtajalta virkaan soveltuva ylempi korkeakoulututkinto;"}}},{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/2/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/2","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/2/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/2/alkup/fin/txt","content_fi":"2) opettajalta erityisopettajan tutkinto tai muutoin hankittu erityisopettajan pätevyys;"}}},{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/3/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/3","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/3/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/3/alkup/fin/txt","content_fi":"3) ammattiopettajalta asianomaisen alan opettajakoulutus sekä ammattikasvatushallituksen hyväksymä erityisopettajakoulutus;"}}},{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/4/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/4","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/4/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/4/alkup/fin/txt","content_fi":"4) ylihoitajalta terveydenhoitajan tai muu soveltuva sairaanhoitajan erikoistutkinto ja sairaanhoitajalta sairaanhoitajan tutkinto;"}}},{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/5/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/5","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/5/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/5/alkup/fin/txt","content_fi":"5) sosiaalihuoltajalta sosiaalihuoltajan, sosiaalihoitajan tai muu vastaava sosiaalityöntekijän koulutus;"}}},{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/6/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/6","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/6/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/6/alkup/fin/txt","content_fi":"6) ohjaajalta sosiaalihallituksen hyväksymä ohjaajan koulutus;"}}},{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/7/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/7","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/7/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/7/alkup/fin/txt","content_fi":"7) alikonemestarilta alikonemestarikirjan saamiseksi tarvittava pätevyys;"}}},{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/8/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/8","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/8/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/8/alkup/fin/txt","content_fi":"8) ylikatsastajalta, katsastajalta ja työnjohtajalta asianomaisen alan ammattikoulutus;"}}},{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/9/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/9","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/9/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/9/alkup/fin/txt","content_fi":"9) emännöitsijältä soveltuva ravintotalousalan ammattikoulutus; sekä"}}},{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/10/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/10","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/10/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/20/momentti/2/kohta/10/alkup/fin/txt","content_fi":"10) kanslistilta, toimistoapulaiselta ja muilta toimenhaltijoilta toimeen soveltuva koulutus."}}}],"isMemberOf":"sfsd:1977/988/luku/5/pykala/20/momentti/2","version":"sfl:Original","versionDate":"1997-08-01"}],"isMemberOf":"sfsd:1977/988/luku/5/pykala/20","version":"sfl:Original","versionDate":"1997-08-01"},{"@id":"sfsd:1977/988/luku/5/pykala/21/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1977/988/luku/5/pykala/21/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/21/momentti/1","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/21/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/21/momentti/1/alkup/fin/txt","content_fi":"Toimintayksikön johtajan ja apulaisjohtajan nimittää tai ottaa sosiaalihallitus."}}},{"@id":"sfsd:1977/988/luku/5/pykala/21/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/21/momentti/2","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/21/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/21/momentti/2/alkup/fin/txt","content_fi":"Toimintayksikön muut viran ja toimen haltijat nimittää sekä ylimääräiset toimenhaltijat, tilapäiset toimihenkilöt ja työsopimussuhteisen henkilökunnan ottaa toimintayksikön johtokunta."}}},{"@id":"sfsd:1977/988/luku/5/pykala/21/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/21/momentti/3","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/21/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/21/momentti/3/alkup/fin/txt","content_fi":"Ohjesäännössä voidaan kuitenkin määrätä, että tilapäiset toimihenkilöt ja työsopimussuhteisen henkilökunnan ottaa toimintayksikön johtaja."}}}],"isMemberOf":"sfsd:1977/988/luku/5/pykala/21","version":"sfl:Original","versionDate":"1997-08-01"},{"@id":"sfsd:1977/988/luku/5/pykala/22/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1977/988/luku/5/pykala/22/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/22/momentti/1","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/22/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/22/momentti/1/alkup/fin/txt","content_fi":"Toimintayksikön viran ja toimen haltijalle myöntää virkavapauden ja eron se viranomainen, jolle viran tai toimen täyttäminen kuuluu. Alle kolme kuukautta kestävän virkavapauden myöntää kuitenkin johtajalle johtokunta ja muille johtaja."}}},{"@id":"sfsd:1977/988/luku/5/pykala/22/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/22/momentti/2","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/22/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/22/momentti/2/alkup/fin/txt","content_fi":"Viran tai toimen hoidosta virkavapauden aikana päättää se viranomainen, joka on virkavapauden myöntänyt."}}},{"@id":"sfsd:1977/988/luku/5/pykala/22/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/5/pykala/22/momentti/3","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/pykala/22/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/5/pykala/22/momentti/3/alkup/fin/txt","content_fi":"Avoinna olevan viran tai toimen väliaikaisen hoitajan määrää se viranomainen, joka siitä myöntää virkavapauden."}}}],"isMemberOf":"sfsd:1977/988/luku/5/pykala/22","version":"sfl:Original","versionDate":"1997-08-01"}],"isMemberOf":"sfsd:1977/988/luku/5","version":"sfl:Consolidated","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/5/ajantasa/19791001/fin","title_fi":"Valtion erityishuollon toimintayksiköt"}},{"@id":"sfsd:1977/988/luku/6/ajantasa/19970801","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1977/988/luku/6/pykala/23/ajantasa/19840101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1977/988/luku/6/pykala/23","repealedBy":"sfsd:1983/548/alkup","version":"sfl:Consolidated","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/6/pykala/23/ajantasa/19840101/fin","hasFormat":{"@id":"sfsd:1977/988/luku/6/pykala/23/ajantasa/19840101/fin/txt","content_fi":"23 §(17.6.1983/548)\n23 § on kumottu A:lla 17.6.1983/548."}}},{"@id":"sfsd:1977/988/luku/6/pykala/24/ajantasa/19910301","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1977/988/luku/6/pykala/24/momentti/1/ajantasa/19910301","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:1991/416/alkup","isMemberOf":"sfsd:1977/988/luku/6/pykala/24/momentti/1","version":"sfl:Consolidated","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/6/pykala/24/momentti/1/ajantasa/19910301/fin","hasFormat":{"@id":"sfsd:1977/988/luku/6/pykala/24/momentti/1/ajantasa/19910301/fin/txt","content_fi":"Erityishuoltoa saavan henkilön erityishuollon toimintayksikössä tekemä työ tai sen toimesta hänen tehtäväkseen järjestetty työ jää toimintayksikön hyväksi. Työtoimintaan osallistuvalle voidaan kuitenkin sosiaali- ja terveysministeriön vahvistamien perusteiden mukaisesti suorittaa työosuusrahaa.\n(22.2.1991/416)"}}},{"@id":"sfsd:1977/988/luku/6/pykala/24/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/6/pykala/24/momentti/2","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/6/pykala/24/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/6/pykala/24/momentti/2/alkup/fin/txt","content_fi":"Erityishuolto-ohjelmassa voidaan määrätä, että työn tulos jää erityishuoltoa saavan henkilön hyväksi."}}}],"isMemberOf":"sfsd:1977/988/luku/6/pykala/24","version":"sfl:Consolidated","versionDate":"1997-08-01"},{"@id":"sfsd:1977/988/luku/6/pykala/25/alkup","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1977/988/luku/6/pykala/25","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/6/pykala/25/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/6/pykala/25/alkup/fin/txt","content_fi":"25–27 §\n\n\n25–27 § on kumottu A:lla 17.6.1983/548."}}},{"@id":"sfsd:1977/988/luku/6/pykala/25/ajantasa/19840101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1977/988/luku/6/pykala/25","repealedBy":"sfsd:1983/548/alkup","version":"sfl:Consolidated","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/6/pykala/25/ajantasa/19840101/fin","hasFormat":{"@id":"sfsd:1977/988/luku/6/pykala/25/ajantasa/19840101/fin/txt","content_fi":"25-27 §\n25-27 § on kumottu A:lla 17.6.1983/548."}}},{"@id":"sfsd:1977/988/luku/6/pykala/28/ajantasa/19891101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1989/938/alkup","hasPart":[{"@id":"sfsd:1977/988/luku/6/pykala/28/momentti/1/ajantasa/19891101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/6/pykala/28/momentti/1","version":"sfl:Consolidated","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/6/pykala/28/momentti/1/ajantasa/19891101/fin","hasFormat":{"@id":"sfsd:1977/988/luku/6/pykala/28/momentti/1/ajantasa/19891101/fin/txt","content_fi":"Toimivaltainen kehitysvammalaissa ja sen voimaanpanolaissa sekä tässä asetuksessa mainituissa lääninhallituksen tai lääninoikeuden käsiteltävissä asioissa on, jollei muualla laissa tai asetuksessa ole toisin säädetty, sen läänin lääninhallitus ja vastaavasti lääninoikeus, jonka alueella erityishuoltopiirin kuntainliitolla on kotipaikka."}}},{"@id":"sfsd:1977/988/luku/6/pykala/28/momentti/2/ajantasa/19891101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/6/pykala/28/momentti/2","version":"sfl:Consolidated","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/6/pykala/28/momentti/2/ajantasa/19891101/fin","hasFormat":{"@id":"sfsd:1977/988/luku/6/pykala/28/momentti/2/ajantasa/19891101/fin/txt","content_fi":"Kehitysvammalain 30, 33, 36, 38 ja 41 §:ssä tarkoitetuissa henkilöä koskevissa asioissa on kuitenkin toimivaltainen sen läänin lääninhallitus tai lääninoikeus, jonka alueella henkilön kotikunta on."}}},{"@id":"sfsd:1977/988/luku/6/pykala/28/momentti/3/ajantasa/19840101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/6/pykala/28/momentti/3","repealedBy":"sfsd:1983/548/alkup","version":"sfl:Consolidated","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/6/pykala/28/momentti/3/ajantasa/19840101/fin","hasFormat":{"@id":"sfsd:1977/988/luku/6/pykala/28/momentti/3/ajantasa/19840101/fin/txt","content_fi":"3 momentti on kumottu A:lla 17.6.1983/548."}}},{"@id":"sfsd:1977/988/luku/6/pykala/28/momentti/3/ajantasa/19891101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/6/pykala/28/momentti/3","version":"sfl:Consolidated","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/6/pykala/28/momentti/3/ajantasa/19891101/fin","hasFormat":{"@id":"sfsd:1977/988/luku/6/pykala/28/momentti/3/ajantasa/19891101/fin/txt","content_fi":"3 momentti on kumottu A:lla 17.6.1983/548."}}}],"isMemberOf":"sfsd:1977/988/luku/6/pykala/28","version":"sfl:Consolidated","versionDate":"1997-08-01"},{"@id":"sfsd:1977/988/luku/6/pykala/29/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1977/988/luku/6/pykala/29/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/6/pykala/29/momentti/1","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/6/pykala/29/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/6/pykala/29/momentti/1/alkup/fin/txt","content_fi":"Mitä tässä asetuksessa on säädetty erityishuoltopiirin kuntainliitosta on soveltuvin osin voimassa erityishuollon kuntainliitosta."}}},"isMemberOf":"sfsd:1977/988/luku/6/pykala/29","version":"sfl:Original","versionDate":"1997-08-01"},{"@id":"sfsd:1977/988/luku/6/pykala/30/ajantasa/19970801","@type":"sfl:SectionVersion","amendedBy":"sfsd:1997/173/alkup","hasPart":{"@id":"sfsd:1977/988/luku/6/pykala/30/momentti/1/ajantasa/19970801","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/6/pykala/30/momentti/1","version":"sfl:Consolidated","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/6/pykala/30/momentti/1/ajantasa/19970801/fin","hasFormat":{"@id":"sfsd:1977/988/luku/6/pykala/30/momentti/1/ajantasa/19970801/fin/txt","content_fi":"Tarkempia ohjeita tämän asetuksen täytäntöönpanosta antaa tarvittaessa asianomainen ministeriö."}}},"isMemberOf":"sfsd:1977/988/luku/6/pykala/30","version":"sfl:Consolidated","versionDate":"1997-08-01"}],"isMemberOf":"sfsd:1977/988/luku/6","version":"sfl:Consolidated","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/6/ajantasa/19970801/fin","title_fi":"Erinäisiä säännöksiä"}},{"@id":"sfsd:1977/988/luku/7/alkup","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1977/988/luku/7/pykala/31/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1977/988/luku/7/pykala/31/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/7/pykala/31/momentti/1","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/7/pykala/31/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/7/pykala/31/momentti/1/alkup/fin/txt","content_fi":"Tämä asetus tulee voimaan 1 päivänä tammikuuta 1978."}}},"isMemberOf":"sfsd:1977/988/luku/7/pykala/31","version":"sfl:Original","versionDate":"1997-08-01"},{"@id":"sfsd:1977/988/luku/7/pykala/32/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1977/988/luku/7/pykala/32/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/7/pykala/32/momentti/1","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/7/pykala/32/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/7/pykala/32/momentti/1/alkup/fin/txt","content_fi":"Ennen tämän asetuksen voimaantuloa voidaan ryhtyä sen täytäntöönpanon edellyttämiin toimenpiteisiin."}}},"isMemberOf":"sfsd:1977/988/luku/7/pykala/32","version":"sfl:Original","versionDate":"1997-08-01"},{"@id":"sfsd:1977/988/luku/7/pykala/33/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1977/988/luku/7/pykala/33/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/7/pykala/33/momentti/1","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/7/pykala/33/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/7/pykala/33/momentti/1/alkup/fin/txt","content_fi":"Milloin kehitysvammalain 6 §:n 2 momentissa tarkoitettu erityishuoltopiirin kuntainliitto perustetaan mainitun lain voimaantulon jälkeen, tulee sen järjestää mainitussa laissa kuntien velvollisuudeksi säädetty erityishuolto viimeistään kuntainliiton perustamista seuraavan toisen kalenterikuukauden alusta lukien."}}},{"@id":"sfsd:1977/988/luku/7/pykala/33/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/7/pykala/33/momentti/2","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/7/pykala/33/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/7/pykala/33/momentti/2/alkup/fin/txt","content_fi":"Kehitysvammalain voimaanpanolain 5 §:n 1 momentissa tarkoitettu kunnallinen vajaamielislaitos tai muu kunnallinen erityishuollon toimintayksikkö voi jatkaa toimintaansa saman lain 4 §:n 1 momentissa tarkoitetussa laajuudessa siihen asti kunnes erityishuoltopiirin kuntainliitto on perustettu ja aloittaa erityishuollon järjestämisen 1 momentissa tarkoitetulla tavalla. Sen jälkeen noudatetaan, mitä kehitysvammalain 14 §:n 1 momentissa on säädetty."}}},{"@id":"sfsd:1977/988/luku/7/pykala/33/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/7/pykala/33/momentti/3","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/7/pykala/33/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/7/pykala/33/momentti/3/alkup/fin/txt","content_fi":"Kuntainliittoon, joka ylläpitää laitosta tai muuta erityshuoltoa järjestävää toimintayksikköä, sovelletaan 2 momentissa tarkoitettuna aikana mitä kehitysvammalaissa on erityishuoltopiirin kuntainliiton osalta säädetty."}}}],"isMemberOf":"sfsd:1977/988/luku/7/pykala/33","version":"sfl:Original","versionDate":"1997-08-01"},{"@id":"sfsd:1977/988/luku/7/pykala/34/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1977/988/luku/7/pykala/34/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/988/luku/7/pykala/34/momentti/1","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/7/pykala/34/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/988/luku/7/pykala/34/momentti/1/alkup/fin/txt","content_fi":"Henkilö, joka ennen tämän asetuksen voimaantuloa on ollut kelpoinen johonkin valtion vajaamielislaitoksen virkaan tai toimeen ja joka tämän asetuksen voimaantullessa on tällaisen viran tai toimen haltijana, säilyttää kelpoisuutensa vastaavaan valtion erityishuollon toimintayksikön virkaan tai toimeen tämän asetuksen tullessa voimaan."}}},"isMemberOf":"sfsd:1977/988/luku/7/pykala/34","version":"sfl:Original","versionDate":"1997-08-01"}],"isMemberOf":"sfsd:1977/988/luku/7","version":"sfl:Original","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/luku/7/alkup/fin","title_fi":"Voimaantulosäännökset"}}],"isAbout":"http://data.finlex.fi/voc/finlex/kw1956","isMemberOf":"sfsd:1977/988","version":"sfl:Consolidated","versionDate":"1997-08-01","languageVersion":{"@id":"sfsd:1977/988/ajantasa/19970801/fin","title_fi":"Asetus kehitysvammaisten erityishuollosta"}},"@context":{"languageVersion":{"@id":"eli:is_realized_by","@type":"@id"},"temporalVersions":{"@id":"eli:has_member","@type":"@id"},"temporalVersion":{"@id":"eli:has_member","@type":"@id"},"hasFormat":{"@id":"eli:is_embodied_by","@type":"@id"},"idLocal":"eli:id_local","dateDocument":{"@id":"eli:date_document","@type":"xsd:date"},"hasPart":{"@id":"eli:has_part","@type":"@id"},"isAbout":{"@id":"eli:is_about","@type":"@id"},"isMemberOf":{"@id":"eli:is_member_of","@type":"@id"},"version":{"@id":"eli:version","@type":"@id"},"versionDate":{"@id":"eli:version_date","@type":"xsd:date"},"title_fi":{"@id":"eli:title","@language":"fi"},"content_fi":{"@id":"sfl:text","@language":"fi"},"amendedBy":{"@id":"eli:amended_by","@type":"@id"},"repealedBy":{"@id":"eli:repealed_by","@type":"@id"},"cites":{"@id":"eli:cites","@type":"@id"},"inForce":{"@id":"eli:in_force","@type":"@id"},"typeDocument":{"@id":"eli:type_document","@type":"@id"},"dcterms":"http://purl.org/dc/terms/","xsd":"http://www.w3.org/2001/XMLSchema#","common":"http://data.finlex.fi/common/","rdf":"http://www.w3.org/1999/02/22-rdf-syntax-ns#","sfcl":"http://data.finlex.fi/schema/scf