/pykala/16","version":"sfl:Original","versionDate":"2016-06-10"}],"isMemberOf":"sfsd:1977/519/luku/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/1/ajantasa/20160610/fin","title_fi":"Yleiset säännökset"}},{"@id":"sfsd:1977/519/luku/2/ajantasa/19930101","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1977/519/luku/2/pykala/17/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/2/pykala/17/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/2/pykala/17/momentti/1","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/2/pykala/17/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/2/pykala/17/momentti/1/alkup/fin/txt","content_fi":"Erityishuoltopiirin kuntainliiton liittovaltuustoon valitsevat jäsenkuntien valtuustot jäseniä seuraavasti:"}}},{"@id":"sfsd:1977/519/luku/2/pykala/17/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/2/pykala/17/momentti/2","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/2/pykala/17/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/2/pykala/17/momentti/2/alkup/fin/txt","content_fi":"Kullekin jäsenelle valitaan henkilökohtainen varajäsen."}}}],"isMemberOf":"sfsd:1977/519/luku/2/pykala/17","version":"sfl:Original","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/2/pykala/18/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1977/519/luku/2/pykala/18/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/2/pykala/18/momentti/1","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/2/pykala/18/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/2/pykala/18/momentti/1/alkup/fin/txt","content_fi":"Jäsenkuntaa edustavien jäsenten äänimäärä liittovaltuustossa määräytyy kunnan viimeksi toimitettuun henkikirjoitukseen perustuvan asukasluvun mukaan siten, että kuntaa edustavilla jäsenillä on yksi ääni jokaista alkavaa 100 asukasta kohti. Äänimäärä voi kuitenkin olla enintään kolmannes kaikkien jäsenkuntia edustavien jäsenten yhteenlasketusta rajoittamattomasta äänimäärästä. Kuntaa edustavien jäsenten äänimäärä jakautuu tasan heistä saapuvilla olevien kesken."}}},"isMemberOf":"sfsd:1977/519/luku/2/pykala/18","version":"sfl:Original","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/2/pykala/19/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/2/pykala/19/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/2/pykala/19/momentti/1","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/2/pykala/19/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/2/pykala/19/momentti/1/alkup/fin/txt","content_fi":"Kuntainliiton hallintoa hoitaa liittohallitus."}}},{"@id":"sfsd:1977/519/luku/2/pykala/19/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/2/pykala/19/momentti/2","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/2/pykala/19/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/2/pykala/19/momentti/2/alkup/fin/txt","content_fi":"Liittohallituksen apuna voi olla lautakuntia, milloin niitä erityishuoltopiirin kuntainliiton hallinnon tai osa-alueiden erityishuollon järjestämisen vuoksi pidetään tarkoituksenmukaisina."}}}],"isMemberOf":"sfsd:1977/519/luku/2/pykala/19","version":"sfl:Original","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/2/pykala/19/ajantasa/19930101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1977/519/luku/2/pykala/19","repealedBy":"sfsd:1992/739/alkup","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/2/pykala/19/ajantasa/19930101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/2/pykala/19/ajantasa/19930101/fin/txt","content_fi":"19–20 §\n\n\n19–20 § on kumottu L:lla 3.8.1992/739."}}},{"@id":"sfsd:1977/519/luku/2/pykala/21/ajantasa/19840201","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1977/519/luku/2/pykala/21","repealedBy":"sfsd:1984/26/alkup","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/2/pykala/21/ajantasa/19840201/fin","hasFormat":{"@id":"sfsd:1977/519/luku/2/pykala/21/ajantasa/19840201/fin/txt","content_fi":"21 §(13.1.1984/26)\n\n21 § on kumottu L:lla 13.1.1984/26."}}},{"@id":"sfsd:1977/519/luku/2/pykala/22/ajantasa/19930101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1977/519/luku/2/pykala/22","repealedBy":"sfsd:1992/739/alkup","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/2/pykala/22/ajantasa/19930101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/2/pykala/22/ajantasa/19930101/fin/txt","content_fi":"22 §(3.8.1992/739)\n\n22 § on kumottu L:lla 3.8.1992/739."}}},{"@id":"sfsd:1977/519/luku/2/pykala/23/ajantasa/19930101","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/2/pykala/23/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/2/pykala/23/momentti/1","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/2/pykala/23/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/2/pykala/23/momentti/1/alkup/fin/txt","content_fi":"Erityishuollon yksilöllistä järjestämistä varten erityishuoltopiirin kuntainliitossa on erityishuollon johtoryhmä."}}},{"@id":"sfsd:1977/519/luku/2/pykala/23/momentti/2/ajantasa/19930101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:1992/739/alkup","isMemberOf":"sfsd:1977/519/luku/2/pykala/23/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/2/pykala/23/momentti/2/ajantasa/19930101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/2/pykala/23/momentti/2/ajantasa/19930101/fin/txt","content_fi":"Erityishuollon johtoryhmän jäseninä on vähintään kolme kuntainliiton johtavaa viranhaltijaa siten, että johtoryhmässä on edustettuna lääketieteellinen, kasvatustieteellinen ja sosiaalihuollollinen asiantuntemus.\n(3.8.1992/739)"}}},{"@id":"sfsd:1977/519/luku/2/pykala/23/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/2/pykala/23/momentti/3","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/2/pykala/23/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/2/pykala/23/momentti/3/alkup/fin/txt","content_fi":"Asioiden käsittelystä erityishuollon johtoryhmässä on soveltuvin osin voimassa mitä kunnallislaissa kunnanhallituksen jaostosta säädetään."}}}],"isMemberOf":"sfsd:1977/519/luku/2/pykala/23","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/2/pykala/24/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/2/pykala/24/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/2/pykala/24/momentti/1","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/2/pykala/24/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/2/pykala/24/momentti/1/alkup/fin/txt","content_fi":"Erityishuoltopiirin kuntainliiton toimintayksikössä tai sen osassa tulee olla vastaava johtaja, joka vastaa yksikössä annettavasta erityishuollosta."}}},{"@id":"sfsd:1977/519/luku/2/pykala/24/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/2/pykala/24/momentti/2","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/2/pykala/24/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/2/pykala/24/momentti/2/alkup/fin/txt","content_fi":"Vastaava johtaja voi olla yhteinen kahdelle tai useammalle toimintayksikölle."}}},{"@id":"sfsd:1977/519/luku/2/pykala/24/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/2/pykala/24/momentti/3","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/2/pykala/24/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/2/pykala/24/momentti/3/alkup/fin/txt","content_fi":"Vastaavana johtajana voi toimia myös erityishuollon johtoryhmän jäsen."}}}],"isMemberOf":"sfsd:1977/519/luku/2/pykala/24","version":"sfl:Original","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/2/pykala/24/ajantasa/19930101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1977/519/luku/2/pykala/24","repealedBy":"sfsd:1992/739/alkup","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/2/pykala/24/ajantasa/19930101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/2/pykala/24/ajantasa/19930101/fin/txt","content_fi":"24–25 §\n\n\n24–25 § on kumottu L:lla 3.8.1992/739."}}},{"@id":"sfsd:1977/519/luku/2/pykala/26/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1977/519/luku/2/pykala/26/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/2/pykala/26/momentti/1","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/2/pykala/26/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/2/pykala/26/momentti/1/alkup/fin/txt","content_fi":"Erityishuoltopiirin kuntainliittoon sovelletaan tässä laissa säädetyin poikkeuksin kunnallislain säännöksiä."}}},"isMemberOf":"sfsd:1977/519/luku/2/pykala/26","version":"sfl:Original","versionDate":"2016-06-10"}],"isMemberOf":"sfsd:1977/519/luku/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/2/ajantasa/19930101/fin","title_fi":"Erityishuoltopiirin kuntainliiton hallinto"}},{"@id":"sfsd:1977/519/luku/3/ajantasa/20160610","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1977/519/luku/3/pykala/27/ajantasa/19840201","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/3/pykala/27/momentti/1/ajantasa/19840201","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:1984/26/alkup","isMemberOf":"sfsd:1977/519/luku/3/pykala/27/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/27/momentti/1/ajantasa/19840201/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/27/momentti/1/ajantasa/19840201/fin/txt","content_fi":"Erityishuoltopiirin kuntainliitto antaa erityishuoltoa ensisijaisesti kuntainliittoon kuuluvista kunnista oleville henkilöille, jollei 6 §:n 4 momentin mukaisista järjestelyistä tai 11–13 §:n nojalla tehdyistä sopimuksista taikka 12 §:n 3 momentin tai 13 §:n 2 momentin nojalla annetuista määräyksistä muuta johdu.\n(13.1.1984/26)"}}},{"@id":"sfsd:1977/519/luku/3/pykala/27/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/27/momentti/2","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/27/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/27/momentti/2/alkup/fin/txt","content_fi":"Milloin erityishuoltopiirin kuntainliiton toimintayksikössä erityishuollossa olevan henkilön kotipaikka muuttuu tai milloin hänen erityishuoltonsa järjestäminen ei 1 momentin säännöksistä johtuen muutoin kuuluisi sille erityishuoltopiirin kuntainliitolle, jonka toimintayksikössä hän on, tulee tämän kuntainliiton ilmoittaa asiasta sille erityishuoltopiirin kuntainliitolle, jolle henkilön erityishuollon järjestäminen 1 momentin säännösten mukaan kuuluisi. Viimeksi mainitun kuntainliiton tulee ryhtyä tarpeellisiin toimenpiteisiin henkilön erityishuollon järjestämiseksi. Erityishuollon antamista on kuitenkin jatkettava entisessä paikassa siihen asti, kunnes se voidaan tarkoituksenmukaisesti ja henkilön erityishuoltomahdollisuuksia heikentämättä järjestää muualla."}}}],"isMemberOf":"sfsd:1977/519/luku/3/pykala/27","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/3/pykala/28/ajantasa/19970801","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/28","repealedBy":"sfsd:1996/1369/alkup","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/28/ajantasa/19970801/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/28/ajantasa/19970801/fin/txt","content_fi":"28 §(30.12.1996/1369)\n\n28 § on kumottu L:lla 30.12.1996/1369."}}},{"@id":"sfsd:1977/519/luku/3/pykala/29/ajantasa/19840203","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/29","repealedBy":"sfsd:1983/484/alkup","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/29/ajantasa/19840203/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/29/ajantasa/19840203/fin/txt","content_fi":"29 §(27.5.1983/484)\n\n29 § on kumottu L:lla 27.5.1983/484."}}},{"@id":"sfsd:1977/519/luku/3/pykala/30/ajantasa/19970801","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/30","repealedBy":"sfsd:1996/1369/alkup","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/30/ajantasa/19970801/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/30/ajantasa/19970801/fin/txt","content_fi":"30 §(30.12.1996/1369)\n\n30 § on kumottu L:lla 30.12.1996/1369."}}},{"@id":"sfsd:1977/519/luku/3/pykala/31/ajantasa/19810101","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/3/pykala/31/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/31/momentti/1","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/31/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/31/momentti/1/alkup/fin/txt","content_fi":"Erityishuoltoon pyrkimistä koskeva aloite tulee tehdä erityishuoltopiirin kuntainliitolle tai henkilön kotikunnan sosiaalilautakunnalle."}}},{"@id":"sfsd:1977/519/luku/3/pykala/31/momentti/2/ajantasa/19810101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:1980/952/alkup","isMemberOf":"sfsd:1977/519/luku/3/pykala/31/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/31/momentti/2/ajantasa/19810101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/31/momentti/2/ajantasa/19810101/fin/txt","content_fi":"Erityishuollon antamisesta ja lopettamisesta päättää erityishuollon johtoryhmä, jollei erityishuoltopiirin johtosäännössä toisin määrätä.\n(19.12.1980/952)"}}}],"isMemberOf":"sfsd:1977/519/luku/3/pykala/31","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/3/pykala/32/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/1/johdanto/ajantasa/20160610","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/32/momentti/1/johdanto","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/1/johdanto/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/1/johdanto/ajantasa/20160610/fin/txt","content_fi":"Henkilö voidaan määrätä erityishuollon toimintayksikköön tahdostaan riippumatta, jos:"}}},{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/1/kohta/1/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/32/momentti/1/kohta/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/1/kohta/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/1/kohta/1/ajantasa/20160610/fin/txt","content_fi":"1) hän ei kykene tekemään hoitoaan ja huolenpitoaan koskevia ratkaisuja eikä ymmärtämään käyttäytymisensä seurauksia;"}}},{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/1/kohta/2/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/32/momentti/1/kohta/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/1/kohta/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/1/kohta/2/ajantasa/20160610/fin/txt","content_fi":"2) hän todennäköisesti vakavasti vaarantaa terveyttään tai turvallisuuttaan taikka muiden henkilöiden terveyttä tai turvallisuutta; ja"}}},{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/1/kohta/3/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/32/momentti/1/kohta/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/1/kohta/3/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/1/kohta/3/ajantasa/20160610/fin/txt","content_fi":"3) hänen hoitoaan ja huolenpitoaan ei voida järjestää muulla tavoin."}}}],"isMemberOf":"sfsd:1977/519/luku/3/pykala/32/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/2/ajantasa/20160610","@type":"sfl:SubsectionVersion","cites":"sfsd:2014/1301/pykala/21","isMemberOf":"sfsd:1977/519/luku/3/pykala/32/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/2/ajantasa/20160610/fin/txt","content_fi":"Tahdosta riippumaton erityishuolto toteutetaan sosiaalihuoltolain (1301/2014) 21 §:n 4 momentissa tarkoitetun tehostetun palveluasumisen yksikössä tai 22 §:ssä tarkoitetussa laitoksessa taikka vastaavassa yksityisessä sosiaalihuollon toimintayksikössä tai laitoksessa, jossa on riittävä lääketieteen, psykologian ja sosiaalityön asiantuntemus vaativan hoidon ja huolenpidon toteuttamista ja seurantaa varten."}}},{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/3/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/32/momentti/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/3/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/3/ajantasa/20160610/fin/txt","content_fi":"Henkilön määräämisestä erityishuollon toimintayksikköön tahdostaan riippumatta on tehtävä erityishuollon johtoryhmälle kirjallinen hakemus. Hakemuksen on oikeutettu tekemään henkilön laillinen edustaja taikka hänen omaisensa tai muu läheisensä."}}},{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/4/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/32/momentti/4","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/4/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/32/momentti/4/ajantasa/20160610/fin/txt","content_fi":"Jollei 3 momentissa tarkoitettuja hakemuksen tekemiseen oikeutettuja ole tai jolleivät he suostu hakemuksen tekemiseen, hakemuksen voi tehdä myös se sosiaalilautakunta tai muu sosiaalihuollosta vastaava toimielin, jonka alueella henkilö oleskelee, taikka lautakunnan tai muun sosiaalihuollosta vastaavan toimielimen tehtävään määräämä viranhaltija. Vastaavin edellytyksin voi hakemuksen rangaistuslaitoksessa olevan osalta tehdä laitoksen johtaja."}}}],"isMemberOf":"sfsd:1977/519/luku/3/pykala/32","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/3/pykala/33/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/3/pykala/33/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","cites":["sfsd:2003/434","sfsd:2000/812"],"isMemberOf":"sfsd:1977/519/luku/3/pykala/33/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/33/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/33/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Jos erityishuollon johtoryhmä 32 §:ssä tarkoitetun hakemuksen ja muiden tarvittavien sosiaali- ja terveydenhuollon tutkimusten ja selvitysten johdosta katsoo olevan ilmeistä, että edellytykset henkilön määräämiselle erityishuollon toimintayksikköön tahdostaan riippumatta ovat olemassa, johtoryhmän on määrättävä henkilö toimitettavaksi tutkimukseen 32 §:n 2 momentissa tarkoitettuun erityishuollon toimintayksikköön. Tutkimukseen määräämisestä on tehtävä kirjallinen päätös kiireellisissä tilanteissa viivytyksettä ja muulloin viimeistään seitsemän päivän kuluessa 32 §:ssä tarkoitetun hakemuksen saapumisesta. Ennen tutkimukseen määräämistä on selvitettävä henkilön oma mielipide. Henkilön ja hänen huoltajansa tai muun laillisen edustajansa kuulemisesta säädetään hallintolaissa (434/2003). Jollei täysi-ikäisellä henkilöllä ole laillista edustajaa, on sosiaalihuollon asiakkaan asemasta ja oikeuksista annetun lain (812/2000), jäljempänä sosiaalihuollon asiakaslaki, 9 §:n 1 momentissa tarkoitetussa tilanteessa henkilön palvelujen suunnitteluun ja toteuttamiseen osallistuvalle omaiselle tai muulle läheiselle varattava tilaisuus tulla kuulluksi hallintolaissa säädetyllä tavalla. Lisäksi alaikäisen henkilön vanhemmille ja henkilölle, jonka hoidossa ja kasvatuksessa alaikäinen on ollut välittömästi ennen tutkimukseen määräämistä, on varattava tilaisuus tulla kuulluksi hallintolaissa säädetyllä tavalla."}}},{"@id":"sfsd:1977/519/luku/3/pykala/33/momentti/2/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/33/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/33/momentti/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/33/momentti/2/ajantasa/20160610/fin/txt","content_fi":"Henkilöä voidaan pitää tutkittavana enintään 14 päivää tutkimukseen määräämistä koskevan päätöksen tekemisestä. Tutkittavana pitäminen on lopetettava heti, jos tutkimusaikana ilmenee, ettei henkilön määräämiseen erityishuollon toimintayksikköön tahdostaan riippumatta ole edellytyksiä. Laillistetun lääkärin, laillistetun psykologin ja laillistetun sosiaalityöntekijän, jotka ovat virkasuhteessa ja perehtyneet kehitysvammahuoltoon, sekä tarvittaessa muiden sosiaali- tai terveydenhuollon ammattihenkilöiden on suoritettava henkilöä koskeva tutkimus. Tutkimusta suoritettaessa on otettava huomioon henkilön sosiaali- tai terveydenhuollon tarpeen taikka toimintakyvyn arviointia varten aiemmin tehdyt selvitykset. Tutkimusta suoritettaessa on selvitettävä lisäksi henkilön oma mielipide ja varattava henkilölle ja muille 1 momentissa tarkoitetuille tahoille tilaisuus tulla kuulluksi hallintolaissa säädetyllä tavalla. Tutkimuksesta on laadittava tutkimuksen suorittaneiden lääkärin, psykologin ja sosiaalityöntekijän allekirjoittama tutkimuslausunto, jonka on sisällettävä perusteltu kannanotto siitä, ovatko edellytykset henkilön määräämiselle erityishuollon toimintayksikköön tahdostaan riippumatta olemassa."}}},{"@id":"sfsd:1977/519/luku/3/pykala/33/momentti/3/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/33/momentti/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/33/momentti/3/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/33/momentti/3/ajantasa/20160610/fin/txt","content_fi":"Kun tutkimus on suoritettu, erityishuollon johtoryhmän on päätettävä määrätäänkö henkilö erityishuollon toimintayksikköön tahdostaan riippumatta. Päätös on tehtävä kirjallisesti viimeistään 14 päivän kuluessa tutkimukseen määräämistä koskevan päätöksen tekemisestä. Päätöksen on sisällettävä perusteltu kannanotto siitä, ovatko edellytykset henkilön määräämiselle erityishuollon toimintayksikköön tahdostaan riippumatta olemassa. Päätös henkilön määräämisestä erityishuollon toimintayksikköön tahdostaan riippumatta on välittömästi, kuitenkin viimeistään 14 päivän kuluessa sen tekemisestä, alistettava hallinto-oikeuden vahvistettavaksi. Hallinto-oikeuden tulee käsitellä asia kiireellisenä."}}}],"isMemberOf":"sfsd:1977/519/luku/3/pykala/33","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/3/pykala/34/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":{"@id":"sfsd:1977/519/luku/3/pykala/34/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/34/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/34/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/34/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Erityishuollon yksilölliseksi toteuttamiseksi erityishuollon johtoryhmän tai sen määräämissä rajoissa toimintayksikön vastaavan johtajan tulee hyväksyä erityishuolto-ohjelma jokaista erityishuollon tarpeessa olevaa henkilöä varten. Ohjelma on mahdollisuuksien mukaan laadittava yhteistyössä henkilön itsensä ja hänen laillisen edustajansa tai sosiaalihuollon asiakaslain 9 §:n 1 momentissa tarkoitetuissa tilanteissa henkilön laillisen edustajan taikka hänen palvelujensa suunnitteluun ja toteuttamiseen osallistuvan omaisensa tai muun läheisensä sekä sosiaalilautakunnan tai muun sosiaalihuollosta vastaavan toimielimen kanssa. Ohjelmaa on tarpeen mukaan tarkistettava."}}},"isMemberOf":"sfsd:1977/519/luku/3/pykala/34","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/3/pykala/35/ajantasa/19840201","@type":"sfl:SectionVersion","amendedBy":"sfsd:1984/26/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/3/pykala/35/momentti/1/ajantasa/19840201","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/35/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/35/momentti/1/ajantasa/19840201/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/35/momentti/1/ajantasa/19840201/fin/txt","content_fi":"Yksilöllistä erityishuolto-ohjelmaa toteutettaessa on pyrittävä siihen, että sellaisen henkilön asuminen, joka ei voi asua omassa kodissaan mutta joka ei ole laitoshuollon tarpeessa, järjestetään muulla tavoin."}}},{"@id":"sfsd:1977/519/luku/3/pykala/35/momentti/2/ajantasa/19840201","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/35/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/35/momentti/2/ajantasa/19840201/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/35/momentti/2/ajantasa/19840201/fin/txt","content_fi":"Erityishuollon tarpeessa olevalle on myös pyrittävä järjestämään työhönvalmennusta, työtoimintaa sekä muuta virikkeitä antavaa toimintaa."}}}],"isMemberOf":"sfsd:1977/519/luku/3/pykala/35","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/3/pykala/36/ajantasa/20100101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2009/1539/alkup","hasPart":{"@id":"sfsd:1977/519/luku/3/pykala/36/momentti/1/ajantasa/20100101","@type":"sfl:SubsectionVersion","cites":"sfsd:1982/710/pykala/6","isMemberOf":"sfsd:1977/519/luku/3/pykala/36/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/36/momentti/1/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/36/momentti/1/ajantasa/20100101/fin/txt","content_fi":"Aluehallintovirasto voi määrätä erityishuolto-ohjelmaa oikaistavaksi tarpeellisiksi katsomiltaan osin, milloin henkilö itse tai hänen huoltajansa tai muu edunvalvojansa taikka sosiaalihuoltolain (710/1982) 6 §:n 1 momentissa tarkoitettu kunnan määräämä monijäseninen toimielin katsoo, ettei ohjelma ole tarkoituksenmukainen."}}},"isMemberOf":"sfsd:1977/519/luku/3/pykala/36","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/3/pykala/37/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":{"@id":"sfsd:1977/519/luku/3/pykala/37/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","cites":"sfsd:1990/1116/pykala/19","isMemberOf":"sfsd:1977/519/luku/3/pykala/37/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/37/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/37/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Jos erityishuollon toimintayksikköön tahdostaan riippumatta määrättyä henkilöä tutkittaessa tai hänen hoitoaan ja huolenpitoaan järjestettäessä käy ilmi, että edellytykset määrätä henkilö erityishuollon toimintayksikköön tahdostaan riippumatta puuttuvat, erityishuollon johtoryhmän tai mielenterveyslain (1116/1990) 19 §:n 2 momentissa tarkoitetussa tilanteessa Terveyden ja hyvinvoinnin laitoksen on erityishuollon johtoryhmän esityksestä heti tehtävä päätös tahdosta riippumattoman erityishuollon lopettamisesta."}}},"isMemberOf":"sfsd:1977/519/luku/3/pykala/37","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/3/pykala/38/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/3/pykala/38/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/38/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/38/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/38/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Henkilöä voidaan pitää erityishuollon toimintayksikössä tahdosta riippumattomaan erityishuoltoon määräämistä koskevan päätöksen nojalla enintään puolen vuoden ajan. Jos ennen tämän määräajan päättymistä näyttää ilmeiseltä, että edellytykset henkilön määräämiselle erityishuollon toimintayksikköön tahdostaan riippumatta ovat edelleen olemassa, henkilölle on tehtävä ilman erillistä tutkimukseen määräämistä koskevaa päätöstä 33 §:n 2 momentissa tarkoitetulla tavalla uusi tutkimus ja hänestä on annettava uusi tutkimuslausunto. Tahdosta riippumattoman erityishuollon jatkamisesta erityishuollon toimintayksikössä enintään puolen vuoden ajan on päätettävä 33 §:n 3 momentin mukaisesti kirjallisella päätöksellä ennen kuin puoli vuotta on kulunut tahdosta riippumattomaan erityishuoltoon määräämisestä. Päätöksen alistamiseen hallinto-oikeudelle sovelletaan 33 §:n 3 momenttia. Tämän jälkeen tahdosta riippumattoman erityishuollon jatkamisen edellytykset on selvitettävä vastaavalla tavalla vähintään puolen vuoden välein."}}},{"@id":"sfsd:1977/519/luku/3/pykala/38/momentti/2/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/38/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/38/momentti/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/38/momentti/2/ajantasa/20160610/fin/txt","content_fi":"Erityishuollon toimintayksikköön tahdosta riippumatta määrätyllä henkilöllä ja hänen laillisella edustajallaan on oikeus saada tahdosta riippumattoman erityishuollon jatkamisen edellytykset erityishuollon johtoryhmän arvioitavaksi erityishuollon kestäessä myös ennen puolen vuoden enimmäisajan täyttymistä. Jollei täysi-ikäisellä henkilöllä ole laillista edustajaa, on mainittu oikeus sosiaalihuollon asiakaslain 9 §:n 1 momentissa tarkoitetuissa tilanteissa henkilön lisäksi hänen palvelujensa suunnitteluun ja toteuttamiseen osallistuvalla omaisellaan tai muulla läheisellään. Mikäli aiemmin tehdystä arvioinnista on kulunut alle kuukausi, ja on ilmeistä, että muutosta henkilön tilassa ei ole tapahtunut, voidaan arvio jättää tekemättä. Arvion tekemättä jättämisen peruste on kirjattava asiakasasiakirjoihin."}}}],"isMemberOf":"sfsd:1977/519/luku/3/pykala/38","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/3/pykala/39/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":{"@id":"sfsd:1977/519/luku/3/pykala/39/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/39/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/39/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/39/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Erityishuoltopiirin kuntayhtymän tai erityishuoltoa järjestävän kunnan on huolehdittava tutkimukseen määrätyn tai järjestämässään erityishuollossa olevan henkilön kuljetuksesta kuntayhtymän tai kunnan toimintayksiköiden välillä. Lisäksi kuntayhtymän tai kunnan on huolehdittava henkilön erityishuollon saamiseksi välttämättömistä kuljetuksista tai korvattava niistä aiheutuvat kustannukset."}}},"isMemberOf":"sfsd:1977/519/luku/3/pykala/39","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/3/pykala/40/ajantasa/20160610","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/40","repealedBy":"sfsd:2016/381/alkup","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/40/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/40/ajantasa/20160610/fin/txt","content_fi":"40–41 §\n\n\n40–41 § on kumottu L:lla 20.5.2016/381."}}},{"@id":"sfsd:1977/519/luku/3/pykala/40/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/3/pykala/40/momentti/1/alkup","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/3/pykala/40/momentti/1/johdanto/alkup","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/40/momentti/1/johdanto","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/40/momentti/1/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/40/momentti/1/johdanto/alkup/fin/txt","content_fi":"Valtio huolehtii erityishuollon järjestämisestä seuraaville erityishuollon tarpeessa oleville henkilöille:"}}},{"@id":"sfsd:1977/519/luku/3/pykala/40/momentti/1/kohta/1/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/40/momentti/1/kohta/1","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/40/momentti/1/kohta/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/40/momentti/1/kohta/1/alkup/fin/txt","content_fi":"1) vankilassa taikka muussa valtion laitoksessa pidettävälle tai huollettavana olevalle; sekä"}}},{"@id":"sfsd:1977/519/luku/3/pykala/40/momentti/1/kohta/2/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/40/momentti/1/kohta/2","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/40/momentti/1/kohta/2/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/40/momentti/1/kohta/2/alkup/fin/txt","content_fi":"2) rikoksesta syytetylle, jota sen mukaan kuin erikseen säädetään, pidetään erityishuollon toimintayksikössä tai joka mielentilansa vuoksi on jätetty rangaistukseen tuomitsematta ja jolle on määrätty annettavaksi erityishuoltoa."}}}],"isMemberOf":"sfsd:1977/519/luku/3/pykala/40/momentti/1","version":"sfl:Original","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/3/pykala/40/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3/pykala/40/momentti/2","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/pykala/40/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3/pykala/40/momentti/2/alkup/fin/txt","content_fi":"Valtio voi järjestää muutakin kuin 1 momentissa ja 15 §:ssä tarkoitettua erityishuoltoa sen mukaan kuin asetuksella tarkemmin säädetään."}}}],"isMemberOf":"sfsd:1977/519/luku/3/pykala/40","version":"sfl:Original","versionDate":"2016-06-10"}],"isMemberOf":"sfsd:1977/519/luku/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3/ajantasa/20160610/fin","title_fi":"Erityishuollon järjestäminen"}},{"@id":"sfsd:1977/519/luku/3a/ajantasa/20160610","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1977/519/luku/3a/pykala/42/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":{"@id":"sfsd:1977/519/luku/3a/pykala/42/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Erityishuolto on järjestettävä ja erityishuollossa olevaa henkilöä on kohdeltava siten, ettei hänen ihmisarvoaan loukata sekä että hänen vakaumustaan ja hänen yksityisyyttään kunnioitetaan. Erityishuoltoa toteutettaessa on otettava huomioon erityishuollossa olevan henkilön toivomukset, mielipide, etu ja yksilölliset tarpeet. Erityishuollossa olevalle henkilölle on turvattava mahdollisuus osallistumiseen ja vaikuttamiseen omissa asioissaan. Erityishuollossa olevan henkilön hyvinvointia, terveyttä ja turvallisuutta on ylläpidettävä ja edistettävä."}}},"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42/ajantasa/20160610/fin","title_fi":"Itsemääräämisoikeuden vahvistaminen"}},{"@id":"sfsd:1977/519/luku/3a/pykala/42a/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42a/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Erityishuollossa olevan henkilön palvelu- ja hoitosuunnitelmaan on kirjattava toimenpiteet, joilla tuetaan ja edistetään henkilön itsenäistä suoriutumista ja itsemääräämisoikeuden toteutumista. Palvelu- ja hoitosuunnitelma on tarkistettava tarvittaessa, kuitenkin vähintään kuuden kuukauden välein, ellei se ole ilmeisen tarpeetonta. Palvelu- ja hoitosuunnitelmaa tarkistettaessa tulee erityisesti arvioida käytetyn rajoitustoimenpiteen vaikutusta palvelu- ja hoitosuunnitelmaan sekä erityishuolto-ohjelmaan."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/ajantasa/20160610","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/johdanto/ajantasa/20160610","@type":"sfl:PreambleVersion","cites":"sfsd:2015/254/luku/3","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/johdanto","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/johdanto/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/johdanto/ajantasa/20160610/fin/txt","content_fi":"Sen lisäksi, mitä sosiaalihuollon asiakasasiakirjoista annetun lain (254/2015) 3 luvussa säädetään, palvelu- ja hoitosuunnitelman tulee sisältää tiedot:"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/kohta/1/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/kohta/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/kohta/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/kohta/1/ajantasa/20160610/fin/txt","content_fi":"1) toimenpiteistä henkilön itsenäisen suoriutumisen tukemiseksi ja edistämiseksi sekä itsemääräämisoikeuden vahvistamiseksi;"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/kohta/2/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/kohta/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/kohta/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/kohta/2/ajantasa/20160610/fin/txt","content_fi":"2) kohtuullisista mukautuksista henkilön täysimääräisen osallistumisen ja osallisuuden turvaamiseksi;"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/kohta/3/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/kohta/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/kohta/3/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/kohta/3/ajantasa/20160610/fin/txt","content_fi":"3) henkilön käyttämistä kommunikaatiomenetelmistä;"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/kohta/4/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/kohta/4","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/kohta/4/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/kohta/4/ajantasa/20160610/fin/txt","content_fi":"4) keinoista, joilla henkilön erityishuolto toteutetaan ensisijaisesti ilman rajoitustoimenpiteitä;"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/kohta/5/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/kohta/5","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/kohta/5/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2/kohta/5/ajantasa/20160610/fin/txt","content_fi":"5) rajoitustoimenpiteistä, joita henkilön erityishuollossa arvioidaan jouduttavan käyttämään."}}}],"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42a/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/3/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42a/momentti/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/3/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/3/ajantasa/20160610/fin/txt","content_fi":"Palvelu- ja hoitosuunnitelma on laadittava yhteistyössä erityishuollossa olevan henkilön ja hänen laillisen edustajansa tai sosiaalihuollon asiakaslain 9 §:n 1 momentissa tarkoitetuissa tilanteissa henkilön laillisen edustajan taikka hänen palvelujensa suunnitteluun ja toteuttamiseen osallistuvan omaisensa tai muun läheisensä kanssa, ellei siihen ole ilmeistä estettä."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/4/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42a/momentti/4","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/4/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/4/ajantasa/20160610/fin/txt","content_fi":"Erityishuollon toimintayksikössä on oltava sen toimintaan ja erityishuollossa olevien henkilöiden erityisiin tarpeisiin nähden riittävä määrä sosiaali- ja terveydenhuollon ammattihenkilöitä ja muuta henkilökuntaa."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5/ajantasa/20160610","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5/johdanto/ajantasa/20160610","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5/johdanto","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5/johdanto/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5/johdanto/ajantasa/20160610/fin/txt","content_fi":"Erityishuoltoa annettaessa on huolehdittava, että:"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5/kohta/1/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5/kohta/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5/kohta/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5/kohta/1/ajantasa/20160610/fin/txt","content_fi":"1) erityishuollon toimintayksikön henkilökunta perehdytetään ja ohjeistetaan työmenetelmiin ja keinoihin, joiden avulla tuetaan ja edistetään erityishuollossa olevien henkilöiden itsenäistä suoriutumista ja itsemääräämisoikeuden toteutumista;"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5/kohta/2/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5/kohta/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5/kohta/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5/kohta/2/ajantasa/20160610/fin/txt","content_fi":"2) erityishuollon toimintayksikön henkilökuntaan kuuluvat sosiaali- tai terveydenhuollon ammattihenkilöt koulutetaan rajoitustoimenpiteiden käyttöä edellyttävien tilanteiden ennalta ehkäisemiseen ja rajoitustoimenpiteiden asianmukaiseen käyttämiseen;"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5/kohta/3/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5/kohta/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5/kohta/3/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5/kohta/3/ajantasa/20160610/fin/txt","content_fi":"3) erityishuollon toimintayksikössä edistetään rajoitustoimenpiteille vaihtoehtoisten ja kuntouttavien toimintatapojen käyttöön ottamista;"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5/kohta/4/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5/kohta/4","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5/kohta/4/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5/kohta/4/ajantasa/20160610/fin/txt","content_fi":"4) erityishuollossa olevien henkilöiden itsenäistä suoriutumista ja itsemääräämisoikeutta tuetaan ja edistetään asianmukaisin kalustein, välinein ja tilaratkaisuin."}}}],"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42a/momentti/5","version":"sfl:Consolidated","versionDate":"2016-06-10"}],"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42a","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42a/ajantasa/20160610/fin","title_fi":"Toimenpiteet itsenäisen suoriutumisen ja itsemääräämisoikeuden tukemiseksi"}},{"@id":"sfsd:1977/519/luku/3a/pykala/42b/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/3a/pykala/42b/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42b/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42b/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42b/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Erityishuollossa voidaan jäljempänä säädetyin edellytyksin käyttää 42 f–42 n §:ssä tarkoitettuja rajoitustoimenpiteitä  järjestettäessä sosiaalihuoltolain 21 §:n 4 momentissa tarkoitettua tehostettua palveluasumista tai 22 §:ssä tarkoitettuja laitospalveluja taikka vastaavia yksityisiä palveluja. Jäljempänä 42 n §:n 3 momentissa tarkoitettua muuta kuin lyhytkestoista poistumisen estämistä voidaan käyttää vain tahdosta riippumattomassa erityishuollossa."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42b/momentti/2/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42b/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42b/momentti/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42b/momentti/2/ajantasa/20160610/fin/txt","content_fi":"Edellytyksenä rajoitustoimenpiteiden käytölle on lisäksi, että tehostetun palveluasumisen yksiköllä ja laitoksella on käytettävissään riittävä lääketieteen, psykologian ja sosiaalityön asiantuntemus vaativan hoidon ja huolenpidon toteuttamista ja seurantaa varten."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42b/momentti/3/ajantasa/20160610","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/3a/pykala/42b/momentti/3/johdanto/ajantasa/20160610","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42b/momentti/3/johdanto","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42b/momentti/3/johdanto/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42b/momentti/3/johdanto/ajantasa/20160610/fin/txt","content_fi":"Jäljempänä 42 f–42 h ja 42 k §:ssä tarkoitettuja rajoitustoimenpiteitä voidaan käyttää 1 momentissa säädetyn lisäksi järjestettäessä erityishuollossa:"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42b/momentti/3/kohta/1/ajantasa/20160610","@type":"sfl:ParagraphVersion","cites":"sfsd:1982/710/pykala/27e","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42b/momentti/3/kohta/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42b/momentti/3/kohta/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42b/momentti/3/kohta/1/ajantasa/20160610/fin/txt","content_fi":"1) sosiaalihuoltolain (710/1982) 27 e §:ssä tai tämän lain 35 §:n 2 momentissa tarkoitettua työtoimintaa, joka järjestetään sosiaalihuollon toimintayksikössä, jossa on riittävä määrä sosiaali- tai terveydenhuollon ammattihenkilöitä; tai"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42b/momentti/3/kohta/2/ajantasa/20160610","@type":"sfl:ParagraphVersion","cites":"sfsd:1987/380/pykala/8b","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42b/momentti/3/kohta/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42b/momentti/3/kohta/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42b/momentti/3/kohta/2/ajantasa/20160610/fin/txt","content_fi":"2) vammaisuuden perusteella järjestettävistä palveluista ja tukitoimista annetun lain (380/1987) 8 b §:ssä tarkoitettua päivätoimintaa tai vastaavaa tämän lain 35 §:n 2 momentissa tarkoitettua toimintaa, joka järjestetään sosiaalihuollon toimintayksikössä, jossa on riittävä määrä sosiaali- tai terveydenhuollon ammattihenkilöitä."}}}],"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42b/momentti/3","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/3a/pykala/42b/momentti/4/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42b/momentti/4","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42b/momentti/4/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42b/momentti/4/ajantasa/20160610/fin/txt","content_fi":"Lisäksi 3 momentissa tarkoitetuissa toimintayksiköissä voidaan toteuttaa 42 k ja 42 m §:ssä sekä 42 n §:n 3 momentissa tarkoitettua rajoitustoimenpidettä, jos rajoitustoimenpiteen käytöstä on tehty kirjallinen päätös mainituissa säännöksissä tarkoitetulla tavalla. Jos 3 momentissa tarkoitetussa toimintayksikössä käytetään 42 k tai 42 m §:ssä tai 42 n §:n 3 momentissa tarkoitettua rajoitustoimenpidettä, toimintayksiköllä on oltava käytettävissään 2 momentissa tarkoitettu riittävä lääketieteen, psykologian ja sosiaalityön asiantuntemus."}}}],"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42b","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42b/ajantasa/20160610/fin","title_fi":"Rajoitustoimenpiteiden käyttöä koskevien säännösten soveltamisala"}},{"@id":"sfsd:1977/519/luku/3a/pykala/42c/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":{"@id":"sfsd:1977/519/luku/3a/pykala/42c/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","cites":"sfsd:1974/412","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42c/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42c/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42c/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Tämän luvun nojalla julkista valtaa käyttävään henkilöön sovelletaan rikosoikeudellista virkavastuuta koskevia säännöksiä hänen hoitaessaan tässä luvussa tarkoitettuja tehtäviä silloinkin, kun hän ei ole virkasuhteessa valtioon, kuntaan tai kuntayhtymään. Vahingonkorvausvastuusta säädetään vahingonkorvauslaissa (412/1974)."}}},"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42c","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42c/ajantasa/20160610/fin","title_fi":"Virkavastuu"}},{"@id":"sfsd:1977/519/luku/3a/pykala/42d/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/3a/pykala/42d/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/3a/pykala/42d/momentti/1/johdanto/ajantasa/20160610","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42d/momentti/1/johdanto","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42d/momentti/1/johdanto/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42d/momentti/1/johdanto/ajantasa/20160610/fin/txt","content_fi":"Erityishuoltoa toteutetaan ensisijaisesti yhteisymmärryksessä erityishuollossa olevan henkilön kanssa. Erityishuollossa voidaan käyttää 42 f–42 n §:ssä tarkoitettuja rajoitustoimenpiteitä ainoastaan silloin, kun:"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42d/momentti/1/kohta/1/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42d/momentti/1/kohta/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42d/momentti/1/kohta/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42d/momentti/1/kohta/1/ajantasa/20160610/fin/txt","content_fi":"1) erityishuollossa oleva henkilö ei kykene tekemään hoitoaan ja huolenpitoaan koskevia ratkaisuja eikä ymmärtämään käyttäytymisensä seurauksia;"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42d/momentti/1/kohta/2/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42d/momentti/1/kohta/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42d/momentti/1/kohta/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42d/momentti/1/kohta/2/ajantasa/20160610/fin/txt","content_fi":"2) rajoitustoimenpiteen käyttäminen on välttämätöntä hänen terveytensä tai turvallisuutensa taikka muiden terveyden tai turvallisuuden suojaamiseksi taikka merkittävän omaisuusvahingon ehkäisemiseksi; ja"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42d/momentti/1/kohta/3/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42d/momentti/1/kohta/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42d/momentti/1/kohta/3/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42d/momentti/1/kohta/3/ajantasa/20160610/fin/txt","content_fi":"3) muut, lievemmät keinot eivät ole tilanteeseen soveltuvia tai riittäviä."}}}],"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42d/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/3a/pykala/42d/momentti/2/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42d/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42d/momentti/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42d/momentti/2/ajantasa/20160610/fin/txt","content_fi":"Rajoitustoimenpiteen on oltava henkilön hoidon ja huolenpidon kannalta perusteltu, tarkoitukseen sopiva ja oikeassa suhteessa tavoiteltuun päämäärään nähden. Jos henkilöön kohdistetaan useampia rajoitustoimenpiteitä samanaikaisesti tai peräkkäin, niiden yhteisvaikutukseen on kiinnitettävä erityistä huomiota."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42d/momentti/3/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42d/momentti/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42d/momentti/3/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42d/momentti/3/ajantasa/20160610/fin/txt","content_fi":"Rajoitustoimenpide on toteutettava erityishuollossa olevan henkilön ihmisarvoa kunnioittaen, mahdollisimman turvallisesti ja hänen perustarpeistaan huolehtien. Rajoitustoimenpiteen käyttö on lopetettava heti, kun se ei enää ole välttämätöntä tai jos se vaarantaa erityishuollossa olevan henkilön terveyden tai turvallisuuden. Jos rajoitustoimenpide kohdistuu alaikäiseen, rajoitustoimenpidettä käytettäessä on otettava huomioon alaikäisen etu sekä hänen ikänsä ja kehitystasonsa."}}}],"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42d","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42d/ajantasa/20160610/fin","title_fi":"Rajoitustoimenpiteiden käytön yleiset edellytykset"}},{"@id":"sfsd:1977/519/luku/3a/pykala/42e/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":{"@id":"sfsd:1977/519/luku/3a/pykala/42e/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42e/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42e/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42e/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Jos erityishuoltoa annettaessa on käytetty 42 f–42 n §:ssä tarkoitettua rajoitustoimenpidettä, toimintayksikössä on viipymättä arvioitava rajoitustoimenpiteen käyttöön johtaneita syitä ja keinoja, joiden avulla voidaan jatkossa vähentää rajoitustoimenpiteiden käyttöä yksikössä. Jos muussa kuin tahdosta riippumattomassa erityishuollossa olevan henkilön erityishuollossa on käytetty toistuvasti tai pitkäaikaisesti 42 j, 42 l tai 42 m §:ssä tai 42 n §:n 2 momentissa tarkoitettua rajoitustoimenpidettä, toimintayksikössä on arvioitava, täyttyvätkö henkilön kohdalla 32 §:n 1 momentissa tarkoitetut edellytykset henkilön määräämiselle tahdosta riippumattomaan erityishuoltoon, ja saatettava kysymys tarvittaessa 32 §:n 4 momentissa tarkoitetun sosiaalihuollosta vastaavan toimielimen arvioitavaksi."}}},"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42e","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42e/ajantasa/20160610/fin","title_fi":"Rajoitustoimenpiteiden käytön arviointi ja vähentäminen"}},{"@id":"sfsd:1977/519/luku/3a/pykala/42f/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/3a/pykala/42f/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42f/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42f/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42f/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Toimintayksikön henkilökuntaan kuuluva sosiaali- tai terveydenhuollon ammattihenkilö voi pitää lyhytaikaisesti kiinni erityishuollossa olevasta henkilöstä rauhoittamistarkoituksessa. Kiinnipitäminen voi sisältää myös henkilön siirtämisen toimintayksikön tiloissa. Kiinnipitäminen on toteutettava hyväksyttävää hoidollista menetelmää käyttäen."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42f/momentti/2/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42f/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42f/momentti/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42f/momentti/2/ajantasa/20160610/fin/txt","content_fi":"Ratkaisun kiinnipitämisestä tekee toimintayksikön henkilökuntaan kuuluva sosiaali- tai terveydenhuollon ammattihenkilö."}}}],"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42f","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42f/ajantasa/20160610/fin","title_fi":"Kiinnipitäminen"}},{"@id":"sfsd:1977/519/luku/3a/pykala/42g/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/3a/pykala/42g/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42g/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42g/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42g/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Toimintayksikön henkilökuntaan kuuluva sosiaali- tai terveydenhuollon ammattihenkilö voi ottaa erityishuollossa olevalta henkilöltä toimintayksikön haltuun aineet ja esineet, jotka ominaisuuksiensa puolesta soveltuvat vaarantamaan vakavasti terveyttä tai turvallisuutta taikka merkittävästi vahingoittamaan omaisuutta ja joita todennäköisesti käytettäisiin tähän tarkoitukseen."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42g/momentti/2/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42g/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42g/momentti/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42g/momentti/2/ajantasa/20160610/fin/txt","content_fi":"Ratkaisun aineiden tai esineiden haltuunotosta tekee toimintayksikön henkilökuntaan kuuluva sosiaali- tai terveydenhuollon ammattihenkilö. Toimintayksikön vastaavan johtajan on tehtävä haltuunotosta kirjallinen päätös, jollei aineita tai esineitä palauteta erityishuollossa olevalle henkilölle yhden vuorokauden kuluessa haltuunotosta. Haltuun otettu omaisuus on palautettava erityishuollossa olevalle henkilölle viimeistään toimintayksikössä annetun erityishuollon päätyttyä, jollei sen luovuttamisesta tai hävittämisestä muussa laissa toisin säädetä."}}}],"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42g","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42g/ajantasa/20160610/fin","title_fi":"Aineiden ja esineiden haltuunotto"}},{"@id":"sfsd:1977/519/luku/3a/pykala/42h/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/3a/pykala/42h/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42h/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42h/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42h/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Toimintayksikön henkilökuntaan kuuluva sosiaali- tai terveydenhuollon ammattihenkilö voi tehdä erityishuollossa olevalle henkilölle henkilöntarkastuksen tämän vastustuksesta riippumatta, jos on perusteltua syytä epäillä, että henkilöllä on vaatteissaan tai muutoin yllään taikka mukanaan olevissa tavaroissa 42 g §:n 1 momentissa tarkoitettuja aineita tai esineitä. Edellytyksenä on lisäksi, että erityishuollossa oleva tai muu henkilö todennäköisesti käyttäisi aineita tai esineitä vaarantamaan vakavasti terveyttä tai turvallisuutta taikka merkittävästi vahingoittamaan omaisuutta."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42h/momentti/2/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42h/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42h/momentti/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42h/momentti/2/ajantasa/20160610/fin/txt","content_fi":"Henkilöntarkastus on tehtävä toimintayksikön henkilökuntaan kuuluvan toisen sosiaali- tai terveydenhuollon ammattihenkilön läsnä ollessa, jollei erityisestä syystä muuta johdu. Tarkastuksen tekijän ja tarkastuksessa läsnä olevan henkilön on oltava tarkastuksen kohteena olevan henkilön kanssa samaa sukupuolta. Tarkastuksen tekijä ja tarkastuksessa läsnä oleva henkilö voivat kuitenkin olla eri sukupuolta kuin tarkastuksen kohteena oleva henkilö, jos he ovat terveydenhuollon ammattihenkilöitä tai toimenpiteen suorittaminen välittömästi on välttämätöntä tarkastuksen kohteena olevan tai toisen henkilön terveyden tai turvallisuuden varmistamiseksi."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42h/momentti/3/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42h/momentti/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42h/momentti/3/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42h/momentti/3/ajantasa/20160610/fin/txt","content_fi":"Henkilöntarkastusta koskevan ratkaisun tekee toimintayksikön vastaava johtaja. Kiireellisessä tilanteessa ratkaisun voi tehdä toimintayksikön henkilökuntaan kuuluva sosiaali- tai terveydenhuollon ammattihenkilö, jonka on viipymättä ilmoitettava asiasta toimintayksikön vastaavalle johtajalle."}}}],"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42h","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42h/ajantasa/20160610/fin","title_fi":"Henkilöntarkastus"}},{"@id":"sfsd:1977/519/luku/3a/pykala/42i/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/3a/pykala/42i/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42i/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42i/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42i/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Toimintayksikön henkilökuntaan kuuluva sosiaali- tai terveydenhuollon ammattihenkilö voi viedä erityishuollossa olevan henkilön tämän vastustuksesta riippumatta lyhytaikaisesti, enintään kahdeksi tunniksi, erilleen muista henkilöistä rauhoittamistarkoituksessa. Erillään pitämiseen käytettävän huoneen oven voi tarvittaessa lukita."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42i/momentti/2/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42i/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42i/momentti/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42i/momentti/2/ajantasa/20160610/fin/txt","content_fi":"Toimintayksikön henkilökuntaan kuuluvan sosiaali- tai terveydenhuollon ammattihenkilön on valvottava erillään pidettävää henkilöä koko erillään pitämisen ajan olemalla hänen kanssaan samassa tilassa tai sen välittömässä läheisyydessä niin, että henkilökunnalla on mahdollisuus saada yhteys erillään pidettävään henkilöön. Myös erillään pidettävällä henkilöllä on oltava mahdollisuus saada yhteys henkilökuntaan."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42i/momentti/3/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42i/momentti/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42i/momentti/3/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42i/momentti/3/ajantasa/20160610/fin/txt","content_fi":"Ratkaisun lyhytaikaisesta erillään pitämisestä tekee toimintayksikön vastaava johtaja. Kiireellisessä tilanteessa ratkaisun voi tehdä toimintayksikön henkilökuntaan kuuluva sosiaali- tai terveydenhuollon ammattihenkilö, jonka on viipymättä ilmoitettava asiasta toimintayksikön vastaavalle johtajalle."}}}],"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42i","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42i/ajantasa/20160610/fin","title_fi":"Lyhytaikainen erillään pitäminen"}},{"@id":"sfsd:1977/519/luku/3a/pykala/42j/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/3a/pykala/42j/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","cites":"sfsd:1992/785/pykala/6","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42j/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42j/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42j/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Erityishuollossa olevaa henkilöä on hoidettava terveydenhuoltoa annettaessa ensisijaisesti yhteisymmärryksessä hänen kanssaan potilaan  asemasta ja  oikeuksista annetun lain (785/1992) 6–9 §:ssä säädetyllä tavalla."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42j/momentti/2/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42j/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42j/momentti/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42j/momentti/2/ajantasa/20160610/fin/txt","content_fi":"Jos erityishuollossa oleva henkilö, joka ei kykene päättämään hoidostaan, vastustaa terveydenhuollon antamista, henkilöä hoitava lääkäri tai lääkärin ohjeiden mukaan toimintayksikön henkilökuntaan kuuluva terveydenhuollon ammattihenkilö voi antaa henkilölle lääketieteellisesti välttämättömän terveydenhuollon henkilön vastustuksesta riippumatta, jos hoitamatta jättäminen uhkaisi vaarantaa vakavasti henkilön terveyden. Henkilön tilaa on tällöin seurattava ja arvioitava jatkuvasti hänen terveytensä ja turvallisuutensa edellyttämällä tavalla."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42j/momentti/3/ajantasa/20160610","@type":"sfl:SubsectionVersion","cites":"sfsd:2010/629","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42j/momentti/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42j/momentti/3/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42j/momentti/3/ajantasa/20160610/fin/txt","content_fi":"Edellä 2 momentissa tarkoitettua hoito- ja tutkimustoimenpidettä suoritettaessa erityishuollossa olevasta henkilöstä voidaan pitää lyhytaikaisesti kiinni tai hänen liikkumistaan voidaan rajoittaa rajoittavan välineen avulla lyhytaikaisesti enintään niin kauan kuin toimenpiteen suorittaminen välttämättä edellyttää, kuitenkin enintään tunnin ajan. Rajoittavien välineiden on täytettävä terveydenhuollon laitteista ja tarvikkeista annetun lain (629/2010) mukaiset vaatimukset."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42j/momentti/4/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42j/momentti/4","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42j/momentti/4/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42j/momentti/4/ajantasa/20160610/fin/txt","content_fi":"Ratkaisun välttämättömän terveydenhuollon antamisesta erityishuollossa olevan henkilön vastustuksesta riippumatta ja 3 momentissa tarkoitetuista lyhytaikaisista rajoitustoimenpiteistä tekee virkasuhteessa oleva lääkäri. Tehtäessä ratkaisua välttämättömän terveydenhuollon antamisesta vastustuksesta riippumatta on pyydettävä ja otettava huomioon 42 b §:n 2 momentissa tarkoitettujen asiantuntijoiden arviot rajoitustoimenpiteestä. Kiireellisessä tilanteessa ratkaisun välttämättömän terveydenhuollon antamisesta vastustuksesta riippumatta ja 3 momentissa tarkoitetuista lyhytaikaisista rajoitustoimenpiteistä voi tehdä henkilöä hoitava lääkäri tai toimintayksikön henkilökuntaan kuuluva terveydenhuollon ammattihenkilö, jonka on välittömästi ilmoitettava asiasta henkilöä hoitavalle lääkärille."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42j/momentti/5/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42j/momentti/5","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42j/momentti/5/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42j/momentti/5/ajantasa/20160610/fin/txt","content_fi":"Jos on ilmeistä, että tarve välttämättömän terveydenhuollon antamiseen vastustuksesta riippumatta on toistuvaa, virkasuhteessa oleva lääkäri voi tehdä rajoitustoimenpiteen toistuvasta käytöstä kirjallisen päätöksen enintään 30 päiväksi kerrallaan. Tällöinkin rajoitustoimenpidettä voidaan kussakin tilanteessa käyttää vain, jos 2 ja 3 momentissa tarkoitetut edellytykset täyttyvät. Kirjallista päätöstä tehtäessä on pyydettävä ja otettava huomioon 42 b §:n 2 momentissa tarkoitettujen asiantuntijoiden arviot rajoitustoimenpiteestä. Lisäksi 42 b §:n 2 momentissa tarkoitettujen asiantuntijoiden on säännöllisesti seurattava ja arvioitava rajoitustoimenpiteen toistuvaa käyttöä."}}}],"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42j","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42j/ajantasa/20160610/fin","title_fi":"Välttämättömän terveydenhuollon antaminen vastustuksesta riippumatta"}},{"@id":"sfsd:1977/519/luku/3a/pykala/42k/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/3a/pykala/42k/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/3a/pykala/42k/momentti/1/johdanto/ajantasa/20160610","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42k/momentti/1/johdanto","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42k/momentti/1/johdanto/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42k/momentti/1/johdanto/ajantasa/20160610/fin/txt","content_fi":"Jos erityishuollossa olevan henkilön terveys tai turvallisuus muutoin todennäköisesti vaarantuisi, toimintayksikön henkilökuntaan kuuluva sosiaali- tai terveydenhuollon ammattihenkilö voi käyttää henkilöllä:"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42k/momentti/1/kohta/1/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42k/momentti/1/kohta/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42k/momentti/1/kohta/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42k/momentti/1/kohta/1/ajantasa/20160610/fin/txt","content_fi":"1) sängystä putoamista estävää välinettä, joka ei rajoita henkilön raajojen tai kehon liikkeitä, henkilön yö- ja päiväaikaisen lepäämisen ajan ja lyhytaikaisesti muiden päivittäisten toimintojen ajan;"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42k/momentti/1/kohta/2/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42k/momentti/1/kohta/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42k/momentti/1/kohta/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42k/momentti/1/kohta/2/ajantasa/20160610/fin/txt","content_fi":"2) tuolista putoamista estävää välinettä lyhytaikaisesti henkilön ruokailun ja muiden vastaavien päivittäisten toimintojen ajan; ja"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42k/momentti/1/kohta/3/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42k/momentti/1/kohta/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42k/momentti/1/kohta/3/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42k/momentti/1/kohta/3/ajantasa/20160610/fin/txt","content_fi":"3) itsensä vahingoittamista estävää tai turvallisuutta lisäävää välinettä tai asustetta, joka ei rajoita henkilön raajojen tai kehon liikkeitä tai vähäistä enempää henkilön toimintaa, välttämättömän ajan."}}}],"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42k/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/3a/pykala/42k/momentti/2/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42k/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42k/momentti/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42k/momentti/2/ajantasa/20160610/fin/txt","content_fi":"Edellä 1 momentissa tarkoitettujen rajoittavien välineiden ja asusteiden on täytettävä terveydenhuollon laitteista ja tarvikkeista annetun lain mukaiset vaatimukset. Rajoittavaa välinettä tai asustetta voidaan käyttää vain välttämättömän ajan ja ainoastaan tarkoituksensa mukaisella tavalla. Rajoittavaa välinettä tai asustetta käytettäessä erityishuollossa olevan henkilön tilaa on seurattava ja arvioitava hänen terveytensä ja turvallisuutensa edellyttämällä tavalla. Rajoittavan välineen tai asusteen käyttö on lopetettava välittömästi, jos se vaarantaa henkilön terveyden tai turvallisuuden."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42k/momentti/3/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42k/momentti/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42k/momentti/3/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42k/momentti/3/ajantasa/20160610/fin/txt","content_fi":"Rajoittavan välineen tai asusteen käytöstä päivittäisissä toiminnoissa tekee ratkaisun toimintayksikön henkilökuntaan kuuluva sosiaali- tai terveydenhuollon ammattihenkilö toimintayksikön vastaavan johtajan ohjeiden mukaisesti. Jos rajoittavan välineen tai asusteen käyttö on säännönmukaista ja pitkäkestoista, tekee rajoittavan välineen tai asusteen toistuvasta käytöstä kirjallisen päätöksen enintään kuuden kuukauden ajaksi toimintayksikön vastaava johtaja. Tällöinkin rajoitustoimenpidettä voidaan kussakin tilanteessa käyttää vain, jos 1 momentissa tarkoitetut edellytykset täyttyvät. Vastaavan johtajan on ohjeita antaessaan ja päätöstä tehdessään pyydettävä ja otettava huomioon 42 b §:n 2 momentissa tarkoitettujen asiantuntijoiden arviot rajoittavan välineen tai asusteen käytöstä. Lisäksi 42 b §:n 2 momentissa tarkoitettujen asiantuntijoiden on säännöllisesti seurattava ja arvioitava rajoittavan välineen tai asusteen toistuvaa käyttöä."}}}],"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42k","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42k/ajantasa/20160610/fin","title_fi":"Rajoittavien välineiden tai asusteiden käyttö päivittäisissä toiminnoissa"}},{"@id":"sfsd:1977/519/luku/3a/pykala/42l/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/3a/pykala/42l/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42l/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42l/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42l/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Toimintayksikön henkilökuntaan kuuluva sosiaali- tai terveydenhuollon ammattihenkilö voi käyttää erityishuollossa olevalla henkilöllä liikkumista tai henkilön toimintaa rajoittavaa välinettä tai asustetta muissa kuin 42 k §:n 1 momentissa tarkoitetuissa tilanteissa vain, jos henkilö muutoin todennäköisesti vaarantaisi vakavasti oman terveytensä tai turvallisuutensa taikka muiden henkilöiden terveyden tai turvallisuuden. Rajoittavaa välinettä tai asustetta voidaan käyttää vain välttämättömän ajan ja ainoastaan tarkoituksensa mukaisella tavalla.  Henkilö voidaan sitoa vain, jolleivät muut keinot ole riittäviä. Sidottuna voidaan pitää vain välttämättömän ajan, kuitenkin yhtäjaksoisesti tai toistuvasti yhteensä enintään kahdeksan tunnin ajan, jona aikana henkilöä hoitavan lääkärin on arvioitava sitomisen edellytykset uudelleen vähintään kahden tunnin välein."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42l/momentti/2/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42l/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42l/momentti/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42l/momentti/2/ajantasa/20160610/fin/txt","content_fi":"Edellä 1 momentissa tarkoitettujen rajoittavien välineiden ja asusteiden on täytettävä terveydenhuollon laitteista ja tarvikkeista annetun lain mukaiset vaatimukset. Rajoittavaa välinettä tai asustetta käytettäessä erityishuollossa olevan henkilön tilaa on seurattava ja arvioitava hänen terveytensä ja turvallisuutensa edellyttämällä tavalla. Sidotun henkilön tilaa on jatkuvasti seurattava siten, että terveydenhuollon ammattihenkilö on näkö- ja kuuloyhteydessä henkilöön. Rajoittavan välineen tai asusteen käyttö on lopetettava välittömästi, jos se vaarantaa henkilön terveyden tai turvallisuuden."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42l/momentti/3/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42l/momentti/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42l/momentti/3/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42l/momentti/3/ajantasa/20160610/fin/txt","content_fi":"Muun kuin sitomiseen käytettävän rajoittavan välineen tai asusteen käytöstä tekee kirjallisen päätöksen toimintayksikön vastaava johtaja. Päätöstä tehtäessä on pyydettävä ja otettava huomioon 42 b §:n 2 momentissa tarkoitettujen asiantuntijoiden arviot rajoittavan välineen tai asusteen käytöstä. Kiireellisessä tilanteessa ratkaisun muun kuin sitomiseen käytettävän rajoittavan välineen tai asusteen käytöstä voi tehdä henkilöä hoitava lääkäri tai toimintayksikön vastaavan johtajan ohjeiden mukaisesti toimintayksikön henkilökuntaan kuuluva sosiaali- tai terveydenhuollon ammattihenkilö. Rajoittavan välineen tai asusteen käytöstä kiireellisessä tilanteessa on välittömästi ilmoitettava toimintayksikön vastaavalle johtajalle, jonka jälkeen asiasta on päätettävä kirjallisella päätöksellä edellä mainitulla tavalla."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42l/momentti/4/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42l/momentti/4","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42l/momentti/4/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42l/momentti/4/ajantasa/20160610/fin/txt","content_fi":"Jos muun kuin sitomiseen käytettävän rajoittavan välineen tai asusteen toistuvan käytön tarve on ilmeinen, voi toimintayksikön vastaava johtaja tehdä rajoittavan välineen tai asusteen toistuvasta käytöstä kirjallisen päätöksen enintään seitsemän päivän ajaksi. Tämän jälkeen rajoittavan välineen tai asusteen toistuvasta käytöstä tekee kirjallisen päätöksen enintään 30 päivän ajaksi virkasuhteessa oleva toimintayksikön vastaava johtaja tai, jos toimintayksikön vastaava johtaja ei ole virkasuhteessa, virkasuhteessa oleva lääkäri tai virkasuhteessa oleva sosiaalityöntekijä. Tällöinkin rajoittavaa välinettä tai asustetta voidaan päätöksen voimassaoloaikana käyttää kussakin tilanteessa vain, jos 1 momentissa tarkoitetut edellytykset täyttyvät. Rajoittavan välineen tai asusteen toistuvaa käyttöä koskevaan päätökseen on kirjattava, kuinka pitkän ajan rajoittavaa välinettä tai asustetta voidaan kerrallaan enintään käyttää, ja mikseivät muut keinot ole tilanteeseen soveltuvia tai riittäviä. Päätöstä tehtäessä on pyydettävä ja otettava huomioon 42 b §:n 2 momentissa tarkoitettujen asiantuntijoiden arviot rajoittavan välineen tai asusteen käytöstä. Lisäksi 42 b §:n 2 momentissa tarkoitettujen asiantuntijoiden on säännöllisesti seurattava ja arvioitava rajoittavan välineen tai asusteen toistuvaa käyttöä."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42l/momentti/5/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42l/momentti/5","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42l/momentti/5/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42l/momentti/5/ajantasa/20160610/fin/txt","content_fi":"Henkilön sitomisesta tekee kirjallisen päätöksen virkasuhteessa oleva lääkäri suorittamansa tutkimuksen ja psykiatrisen arvion perusteella. Lisäksi päätöstä tehtäessä on pyydettävä ja otettava huomioon 42 b §:n 2 momentissa tarkoitettujen asiantuntijoiden arviot sitomisesta.  Kiireellisessä tilanteessa kirjallisen päätöksen sitomisesta voi tehdä virkasuhteessa oleva lääkäri tai toimintayksikön henkilökuntaan kuuluva lääkäri suorittamansa tutkimuksen perusteella. Jos henkilöä joudutaan tällöin pitämään sidottuna yli kahden tunnin ajan, tekee kirjallisen päätöksen sitomisesta viimeistään kahta tuntia pidemmältä ajalta virkasuhteessa oleva lääkäri, joka voi päättää asiasta kiireellisessä tilanteessa toimintayksikön henkilökuntaan kuuluvan lääkärin esityksen perusteella puhelin- tai muun etäyhteyden välityksellä."}}}],"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42l","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42l/ajantasa/20160610/fin","title_fi":"Rajoittavien välineiden tai asusteiden käyttö vakavissa vaaratilanteissa"}},{"@id":"sfsd:1977/519/luku/3a/pykala/42m/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/3a/pykala/42m/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42m/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42m/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42m/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Toimintayksikön henkilökuntaan kuuluva sosiaali- tai terveydenhuollon ammattihenkilö voi valvoa erityishuollossa olevan henkilön liikkumista toimintayksikössä, poistumista toimintayksiköstä ja liikkumista toimintayksikön tai sen yhteydessä olevan piha-alueen ulkopuolella, jos henkilö muutoin todennäköisesti vaarantaisi oman terveytensä tai turvallisuutensa taikka muiden henkilöiden terveyden tai turvallisuuden. Jolleivät muut keinot ole riittäviä, henkilön liikkumista voidaan valvoa henkilön ylle kiinnitettävän teknisen valvontalaitteen avulla, suunnitellusti saattajan avulla tai muulla vastaavalla tavalla. Henkilön liikkumista koskeva suunnitelma on kirjattava henkilön palvelu- ja hoitosuunnitelmaan. Valvottaessa henkilön liikkumista on erityisesti huolehdittava siitä, ettei muiden henkilöiden liikkumisvapautta rajoiteta."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42m/momentti/2/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42m/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42m/momentti/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42m/momentti/2/ajantasa/20160610/fin/txt","content_fi":"Jolleivät 1 momentissa tarkoitetut keinot ole riittäviä henkilön liikkumisen valvomiseksi yöaikaan, henkilön oman huoneen ovi voidaan lukita yöksi korkeintaan kahdeksan tunnin ajaksi, kuitenkin niin, että henkilöllä on mahdollisuus tarvittaessa poistua huoneesta saatettuna. Toimintayksikön henkilökuntaan kuuluvan sosiaali- tai terveydenhuollon ammattihenkilön on valvottava lukitussa huoneessa olevaa henkilöä koko lukitsemisen ajan olemalla hänen välittömässä läheisyydessään niin, että henkilökunnalla on mahdollisuus saada yhteys huoneessa olevaan henkilöön. Myös henkilöllä itsellään on oltava mahdollisuus saada yhteys henkilökuntaan."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42m/momentti/3/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42m/momentti/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42m/momentti/3/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42m/momentti/3/ajantasa/20160610/fin/txt","content_fi":"Valvotusta liikkumisesta enintään seitsemän päivän ajaksi tekee kirjallisen päätöksen toimintayksikön vastaava johtaja. Tätä pidemmästä, yhteensä enintään kuusi kuukautta kestävästä valvotusta liikkumisesta tekee kirjallisen päätöksen virkasuhteessa oleva toimintayksikön vastaava johtaja tai, jos toimintayksikön vastaava johtaja ei ole virkasuhteessa, virkasuhteessa oleva sosiaalityöntekijä. Tehtäessä päätöstä valvotusta liikkumisesta on pyydettävä ja otettava huomioon 42 b §:n 2 momentissa tarkoitettujen asiantuntijoiden arviot henkilön valvotusta liikkumisesta. Lisäksi 42 b §:n 2 momentissa tarkoitettujen asiantuntijoiden on säännöllisesti seurattava ja arvioitava rajoitustoimenpiteen käyttöä."}}}],"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42m","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42m/ajantasa/20160610/fin","title_fi":"Valvottu liikkuminen"}},{"@id":"sfsd:1977/519/luku/3a/pykala/42n/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/3a/pykala/42n/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42n/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42n/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42n/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Toimintayksikön henkilökuntaan kuuluva sosiaali- tai terveydenhuollon ammattihenkilö voi estää erityishuollossa olevaa henkilöä poistumasta toimintayksiköstä tai toimintayksikön yhteydessä olevalta piha-alueelta, jos henkilö saattaisi poistumisellaan itsensä tai toisen henkilön alttiiksi välittömästi uhkaavalle ja vakavalle terveyteen tai turvallisuuteen kohdistuvalle vaaralle. Vastaavin edellytyksin toimintayksikön henkilökuntaan kuuluva sosiaali- tai terveydenhuollon ammattihenkilö voi noutaa erityishuollossa olevan henkilön takaisin toimintayksikköön tai sen yhteydessä olevalle piha-alueelle, jos henkilö tavoitetaan sen välittömästä läheisyydestä. Poistumisen estämiseksi ja henkilön noutamiseksi saadaan käyttää voimakeinoja, jos ne ovat välttämättömiä, kun otetaan huomioon vastustuksen laatu ja voimakkuus, tilanteen uhkaavuus sekä muut olosuhteet."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42n/momentti/2/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42n/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42n/momentti/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42n/momentti/2/ajantasa/20160610/fin/txt","content_fi":"Kiireellisessä tilanteessa 1 momentissa tarkoitetun poistumisen estämistä koskevan lyhytkestoisen ratkaisun tekee toimintayksikön henkilökuntaan kuuluva sosiaali- tai terveydenhuollon ammattihenkilö, jonka on viipymättä ilmoitettava asiasta toimintayksikön vastaavalle johtajalle."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42n/momentti/3/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42n/momentti/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42n/momentti/3/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42n/momentti/3/ajantasa/20160610/fin/txt","content_fi":"Tahdosta riippumattomaan erityishuoltoon määrätyn henkilön poistuminen voidaan estää muutoin kuin lyhytkestoisesti, jollei 42 m §:ssä tarkoitettu valvottu liikkuminen ole toimenpiteenä soveltuva tai riittävä. Enintään seitsemän päivää kestävästä poistumisen estämisestä tekee kirjallisen päätöksen toimintayksikön vastaava johtaja. Tätä pidemmästä, yhteensä enintään 30 päivää kestävästä poistumisen estämisestä tekee kirjallisen päätöksen virkasuhteessa oleva toimintayksikön vastaava johtaja tai, jos toimintayksikön vastaava johtaja ei ole virkasuhteessa, virkasuhteessa oleva sosiaalityöntekijä. Poistumisen estämistä koskevaa päätöstä tehtäessä on pyydettävä ja otettava huomioon 42 b §:n 2 momentissa tarkoitettujen asiantuntijoiden arviot henkilön poistumisen estämisestä. Lisäksi 42 b §:n 2 momentissa tarkoitettujen asiantuntijoiden on säännöllisesti seurattava ja arvioitava rajoitustoimenpiteen käyttöä. Poistumisen estämisen aikana tapahtuvaa henkilön ulkoilua ja muuta liikkumista koskeva suunnitelma on kirjattava henkilön palvelu- ja hoitosuunnitelmaan. Poistumisen estämisen yhteydessä on erityisesti huolehdittava siitä, ettei muiden henkilöiden liikkumisvapautta rajoiteta."}}}],"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42n","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42n/ajantasa/20160610/fin","title_fi":"Poistumisen estäminen"}},{"@id":"sfsd:1977/519/luku/3a/pykala/42o/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42o/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Jos erityishuollossa olevaan henkilöön on kohdistettu 42 f–42 n §:ssä tarkoitettu rajoitustoimenpide, rajoitustoimenpiteen käyttöä on arvioitava erityishuollossa olevan henkilön kanssa viipymättä sen käytön päättymisen jälkeen. Jälkiselvittelyssä on arvioitava rajoitustoimenpiteen perusteita ja keinoja, joiden avulla voidaan jatkossa välttää rajoitustoimenpiteiden käyttö."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/ajantasa/20160610","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/johdanto/ajantasa/20160610","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/johdanto","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/johdanto/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/johdanto/ajantasa/20160610/fin/txt","content_fi":"Erityishuollossa olevaa henkilöä koskeviin asiakas- tai potilasasiakirjoihin on kirjattava:"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/kohta/1/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/kohta/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/kohta/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/kohta/1/ajantasa/20160610/fin/txt","content_fi":"1) 42 f–42 n §:ssä tarkoitetun rajoitustoimenpiteen käyttö ja sen perusteet;"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/kohta/2/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/kohta/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/kohta/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/kohta/2/ajantasa/20160610/fin/txt","content_fi":"2) henkilön näkemys rajoitustoimenpiteen käytöstä ja sen perusteista;"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/kohta/3/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/kohta/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/kohta/3/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/kohta/3/ajantasa/20160610/fin/txt","content_fi":"3) rajoitustoimenpiteen vaikutukset erityishuollossa olevaan henkilöön;"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/kohta/4/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/kohta/4","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/kohta/4/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/kohta/4/ajantasa/20160610/fin/txt","content_fi":"4) rajoitustoimenpiteen alkamis- ja päättymisajankohta;"}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/kohta/5/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/kohta/5","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/kohta/5/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2/kohta/5/ajantasa/20160610/fin/txt","content_fi":"5) rajoitustoimenpidettä koskevan ratkaisun tai päätöksen tehnyt ja toimenpiteen suorittanut henkilö."}}}],"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42o/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10"}],"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42o","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42o/ajantasa/20160610/fin","title_fi":"Rajoitustoimenpiteen jälkiselvittely ja kirjaaminen"}},{"@id":"sfsd:1977/519/luku/3a/pykala/42p/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/3a/pykala/42p/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42p/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42p/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42p/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Erityishuollossa olevalle henkilölle  on viipymättä annettava selvitys rajoitustoimenpiteen sisällöstä ja perusteista sekä käytettävissä olevista oikeusturvakeinoista. Henkilön lailliselle edustajalle tai sosiaalihuollon asiakaslain 9 §:n 1 momentissa tarkoitetussa tilanteessa henkilön lailliselle edustajalle taikka hänen palvelujensa suunnitteluun ja toteuttamiseen osallistuvalle omaiselleen tai muulle läheiselleen mainittu selvitys on annettava vähintään kuukausittain. Selvitys on annettava ymmärrettävässä muodossa ja saavutettavalla tavalla ottaen huomioon vastaanottajan käyttämät kommunikaatiomenetelmät. Selvitys käytettyjen rajoitustoimenpiteiden sisällöstä ja perusteista on annettava lisäksi sosiaalihuoltolain 42 §:ssä tarkoitetulle henkilön omatyöntekijälle kuukausittain."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42p/momentti/2/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42p/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42p/momentti/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42p/momentti/2/ajantasa/20160610/fin/txt","content_fi":"Jos erityishuollossa olevaan henkilöön on kohdistettu rajoitustoimenpide hänen ollessaan tilassa, jossa hän ei ole kyennyt ymmärtämään toimenpiteen merkitystä, hänelle on annettava 1 momentissa tarkoitettu selvitys heti, kun hän kykenee ymmärtämään asian merkityksen."}}},{"@id":"sfsd:1977/519/luku/3a/pykala/42p/momentti/3/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42p/momentti/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42p/momentti/3/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42p/momentti/3/ajantasa/20160610/fin/txt","content_fi":"Jos erityishuollossa olevaan henkilöön, joka ei tilansa takia kykene ymmärtämään asian merkitystä, on kohdistettu sellainen rajoitustoimenpide, johon saa 81 b §:n 1 tai 2 momentin mukaan hakea muutosta valittamalla, rajoitustoimenpidettä koskeva päätös on muutoksenhakuohjeineen annettava tiedoksi erityishuollossa olevan henkilön lailliselle edustajalle tai sosiaalihuollon asiakaslain 9 §:n 1 momentissa tarkoitetussa tilanteessa henkilön lailliselle edustajalle taikka hänen palvelujensa suunnitteluun ja toteuttamiseen osallistuvalle omaiselleen tai muulle läheiselleen."}}}],"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42p","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42p/ajantasa/20160610/fin","title_fi":"Rajoitustoimenpidettä koskeva selvitys ja tiedoksianto"}},{"@id":"sfsd:1977/519/luku/3a/pykala/42q/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":{"@id":"sfsd:1977/519/luku/3a/pykala/42q/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/3a/pykala/42q/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42q/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/3a/pykala/42q/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Edellä 32 §:n 2 momentissa tarkoitetusta riittävästä lääketieteen, psykologian ja sosiaalityön asiantuntemuksesta, 42 a §:n 4 momentissa tarkoitetusta sosiaali- ja terveydenhuollon ammattihenkilöiden ja muun henkilökunnan riittävyydestä ja 42 a §:n 5 momentin 2 kohdassa tarkoitetusta sosiaali- ja terveydenhuollon ammattihenkilöiden koulutuksesta voidaan antaa tarkempia säännöksiä sosiaali- ja terveysministeriön asetuksella."}}},"isMemberOf":"sfsd:1977/519/luku/3a/pykala/42q","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/pykala/42q/ajantasa/20160610/fin","title_fi":"Asetuksenantovaltuus"}}],"isMemberOf":"sfsd:1977/519/luku/3a","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/3a/ajantasa/20160610/fin","title_fi":"Itsemääräämisoikeuden vahvistaminen ja rajoitustoimenpiteiden käyttö erityishuollossa (20.5.2016/381)"}},{"@id":"sfsd:1977/519/luku/4/ajantasa/19930101","@type":"sfl:ChapterVersion","hasPart":{"@id":"sfsd:1977/519/luku/4/pykala/43/ajantasa/19930101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1992/739/alkup","hasPart":{"@id":"sfsd:1977/519/luku/4/pykala/43/momentti/1/ajantasa/19930101","@type":"sfl:SubsectionVersion","cites":"sfsd:1992/734","isMemberOf":"sfsd:1977/519/luku/4/pykala/43/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/4/pykala/43/momentti/1/ajantasa/19930101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/4/pykala/43/momentti/1/ajantasa/19930101/fin/txt","content_fi":"Erityishuollosta voidaan periä maksuja siten kuin sosiaali- ja terveydenhuollon asiakasmaksuista annetussa laissa (734/92) säädetään."}}},"isMemberOf":"sfsd:1977/519/luku/4/pykala/43","version":"sfl:Consolidated","versionDate":"2016-06-10"},"isMemberOf":"sfsd:1977/519/luku/4","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/4/ajantasa/19930101/fin","title_fi":"Maksut"}},{"@id":"sfsd:1977/519/luku/5/ajantasa/19930101","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1977/519/luku/5/pykala/44/ajantasa/19890101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1988/299/alkup","hasPart":{"@id":"sfsd:1977/519/luku/5/pykala/44/momentti/1/ajantasa/19890101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/5/pykala/44/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/5/pykala/44/momentti/1/ajantasa/19890101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/5/pykala/44/momentti/1/ajantasa/19890101/fin/txt","content_fi":"Erityishuoltopiirin kuntainliiton muulle kuin sanottuun kuntainliittoon kuuluvasta kunnasta olevalle henkilölle järjestämän erityishuollon aiheuttamista kustannuksista on sen kuntainliiton, johon henkilön kotikunta kuuluu, suoritettava korvaus."}}},"isMemberOf":"sfsd:1977/519/luku/5/pykala/44","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/5/pykala/45/ajantasa/19930101","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/5/pykala/45/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/5/pykala/45/momentti/1","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/5/pykala/45/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/5/pykala/45/momentti/1/alkup/fin/txt","content_fi":"Jollei toisin ole sovittu, 44 §:n 1 ja 2 momentissa tarkoitettu korvaus lasketaan siten, kuin tässä pykälässä säädetään. Sama koskee soveltuvin osin 44 §:n 3 momentin mukaan suoritettavan korvauksen määräämistä."}}},{"@id":"sfsd:1977/519/luku/5/pykala/45/momentti/2/ajantasa/19930101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:1992/739/alkup","isMemberOf":"sfsd:1977/519/luku/5/pykala/45/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/5/pykala/45/momentti/2/ajantasa/19930101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/5/pykala/45/momentti/2/ajantasa/19930101/fin/txt","content_fi":"Korvauksen määräämiseksi toimintayksikön edellisen varainhoitovuoden käyttökustannusten kokonaismäärään lisätään 10 prosenttia. Näin saadusta määrästä vähennetään 43 §:n mukaiset maksut ja korvaukset ja toimintayksikön muut käyttötulot, lukuun ottamatta korkotuloja, valtionosuutta ja kuntien maksuosuuksia käyttökustannuksiin sekä muita tässä laissa säädettyjä erityishuollon korvauksia samoin kuin tuloja, jotka kunta tai kuntainliitto saa sellaisesta toiminnasta, minkä kustannuksiin valtionosuutta ei suoriteta. Erotus jaetaan luvulla, jona käytetään käyttöpäivien tai käyntikertojen kokonaismäärää toiminnan laadusta riippuen, minkä jälkeen osamäärä kerrotaan kysymyksessä olevan henkilön käyttöpäivien tai käyntikertojen luvulla. Saatuun määrään lisätään kutakin käyttöpäivää tai käyntikertaa kohti henkilön suoritettavaksi säädetyn korvauksen määrä, milloin korvausta tai maksua ei peritä.\n(3.8.1992/739)"}}}],"isMemberOf":"sfsd:1977/519/luku/5/pykala/45","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/5/pykala/46/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1977/519/luku/5/pykala/46/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/5/pykala/46/momentti/1","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/5/pykala/46/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/5/pykala/46/momentti/1/alkup/fin/txt","content_fi":"Milloin toimintayksikön edellisen vuoden tilinpäätöstä ei vielä ole hyväksytty, voidaan 45 §:ssä tarkoitetuista korvauksista periä ennakkona määrä, joka vastaa toimintayksikön edellisen vuoden arvioituja käyttökustannuksia, sanotun pykälän 2 momentissa tarkoitetuin tavoin käyttöpäivää tai käyntikertaa kohti laskettuna."}}},"isMemberOf":"sfsd:1977/519/luku/5/pykala/46","version":"sfl:Original","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/5/pykala/47/ajantasa/19840201","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1977/519/luku/5/pykala/47","repealedBy":"sfsd:1984/26/alkup","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/5/pykala/47/ajantasa/19840201/fin","hasFormat":{"@id":"sfsd:1977/519/luku/5/pykala/47/ajantasa/19840201/fin/txt","content_fi":"47 §(13.1.1984/26)\n\n47 § on kumottu L:lla 13.1.1984/26."}}}],"isMemberOf":"sfsd:1977/519/luku/5","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/5/ajantasa/19930101/fin","title_fi":"Korvaukset"}},{"@id":"sfsd:1977/519/luku/6/ajantasa/20100101","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1977/519/luku/6/pykala/48/ajantasa/20100101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2009/1717/alkup","hasPart":{"@id":"sfsd:1977/519/luku/6/pykala/48/momentti/1/ajantasa/20100101","@type":"sfl:SubsectionVersion","cites":["sfsd:1992/733","sfsd:2009/1704"],"isMemberOf":"sfsd:1977/519/luku/6/pykala/48/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/6/pykala/48/momentti/1/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/6/pykala/48/momentti/1/ajantasa/20100101/fin/txt","content_fi":"Tämän lain nojalla järjestettävään toimintaan sovelletaan sosiaali- ja terveydenhuollon suunnittelusta ja valtionavustuksesta annettua lakia (733/1992) sekä kunnan peruspalvelujen valtionosuudesta annettua lakia (1704/2009), jollei lailla toisin säädetä."}}},"isMemberOf":"sfsd:1977/519/luku/6/pykala/48","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/6/pykala/49/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/6/pykala/49/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/6/pykala/49/momentti/1","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/6/pykala/49/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/6/pykala/49/momentti/1/alkup/fin/txt","content_fi":"Siitä poiketen, mitä kuntien ja kuntainliittojen valtionosuuksista ja -avustuksista annetun lain 5 §:n 8 kohdassa on säädetty käyttökustannuksina pidetään myös erityishuoltopiirin kuntainliiton yleishallinnosta sekä kuntainliiton toimintayksikön hallintoelinten jäsenille maksettavista palkkioista ja muista korvauksista aiheutuvia välittömiä hallintokustannuksia."}}},{"@id":"sfsd:1977/519/luku/6/pykala/49/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/6/pykala/49/momentti/2","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/6/pykala/49/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/6/pykala/49/momentti/2/alkup/fin/txt","content_fi":"Valtionosuutta ei suoriteta 73 §:n 2 momentissa tarkoitetuista korvauksista aiheutuviin kustannuksiin."}}}],"isMemberOf":"sfsd:1977/519/luku/6/pykala/49","version":"sfl:Original","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/6/pykala/49/ajantasa/19840101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1977/519/luku/6/pykala/49","repealedBy":"sfsd:1982/702/alkup","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/6/pykala/49/ajantasa/19840101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/6/pykala/49/ajantasa/19840101/fin/txt","content_fi":"49–61 §\n\n\n49–61 § on kumottu L:lla 17.9.1982/702."}}}],"isMemberOf":"sfsd:1977/519/luku/6","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/6/ajantasa/20100101/fin","title_fi":"Suunnittelu ja valtionosuus (17.9.1982/702)"}},{"@id":"sfsd:1977/519/luku/7/ajantasa/19840101","@type":"sfl:ChapterVersion","amendedBy":"sfsd:1982/702/alkup","hasPart":{"@id":"sfsd:1977/519/luku/7/pykala/62/ajantasa/19840101","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1977/519/luku/7/pykala/62/momentti/1/ajantasa/19840101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:1982/702/alkup","isMemberOf":"sfsd:1977/519/luku/7/pykala/62/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/7/pykala/62/momentti/1/ajantasa/19840101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/7/pykala/62/momentti/1/ajantasa/19840101/fin/txt","content_fi":"7 luku on kumottu L:lla 17.9.1982/702."}}},"isMemberOf":"sfsd:1977/519/luku/7/pykala/62","repealedBy":"sfsd:1982/702/alkup","version":"sfl:Consolidated","versionDate":"2016-06-10"},"isMemberOf":"sfsd:1977/519/luku/7","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/8/ajantasa/19840101","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1977/519/luku/8/pykala/65/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/8/pykala/65/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/8/pykala/65/momentti/1","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/8/pykala/65/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/8/pykala/65/momentti/1/alkup/fin/txt","content_fi":"Erityishuoltoalan henkilökunnan koulutuksesta tai erityishuollon tutkimuksesta huolehtivalla korkeakoululla tai muulla viranomaisella on oikeus käyttää erityishuoltopiirin kuntainliiton toimintayksiköitä erityishuoltoalan koulutus- tai tutkimustoiminnan järjestämiseen."}}},{"@id":"sfsd:1977/519/luku/8/pykala/65/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/8/pykala/65/momentti/2","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/8/pykala/65/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/8/pykala/65/momentti/2/alkup/fin/txt","content_fi":"Edellä 16 §:ssä tarkoitetun toimintayksikön ylläpitäjä voi sopia korkeakoulun tai muun viranomaisen kanssa toimintayksikön käyttämisestä 1 momentissa tarkoitettuun koulutustai tutkimustoimintaan."}}}],"isMemberOf":"sfsd:1977/519/luku/8/pykala/65","version":"sfl:Original","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/8/pykala/66/ajantasa/19840101","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/8/pykala/66/momentti/1/ajantasa/19840101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:1982/702/alkup","isMemberOf":"sfsd:1977/519/luku/8/pykala/66/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/8/pykala/66/momentti/1/ajantasa/19840101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/8/pykala/66/momentti/1/ajantasa/19840101/fin/txt","content_fi":"Korkeakoulu tai muu viranomainen suorittaa erityishuoltopiirin kuntainliitolle täyden korvauksen perustamiskustannuksista, jotka ovat aiheutuneet sen koulutus- tai tutkimuskäytössä olevien, erityishuoltopiirin kuntainliiton toimintayksikössä sijaitsevien huonetilojen rakentamisesta, perusparannuksesta, laajentamisesta tai rakenteellisesti uudelleen järjestämisestä, kiinteät kojeet ja laitteet mukaan luettuina, sekä käyttökustannuksista, jotka aiheutuvat sanottujen huonetilojen kunnossapidosta. Mikäli huonetiloja kuitenkin käytetään osittain myös erityishuoltopiirin kuntainliiton toiminnassa, on korvausta tältä osin soviteltava.\n(17.9.1982/702)"}}},{"@id":"sfsd:1977/519/luku/8/pykala/66/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/8/pykala/66/momentti/2","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/8/pykala/66/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/8/pykala/66/momentti/2/alkup/fin/txt","content_fi":"Mitä 1 momentissa on sanottu, sovelletaan muihinkin erityishuoltopiirin kuntainliiton perustamis- ja käyttökustannuksiin, jotka aiheutuvat pääasiallisesti erityishuoltoalan opetustai tutkimustoiminnan järjestämisestä erityishuoltopiirin kuntainliiton toimintayksikössä."}}}],"isMemberOf":"sfsd:1977/519/luku/8/pykala/66","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/8/pykala/67/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/8/pykala/67/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/8/pykala/67/momentti/1","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/8/pykala/67/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/8/pykala/67/momentti/1/alkup/fin/txt","content_fi":"Edellä 65 §:n 1 momentissa tarkoitettujen toimintojen järjestämisestä sekä 66 §:ssä tarkoitettujen korvausten määräytymisperusteista tulee korkeakoulun tai muun viranomaisen sopia erityishuoltopiirin kuntainliiton kanssa."}}},{"@id":"sfsd:1977/519/luku/8/pykala/67/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/8/pykala/67/momentti/2","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/8/pykala/67/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/8/pykala/67/momentti/2/alkup/fin/txt","content_fi":"Edellä 1 momentissa tarkoitettu sopimus, joka on asetuksella säädettävissä tapauksissa alistettava valtioneuvoston hyväksyttäväksi, on laadittava siten, että siinä otetaan tasapuolisesti huomioon toisaalta erityishuoltopiirin kuntainliiton velvollisuus erityishuollon järjestämiseen sekä toisaalta korkeakoulun tai muun viranomaisen säännöksiin tai määräyksiin taikka vahvistettuihin koulutus- ja tutkimussuunnitelmiin perustuva velvollisuus erityishuoltoalan koulutus- tai tutkimustoiminnan järjestämiseen."}}},{"@id":"sfsd:1977/519/luku/8/pykala/67/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/8/pykala/67/momentti/3","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/8/pykala/67/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/8/pykala/67/momentti/3/alkup/fin/txt","content_fi":"Jollei 1 momentissa tarkoitettua sopimusta saada aikaan, valtioneuvosto voi, jos järjestely erityishuoltoalan koulutus- tai tutkimustoiminnan tahi erityishuollon tarkoituksenmukaisen järjestämisen kannalta on välttämätöntä, määrätä erityishuoltopiirin kuntainliiton toimintayksiköiden käyttämisestä sanottuihin tarkoituksiin sekä siitä aiheutuvien kustannusten korvaamisesta. Tällöin on noudatettava, mitä 2 momentissa sekä 66 §:ssä säädetään."}}}],"isMemberOf":"sfsd:1977/519/luku/8/pykala/67","version":"sfl:Original","versionDate":"2016-06-10"}],"isMemberOf":"sfsd:1977/519/luku/8","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/8/ajantasa/19840101/fin","title_fi":"Erityishuoltoalan koulutus- ja tutkimustoiminnan järjestäminen"}},{"@id":"sfsd:1977/519/luku/9/ajantasa/19930101","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1977/519/luku/9/pykala/68/ajantasa/19930101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1992/739/alkup","hasPart":{"@id":"sfsd:1977/519/luku/9/pykala/68/momentti/1/ajantasa/19930101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/9/pykala/68/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/9/pykala/68/momentti/1/ajantasa/19930101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/9/pykala/68/momentti/1/ajantasa/19930101/fin/txt","content_fi":"Erityishuoltopiirin kuntainliitto voi sopia sellaisen terveydenhuoltoalan kuntainliiton kanssa, jonka toimialue käsittää erityishuoltopiirin alueen, että erityishuoltopiirin kuntainliiton varat ja velat siirtyvät terveydenhuoltoalan kuntainliitolle."}}},"isMemberOf":"sfsd:1977/519/luku/9/pykala/68","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/9/pykala/69/ajantasa/19930101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1992/739/alkup","hasPart":{"@id":"sfsd:1977/519/luku/9/pykala/69/momentti/1/ajantasa/19930101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/9/pykala/69/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/9/pykala/69/momentti/1/ajantasa/19930101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/9/pykala/69/momentti/1/ajantasa/19930101/fin/txt","content_fi":"Edellä 68 §:ssä mainitun sopimuksen voimaantulosta lukien terveydenhuoltoalan kuntainliitosta on soveltuvin osin voimassa, sen lisäksi mitä muuten on säädetty, mitä erityishuoltopiirin kuntainliitosta tässä laissa säädetään."}}},"isMemberOf":"sfsd:1977/519/luku/9/pykala/69","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/9/pykala/70/ajantasa/19930101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1977/519/luku/9/pykala/70","repealedBy":"sfsd:1992/739/alkup","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/9/pykala/70/ajantasa/19930101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/9/pykala/70/ajantasa/19930101/fin/txt","content_fi":"70 §(3.8.1992/739)\n\n70 § on kumottu L:lla 3.8.1992/739."}}}],"isMemberOf":"sfsd:1977/519/luku/9","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/9/ajantasa/19930101/fin","title_fi":"Erityishuollon järjestämisestä eräissä tapauksissa"}},{"@id":"sfsd:1977/519/luku/10/ajantasa/20160610","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1977/519/luku/10/pykala/71/ajantasa/20160101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2015/1649/alkup","hasPart":{"@id":"sfsd:1977/519/luku/10/pykala/71/momentti/1/ajantasa/20160101","@type":"sfl:SubsectionVersion","cites":"sfsd:1982/710/pykala/27e","isMemberOf":"sfsd:1977/519/luku/10/pykala/71/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/71/momentti/1/ajantasa/20160101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/71/momentti/1/ajantasa/20160101/fin/txt","content_fi":"Tämän lain nojalla järjestettyyn työtoimintaan sovelletaan, mitä sosiaalihuoltolain (710/1982) 27 e §:n 3 momentissa säädetään."}}},"isMemberOf":"sfsd:1977/519/luku/10/pykala/71","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/10/pykala/72/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1977/519/luku/10/pykala/72/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/72/momentti/1","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/72/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/72/momentti/1/alkup/fin/txt","content_fi":"Erityishuoltoa järjestävää toimintayksikköä ei saa ottaa käyttöön ennen kuin lääninhallitus on hyväksynyt sen tarkoitukseensa."}}},"isMemberOf":"sfsd:1977/519/luku/10/pykala/72","version":"sfl:Original","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/10/pykala/72/ajantasa/19790330","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/72","repealedBy":"sfsd:1979/386/alkup","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/72/ajantasa/19790330/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/72/ajantasa/19790330/fin/txt","content_fi":"72 §(30.3.1979/386)\n\n72 § on kumottu L:lla 30.3.1979/386."}}},{"@id":"sfsd:1977/519/luku/10/pykala/73/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/10/pykala/73/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/73/momentti/1","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/73/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/73/momentti/1/alkup/fin/txt","content_fi":"Jollei toisin ole sovittu, jäsenkuntien osuudet erityishuoltopiirin kuntainliiton varoihin ja vastuu sen veloista määräytyvät niiden osuuksien suhteessa, joilla jäsenkunnat ovat osallistuneet kuntainliiton perustamiskustannuksiin. Maksuosuudet muunnetaan kuitenkin sen mukaan kuin asetuksella tarkemmin säädetään jäsenkuntien osuuksien määräämisajankohdan rahanarvoon huomioon ottaen kohtuullinen arvonvähennys."}}},{"@id":"sfsd:1977/519/luku/10/pykala/73/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/73/momentti/2","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/73/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/73/momentti/2/alkup/fin/txt","content_fi":"Milloin erityishuoltopiirijakoa muutettaessa erityishuoltopiirin kuntainliiton jäsenkunta siirtyy toiseen erityishuoltopiiriin, luovuttavan kuntainliiton on suoritettava vastaanottavalle kuntainliitolle korvaus kunnan osuudesta kuntainliiton varoihin velat vähennettyinä."}}}],"isMemberOf":"sfsd:1977/519/luku/10/pykala/73","version":"sfl:Original","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/10/pykala/74/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1977/519/luku/10/pykala/74/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/74/momentti/1","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/74/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/74/momentti/1/alkup/fin/txt","content_fi":"Kelpoisuudesta erityishuoltopiirin kuntainliiton ja erityishuoltoa järjestävän kunnan virkoihin voidaan säätää asetuksella."}}},"isMemberOf":"sfsd:1977/519/luku/10/pykala/74","version":"sfl:Original","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/10/pykala/75/ajantasa/20100101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2008/672/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/10/pykala/75/momentti/1/ajantasa/20100101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2009/1539/alkup","isMemberOf":"sfsd:1977/519/luku/10/pykala/75/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/75/momentti/1/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/75/momentti/1/ajantasa/20100101/fin/txt","content_fi":"Sosiaali- ja terveysalan lupa- ja valvontavirasto ja aluehallintovirasto voivat tarkastaa kunnan ja kuntayhtymän tässä laissa tarkoitetun toiminnan sekä toiminnan järjestämisessä käytettävät toimintayksiköt ja toimitilat silloin, kun tarkastuksen tekemiseen on perusteltu syy. Sosiaali- ja terveysalan lupa- ja valvontavirasto voi lisäksi perustellusta syystä määrätä aluehallintoviraston tekemään tarkastuksen. Tarkastus voidaan tehdä ennalta ilmoittamatta.\n(22.12.2009/1539)"}}},{"@id":"sfsd:1977/519/luku/10/pykala/75/momentti/2/ajantasa/20081219","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/75/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/75/momentti/2/ajantasa/20081219/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/75/momentti/2/ajantasa/20081219/fin/txt","content_fi":"Tarkastaja on päästettävä kaikkiin toimipaikan tiloihin. Tarkastuksessa on salassapitosäännösten estämättä esitettävä kaikki tarkastajan pyytämät asiakirjat, jotka ovat välttämättömiä tarkastuksen toimittamiseksi. Lisäksi tarkastajalle on salassapitosäännösten estämättä annettava maksutta hänen pyytämänsä jäljennökset tarkastuksen toimittamiseksi välttämättömistä asiakirjoista. Tarkastajalla on myös oikeus ottaa valokuvia tarkastuksen aikana. Tarkastajan apuna voi olla tarkastuksen toteuttamiseksi tarpeellisia asiantuntijoita."}}},{"@id":"sfsd:1977/519/luku/10/pykala/75/momentti/3/ajantasa/20100101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2009/1539/alkup","isMemberOf":"sfsd:1977/519/luku/10/pykala/75/momentti/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/75/momentti/3/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/75/momentti/3/ajantasa/20100101/fin/txt","content_fi":"Poliisin on tarvittaessa annettava Sosiaali- ja terveysalan lupa- ja valvontavirastolle ja aluehallintovirastolle virka-apua tarkastuksen suorittamiseksi.\n(22.12.2009/1539)"}}},{"@id":"sfsd:1977/519/luku/10/pykala/75/momentti/4/ajantasa/20081219","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/75/momentti/4","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/75/momentti/4/ajantasa/20081219/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/75/momentti/4/ajantasa/20081219/fin/txt","content_fi":"Tarkastuksesta on pidettävä pöytäkirjaa."}}},{"@id":"sfsd:1977/519/luku/10/pykala/75/momentti/5/ajantasa/20081219","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/75/momentti/5","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/75/momentti/5/ajantasa/20081219/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/75/momentti/5/ajantasa/20081219/fin/txt","content_fi":"Tarkastuksessa erityisesti huomioon otettavista asioista ja tarkastusmenettelyn tarkemmasta sisällöstä sekä tarkastuksessa pidettävästä pöytäkirjasta ja sen säilyttämisestä ja säilyttämisajasta voidaan tarvittaessa säätää valtioneuvoston asetuksella."}}}],"isMemberOf":"sfsd:1977/519/luku/10/pykala/75","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/10/pykala/75a/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/10/pykala/75a/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/75a/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/75a/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/75a/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Aluehallintoviraston on erityisesti valvottava 3 a luvun nojalla tapahtuvaa rajoitustoimenpiteiden käyttöä. Aluehallintovirasto voi valvontaa toteuttaessaan varata erityishuollossa olevalle henkilölle tilaisuuden luottamukselliseen keskusteluun aluehallintoviraston edustajan kanssa."}}},{"@id":"sfsd:1977/519/luku/10/pykala/75a/momentti/2/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/75a/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/75a/momentti/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/75a/momentti/2/ajantasa/20160610/fin/txt","content_fi":"Jos toimintayksikössä on käytetty 42 l §:ssä tarkoitettua sitomista, toimintayksikön on tehtävä siitä kahden viikon kuluessa ilmoitus aluehallintovirastolle. Aluehallintovirastolle tehtävässä ilmoituksessa on mainittava henkilön tunnistetiedot, tiedot toimenpiteestä ja sen syystä sekä toimenpiteestä päättäneen lääkärin nimi. Aluehallintoviraston tulee hävittää henkilöä koskevat tunnistetiedot kahden vuoden kuluttua tietojen saamisesta."}}}],"isMemberOf":"sfsd:1977/519/luku/10/pykala/75a","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/10/pykala/76/ajantasa/20100101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2009/1539/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/10/pykala/76/momentti/1/ajantasa/20100101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/76/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/76/momentti/1/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/76/momentti/1/ajantasa/20100101/fin/txt","content_fi":"Jos kehitysvammaisten erityishuollon järjestämisessä tai toteuttamisessa havaitaan asiakasturvallisuutta vaarantavia puutteita tai muita epäkohtia taikka toiminta on muutoin tämän lain vastaista, Sosiaali- ja terveysalan lupa- ja valvontavirasto tai aluehallintovirasto voi antaa määräyksen puutteiden korjaamisesta tai epäkohtien poistamisesta. Määräystä annettaessa on asetettava määräaika, jonka kuluessa tarpeelliset toimenpiteet on suoritettava. Jos asiakasturvallisuus sitä edellyttää, toiminta voidaan määrätä välittömästi keskeytettäväksi taikka toimintayksikön, sen osan tai laitteen käyttö kieltää välittömästi."}}},{"@id":"sfsd:1977/519/luku/10/pykala/76/momentti/2/ajantasa/20100101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/76/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/76/momentti/2/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/76/momentti/2/ajantasa/20100101/fin/txt","content_fi":"Sosiaali- ja terveysalan lupa- ja valvontavirasto tai aluehallintovirasto voi velvoittaa kunnan tai kuntayhtymän noudattamaan 1 momentissa tarkoitettua määräystä sakon uhalla tai uhalla, että toiminta keskeytetään, taikka että toimintayksikön, sen osan tai laitteen käyttö kielletään."}}},{"@id":"sfsd:1977/519/luku/10/pykala/76/momentti/3/ajantasa/20100101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/76/momentti/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/76/momentti/3/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/76/momentti/3/ajantasa/20100101/fin/txt","content_fi":"Sosiaali- ja terveysalan lupa- ja valvontaviraston ja aluehallintoviraston päätöstä toiminnan keskeyttämisestä taikka toimintayksikön, sen osan tai laitteen käytön kieltämisestä on noudatettava muutoksenhausta huolimatta, jollei muutoksenhakuviranomainen toisin määrää."}}},{"@id":"sfsd:1977/519/luku/10/pykala/76/momentti/4/ajantasa/20100101","@type":"sfl:SubsectionVersion","cites":"sfsd:1987/395","isMemberOf":"sfsd:1977/519/luku/10/pykala/76/momentti/4","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/76/momentti/4/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/76/momentti/4/ajantasa/20100101/fin/txt","content_fi":"Mitä tässä pykälässä säädetään, ei koske lääkelaissa (395/1987) tarkoitettua toimintaa, jonka valvonnasta vastaa Lääkealan turvallisuus- ja kehittämiskeskus. Jos Sosiaali- ja terveysalan lupa- ja valvontavirasto tai aluehallintovirasto on valvonnassaan havainnut lääkehuoltoa koskevia puutteita tai muita epäkohtia, niistä on ilmoitettava Lääkealan turvallisuus- ja kehittämiskeskukselle."}}}],"isMemberOf":"sfsd:1977/519/luku/10/pykala/76","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/10/pykala/77/ajantasa/20100101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2009/1539/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/10/pykala/77/momentti/1/ajantasa/20100101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/77/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/77/momentti/1/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/77/momentti/1/ajantasa/20100101/fin/txt","content_fi":"Jos kehitysvammaisten erityishuollon ohjauksen ja valvonnan yhteydessä todetaan, että kunta tai kuntayhtymä on tämän lain mukaista toimintaa järjestäessään tai toteuttaessaan menetellyt virheellisesti tai jättänyt velvollisuutensa täyttämättä, Sosiaali- ja terveysalan lupa- ja valvontavirasto ja aluehallintovirasto voivat antaa kunnalle tai kuntayhtymälle taikka virheellisestä toiminnasta vastuussa olevalle virkamiehelle huomautuksen vastaisen toiminnan varalle."}}},{"@id":"sfsd:1977/519/luku/10/pykala/77/momentti/2/ajantasa/20100101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/77/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/77/momentti/2/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/77/momentti/2/ajantasa/20100101/fin/txt","content_fi":"Sosiaali- ja terveysalan lupa- ja valvontavirasto ja aluehallintovirasto voivat, jos asia ei anna aihetta huomautukseen tai muihin toimenpiteisiin, kiinnittää valvottavan huomiota toiminnan asianmukaiseen järjestämiseen ja hyvän hallintotavan noudattamiseen."}}},{"@id":"sfsd:1977/519/luku/10/pykala/77/momentti/3/ajantasa/20100101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/77/momentti/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/77/momentti/3/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/77/momentti/3/ajantasa/20100101/fin/txt","content_fi":"Tässä pykälässä tarkoitettuun Sosiaali- ja terveysalan lupa- ja valvontaviraston tai aluehallintoviraston antamaan huomautukseen ja huomion kiinnittämiseen ei saa hakea muutosta valittamalla."}}}],"isMemberOf":"sfsd:1977/519/luku/10/pykala/77","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/10/pykala/78/ajantasa/20150101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/78","repealedBy":"sfsd:2014/1105/alkup","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/78/ajantasa/20150101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/78/ajantasa/20150101/fin/txt","content_fi":"78 §(12.12.2014/1105)\n\n78 § on kumottu L:lla 12.12.2014/1105."}}},{"@id":"sfsd:1977/519/luku/10/pykala/79/ajantasa/19840101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1982/702/alkup","hasPart":{"@id":"sfsd:1977/519/luku/10/pykala/79/momentti/1/ajantasa/19840101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/79/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/79/momentti/1/ajantasa/19840101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/79/momentti/1/ajantasa/19840101/fin/txt","content_fi":"Erityishuoltoa järjestävä kunta sekä 16 §:ssä tarkoitettu yksityinen erityishuollon järjestäjä on velvollinen antamaan erityishuoltopiirin kuntainliitolle tämän tarpeelliseksi katsomia tietoja ja selvityksiä."}}},"isMemberOf":"sfsd:1977/519/luku/10/pykala/79","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/10/pykala/80/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":{"@id":"sfsd:1977/519/luku/10/pykala/80/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","cites":"sfsd:1996/586","isMemberOf":"sfsd:1977/519/luku/10/pykala/80/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/80/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/80/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Muuhun sosiaalilautakunnan tai muun sosiaalihuollosta vastaavan toimielimen päätökseen kuin 81 a §:ssä tarkoitettuun päätökseen saa hakea muutosta valittamalla hallinto-oikeuteen siten kuin hallintolainkäyttölaissa (586/1996) säädetään."}}},"isMemberOf":"sfsd:1977/519/luku/10/pykala/80","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/10/pykala/81/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":{"@id":"sfsd:1977/519/luku/10/pykala/81/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","cites":"sfsd:2015/410","isMemberOf":"sfsd:1977/519/luku/10/pykala/81/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/81/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/81/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Erityishuoltopiirin kuntayhtymän yhtymävaltuuston tai yhtymäkokouksen, yhtymähallituksen, lautakunnan, johtokunnan, yhteisjohtokunnan, erityishuollon johtoryhmän tai muun kunnallisen toimielimen tai viranhaltijan päätökseen saa hakea muutosta siten kuin kuntalaissa (410/2015) säädetään, jollei tässä laissa toisin säädetä."}}},"isMemberOf":"sfsd:1977/519/luku/10/pykala/81","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/10/pykala/81a/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":{"@id":"sfsd:1977/519/luku/10/pykala/81a/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/81a/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/81a/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/81a/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Muun kuin tahdosta riippumattoman erityishuollon antamista tai lopettamista koskevaan päätökseen taikka muuhun kuin tahdosta riippumattomaan erityishuoltoon liittyvään yksilöllisen erityishuolto-ohjelman hyväksymistä koskevaan päätökseen saa vaatia oikaisua aluehallintovirastolta siten kuin hallintolaissa säädetään. Oikaisuvaatimukseen annettuun päätökseen saa hakea muutosta valittamalla hallinto-oikeuteen siten kuin hallintolainkäyttölaissa säädetään."}}},"isMemberOf":"sfsd:1977/519/luku/10/pykala/81a","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/10/pykala/81b/ajantasa/20160610","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/johdanto/ajantasa/20160610","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/johdanto","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/johdanto/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/johdanto/ajantasa/20160610/fin/txt","content_fi":"Hallinto-oikeudelta saa hakea valittamalla muutosta siten kuin hallintolainkäyttölaissa säädetään:"}}},{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/1/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/1/ajantasa/20160610/fin/txt","content_fi":"1) 33 §:n 1 momentissa tarkoitettuun kirjalliseen päätökseen, joka koskee henkilön määräämistä toimitettavaksi tutkimukseen;"}}},{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/2/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/2/ajantasa/20160610/fin/txt","content_fi":"2) 33 §:n 3 momentissa tarkoitettuun kirjalliseen päätökseen, joka koskee henkilön määräämistä erityishuollon toimintayksikköön tahdostaan riippumatta;"}}},{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/3/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/3/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/3/ajantasa/20160610/fin/txt","content_fi":"3) 38 §:n 1 momentissa tarkoitettuun kirjalliseen päätökseen, joka koskee tahdosta riippumattoman erityishuollon jatkamista;"}}},{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/4/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/4","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/4/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/4/ajantasa/20160610/fin/txt","content_fi":"4) 42 g §:n 2 momentissa tarkoitettuun aineiden ja esineiden haltuunottoa koskevaan kirjalliseen päätökseen;"}}},{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/5/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/5","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/5/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/5/ajantasa/20160610/fin/txt","content_fi":"5) 42 j §:n 5 momentissa tarkoitettuun kirjalliseen päätökseen, joka koskee välttämättömän terveydenhuollon antamista henkilön vastustuksesta riippumatta;"}}},{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/6/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/6","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/6/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/6/ajantasa/20160610/fin/txt","content_fi":"6) 42 k §:n 3 momentissa tarkoitettuun kirjalliseen päätökseen, joka koskee rajoittavan välineen tai asusteen toistuvaa käyttöä päivittäisissä toiminnoissa;"}}},{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/7/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/7","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/7/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/7/ajantasa/20160610/fin/txt","content_fi":"7) 42 l §:ssä tarkoitettuun kirjalliseen päätökseen, joka koskee rajoittavan välineen tai asusteen käyttöä vakavissa vaaratilanteissa tai sitomista;"}}},{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/8/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/8","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/8/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/8/ajantasa/20160610/fin/txt","content_fi":"8) 42 m §:ssä tarkoitettua valvottua liikkumista koskevaan kirjalliseen päätökseen; ja"}}},{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/9/ajantasa/20160610","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/9","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/9/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/1/kohta/9/ajantasa/20160610/fin/txt","content_fi":"9) 42 n §:n 3 momentissa tarkoitettuun poistumisen estämistä koskevaan kirjalliseen päätökseen."}}}],"isMemberOf":"sfsd:1977/519/luku/10/pykala/81b/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/2/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/81b/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/2/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/2/ajantasa/20160610/fin/txt","content_fi":"Yksityisen sosiaalihuollon toimintayksikön tai laitoksen vastaavan johtajan kirjalliseen päätökseen, joka koskee 42 g §:n 2 momentissa tarkoitettua aineiden ja esineiden haltuunottoa, 42 k §:n 3 momentissa tarkoitettua rajoittavan välineen tai asusteen toistuvaa käyttöä päivittäisissä toiminnoissa, 42 l §:n 3 ja 4 momentissa tarkoitettua rajoittavan välineen tai asusteen käyttöä vakavissa vaaratilanteissa, 42 m §:ssä tarkoitettua valvottua liikkumista ja 42 n §:n 3 momentissa tarkoitettua poistumisen estämistä, sekä yksityisen sosiaalihuollon toimintayksikön tai laitoksen lääkärin kirjalliseen päätökseen, joka koskee 42 l §:n 5 momentissa tarkoitettua sitomista, saa hakea muutosta siten kuin tämän pykälän 1 momentissa säädetään."}}},{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/3/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/81b/momentti/3","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/3/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/81b/momentti/3/ajantasa/20160610/fin/txt","content_fi":"Muutosta saa 1 ja 2 momentissa tarkoitetussa asiassa hakea erityishuollossa oleva henkilö ja hänen laillinen edustajansa. Jollei täysi-ikäisellä erityishuollossa olevalla henkilöllä ole laillista edustajaa, saa muutosta hakea sosiaalihuollon asiakaslain 9 §:n 1 momentissa tarkoitetuissa tilanteissa myös hänen palvelujensa suunnitteluun ja toteuttamiseen osallistuva omaisensa tai muu läheisensä."}}}],"isMemberOf":"sfsd:1977/519/luku/10/pykala/81b","repealedBy":"sfsd:2016/381/alkup","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/10/pykala/81c/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":{"@id":"sfsd:1977/519/luku/10/pykala/81c/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/81c/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/81c/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/81c/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Jos kunnallisen toimielimen tai viranhaltijan päättämä asia voidaan kuntalain 92 §:n nojalla ottaa ylemmän toimielimen käsiteltäväksi, päätökseen saa hakea muutosta siten kuin kuntalaissa säädetään."}}},"isMemberOf":"sfsd:1977/519/luku/10/pykala/81c","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/10/pykala/81d/ajantasa/20160610","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/381/alkup","hasPart":{"@id":"sfsd:1977/519/luku/10/pykala/81d/momentti/1/ajantasa/20160610","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/81d/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/81d/momentti/1/ajantasa/20160610/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/81d/momentti/1/ajantasa/20160610/fin/txt","content_fi":"Hallinto-oikeuden päätökseen 33 §:n 3 momentissa tarkoitetussa asiassa saa hakea muutosta valittamalla korkeimpaan hallinto-oikeuteen. Hallinto-oikeuden muuhun päätökseen saa hakea muutosta valittamalla vain, jos korkein hallinto-oikeus myöntää valitusluvan."}}},"isMemberOf":"sfsd:1977/519/luku/10/pykala/81d","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/10/pykala/82/ajantasa/20100101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2009/1539/alkup","hasPart":[{"@id":"sfsd:1977/519/luku/10/pykala/82/momentti/1/ajantasa/20100101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/82/momentti/1","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/82/momentti/1/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/82/momentti/1/ajantasa/20100101/fin/txt","content_fi":"Muutoksenhausta valtioneuvoston, sosiaali- ja terveysministeriön, aluehallintoviraston ja hallinto-oikeuden päätökseen säädetään hallintolainkäyttölaissa."}}},{"@id":"sfsd:1977/519/luku/10/pykala/82/momentti/2/ajantasa/20100101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/82/momentti/2","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/82/momentti/2/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/82/momentti/2/ajantasa/20100101/fin/txt","content_fi":"Aluehallintoviraston 36 §:n nojalla tekemään päätökseen ei saa hakea muutosta valittamalla."}}}],"isMemberOf":"sfsd:1977/519/luku/10/pykala/82","version":"sfl:Consolidated","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/10/pykala/83/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1977/519/luku/10/pykala/83/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/83/momentti/1","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/83/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/83/momentti/1/alkup/fin/txt","content_fi":"Erityishuoltopiirin kuntainliiton liittovaltuuston, liittohallituksen, lautakunnan, johtokunnan, yhteisjohtokunnan, erityishuollon johtoryhmän tai viranhaltijan päätös voidaan panna täytäntöön, ennenkuin se on saanut lainvoiman, siten kuin kunnallislain 146 §:ssä säädetään. Sanotun säännöksen estämättä voidaan päätös muutoksenhausta huolimatta panna täytäntöön, jos se on laadultaan sellainen, että se on viivytyksettä täytäntöönpantava, tai jos päätöksen voimaantulemista ei voida erityishuollollisista syistä siirtää tuonnemmaksi ja mainittu toimielin on määrännyt päätöksen heti täytäntöönpantavaksi."}}},"isMemberOf":"sfsd:1977/519/luku/10/pykala/83","version":"sfl:Original","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/10/pykala/84/ajantasa/19930101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/84","repealedBy":"sfsd:1992/739/alkup","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/84/ajantasa/19930101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/84/ajantasa/19930101/fin/txt","content_fi":"84 §(3.8.1992/739)\n\n84 § on kumottu L:lla 3.8.1992/739."}}},{"@id":"sfsd:1977/519/luku/10/pykala/85/ajantasa/19840101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/85","repealedBy":"sfsd:1982/702/alkup","version":"sfl:Consolidated","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/85/ajantasa/19840101/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/85/ajantasa/19840101/fin/txt","content_fi":"85 §(17.9.1982/702)\n\n85 § on kumottu L:lla 17.9.1982/702."}}},{"@id":"sfsd:1977/519/luku/10/pykala/86/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1977/519/luku/10/pykala/86/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/86/momentti/1","version":"sfl:Original","versionDate":"2016-06-10","languageVersion":{"@id":"sfsd:1977/519/luku/10/pykala/86/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1977/519/luku/10/pykala/86/momentti/1/alkup/fin/txt","content_fi":"Tarkemmat säännökset tämän lain täytäntöönpanosta ja soveltamisesta annetaan asetuksella."}}},"isMemberOf":"sfsd:1977/519/luku/10/pykala/86","version":"sfl:Original","versionDate":"2016-06-10"},{"@id":"sfsd:1977/519/luku/10/pykala/87/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1977/519/luku/10/pykala/87/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1977/519/luku/10/pykala/87/moment