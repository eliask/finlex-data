-15","languageVersion":{"@id":"sfsd:1996/247/luku/6/pykala/54/momentti/1/kohta/11/ajantasa/20130901/fin","hasFormat":{"@id":"sfsd:1996/247/luku/6/pykala/54/momentti/1/kohta/11/ajantasa/20130901/fin/txt","content_fi":"11) laiminlyö lopetusasetuksen 17 artiklan 1 kohdassa tarkoitetun eläinten hyvinvoinnista vastaavan henkilön nimeämisen teurastamoon taikka"}}},{"@id":"sfsd:1996/247/luku/6/pykala/54/momentti/1/kohta/12/ajantasa/20130901","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/247/luku/6/pykala/54/momentti/1/kohta/12","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/6/pykala/54/momentti/1/kohta/12/ajantasa/20130901/fin","hasFormat":{"@id":"sfsd:1996/247/luku/6/pykala/54/momentti/1/kohta/12/ajantasa/20130901/fin/txt","content_fi":"12) laiminlyö lopetusasetuksen 17 artiklan 5 kohdassa tarkoitetun kirjanpidon toimista eläinten hyvinvoinnin parantamiseksi."}}}],"isMemberOf":"sfsd:1996/247/luku/6/pykala/54/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-15"},"isMemberOf":"sfsd:1996/247/luku/6/pykala/54","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/6/pykala/54/ajantasa/20130901/fin","title_fi":"Eläinsuojelurikkomus"}},{"@id":"sfsd:1996/247/luku/6/pykala/55/ajantasa/20110301","@type":"sfl:SectionVersion","amendedBy":"sfsd:2011/15/alkup","hasPart":[{"@id":"sfsd:1996/247/luku/6/pykala/55/momentti/1/ajantasa/20110301","@type":"sfl:SubsectionVersion","cites":"sfsd:1889/39/luku/17/pykala/23","isMemberOf":"sfsd:1996/247/luku/6/pykala/55/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/6/pykala/55/momentti/1/ajantasa/20110301/fin","hasFormat":{"@id":"sfsd:1996/247/luku/6/pykala/55/momentti/1/ajantasa/20110301/fin/txt","content_fi":"Eläintenpitokiellosta säädetään rikoslain 17 luvun 23 §:ssä."}}},{"@id":"sfsd:1996/247/luku/6/pykala/55/momentti/2/ajantasa/20110301","@type":"sfl:SubsectionVersion","cites":"sfsd:2010/372","isMemberOf":"sfsd:1996/247/luku/6/pykala/55/momentti/2","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/6/pykala/55/momentti/2/ajantasa/20110301/fin","hasFormat":{"@id":"sfsd:1996/247/luku/6/pykala/55/momentti/2/ajantasa/20110301/fin/txt","content_fi":"Jos henkilölle on määrätty eläintenpitokielto, tai muutoksenhakutuomioistuin kieltää henkilölle määrätyn kiellon täytäntöönpanon, tuomioistuimen on ilmoitettava ratkaisusta Oikeusrekisterikeskukselle. Tuomioistuimen velvollisuudesta tehdä ilmoitus merkitsemällä ratkaisusta tieto oikeushallinnon valtakunnallisen tietojärjestelmän ratkaisu- ja päätösilmoitusjärjestelmään tai toimittamalla tiedot Oikeusrekisterikeskukselle säädetään tarvittaessa oikeusministeriön asetuksella. Merkinnän tekemiseen ja tietojen toimittamiseen sovelletaan, mitä oikeushallinnon valtakunnallisesta tietojärjestelmästä annetussa laissa (372/2010) ja sen nojalla säädetään."}}}],"isMemberOf":"sfsd:1996/247/luku/6/pykala/55","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/6/pykala/55/ajantasa/20110301/fin","title_fi":"Eläintenpitokielto"}},{"@id":"sfsd:1996/247/luku/6/pykala/56/ajantasa/19991201","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1996/247/luku/6/pykala/56","repealedBy":"sfsd:1999/662/alkup","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/6/pykala/56/ajantasa/19991201/fin","hasFormat":{"@id":"sfsd:1996/247/luku/6/pykala/56/ajantasa/19991201/fin/txt","content_fi":"56 §(21.5.1999/662)\n\n56 § on kumottu L:lla 21.5.1999/662."}}},{"@id":"sfsd:1996/247/luku/6/pykala/57/ajantasa/20110301","@type":"sfl:SectionVersion","amendedBy":"sfsd:2011/15/alkup","hasPart":{"@id":"sfsd:1996/247/luku/6/pykala/57/momentti/1/ajantasa/20110301","@type":"sfl:SubsectionVersion","cites":"sfsd:1889/39/luku/17/pykala/23a","isMemberOf":"sfsd:1996/247/luku/6/pykala/57/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/6/pykala/57/momentti/1/ajantasa/20110301/fin","hasFormat":{"@id":"sfsd:1996/247/luku/6/pykala/57/momentti/1/ajantasa/20110301/fin/txt","content_fi":"Eläinsuojelurikokseen liittyvästä menettämisseuraamuksesta säädetään rikoslain 17 luvun 23 a §:ssä."}}},"isMemberOf":"sfsd:1996/247/luku/6/pykala/57","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/6/pykala/57/ajantasa/20110301/fin","title_fi":"Menettämisseuraamus"}}],"isMemberOf":"sfsd:1996/247/luku/6","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/6/ajantasa/20130901/fin","title_fi":"Seuraamukset"}},{"@id":"sfsd:1996/247/luku/7/ajantasa/20180815","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1996/247/luku/7/pykala/58/ajantasa/20060501","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1996/247/luku/7/pykala/58/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/247/luku/7/pykala/58/momentti/1","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/58/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1996/247/luku/7/pykala/58/momentti/1/alkup/fin/txt","content_fi":"Eläimen omistajan tai haltijan on suoritettava 42 ja 44 §:ssä tarkoitetuista toimenpiteistä aiheutuvat kustannukset. Jos eläin on myyty tai toimitettu teurastettavaksi tai eläimestä on saatu muuta tuloa, kustannukset on vähennettävä eläimen kauppahinnasta tai eläimestä saadusta muusta tulosta ja jäännös annettava eläimen omistajalle tai haltijalle."}}},{"@id":"sfsd:1996/247/luku/7/pykala/58/momentti/2/ajantasa/20060501","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2006/300/alkup","isMemberOf":"sfsd:1996/247/luku/7/pykala/58/momentti/2","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/58/momentti/2/ajantasa/20060501/fin","hasFormat":{"@id":"sfsd:1996/247/luku/7/pykala/58/momentti/2/ajantasa/20060501/fin/txt","content_fi":"Edellä 44 §:ssä tarkoitetuista toimenpiteistä aiheutuvat kustannukset voidaan erityisistä syistä maksaa etukäteen valtion talousarviossa eläinlääkintähuoltoon varatuista määrärahoista. Eläimen omistaja tai haltija on vastuussa kustannusten lopullisesta suorittamisesta siten kuin 1 momentissa säädetään.\n(28.4.2006/300)"}}},{"@id":"sfsd:1996/247/luku/7/pykala/58/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/247/luku/7/pykala/58/momentti/3","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/58/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1996/247/luku/7/pykala/58/momentti/3/alkup/fin/txt","content_fi":"Kustannukset saadaan periä ilman tuomiota ja päätöstä siinä järjestyksessä kuin verojen ja maksujen perimisestä ulosottotoimin säädetään. Jollei kustannuksia saada perityksi, korvataan ne valtion varoista."}}}],"isMemberOf":"sfsd:1996/247/luku/7/pykala/58","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/58/ajantasa/20060501/fin","title_fi":"Kustannukset"}},{"@id":"sfsd:1996/247/luku/7/pykala/59/ajantasa/20091101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2009/767/alkup","hasPart":{"@id":"sfsd:1996/247/luku/7/pykala/59/momentti/1/ajantasa/20091101","@type":"sfl:SubsectionVersion","cites":"sfsd:2009/765/pykala/23","isMemberOf":"sfsd:1996/247/luku/7/pykala/59/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/59/momentti/1/ajantasa/20091101/fin","hasFormat":{"@id":"sfsd:1996/247/luku/7/pykala/59/momentti/1/ajantasa/20091101/fin/txt","content_fi":"Kunnaneläinlääkärin 39 §:n nojalla suorittamasta tarkastuksesta sekä 48 §:n nojalla suorittamasta selvityksestä tai tutkimuksesta kunnalle valtion varoista maksettavasta korvauksesta säädetään eläinlääkintähuoltolain (765/2009) 23 §:ssä."}}},"isMemberOf":"sfsd:1996/247/luku/7/pykala/59","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/59/ajantasa/20091101/fin","title_fi":"Kunnalle maksettava korvaus"}},{"@id":"sfsd:1996/247/luku/7/pykala/60/ajantasa/20070105","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1996/247/luku/7/pykala/60","repealedBy":"sfsd:2006/1430/alkup","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/60/ajantasa/20070105/fin","hasFormat":{"@id":"sfsd:1996/247/luku/7/pykala/60/ajantasa/20070105/fin/txt","content_fi":"60 §(29.12.2006/1430)\n\n60 § on kumottu L:lla 29.12.2006/1430."}}},{"@id":"sfsd:1996/247/luku/7/pykala/61/ajantasa/20130901","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1996/247/luku/7/pykala/61/momentti/1/ajantasa/20130901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2013/584/alkup","cites":"sfsd:1974/412","isMemberOf":"sfsd:1996/247/luku/7/pykala/61/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/61/momentti/1/ajantasa/20130901/fin","hasFormat":{"@id":"sfsd:1996/247/luku/7/pykala/61/momentti/1/ajantasa/20130901/fin/txt","content_fi":"Kunnaneläinlääkärin ja kunnan terveydensuojeluvalvontaa hoitavan viranhaltijan tämän lain tai sen nojalla annettujen säännösten ja määräysten sekä lopetusasetuksen noudattamista valvoessaan aiheuttamasta mahdollisesta vahingosta vastaa valtio siten kuin vahingonkorvauslaissa (412/1974) julkisyhteisön korvausvastuusta säädetään.\n(9.8.2013/584)"}}},{"@id":"sfsd:1996/247/luku/7/pykala/61/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/247/luku/7/pykala/61/momentti/2","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/61/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1996/247/luku/7/pykala/61/momentti/2/alkup/fin/txt","content_fi":"Valtion oikeuteen periä kunnaneläinlääkäriltä ja kunnan terveydensuojeluvalvontaa hoitavalta viranhaltijalta vahingonkärsijälle maksamansa korvausmäärä noudatetaan soveltuvin osin vahingonkorvauslain työnantajan työntekijään kohdistuvaa regressioikeutta koskevia säännöksiä."}}},{"@id":"sfsd:1996/247/luku/7/pykala/61/momentti/3/ajantasa/20100101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2009/1477/alkup","isMemberOf":"sfsd:1996/247/luku/7/pykala/61/momentti/3","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/61/momentti/3/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:1996/247/luku/7/pykala/61/momentti/3/ajantasa/20100101/fin/txt","content_fi":"Mitä 1 ja 2 momentissa säädetään, sovelletaan myös Elintarviketurvallisuusviraston tai aluehallintoviraston tässä laissa tarkoitettuja tehtäviä suorittamaan määräämän eläinlääkärin mahdollisesti aiheuttamien vahinkojen korvaamiseen ja korvausten takaisinperintään.\n(22.12.2009/1477)"}}}],"isMemberOf":"sfsd:1996/247/luku/7/pykala/61","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/61/ajantasa/20130901/fin","title_fi":"Vahingonkorvaus"}},{"@id":"sfsd:1996/247/luku/7/pykala/62/ajantasa/20180815","@type":"sfl:SectionVersion","amendedBy":"sfsd:1999/662/alkup","hasPart":{"@id":"sfsd:1996/247/luku/7/pykala/62/momentti/1/ajantasa/20180815","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1996/247/luku/7/pykala/62/momentti/1/johdanto/ajantasa/20180815","@type":"sfl:PreambleVersion","amendedBy":"sfsd:2018/668/alkup","cites":"sfsd:1999/621","isMemberOf":"sfsd:1996/247/luku/7/pykala/62/momentti/1/johdanto","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/62/momentti/1/johdanto/ajantasa/20180815/fin","hasFormat":{"@id":"sfsd:1996/247/luku/7/pykala/62/momentti/1/johdanto/ajantasa/20180815/fin/txt","content_fi":"Viranomaisten toiminnan julkisuudesta annetussa laissa (621/1999) säädetyn salassapitovelvollisuuden estämättä saa tämän lain sekä sen nojalla annettujen säännösten ja määräysten noudattamista valvottaessa sekä 48 §:ssä tarkoitettua selvitystä ja tutkimusta suoritettaessa saatuja tietoja yksityisen ja yhteisön taloudellisesta asemasta, liikesalaisuudesta sekä yksityisen henkilökohtaisista oloista luovuttaa: (10.8.2018/668)"}}},{"@id":"sfsd:1996/247/luku/7/pykala/62/momentti/1/kohta/1/ajantasa/19991201","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/247/luku/7/pykala/62/momentti/1/kohta/1","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/62/momentti/1/kohta/1/ajantasa/19991201/fin","hasFormat":{"@id":"sfsd:1996/247/luku/7/pykala/62/momentti/1/kohta/1/ajantasa/19991201/fin/txt","content_fi":"1) valvontaviranomaisille tämän lain mukaisten tehtävien suorittamista varten;"}}},{"@id":"sfsd:1996/247/luku/7/pykala/62/momentti/1/kohta/2/ajantasa/19991201","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/247/luku/7/pykala/62/momentti/1/kohta/2","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/62/momentti/1/kohta/2/ajantasa/19991201/fin","hasFormat":{"@id":"sfsd:1996/247/luku/7/pykala/62/momentti/1/kohta/2/ajantasa/19991201/fin/txt","content_fi":"2) syyttäjä-, poliisi- ja tulliviranomaisille rikoksen selvittämiseksi;"}}},{"@id":"sfsd:1996/247/luku/7/pykala/62/momentti/1/kohta/3/ajantasa/19991201","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1996/247/luku/7/pykala/62/momentti/1/kohta/3","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/62/momentti/1/kohta/3/ajantasa/19991201/fin","hasFormat":{"@id":"sfsd:1996/247/luku/7/pykala/62/momentti/1/kohta/3/ajantasa/19991201/fin/txt","content_fi":"3) Suomea sitovan kansainvälisen sopimuksen tai Euroopan yhteisön oikeusjärjestyksen edellyttämille ulkomaisille toimielimille ja tarkastajille kyseisen sopimuksen tai oikeusjärjestyksen niin edellyttäessä."}}}],"isMemberOf":"sfsd:1996/247/luku/7/pykala/62/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-15"},"isMemberOf":"sfsd:1996/247/luku/7/pykala/62","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/62/ajantasa/20180815/fin","title_fi":"Salassa pidettävien tietojen luovuttaminen"}},{"@id":"sfsd:1996/247/luku/7/pykala/63/ajantasa/20130901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2013/584/alkup","hasPart":{"@id":"sfsd:1996/247/luku/7/pykala/63/momentti/1/ajantasa/20130901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/247/luku/7/pykala/63/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/63/momentti/1/ajantasa/20130901/fin","hasFormat":{"@id":"sfsd:1996/247/luku/7/pykala/63/momentti/1/ajantasa/20130901/fin/txt","content_fi":"Jos on syytä epäillä, että tätä lakia tai sen nojalla annettuja säännöksiä tai määräyksiä taikka lopetusasetusta on rikottu, tässä laissa tarkoitetun valvontaviranomaisen on viipymättä tehtävä asiasta ilmoitus poliisille."}}},"isMemberOf":"sfsd:1996/247/luku/7/pykala/63","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/63/ajantasa/20130901/fin","title_fi":"Ilmoitusvelvollisuus"}},{"@id":"sfsd:1996/247/luku/7/pykala/64/ajantasa/20130901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2013/584/alkup","hasPart":{"@id":"sfsd:1996/247/luku/7/pykala/64/momentti/1/ajantasa/20130901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/247/luku/7/pykala/64/momentti/1","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/64/momentti/1/ajantasa/20130901/fin","hasFormat":{"@id":"sfsd:1996/247/luku/7/pykala/64/momentti/1/ajantasa/20130901/fin/txt","content_fi":"Lapsen huoltajien ja muiden henkilöiden, joilla on valvonnassaan viittätoista vuotta nuorempi lapsi, on huolehdittava siitä, ettei lapsi kohtele eläimiä tämän lain tai sen nojalla annettujen säännösten tai määräysten taikka lopetusasetuksen vastaisesti."}}},"isMemberOf":"sfsd:1996/247/luku/7/pykala/64","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/64/ajantasa/20130901/fin","title_fi":"Valvontavelvoite"}},{"@id":"sfsd:1996/247/luku/7/pykala/65/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1996/247/luku/7/pykala/65/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/247/luku/7/pykala/65/momentti/1","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/65/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1996/247/luku/7/pykala/65/momentti/1/alkup/fin/txt","content_fi":"Tarkemmat säännökset tämän lain täytäntöönpanosta annetaan asetuksella."}}},"isMemberOf":"sfsd:1996/247/luku/7/pykala/65","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/pykala/65/alkup/fin","title_fi":"Tarkemmat säännökset"}}],"isMemberOf":"sfsd:1996/247/luku/7","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/7/ajantasa/20180815/fin","title_fi":"Erinäiset säännökset"}},{"@id":"sfsd:1996/247/luku/8/alkup","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1996/247/luku/8/pykala/66/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1996/247/luku/8/pykala/66/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/247/luku/8/pykala/66/momentti/1","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/8/pykala/66/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1996/247/luku/8/pykala/66/momentti/1/alkup/fin/txt","content_fi":"Tämä laki tulee voimaan 1 päivänä heinäkuuta 1996."}}},{"@id":"sfsd:1996/247/luku/8/pykala/66/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/247/luku/8/pykala/66/momentti/2","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/8/pykala/66/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1996/247/luku/8/pykala/66/momentti/2/alkup/fin/txt","content_fi":"Tällä lailla kumotaan 27 päivänä tammikuuta 1971 annettu eläinsuojelulaki (91/71) siihen myöhemmin tehtyine muutoksineen sekä kotieläinten liikkuvasta kaupasta 10 päivänä toukokuuta 1968 annettu asetus (280/68) siihen myöhemmin tehtyine muutoksineen."}}},{"@id":"sfsd:1996/247/luku/8/pykala/66/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/247/luku/8/pykala/66/momentti/3","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/8/pykala/66/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1996/247/luku/8/pykala/66/momentti/3/alkup/fin/txt","content_fi":"Kumottavan lain 9 §:n 1 momentin ja 11 §:n 2 momentin nojalla 20 päivänä joulukuuta 1985 annettu asetus koe-eläintoiminnasta (1076/85) jää edelleen voimaan."}}},{"@id":"sfsd:1996/247/luku/8/pykala/66/momentti/4/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/247/luku/8/pykala/66/momentti/4","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/8/pykala/66/momentti/4/alkup/fin","hasFormat":{"@id":"sfsd:1996/247/luku/8/pykala/66/momentti/4/alkup/fin/txt","content_fi":"Tämän lain voimaan tullessa vireillä olevat asiat käsitellään tämän lain voimaan tullessa voimassa olleiden säännösten mukaisesti."}}},{"@id":"sfsd:1996/247/luku/8/pykala/66/momentti/5/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/247/luku/8/pykala/66/momentti/5","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/8/pykala/66/momentti/5/alkup/fin","hasFormat":{"@id":"sfsd:1996/247/luku/8/pykala/66/momentti/5/alkup/fin/txt","content_fi":"Ennen lain voimaantuloa voidaan ryhtyä sen täytäntöönpanemiseksi tarvittaviin toimenpiteisiin."}}}],"isMemberOf":"sfsd:1996/247/luku/8/pykala/66","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/8/pykala/66/alkup/fin","title_fi":"Voimaantulo"}},{"@id":"sfsd:1996/247/luku/8/pykala/67/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1996/247/luku/8/pykala/67/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/247/luku/8/pykala/67/momentti/1","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/8/pykala/67/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1996/247/luku/8/pykala/67/momentti/1/alkup/fin/txt","content_fi":"Sen, joka tämän lain voimaan tullessa harjoittaa 21 §:n 2 momentissa, 23 §:n 1 momentissa tai 24 §:n 1 momentissa tarkoitettua toimintaa, on vuoden kuluessa lain voimaantulosta tehtävä toimintaa koskeva ilmoitus lääninhallitukselle."}}},{"@id":"sfsd:1996/247/luku/8/pykala/67/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/247/luku/8/pykala/67/momentti/2","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/8/pykala/67/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1996/247/luku/8/pykala/67/momentti/2/alkup/fin/txt","content_fi":"Toiminnanharjoittajan, joka tämän lain voimaan tullessa kuljettaa eläimiä tulonhankkimistarkoituksessa, on haettava 30 §:ssä tarkoitettu lupa kuuden kuukauden kuluessa tämän lain voimaantulosta."}}},{"@id":"sfsd:1996/247/luku/8/pykala/67/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1996/247/luku/8/pykala/67/momentti/3","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/8/pykala/67/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1996/247/luku/8/pykala/67/momentti/3/alkup/fin/txt","content_fi":"Kunnan on huolehdittava 15 §:n 1 momentissa tarkoitettujen eläinten hoidon järjestämisestä kahden vuoden kuluessa lain voimaantulosta."}}}],"isMemberOf":"sfsd:1996/247/luku/8/pykala/67","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/8/pykala/67/alkup/fin","title_fi":"Siirtymäsäännökset"}}],"isMemberOf":"sfsd:1996/247/luku/8","version":"sfl:Original","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/luku/8/alkup/fin","title_fi":"Voimaantulo- ja siirtymäsäännökset"}}],"isAbout":["http://data.finlex.fi/voc/finlex/kw1293","http://data.finlex.fi/voc/finlex/kw5950"],"isMemberOf":"sfsd:1996/247","relatedTo":"http://data.finlex.fi/eli/he/1995/36","version":"sfl:Consolidated","versionDate":"2018-08-15","languageVersion":{"@id":"sfsd:1996/247/ajantasa/20180815/fin","title_fi":"Eläinsuojelulaki"}},"@context":{"languageVersion":{"@id":"eli:is_realized_by","@type":"@id"},"temporalVersions":{"@id":"eli:has_member","@type":"@id"},"temporalVersion":{"@id":"eli:has_member","@type":"@id"},"hasFormat":{"@id":"eli:is_embodied_by","@type":"@id"},"idLocal":"eli:id_local","dateDocument":{"@id":"eli:date_document","@type":"xsd:date"},"hasPart":{"@id":"eli:has_part","@type":"@id"},"isAbout":{"@id":"eli:is_about","@type":"@id"},"isMemberOf":{"@id":"eli:is_member_of","@type":"@id"},"relatedTo":{"@id":"eli:related_to","@type":"@id"},"version":{"@id":"eli:version","@type":"@id"},"versionDate":{"@id":"eli:version_date","@type":"xsd:date"},"title_fi":{"@id":"eli:title","@language":"fi"},"content_fi":{"@id":"sfl:text","@language":"fi"},"amendedBy":{"@id":"eli:amended_by","@type":"@id"},"cites":{"@id":"eli:cites","@type":"@id"},"repealedBy":{"@id":"eli:repealed_by","@type":"@id"},"inForce":{"@id":"eli:in_force","@type":"@id"},"typeDocument":{"@id":"eli:type_document","@type":"@id"},"dcterms":"http://purl.org/dc/terms/","xsd":"http://www.w3.org/2001/XMLSchema#","common":"http://data.finlex.fi/common/","rdf":"http://www.w3.org/1999/02/22-rdf-syntax-ns#","sfcl":"http://data.finlex.fi/schema/scfl/","sfl":"http://data.finle