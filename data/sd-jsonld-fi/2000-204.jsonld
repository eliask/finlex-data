uku/2/pykala/12/alkup/fin","title_fi":"Edustustojen tehtävät"}},{"@id":"sfsd:2000/204/luku/2/pykala/13/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2000/204/luku/2/pykala/13/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/2/pykala/13/momentti/1","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/2/pykala/13/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/2/pykala/13/momentti/1/alkup/fin/txt","content_fi":"Edustusto voi antaa muille viranomaisille virka-apua, jos se on perusteltua ottaen huomioon edustuston tehtävät, käytettävissä olevat voimavarat ja mahdollisuudet virka-avun antamiseen sekä paikalliset olosuhteet, tai sen mukaan kuin virka-avun antamisesta erikseen säädetään."}}},"isMemberOf":"sfsd:2000/204/luku/2/pykala/13","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/2/pykala/13/alkup/fin","title_fi":"Edustuston antama virka-apu"}}],"isMemberOf":"sfsd:2000/204/luku/2","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/2/alkup/fin","title_fi":"Ulkomaanedustus"}},{"@id":"sfsd:2000/204/luku/3/ajantasa/20150401","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:2000/204/luku/3/pykala/14/ajantasa/20150401","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2000/204/luku/3/pykala/14/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/14/momentti/1","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/14/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/14/momentti/1/alkup/fin/txt","content_fi":"Ulkoasiainhallinnon virat ovat hallinnonalan yhteisiä. Ulkoasiainhallinnon virassa voidaan toimia eri tehtävissä ja virkapaikoissa ulkoasiainhallinnon organisaatiossa siten kuin tässä laissa säädetään."}}},{"@id":"sfsd:2000/204/luku/3/pykala/14/momentti/2/ajantasa/20150401","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2015/258/alkup","isMemberOf":"sfsd:2000/204/luku/3/pykala/14/momentti/2","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/14/momentti/2/ajantasa/20150401/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/14/momentti/2/ajantasa/20150401/fin/txt","content_fi":"Ulkoasiainneuvoksen, lähetystöneuvoksen, ulkoasiainsihteerin, hallinnollisen ulkoasiainsihteerin, avustajan, hallinnollisen avustajan, osastosihteerin, toimistosihteerin, toimistovirkailijan, virastomestarin ja vahtimestarin virat ovat yleisvirkoja. Muut kuin yleisvirat ovat erityisvirkoja.\n(20.3.2015/258)"}}},{"@id":"sfsd:2000/204/luku/3/pykala/14/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/14/momentti/3","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/14/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/14/momentti/3/alkup/fin/txt","content_fi":"Ulkoasiainhallinnon virkojen kelpoisuusehdoista säädetään tarvittaessa valtioneuvoston asetuksella."}}}],"isMemberOf":"sfsd:2000/204/luku/3/pykala/14","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/14/ajantasa/20150401/fin","title_fi":"Ulkoasiainhallinnon virat"}},{"@id":"sfsd:2000/204/luku/3/pykala/15/ajantasa/20150401","@type":"sfl:SectionVersion","amendedBy":"sfsd:2015/258/alkup","hasPart":[{"@id":"sfsd:2000/204/luku/3/pykala/15/momentti/1/ajantasa/20150401","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/15/momentti/1","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/15/momentti/1/ajantasa/20150401/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/15/momentti/1/ajantasa/20150401/fin/txt","content_fi":"Ulkoasiainhallinnon virkoihin nimittää valtioneuvosto tai ulkoasiainministeriö sen mukaan kuin valtioneuvoston asetuksella säädetään."}}},{"@id":"sfsd:2000/204/luku/3/pykala/15/momentti/2/ajantasa/20150401","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/15/momentti/2","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/15/momentti/2/ajantasa/20150401/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/15/momentti/2/ajantasa/20150401/fin/txt","content_fi":"Ulkoasiainhallinnon yleisvirat voidaan täyttää niitä haettavaksi julistamatta."}}}],"isMemberOf":"sfsd:2000/204/luku/3/pykala/15","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/15/ajantasa/20150401/fin","title_fi":"Virkaan nimittäminen"}},{"@id":"sfsd:2000/204/luku/3/pykala/16/ajantasa/20150401","@type":"sfl:SectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/16","repealedBy":"sfsd:2015/258/alkup","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/16/ajantasa/20150401/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/16/ajantasa/20150401/fin/txt","content_fi":"16 §(20.3.2015/258)\n16 § on kumottu L:lla 20.3.2015/258."}}},{"@id":"sfsd:2000/204/luku/3/pykala/17/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2000/204/luku/3/pykala/17/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/17/momentti/1","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/17/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/17/momentti/1/alkup/fin/txt","content_fi":"Ulkoasiainneuvos, lähetystöneuvos, ulkoasiainsihteeri, hallinnollinen ulkoasiainsihteeri, avustaja ja hallinnollinen avustaja ovat määrättäessä velvollisia virassaan siirtymään toiseen tehtävään ja toiseen virkapaikkaan ulkoasiainhallinnon organisaatiossa."}}},{"@id":"sfsd:2000/204/luku/3/pykala/17/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/17/momentti/2","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/17/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/17/momentti/2/alkup/fin/txt","content_fi":"Muu yleisvirassa oleva virkamies (yleisvirkamies) on määrättäessä velvollinen virassaan siirtymään edustustosta ulkoasiainministeriöön ja ulkoasiainministeriössä tehtävästä toiseen."}}},{"@id":"sfsd:2000/204/luku/3/pykala/17/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/17/momentti/3","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/17/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/17/momentti/3/alkup/fin/txt","content_fi":"Muissa kuin 1 ja 2 momentissa tarkoitetuissa tapauksissa yleisvirkamies voidaan virassaan määrätä tehtävään ja siirtää toiseen virkapaikkaan ulkoasiainhallinnon organisaatiossa, jos virkamies tähän suostuu."}}}],"isMemberOf":"sfsd:2000/204/luku/3/pykala/17","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/17/alkup/fin","title_fi":"Yleisvirkamiehen tehtävään määrääminen ja siirtäminen toiseen virkapaikkaan"}},{"@id":"sfsd:2000/204/luku/3/pykala/18/ajantasa/20150401","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2000/204/luku/3/pykala/18/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/18/momentti/1","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/18/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/18/momentti/1/alkup/fin/txt","content_fi":"Erityisvirassa oleva virkamies (erityisvirkamies) voidaan virassaan määrätä tehtävään ja siirtää määräajaksi ulkoasiainministeriöstä edustustoon, jos virkamies tähän suostuu. Määräaikaa voidaan virkamiehen suostumuksella jatkaa."}}},{"@id":"sfsd:2000/204/luku/3/pykala/18/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/18/momentti/2","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/18/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/18/momentti/2/alkup/fin/txt","content_fi":"Erityisvirkamies voidaan siirtää edustustosta ulkoasiainministeriöön 1 momentissa tarkoitetusta määräajasta riippumatta, jos virkamies tähän suostuu tai jos tähän on painavia syitä."}}},{"@id":"sfsd:2000/204/luku/3/pykala/18/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/18/momentti/3","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/18/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/18/momentti/3/alkup/fin/txt","content_fi":"Edustustosta ulkoasiainministeriöön 1 ja 2 momentissa tarkoitetuissa tapauksissa siirtyvä erityisvirkamies on määrättäessä velvollinen siirtymään virkaansa vastaavaan tehtävään."}}},{"@id":"sfsd:2000/204/luku/3/pykala/18/momentti/4/ajantasa/20150401","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/18/momentti/4","repealedBy":"sfsd:2015/258/alkup","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/18/momentti/4/ajantasa/20150401/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/18/momentti/4/ajantasa/20150401/fin/txt","content_fi":"4 momentti on kumottu L:lla 20.3.2015/258."}}}],"isMemberOf":"sfsd:2000/204/luku/3/pykala/18","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/18/ajantasa/20150401/fin","title_fi":"Erityisvirkamiehen tehtävään määrääminen ja siirtäminen toiseen virkapaikkaan"}},{"@id":"sfsd:2000/204/luku/3/pykala/19/ajantasa/20150401","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2000/204/luku/3/pykala/19/momentti/1/ajantasa/20120301","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2012/89/alkup","isMemberOf":"sfsd:2000/204/luku/3/pykala/19/momentti/1","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/19/momentti/1/ajantasa/20120301/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/19/momentti/1/ajantasa/20120301/fin/txt","content_fi":"Tasavallan presidentti määrää edustuston päällikön ja kiertävän suurlähettilään tehtävään.\n(24.2.2012/89)"}}},{"@id":"sfsd:2000/204/luku/3/pykala/19/momentti/2/ajantasa/20150401","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2015/258/alkup","isMemberOf":"sfsd:2000/204/luku/3/pykala/19/momentti/2","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/19/momentti/2/ajantasa/20150401/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/19/momentti/2/ajantasa/20150401/fin/txt","content_fi":"Valtioneuvosto määrää tehtävään valtiosihteerin kansliapäällikkönä. Muihin ulkoasiainhallinnon tehtäviin määrää valtioneuvosto tai ulkoasiainministeriö sen mukaan kuin valtioneuvoston asetuksella säädetään.\n(20.3.2015/258)"}}},{"@id":"sfsd:2000/204/luku/3/pykala/19/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/19/momentti/3","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/19/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/19/momentti/3/alkup/fin/txt","content_fi":"Muu viranomainen ei voi määrätä 1 momentissa tarkoitettua virkamiestä toiseen tehtävään ennen kuin tasavallan presidentti on määrännyt tehtävän päättymään."}}}],"isMemberOf":"sfsd:2000/204/luku/3/pykala/19","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/19/ajantasa/20150401/fin","title_fi":"Tehtävään määräävä viranomainen"}},{"@id":"sfsd:2000/204/luku/3/pykala/20/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2000/204/luku/3/pykala/20/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/20/momentti/1","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/20/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/20/momentti/1/alkup/fin/txt","content_fi":"Sijaisuuden tai avoinna olevan tehtävän hoidon väliaikaiseksi järjestämiseksi tai muusta erityisestä syystä ulkoasiainhallinnon virkamies voidaan määrätä tehtävään väliaikaisesti määräajaksi tai muutoin rajoitetuksi ajaksi, enintään yhdeksi vuodeksi. Väliaikaisesti ei kuitenkaan voida määrätä edustuston päällikön tehtävään."}}},{"@id":"sfsd:2000/204/luku/3/pykala/20/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/20/momentti/2","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/20/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/20/momentti/2/alkup/fin/txt","content_fi":"Väliaikaisesta tehtävään määräämisestä päättää ulkoasiainministeriö. Tehtävään voidaan erityisestä syystä väliaikaisesti määrätä virkamies, joka ei täytä tehtävään vaadittavia kelpoisuusehtoja."}}},{"@id":"sfsd:2000/204/luku/3/pykala/20/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/20/momentti/3","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/20/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/20/momentti/3/alkup/fin/txt","content_fi":"Virkamies voidaan määrätä tehtävään väliaikaisesti tai määrätä toiseen tehtävään kesken 1 momentissa tarkoitetun määräajan 17 ja 18 §:ssä säädetyin rajoituksin."}}}],"isMemberOf":"sfsd:2000/204/luku/3/pykala/20","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/20/alkup/fin","title_fi":"Väliaikainen tehtävään määrääminen"}},{"@id":"sfsd:2000/204/luku/3/pykala/21/ajantasa/20150401","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2000/204/luku/3/pykala/21/momentti/1/alkup","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:2000/204/luku/3/pykala/21/momentti/1/johdanto/alkup","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/21/momentti/1/johdanto","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/21/momentti/1/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/21/momentti/1/johdanto/alkup/fin/txt","content_fi":"Virkamiehellä tehtävään määräämisen johdosta oleva tehtävä ja 8–10 §:ssä tarkoitetut valtuudet päättyvät"}}},{"@id":"sfsd:2000/204/luku/3/pykala/21/momentti/1/kohta/1/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/21/momentti/1/kohta/1","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/21/momentti/1/kohta/1/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/21/momentti/1/kohta/1/alkup/fin/txt","content_fi":"1) toiseen tehtävään määräämisen perusteella;"}}},{"@id":"sfsd:2000/204/luku/3/pykala/21/momentti/1/kohta/2/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/21/momentti/1/kohta/2","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/21/momentti/1/kohta/2/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/21/momentti/1/kohta/2/alkup/fin/txt","content_fi":"2) määräajan päättyessä, jos tehtävä on määräaikainen; tai"}}},{"@id":"sfsd:2000/204/luku/3/pykala/21/momentti/1/kohta/3/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/21/momentti/1/kohta/3","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/21/momentti/1/kohta/3/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/21/momentti/1/kohta/3/alkup/fin/txt","content_fi":"3) päätöksellä, jonka tekee tehtävään määräävä tai valtuutuksesta päättävä viranomainen."}}}],"isMemberOf":"sfsd:2000/204/luku/3/pykala/21/momentti/1","version":"sfl:Original","versionDate":"2015-04-01"},{"@id":"sfsd:2000/204/luku/3/pykala/21/momentti/2/ajantasa/20150401","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/21/momentti/2","repealedBy":"sfsd:2015/258/alkup","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/21/momentti/2/ajantasa/20150401/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/21/momentti/2/ajantasa/20150401/fin/txt","content_fi":"2 momentti on kumottu L:lla 20.3.2015/258."}}},{"@id":"sfsd:2000/204/luku/3/pykala/21/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/21/momentti/3","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/21/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/21/momentti/3/alkup/fin/txt","content_fi":"Määräaikaisen tehtävän päätyttyä virkamies palaa aikaisempaan tehtäväänsä, jollei häntä määrätä toiseen tehtävään."}}}],"isMemberOf":"sfsd:2000/204/luku/3/pykala/21","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/21/ajantasa/20150401/fin","title_fi":"Tehtävän ja valtuuksien päättyminen"}},{"@id":"sfsd:2000/204/luku/3/pykala/22/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2000/204/luku/3/pykala/22/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/22/momentti/1","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/22/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/22/momentti/1/alkup/fin/txt","content_fi":"Ulkoasiainhallinnon virkamiehen virkapaikasta päätetään virkaan nimittämistä tai tehtävään määräämistä koskevassa päätöksessä tai määrätään erillisellä päätöksellä. Edellä 10 §:n 1 momentissa tarkoitetuissa tapauksissa virkamiehen virkapaikka määräytyy ensiksi määrätyn tehtävän mukaisesti, jollei tasavallan presidentti toisin päätä."}}},{"@id":"sfsd:2000/204/luku/3/pykala/22/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/22/momentti/2","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/22/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/22/momentti/2/alkup/fin/txt","content_fi":"Erillisen päätöksen virkapaikasta tekee ulkoasiainministeriö. Tasavallan presidentin määräämässä tehtävässä olevan virkamiehen osalta virkapaikasta ei kuitenkaan voida päättää ennen kuin tasavallan presidentti on määrännyt tehtävän päättymään. Muutoin virkapaikasta voidaan päättää 7 §:n 2 momentissa sekä 17 ja 18 §:ssä säädetyin rajoituksin."}}},{"@id":"sfsd:2000/204/luku/3/pykala/22/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/22/momentti/3","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/22/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/22/momentti/3/alkup/fin/txt","content_fi":"Jos virkapaikasta ei ole päätetty 1 ja 2 momentin mukaisesti, virkapaikkana on ulkoasiainministeriö."}}}],"isMemberOf":"sfsd:2000/204/luku/3/pykala/22","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/22/alkup/fin","title_fi":"Virkapaikan määrääminen"}},{"@id":"sfsd:2000/204/luku/3/pykala/23/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2000/204/luku/3/pykala/23/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/23/momentti/1","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/23/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/23/momentti/1/alkup/fin/txt","content_fi":"Tasavallan presidentti päättää agrementin pyytämisestä vastaanottajavaltiolta virkamiehelle, joka aiotaan määrätä diplomaattisen edustuston päälliköksi, sivuakkreditoida tai valtuuttaa toimimaan kiertävänä suurlähettiläänä."}}},"isMemberOf":"sfsd:2000/204/luku/3/pykala/23","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/23/alkup/fin","title_fi":"Päätös agrementin pyytämisestä"}},{"@id":"sfsd:2000/204/luku/3/pykala/24/ajantasa/20001201","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2000/204/luku/3/pykala/24/momentti/1/ajantasa/20001201","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/24/momentti/1","repealedBy":"sfsd:2000/962/alkup","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/24/momentti/1/ajantasa/20001201/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/24/momentti/1/ajantasa/20001201/fin/txt","content_fi":"1 momentti on kumottu L:lla 17.11.2000/962."}}},{"@id":"sfsd:2000/204/luku/3/pykala/24/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/3/pykala/24/momentti/2","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/24/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/3/pykala/24/momentti/2/alkup/fin/txt","content_fi":"Ulkomaanedustuksessa palveleva ulkoasiainhallinnon virkamies vastaa häntä henkilökohtaisesti koskevassa riita-asiassa ja myös rikosasiassa, jos rikos on tehty ulkomailla eikä asia lain mukaan kuulu ylemmän tuomioistuimen käsiteltäväksi, Helsingin käräjäoikeudessa."}}}],"isMemberOf":"sfsd:2000/204/luku/3/pykala/24","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/pykala/24/ajantasa/20001201/fin","title_fi":"Oikeuspaikka"}}],"isMemberOf":"sfsd:2000/204/luku/3","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/3/ajantasa/20150401/fin","title_fi":"Ulkoasiainhallinnon virkamiehet"}},{"@id":"sfsd:2000/204/luku/4/ajantasa/20030401","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:2000/204/luku/4/pykala/25/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2000/204/luku/4/pykala/25/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/4/pykala/25/momentti/1","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/25/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/4/pykala/25/momentti/1/alkup/fin/txt","content_fi":"Edustustossa voi toimia puolustusvoimien virkamiehiä sotilas-, laivasto- ja ilmailuasiamiesten sekä sotilasedustajan tehtävissä ja näitä avustavaa puolustusvoimien palvelussuhteessa olevaa henkilökuntaa."}}},{"@id":"sfsd:2000/204/luku/4/pykala/25/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/4/pykala/25/momentti/2","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/25/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/4/pykala/25/momentti/2/alkup/fin/txt","content_fi":"Puolustushallinnon toimialaan kuuluvissa tehtävissä 1 momentissa tarkoitetut henkilöt toimivat sanotun hallinnonalan viranomaisten ohjauksessa ja valvonnassa. Muutoin tässä tarkoitetut henkilöt toimivat edustuston päällikön alaisena sekä ulkoasiainministeriön ohjauksessa ja valvonnassa tämän lain 2 §:n 2 momentin ja 3 §:n mukaisesti."}}},{"@id":"sfsd:2000/204/luku/4/pykala/25/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/4/pykala/25/momentti/3","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/25/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/4/pykala/25/momentti/3/alkup/fin/txt","content_fi":"Tässä pykälässä tarkoitettujen henkilöiden nimittämisestä ja tehtävään määräämisestä, sotilaallisen käskyvallan alaisuudesta sekä ohjauksesta ja valvonnasta puolustushallinnon toimialalla säädetään erikseen."}}}],"isMemberOf":"sfsd:2000/204/luku/4/pykala/25","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/25/alkup/fin","title_fi":"Sotilasasiamiehet ja muut puolustusvoimien palvelussuhteessa olevat henkilöt edustustossa"}},{"@id":"sfsd:2000/204/luku/4/pykala/26/ajantasa/20021001","@type":"sfl:SectionVersion","amendedBy":"sfsd:2002/763/alkup","hasPart":[{"@id":"sfsd:2000/204/luku/4/pykala/26/momentti/1/ajantasa/20021001","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/4/pykala/26/momentti/1","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/26/momentti/1/ajantasa/20021001/fin","hasFormat":{"@id":"sfsd:2000/204/luku/4/pykala/26/momentti/1/ajantasa/20021001/fin/txt","content_fi":"Edustuston henkilökunnan jäsenenä voi ulkoasiainministeriön suostumuksella toimia eri hallinnonalojen viranomaisiin palvelussuhteessa olevia virkamiehiä ja muita henkilöitä asianomaisen viranomaisen tehtävissä, jotka liittyvät ulkomaanedustuksen toimintaan. Valtioneuvoston asetuksella säädetään tarkemmin edellä tarkoitetusta henkilöstöstä ja tehtävistä. Edustuston henkilökunnan jäsenenä voi samoin edellytyksin toimia myös eduskunnan kanslian henkilöstöä."}}},{"@id":"sfsd:2000/204/luku/4/pykala/26/momentti/2/ajantasa/20021001","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/4/pykala/26/momentti/2","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/26/momentti/2/ajantasa/20021001/fin","hasFormat":{"@id":"sfsd:2000/204/luku/4/pykala/26/momentti/2/ajantasa/20021001/fin/txt","content_fi":"Edellä 1 momentissa tarkoitetut henkilöt ovat edustuston päällikön alaisia Suomen ja vieraan valtion tai kansainvälisen järjestön välisiä yleisiä suhteita koskevissa asioissa sekä edustuston sisäistä hallintoa ja järjestystä koskevissa asioissa. Ulkoasiainministeriö voi poistaa tässä tarkoitetun henkilön edustuston henkilökunnan jäsenyydestä, jos siihen on painavia syitä."}}},{"@id":"sfsd:2000/204/luku/4/pykala/26/momentti/3/ajantasa/20021001","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/4/pykala/26/momentti/3","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/26/momentti/3/ajantasa/20021001/fin","hasFormat":{"@id":"sfsd:2000/204/luku/4/pykala/26/momentti/3/ajantasa/20021001/fin/txt","content_fi":"Muilta osin tässä pykälässä tarkoitettujen henkilöiden toiminnassa sovelletaan asianomaista viranomaista koskevia säännöksiä."}}}],"isMemberOf":"sfsd:2000/204/luku/4/pykala/26","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/26/ajantasa/20021001/fin","title_fi":"Eräisiin viranomaisiin palvelussuhteessa olevat henkilöt edustustossa"}},{"@id":"sfsd:2000/204/luku/4/pykala/27/ajantasa/20021001","@type":"sfl:SectionVersion","amendedBy":"sfsd:2002/763/alkup","hasPart":[{"@id":"sfsd:2000/204/luku/4/pykala/27/momentti/1/ajantasa/20021001","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/4/pykala/27/momentti/1","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/27/momentti/1/ajantasa/20021001/fin","hasFormat":{"@id":"sfsd:2000/204/luku/4/pykala/27/momentti/1/ajantasa/20021001/fin/txt","content_fi":"Edustuston henkilökunnan jäsenenä voi toimia henkilö, joka ei ole palvelussuhteessa julkisoikeudelliseen yhteisöön, jos tähän on erityisiä syitä ja henkilön tehtävät liittyvät ulkomaanedustuksen toimintaan. Tässä tarkoitettu henkilö on velvollinen toimimaan yhteistyössä edustuston kanssa ja noudattamaan edustuston sisäistä hallintoa ja järjestystä koskevia määräyksiä ja ohjeita."}}},{"@id":"sfsd:2000/204/luku/4/pykala/27/momentti/2/ajantasa/20021001","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/4/pykala/27/momentti/2","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/27/momentti/2/ajantasa/20021001/fin","hasFormat":{"@id":"sfsd:2000/204/luku/4/pykala/27/momentti/2/ajantasa/20021001/fin/txt","content_fi":"Ulkoasiainministeriö päättää 1 momentissa tarkoitetun henkilön hyväksymisestä edustuston lähetetyn henkilökunnan jäseneksi määräajaksi tai erityisestä syystä toistaiseksi. Ulkoasiainministeriö voi perustellusta syystä poistaa henkilön edustuston henkilökunnan jäsenyydestä."}}},{"@id":"sfsd:2000/204/luku/4/pykala/27/momentti/3/ajantasa/20021001","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/4/pykala/27/momentti/3","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/27/momentti/3/ajantasa/20021001/fin","hasFormat":{"@id":"sfsd:2000/204/luku/4/pykala/27/momentti/3/ajantasa/20021001/fin/txt","content_fi":"Valtioneuvoston asetuksella voidaan antaa tätä pykälää tarkentavia säännöksiä."}}}],"isMemberOf":"sfsd:2000/204/luku/4/pykala/27","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/27/ajantasa/20021001/fin","title_fi":"Edustuston henkilökunnan jäsenyys eräissä tapauksissa"}},{"@id":"sfsd:2000/204/luku/4/pykala/28/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2000/204/luku/4/pykala/28/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/4/pykala/28/momentti/1","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/28/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/4/pykala/28/momentti/1/alkup/fin/txt","content_fi":"Tasavallan presidentti voi myöntää ulkoasiainhallinnon virkamiehelle arvonimen vastaavan viran tai tehtävän sitä seuraamatta käytettäväksi sinä aikana, jonka virkamies on ulkoasiainhallinnon palveluksessa."}}},{"@id":"sfsd:2000/204/luku/4/pykala/28/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/4/pykala/28/momentti/2","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/28/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/4/pykala/28/momentti/2/alkup/fin/txt","content_fi":"Ulkoasiainhallinnossa muutoin kuin 1 momentissa tarkoitetuissa tapauksissa käytettävistä arvonimistä ja virka-arvoista säädetään valtioneuvoston asetuksella."}}}],"isMemberOf":"sfsd:2000/204/luku/4/pykala/28","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/28/alkup/fin","title_fi":"Arvonimet ja virka-arvot"}},{"@id":"sfsd:2000/204/luku/4/pykala/29/ajantasa/20030401","@type":"sfl:SectionVersion","amendedBy":"sfsd:2003/177/alkup","hasPart":{"@id":"sfsd:2000/204/luku/4/pykala/29/momentti/1/ajantasa/20030401","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/4/pykala/29/momentti/1","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/29/momentti/1/ajantasa/20030401/fin","hasFormat":{"@id":"sfsd:2000/204/luku/4/pykala/29/momentti/1/ajantasa/20030401/fin/txt","content_fi":"Tasavallan presidentti päättää ilman valtioneuvoston ratkaisuehdotusta 28 §:n 1 momentissa tarkoitetun arvonimen myöntämisestä sekä toisen valtion pyytämästä agrementista."}}},"isMemberOf":"sfsd:2000/204/luku/4/pykala/29","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/29/ajantasa/20030401/fin","title_fi":"Tasavallan presidentin päätöksenteko ilman valtioneuvoston ratkaisuehdotusta"}},{"@id":"sfsd:2000/204/luku/4/pykala/30/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2000/204/luku/4/pykala/30/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/4/pykala/30/momentti/1","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/30/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/4/pykala/30/momentti/1/alkup/fin/txt","content_fi":"Valittamalla ei saa hakea muutosta päätökseen tehtävään määräämisestä eikä tehtävän päättymisestä, jos päätös on tehty virkamiehen suostumuksella eikä virkapaikka päätöksen johdosta muutu. Valittamalla ei myöskään saa hakea muutosta päätökseen, joka koskee edustuston henkilökunnan jäsenyyttä tai virka-arvoa."}}},{"@id":"sfsd:2000/204/luku/4/pykala/30/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/4/pykala/30/momentti/2","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/30/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/4/pykala/30/momentti/2/alkup/fin/txt","content_fi":"Muutoksen hakemisesta tässä laissa tarkoitettuun päätökseen muissa kuin 1 momentissa tarkoitetuissa tapauksissa säädetään hallintolainkäyttölaissa (586/1996)."}}},{"@id":"sfsd:2000/204/luku/4/pykala/30/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/4/pykala/30/momentti/3","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/30/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/4/pykala/30/momentti/3/alkup/fin/txt","content_fi":"Virkapaikkaa, tehtävään määräämistä tai tehtävän päättymistä koskeva päätös voidaan panna täytäntöön lainvoimaa vailla olevana."}}}],"isMemberOf":"sfsd:2000/204/luku/4/pykala/30","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/30/alkup/fin","title_fi":"Muutoksenhaku"}},{"@id":"sfsd:2000/204/luku/4/pykala/31/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2000/204/luku/4/pykala/31/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/4/pykala/31/momentti/1","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/31/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/4/pykala/31/momentti/1/alkup/fin/txt","content_fi":"Valtioneuvoston asetuksella voidaan antaa tarkempia säännöksiä tämän lain täytäntöönpanosta."}}},"isMemberOf":"sfsd:2000/204/luku/4/pykala/31","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/pykala/31/alkup/fin","title_fi":"Tarkemmat säännökset"}}],"isMemberOf":"sfsd:2000/204/luku/4","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/4/ajantasa/20030401/fin","title_fi":"Erityiset säännökset"}},{"@id":"sfsd:2000/204/luku/5/alkup","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:2000/204/luku/5/pykala/32/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2000/204/luku/5/pykala/32/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/5/pykala/32/momentti/1","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/5/pykala/32/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/5/pykala/32/momentti/1/alkup/fin/txt","content_fi":"Tämä laki tulee voimaan 1 päivänä maaliskuuta 2000."}}},{"@id":"sfsd:2000/204/luku/5/pykala/32/momentti/2/alkup","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:2000/204/luku/5/pykala/32/momentti/2/johdanto/alkup","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:2000/204/luku/5/pykala/32/momentti/2/johdanto","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/5/pykala/32/momentti/2/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/5/pykala/32/momentti/2/johdanto/alkup/fin/txt","content_fi":"Tällä lailla kumotaan:"}}},{"@id":"sfsd:2000/204/luku/5/pykala/32/momentti/2/kohta/1/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2000/204/luku/5/pykala/32/momentti/2/kohta/1","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/5/pykala/32/momentti/2/kohta/1/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/5/pykala/32/momentti/2/kohta/1/alkup/fin/txt","content_fi":"1) ulkoasiainhallinnosta 23 päivänä joulukuuta 1987 annettu laki (1164/1987); sekä"}}},{"@id":"sfsd:2000/204/luku/5/pykala/32/momentti/2/kohta/2/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2000/204/luku/5/pykala/32/momentti/2/kohta/2","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/5/pykala/32/momentti/2/kohta/2/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/5/pykala/32/momentti/2/kohta/2/alkup/fin/txt","content_fi":"2) 22 päivänä huhtikuuta 1999 annetun konsulipalvelulain (498/1999) 3 §."}}}],"isMemberOf":"sfsd:2000/204/luku/5/pykala/32/momentti/2","version":"sfl:Original","versionDate":"2015-04-01"},{"@id":"sfsd:2000/204/luku/5/pykala/32/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/5/pykala/32/momentti/3","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/5/pykala/32/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/5/pykala/32/momentti/3/alkup/fin/txt","content_fi":"Ennen lain voimaantuloa voidaan ryhtyä sen täytäntöönpanon edellyttämiin toimenpiteisiin."}}}],"isMemberOf":"sfsd:2000/204/luku/5/pykala/32","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/5/pykala/32/alkup/fin","title_fi":"Voimaantulo"}},{"@id":"sfsd:2000/204/luku/5/pykala/33/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2000/204/luku/5/pykala/33/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/5/pykala/33/momentti/1","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/5/pykala/33/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/5/pykala/33/momentti/1/alkup/fin/txt","content_fi":"Virkamies, jonka virkapaikka lain voimaan tullessa on kunniakonsulaatti, voi tämän lain 7 §:n 2 momentin estämättä toimia kunniakonsulaatissa 31 päivään joulukuuta 2001, jollei toisin säädetä tai tämän lain nojalla toisin päätetä."}}},{"@id":"sfsd:2000/204/luku/5/pykala/33/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/5/pykala/33/momentti/2","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/5/pykala/33/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/5/pykala/33/momentti/2/alkup/fin/txt","content_fi":"Virkamieheen, jolla tämän lain voimaan tullessa on aikaisemman lain nojalla suppeampi siirtymisvelvollisuus kuin hänellä olisi tämän lain mukaan, sovelletaan tämän lain voimaan tullessa voimassa olleita siirtymisvelvollisuutta koskevia säännöksiä niin kauan kuin hän pysyy siinä virassa, jossa hän on tämän lain voimaan tullessa."}}},{"@id":"sfsd:2000/204/luku/5/pykala/33/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2000/204/luku/5/pykala/33/momentti/3","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/5/pykala/33/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:2000/204/luku/5/pykala/33/momentti/3/alkup/fin/txt","content_fi":"Edustuston henkilökunnan jäsenenä tämän lain voimaan tullessa oleva henkilö, joka ei ole ulkoasiainhallinnon palveluksessa ja jota tämän lain nojalla ei voitaisi ottaa edustuston henkilökunnan jäseneksi, voidaan pitää jäsenenä 31 päivään joulukuuta 2001, jollei toisin säädetä tai tämän lain nojalla toisin päätetä."}}}],"isMemberOf":"sfsd:2000/204/luku/5/pykala/33","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/5/pykala/33/alkup/fin","title_fi":"Siirtymäsäännös"}}],"isMemberOf":"sfsd:2000/204/luku/5","version":"sfl:Original","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/luku/5/alkup/fin","title_fi":"Voimaantulosäännökset"}}],"isAbout":["http://data.finlex.fi/voc/finlex/kw3944","http://data.finlex.fi/voc/finlex/kw1598","http://data.finlex.fi/voc/finlex/kw2876","http://data.finlex.fi/voc/finlex/kw4087"],"isMemberOf":"sfsd:2000/204","relatedTo":"http://data.finlex.fi/eli/he/1999/139","version":"sfl:Consolidated","versionDate":"2015-04-01","languageVersion":{"@id":"sfsd:2000/204/ajantasa/20150401/fin","title_fi":"Ulkoasiainhallintolaki"}},"@context":{"languageVersion":{"@id":"eli:is_realized_by","@type":"@id"},"temporalVersions":{"@id":"eli:has_member","@type":"@id"},"temporalVersion":{"@id":"eli:has_member","@type":"@id"},"hasFormat":{"@id":"eli:is_embodied_by","@type":"@id"},"idLocal":"eli:id_local","dateDocument":{"@id":"eli:date_document","@type":"xsd:date"},"hasPart":{"@id":"eli:has_part","@type":"@id"},"isAbout":{"@id":"eli:is_about","@type":"@id"},"isMemberOf":{"@id":"eli:is_member_of","@type":"@id"},"relatedTo":{"@id":"eli:related_to","@type":"@id"},"version":{"@id":"eli:version","@type":"@id"},"versionDate":{"@id":"eli:version_date","@type":"xsd:date"},"title_fi":{"@id":"eli:title","@language":"fi"},"content_fi":{"@id":"sfl:text","@language":"fi"},"amendedBy":{"@id":"eli:amended_by","@type":"@id"},"repealedBy":{"@id":"eli:repealed_by","@type":"@id"},"inForce":{"@id":"eli:in_force","@type":"@id"},"typeDocument":{"@id":"eli:type_document","@type":"@id"},"dcterms":"http://purl.org/dc/terms/","xsd":"http://www.w3.org/2001/XMLSchema#","common":"http://data.finlex.fi/common/","rdf":"http://www.w3.org/1999/02/22-rdf-syntax-ns#","sfcl":"http://data.finlex.fi/schema/scfl/","sfl":"http://data.finlex.fi/schema/sfl/","sfecli":"http://data.finlex.fi/ecli/","sfsd":"http://data.finlex.fi/eli/sd/","skos":"http://www.w3.org/2004/02/skos/core#","text":"http://jena.apache.org/text#","eli":"http://data.europa.eu/eli/ontology#"}}