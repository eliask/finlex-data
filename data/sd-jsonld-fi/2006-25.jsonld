{"@id":"sfsd:2006/25","@type":["http://data.finlex.fi/schema/sfl/NewStatute"],"idLocal":["25/2006"],"inForce":["http://data.europa.eu/eli/ontology#InForce-inForce"],"temporalVersions":["http://data.finlex.fi/eli/sd/2006/25/alkup","http://data.finlex.fi/eli/sd/2006/25/ajantasa/20080101"],"typeDocument":["http://data.finlex.fi/schema/sfl/Act"],"temporalVersion":{"@id":"sfsd:2006/25/ajantasa/20080101","@type":"sfl:StatuteVersion","dateDocument":"2006-01-13","hasPart":[{"@id":"sfsd:2006/25/johtolause/alkup","@type":"sfl:EnactingClauseVersion","isMemberOf":"sfsd:2006/25/johtolause","version":"sfl:Original","versionDate":"2008-01-01","languageVersion":{"@id":"sfsd:2006/25/johtolause/alkup/fin","hasFormat":{"@id":"sfsd:2006/25/johtolause/alkup/fin/txt","content_fi":"Eduskunnan päätöksen mukaisesti säädetään:"}}},{"@id":"sfsd:2006/25/pykala/1/ajantasa/20080101","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2006/25/pykala/1/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2006/25/pykala/1/momentti/1","version":"sfl:Original","versionDate":"2008-01-01","languageVersion":{"@id":"sfsd:2006/25/pykala/1/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2006/25/pykala/1/momentti/1/alkup/fin/txt","content_fi":"Elintarvikkeiden ja maa- ja metsätalouden tuotantotarvikkeiden turvallisuuden ja laadun, eläinten terveyden ja hyvinvoinnin sekä kasvinterveyden valvontaa ja tutkimusta varten on Elintarviketurvallisuusvirasto. Virasto kuuluu maa- ja metsätalousministeriön toimialaan."}}},{"@id":"sfsd:2006/25/pykala/1/momentti/2/ajantasa/20080101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2006/25/pykala/1/momentti/2","repealedBy":"sfsd:2007/990/alkup","version":"sfl:Consolidated","versionDate":"2008-01-01","languageVersion":{"@id":"sfsd:2006/25/pykala/1/momentti/2/ajantasa/20080101/fin","hasFormat":{"@id":"sfsd:2006/25/pykala/1/momentti/2/ajantasa/20080101/fin/txt","content_fi":"2 momentti on kumottu L:lla 9.11.2007/990."}}}],"isMemberOf":"sfsd:2006/25/pykala/