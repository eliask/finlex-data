ersion":"sfl:Consolidated","versionDate":"2018-01-01"},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/34/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/34/momentti/3","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/34/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/34/momentti/3/alkup/fin/txt","content_fi":"Mikäli rakennus tai rakennelma ei ole puinen eikä kivinen se luetaan siihen ryhmään, jota se kestävyydeltään lähinnä vastaa."}}},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/34/momentti/3/ajantasa/19930101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/34/momentti/3","repealedBy":"sfsd:1992/1539/alkup","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/34/momentti/3/ajantasa/19930101/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/34/momentti/3/ajantasa/19930101/fin/txt","content_fi":"3 momentti on kumottu L:lla 30.12.1992/1539."}}}],"isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/34","version":"sfl:Consolidated","versionDate":"2018-01-01"},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/35/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/35/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/35/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/35/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/35/momentti/1/alkup/fin/txt","content_fi":"Muun kuin 30 ja 31 §:ssä tarkoitetun käyttöomaisuuden hankintamenoksi katsotaan myös omaisuuden laajennus-, muutos-, uudistus- sekä muut vastaavat perusparannusmenot sekä, verovelvollisen vaatiessa, suurehkot korjausmenot."}}},"isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/35","version":"sfl:Original","versionDate":"2018-01-01"},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/36/ajantasa/19680624","@type":"sfl:SectionVersion","amendedBy":"sfsd:1980/846/alkup","hasPart":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/36/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/36/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/36/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/36/momentti/1/alkup/fin/txt","content_fi":"Väestönsuojan hankintameno, väestönsuojaa vastaava osa rakennuksen hankintamenosta sekä vesistön pilaantumisen ja ulkoilman saastumisen estämiseksi hankittujen. rakennelmien, laitteiden, koneiden ja muiden hyödykkeiden hankintameno vähennetään enintään 25 prosentin vuotuisin poistoin."}}},"isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/36","version":"sfl:Consolidated","versionDate":"2018-01-01"},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/37/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/37/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/37/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/37/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/37/momentti/1/alkup/fin/txt","content_fi":"Patentin ja muun voimassaoloajaltaan rajoitetun erikseen luovutettavissa olevan aineettoman oikeuden, sekä sellaisen erikseen luovutettavissa olevan aineettoman oikeuden, jonka voimassaoloaikaa ei ole rajoitettu, hankintameno poistetaan yhtä suurin vuotuisin poistoin 10 vuodessa tai verovelvollisen todennäköiseksi osoittamana sitä lyhyempänä oikeuden taloudellisena käyttöaikana"}}},"isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/37","version":"sfl:Original","versionDate":"2018-01-01"},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/38/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/38/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/38/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/38/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/38/momentti/1/alkup/fin/txt","content_fi":"Soran- ja hiekanottopaikan, kaivoksen, kivilouhoksen, turvesuon ja muun sellaisen omaisuuden hankintamenosta poistetaan kunakin verovuonna käytettyä ainesosamäärää vastaava osa."}}},"isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/38","version":"sfl:Original","versionDate":"2018-01-01"},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/39/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/39/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/39/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/39/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/39/momentti/1/alkup/fin/txt","content_fi":"Muun kuin edellä tarkoitetun käyttöomaisuuden, kuten rautateiden, siltojen, laitureiden, patojen ja altaiden hankintameno vähennetään yhtä suurin vuotuisin poistoin omaisuuden todennäköisenä taloudellisena käyttöaikana tai jos tämä käyttöaika on 40 vuotta pitempi, 40 vuodessa."}}},"isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/39","version":"sfl:Original","versionDate":"2018-01-01"},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/40/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/40/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/40/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/40/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/40/momentti/1/alkup/fin/txt","content_fi":"Jos verovelvollinen osoittaa että muun kuin 30 ja 31 §:ssä tarkoitetun kuluvan käyttöomaisuuden käypä arvo on verovuoden päättyessä omaisuuden arvonalenemisen, vahingoittumisen tai muun syyn vuoksi sen poistamatonta hankintamenon osaa pienempi, saadaan hankintamenosta tehdä sellainen lisäpoisto, joka alentaa poistamattoman hankintamenon osan käyvän arvon suuruiseksi, kuitenkin niin, että 36 §:ssä tarkoitetun käyttöomaisuuden hankintamenosta saadaan lisäpoisto tehdä vain omaisuuden vahingoittumisen tai muun siihen rinnastettavan syyn vuoksi."}}},"isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/40","version":"sfl:Original","versionDate":"2018-01-01"},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/41/ajantasa/19900101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1989/1339/alkup","hasPart":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/41/momentti/1/ajantasa/19900101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/41/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/41/momentti/1/ajantasa/19900101/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/41/momentti/1/ajantasa/19900101/fin/txt","content_fi":"Luovutetusta sekä tuhoutuneesta, anastetusta tai muusta syystä menetetystä muusta kuin 30 ja 31 §:ssä tarkoitetusta kuluvasta käyttöomaisuudesta saadut veronalaiset luovutushinnat ja muut vastikkeet tuloutetaan ja vähennyskelpoinen poistamatta oleva hankintamenon osa vähennetään sinä verovuonna, jona käyttöomaisuus on luovutettu tai menetys on todettu, 43 §:ssä säädetyin poikkeuksin."}}},"isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/41","version":"sfl:Consolidated","versionDate":"2018-01-01"},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/42/ajantasa/20040815","@type":"sfl:SectionVersion","amendedBy":"sfsd:2004/717/alkup","hasPart":[{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/42/momentti/1/ajantasa/20040815","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/42/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/42/momentti/1/ajantasa/20040815/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/42/momentti/1/ajantasa/20040815/fin/txt","content_fi":"Jos verovelvollinen osoittaa, että muiden arvopapereiden kuin osakkeiden tai muun kulumattoman käyttöomaisuuden kuin maa-alueen käypä arvo on verovuoden päättyessä sen hankintamenoa tai tämän säännöksen perusteella aikaisemmin tehdyillä poistoilla vähennettyä hankintamenoa olennaisesti alempi, hankintamenosta saa tehdä sellaisen poiston, joka alentaa poistamatta olevan hankintamenon osan käyvän arvon suuruiseksi."}}},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/42/momentti/2/ajantasa/20040815","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/42/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/42/momentti/2/ajantasa/20040815/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/42/momentti/2/ajantasa/20040815/fin/txt","content_fi":"Maa-alueen, arvopaperin ja muun kulumattoman käyttöomaisuuden veronalaiset luovutushinnat ja muut vastikkeet tuloutetaan ja hankintameno tai sen vähennyskelpoinen osa poistetaan 6 b ja 43 §:ssä säädetyin poikkeuksin sinä verovuonna, jona käyttöomaisuus on luovutettu, tuhoutunut tai vahingoittunut. Edellä 6 §:n 1 momentin 1 kohdassa tarkoitetun yhteisön omistamien muiden kuin lain 6 b §:n 1 momentissa tarkoitettujen verovapaasti luovutettavien osakkeiden hankintameno on 6 b §:n 4–6 momentin luovutustappiota koskevien säännösten mukaisesti vähennyskelpoinen, kun osakkeet on luovutettu tai lopullisesti menetetty."}}}],"isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/42","version":"sfl:Consolidated","versionDate":"2018-01-01"},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1992/1539/alkup","hasPart":[{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/1/ajantasa/19980701","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:1998/473/alkup","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/1/ajantasa/19980701/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/1/ajantasa/19980701/fin/txt","content_fi":"Jos käyttöomaisuus tuhoutuu tai vahingoittuu tulipalon tai muun vahinkotapahtuman johdosta, vähennetään 30 §:ssä tarkoitettua menojäännöstä laskettaessa vähentämättä jäänyt osa sekä 33 §:n 3 momentissa ja 34, 36 ja 39 §:ssä tarkoitetusta kuluvasta käyttöomaisuudesta saatu vakuutuskorvaus tai muu vastike, poistamatonta hankintamenoa vastaavaa osaa lukuun ottamatta, verovuonna tai kahtena seuraavana verovuonna verovelvollisen käyttöön ottaman kuluvan käyttöomaisuuden tai toimitilojen hallintaan oikeuttavien osakkeiden hankintamenosta tai omaisuuden kuntoon saattamisesta johtuvista menoista (kuluvan käyttöomaisuuden jälleenhankintavaraus).\n(26.6.1998/473)"}}},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/2/ajantasa/19930101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/2/ajantasa/19930101/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/2/ajantasa/19930101/fin/txt","content_fi":"Jos verovelvollinen luovuttaa toimitiloinaan käyttämänsä rakennuksen tai toimitilojen hallintaan oikeuttavat osakkeet, vähennetään saatu luovutushinta, poistamatonta hankintamenoa vastaavaa osaa lukuun ottamatta, verovuonna tai kahtena seuraavana verovuonna käyttöön otetun verovelvollisen toimitilarakennuksen tai toimitilojen hallintaan oikeuttavien osakkeiden hankintamenosta tai tällaisten tilojen kuntoon saattamisesta johtuvista menoista (toimitilan jälleenhankintavaraus)."}}},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/3/ajantasa/19930101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/3","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/3/ajantasa/19930101/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/3/ajantasa/19930101/fin/txt","content_fi":"Jälleenhankintavaraus muodostetaan verovelvollisen vaatimuksesta. Edellytyksenä jälleenhankintavarauksen muodostamiselle on, että verovelvollinen jatkaa elinkeinotoimintaansa ja on tehnyt vastaavan varauksen kirjanpidossaan."}}},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/4/ajantasa/19930101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/4","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/4/ajantasa/19930101/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/4/ajantasa/19930101/fin/txt","content_fi":"Jos luovutushintaa tai muuta vastiketta taikka sen osaa ei ole 1 tai 2 momentissa tarkoitetulla tavalla vähennetty, luetaan vähentämättä jäänyt määrä 20 prosentilla korotettuna sen verovuoden veronalaiseksi tuloksi, jonka aikana vähentämisen olisi viimeistään tullut tapahtua."}}},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/5/ajantasa/20100901","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2010/506/alkup","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/5","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/5/ajantasa/20100901/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/5/ajantasa/20100901/fin/txt","content_fi":"Verohallinto voi verovelvollisen hakemuksesta erityisistä syistä pidentää 1 ja 2 momentissa tarkoitettua määräaikaa enintään kolmella vuodella. Jos luovutushintaa tai muuta vastiketta tai sen osaa ei ole vähennetty pidennetyn määräajan kuluessakaan, luetaan vähentämättä jäänyt määrä 40 prosentilla korotettuna sen verovuoden veronalaiseksi tuloksi, jonka aikana vähentämisen olisi viimeistään tullut tapahtua.\n(11.6.2010/506)"}}},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/6/ajantasa/20130101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2012/879/alkup","cites":"sfsd:1995/1558/pykala/71e","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/6","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/6/ajantasa/20130101/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/6/ajantasa/20130101/fin/txt","content_fi":"Muutoksenhausta 5 momentissa tarkoitettuun päätökseen säädetään verotusmenettelystä annetun lain (1558/1995) 71 e §:ssä.\n(21.12.2012/879)"}}},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/6/ajantasa/20170101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2016/788/alkup","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/6","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/6/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/43/momentti/6/ajantasa/20170101/fin/txt","content_fi":"Muutoksenhausta 5 momentissa tarkoitettuun päätökseen säädetään verotusmenettelystä annetussa laissa.\n(9.9.2016/788)"}}}],"isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/43","version":"sfl:Consolidated","versionDate":"2018-01-01"},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/44/ajantasa/19930101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/44","repealedBy":"sfsd:1992/1539/alkup","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/44/ajantasa/19930101/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/44/ajantasa/19930101/fin/txt","content_fi":"44 §(30.12.1992/1539)\n\n44 § on kumottu L:lla 30.12.1992/1539."}}},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/45/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/45/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/45/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/45/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/45/momentti/1/alkup/fin/txt","content_fi":"Menoon, jonka verovelvollinen on elinkeinotoiminnassaan suorittanut osallistuakseen toiselle kuuluvan käyttöomaisuuden hankintaan ja käyttöön, sovelletaan tämän luvun vastaavanlaisen käyttöomaisuuden hankintamenoa koskevia säännöksiä, kuitenkin niin, että mikäli käyttöaika on rajoitettu, hankintameno poistetaan yhtä suurin vuotuisin poistoin käyttöaikana."}}},"isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/45","version":"sfl:Original","versionDate":"2018-01-01"},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/45a/ajantasa/19880701","@type":"sfl:SectionVersion","amendedBy":"sfsd:1988/562/alkup","hasPart":[{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/45a/momentti/1/ajantasa/19880701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/45a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/45a/momentti/1/ajantasa/19880701/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/45a/momentti/1/ajantasa/19880701/fin/txt","content_fi":"Ydinenergialaissa tarkoitetusta ydinjätehuollosta verovelvolliselle aiheutuneet menot vähennetään silloinkin, kun kysymys on käyttöomaisuuden hankintamenosta, sinä verovuonna, jona niiden suorittamisvelvollisuus on syntynyt."}}},{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/45a/momentti/2/ajantasa/19880701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/45a/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/45a/momentti/2/ajantasa/19880701/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/3/pykala/45a/momentti/2/ajantasa/19880701/fin/txt","content_fi":"Kauppa- ja teollisuusministeriö antaa veroviranomaisen tai verovelvollisen pyynnöstä lausunnon siitä, kuuluuko käyttöomaisuuden hankinta ydinjätehuoltoon."}}}],"isMemberOf":"sfsd:1968/360/osa/3/luku/3/pykala/45a","version":"sfl:Consolidated","versionDate":"2018-01-01"}],"isMemberOf":"sfsd:1968/360/osa/3/luku/3","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/3/ajantasa/20170101/fin","title_fi":"Käyttöomaisuuden hankintamenon jaksottaminen"}},{"@id":"sfsd:1968/360/osa/3/luku/4/ajantasa/20090101","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46/ajantasa/20090101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1993/152/alkup","hasPart":[{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46/momentti/1/ajantasa/20090101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2008/1077/alkup","cites":["sfsd:1990/1269","sfsd:1990/1270","sfsd:1990/1271"],"isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/46/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46/momentti/1/ajantasa/20090101/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46/momentti/1/ajantasa/20090101/fin/txt","content_fi":"Talletuspankki sekä luottolaitos saa vähentää verovuonna tehdyn luottotappiovarauksen, jonka määrä on enintään 0,6 prosenttia laitoksella verovuoden päättyessä olevien saamisten yhteismäärästä; verovuonna ja aikaisemmin tehtyjen purkamattomien luottotappiovarausten yhteismäärä ei saa kuitenkaan ylittää 5 prosenttia laitoksella verovuoden päättyessä olevien saamisten yhteismäärästä. Jos talletuspankki on kumottujen liikepankkilain (1269/1990), säästöpankkilain (1270/1990) tai osuuspankkilain (1271/1990) nojalla taikka jos luottolaitos on rahoitustoimintalain (1544/1991) nojalla siirtänyt luottotappiovarauksiaan vararahastoon, sen verovuonna ja aikaisemmin tekemien purkamattomien luottotappiovarausten yhteismäärä ei saa ylittää edellä tarkoitettua luottotappiovarausten enimmäismäärää vähennettynä vararahastoon tehdyllä siirrolla.\n(30.12.2008/1077)"}}},{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46/momentti/2/ajantasa/19930215","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/46/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46/momentti/2/ajantasa/19930215/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46/momentti/2/ajantasa/19930215/fin/txt","content_fi":"Jos talletuspankki tai luottolaitos, joka on 1 momentissa tarkoitetulla tavalla siirtänyt luottotappiovarauksiaan vararahastoon, on siirtänyt verovuoden aikana toiselle samaan konserniin kuuluvalle talletuspankille tai luottolaitokselle luottotappiovarauksen perusteena olevia saamisiaan määrän, joka vastaa vähintään saamiset siirtäneen talletuspankin tai luottolaitoksen saamisten määrää siirtoa seuraavassa tilinpäätöksessä, otetaan edellä tarkoitettu vararahastosiirron määrä huomioon mainittujen talletuspankkien ja luottolaitosten verotuksessa luottotappiovarauksen enimmäismäärää laskettaessa siirrettyjen saamisten sekä niiden saamisten suhteessa, jotka ovat luottotappiovarauksen perusteena saamiset siirtäneen talletuspankin ja luottolaitoksen seuraavassa tilinpäätöksessä. Talletuspankkien tai luottolaitosten sulautuessa otetaan sulautuneen talletuspankin tai luottolaitoksen vararahastoon siirtämien luottotappiovarausten määrä samalla tavalla huomioon luottotappiovarausten enimmäismäärää rajoittavana vastaanottavan talletuspankin tai luottolaitoksen verotuksessa kuin se olisi otettu huomioon sulautuneen talletuspankin tai luottolaitoksen verotuksessa."}}},{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46/momentti/3/ajantasa/19930215","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/46/momentti/3","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46/momentti/3/ajantasa/19930215/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46/momentti/3/ajantasa/19930215/fin/txt","content_fi":"3–4 momentit on kumottu L:lla 30.12.2008/1077."}}},{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46/momentti/3/ajantasa/20090101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/46/momentti/3","repealedBy":"sfsd:2008/1077/alkup","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46/momentti/3/ajantasa/20090101/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46/momentti/3/ajantasa/20090101/fin/txt","content_fi":"3–4 momentit on kumottu L:lla 30.12.2008/1077."}}},{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46/momentti/5/ajantasa/19930215","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/46/momentti/5","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46/momentti/5/ajantasa/19930215/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46/momentti/5/ajantasa/19930215/fin/txt","content_fi":"Jos luottotappiovarausten tai niiden ja vararahastoon tehdyn siirron yhteismäärä ylittää tässä pykälässä tarkoitetun vähennyskelpoisten luottotappiovarausten enimmäismäärän, luetaan luottotappiovarauksista enimmäismäärän ylittävä osa sen verovuoden veronalaiseksi tuloksi, jona enimmäismäärä on ylitetty."}}}],"isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/46","version":"sfl:Consolidated","versionDate":"2018-01-01"},{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46a/ajantasa/19930101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1992/1539/alkup","hasPart":[{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46a/momentti/1/ajantasa/19930101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/46a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46a/momentti/1/ajantasa/19930101/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46a/momentti/1/ajantasa/19930101/fin/txt","content_fi":"Yksityinen liikkeen- ja ammatinharjoittaja sekä yhtymä ja kuolinpesä, jonka osakkaina on ainoastaan luonnollisia henkilöitä tai kuolinpesiä, saa vähentää verovuonna tekemänsä toimintavarauksen. Verovuonna ja aikaisemmin tehtyjen purkamattomien toimintavarausten yhteismäärä ei kuitenkaan saa ylittää 30 prosenttia tilikauden päättymistä edeltäneiden 12 kuukauden aikana maksettujen ennakonpidätyksen alaisten palkkojen määrästä. Poiketen siitä, mitä edellä on säädetty, yhtymä tai kuolinpesä, joka on verovuonna 1993 muodostanut voimaantulosäännöksen 9 momentin e kohdassa tarkoitetun siirtymävarauksen, ei saa tehdä toimintavarausta."}}},{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46a/momentti/2/ajantasa/19930101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/46a/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46a/momentti/2/ajantasa/19930101/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46a/momentti/2/ajantasa/19930101/fin/txt","content_fi":"Se osa toimintavarausten määrästä, joka ylittää 1 momentissa tarkoitetun toimintavarausten enimmäismäärän, luetaan sen verovuoden veronalaiseksi tuloksi, jona enimmäismäärä on ylitetty."}}},{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46a/momentti/3/ajantasa/19930101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/46a/momentti/3","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46a/momentti/3/ajantasa/19930101/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46a/momentti/3/ajantasa/19930101/fin/txt","content_fi":"Jos verovelvollinen lopettaa elinkeinotoimintansa, jos yhtymä tai kuolinpesä ei täytä enää 1 momentissa säädettyä edellytystä, jos yhtymä muutetaan yhteisöksi, taikka liikkeen tai ammatin harjoittamista taikka yhtymän tai kuolinpesän harjoittamaa elinkeinotoimintaa jatketaan tuloverolain 24 §:ssä tarkoitetulla tavalla osakeyhtiön muodossa, toimintavaraukset luetaan sen verovuoden veronalaiseksi tuloksi, jona toiminta on päättynyt tai muu tässä momentissa tarkoitettu muutos on tapahtunut."}}}],"isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/46a","version":"sfl:Consolidated","versionDate":"2018-01-01"},{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46b/ajantasa/19880701","@type":"sfl:SectionVersion","amendedBy":"sfsd:1983/222/alkup","hasPart":[{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46b/momentti/1/ajantasa/19880101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:1987/1070/alkup","isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/46b/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46b/momentti/1/ajantasa/19880101/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46b/momentti/1/ajantasa/19880101/fin/txt","content_fi":"Verovelvollinen, joka on vastuussa ydinjätehuoltoon kuuluvista toimenpiteistä, saa vuodelta 1982 toimitettavassa verotuksessa vähentää tulostaan kauppa- ja teollisuusministeriön mainitulle vuodelle atomienergialain (356/57) 4 ja 5 §:n nojalla vahvistaman ydinjätehuoltovarauksen kokonaismäärän. Vuosilta 1983–1987 toimitettavissa verotuksissa verovelvollinen saa vähentää kauppa- ja teollisuusministeriön verovuodelle ja sitä edeltäneelle vuodelle vahvistamien ydinjätehuoltovarausten erotuksen. Näiltä vuosilta toimitettavissa verotuksissa voidaan kuitenkin vähentää enintään seuraavat määrät:\n\n\nVerovuosi\nVähennyksen enimmäismäärä\n\n1983\n220 miljoonaa markkaa\n\n1984\n220    \"\"\n\n1985\n240    \"\"\n\n1986\n260    \"\"\n\n1987\n250    \"\"\n\n\n\n\n\nVähentäminen edellyttää lisäksi, että varaus on tehty myös verovelvollisen kirjanpidossa.\n(18.12.1987/1070)"}}},{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46b/momentti/2/ajantasa/19880701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/46b/momentti/2","repealedBy":"sfsd:1988/562/alkup","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46b/momentti/2/ajantasa/19880701/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46b/momentti/2/ajantasa/19880701/fin/txt","content_fi":"2 momentti on kumottu L:lla 17.6.1988/562."}}}],"isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/46b","version":"sfl:Consolidated","versionDate":"2018-01-01"},{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46b/ajantasa/19880101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1983/222/alkup","hasPart":[{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46b/momentti/1/ajantasa/19880101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:1987/1070/alkup","isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/46b/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46b/momentti/1/ajantasa/19880101/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46b/momentti/1/ajantasa/19880101/fin/txt","content_fi":"Verovelvollinen, joka on vastuussa ydinjätehuoltoon kuuluvista toimenpiteistä, saa vuodelta 1982 toimitettavassa verotuksessa vähentää tulostaan kauppa- ja teollisuusministeriön mainitulle vuodelle atomienergialain (356/57) 4 ja 5 §:n nojalla vahvistaman ydinjätehuoltovarauksen kokonaismäärän. Vuosilta 1983–1987 toimitettavissa verotuksissa verovelvollinen saa vähentää kauppa- ja teollisuusministeriön verovuodelle ja sitä edeltäneelle vuodelle vahvistamien ydinjätehuoltovarausten erotuksen. Näiltä vuosilta toimitettavissa verotuksissa voidaan kuitenkin vähentää enintään seuraavat määrät:\n\n\nVerovuosi\nVähennyksen enimmäismäärä\n\n1983\n220 miljoonaa markkaa\n\n1984\n220    \"\"\n\n1985\n240    \"\"\n\n1986\n260    \"\"\n\n1987\n250    \"\"\n\n\n\n\n\nVähentäminen edellyttää lisäksi, että varaus on tehty myös verovelvollisen kirjanpidossa.\n(18.12.1987/1070)"}}},{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46b/momentti/2/ajantasa/19830401","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/46b/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46b/momentti/2/ajantasa/19830401/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/46b/momentti/2/ajantasa/19830401/fin/txt","content_fi":"2 momentti on kumottu L:lla 17.6.1988/562."}}}],"isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/46b","version":"sfl:Consolidated","versionDate":"2018-01-01"},{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/47/ajantasa/19930101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1992/1539/alkup","hasPart":[{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/47/momentti/1/ajantasa/19930101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/47/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/47/momentti/1/ajantasa/19930101/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/47/momentti/1/ajantasa/19930101/fin/txt","content_fi":"Rakennus-, laivanrakennus- tai metalliteollisuusliikettä harjoittava verovelvollinen, joka on takuusitoumuksen tai muun vastaavan velvoitteen perusteella vastuussa valmistamassaan rakennuksessa, sillassa, laiturissa, tiessä, padossa tai niihin verrattavassa rakennelmassa, ilma-aluksessa, sellaisessa vesialuksessa, jonka rungon suurin pituus on vähintään 10 metriä, tai suurehkossa koneyksikössä ilmenevistä virheellisyyksistä, saa vähentää verovuonna luovuttamansa hyödykkeen takuukorjauksista odotettavissa olevia menoja vastaavan takuuvarauksen."}}},{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/47/momentti/2/ajantasa/19930101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/47/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/47/momentti/2/ajantasa/19930101/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/47/momentti/2/ajantasa/19930101/fin/txt","content_fi":"Meno, jota vastaava takuuvaraus on vähennetty 1 momentin nojalla, ei ole verotuksessa vähennyskelpoinen. Se osa takuuvarauksesta, joka ylittää takuukorjauksista syntyneiden menojen määrän, luetaan veronalaiseksi tuloksi sinä verovuonna, jona takuuaika on päättynyt."}}}],"isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/47","version":"sfl:Consolidated","versionDate":"2018-01-01"},{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/48/ajantasa/19980801","@type":"sfl:SectionVersion","amendedBy":"sfsd:1997/321/alkup","hasPart":[{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/48/momentti/1/ajantasa/19970501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/48/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/48/momentti/1/ajantasa/19970501/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/48/momentti/1/ajantasa/19970501/fin/txt","content_fi":"Siirrot korvaus- ja vakuutusmaksuvastuuseen sekä eläkevastuun peittämiseen käytetyt määrät katsotaan sen verovuoden kuluiksi, jona siirto lain mukaan on tehty tai jona eläkevastuuta on katettu."}}},{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/48/momentti/2/ajantasa/19980801","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/48/momentti/2","repealedBy":"sfsd:1998/511/alkup","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/48/momentti/2/ajantasa/19980801/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/48/momentti/2/ajantasa/19980801/fin/txt","content_fi":"2 momentti on kumottu L:lla 10.7.1998/511."}}}],"isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/48","version":"sfl:Consolidated","versionDate":"2018-01-01"},{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/48/ajantasa/19970501","@type":"sfl:SectionVersion","amendedBy":"sfsd:1997/321/alkup","hasPart":[{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/48/momentti/1/ajantasa/19970501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/48/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/48/momentti/1/ajantasa/19970501/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/48/momentti/1/ajantasa/19970501/fin/txt","content_fi":"Siirrot korvaus- ja vakuutusmaksuvastuuseen sekä eläkevastuun peittämiseen käytetyt määrät katsotaan sen verovuoden kuluiksi, jona siirto lain mukaan on tehty tai jona eläkevastuuta on katettu."}}},{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/48/momentti/2/ajantasa/19970501","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/48/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/48/momentti/2/ajantasa/19970501/fin","hasFormat":{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/48/momentti/2/ajantasa/19970501/fin/txt","content_fi":"2 momentti on kumottu L:lla 10.7.1998/511."}}}],"isMemberOf":"sfsd:1968/360/osa/3/luku/4/pykala/48","version":"sfl:Consolidated","versionDate":"2018-01-01"},{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/48a/ajantasa/20070101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1998/511/alkup","hasPart":[{"@id":"sfsd:1968/360/osa/3/luku/4/pykala/48a/momentti/1/ajantasa/19980801","@type":"sfl:SubsectionVersi