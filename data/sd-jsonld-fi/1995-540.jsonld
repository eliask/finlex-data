uageVersion":{"@id":"sfsd:1995/540/luku/7/pykala/1/momentti/2/kohta/6/alkup/fin","hasFormat":{"@id":"sfsd:1995/540/luku/7/pykala/1/momentti/2/kohta/6/alkup/fin/txt","content_fi":"6) lainhuudatuksen ulkopuolelle jäävän kiinteistön omistajasta ja kiinteistöä hallinnoivasta viranomaisesta;"}}},{"@id":"sfsd:1995/540/luku/7/pykala/1/momentti/2/kohta/7/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1995/540/luku/7/pykala/1/momentti/2/kohta/7","version":"sfl:Original","versionDate":"2017-06-01","languageVersion":{"@id":"sfsd:1995/540/luku/7/pykala/1/momentti/2/kohta/7/alkup/fin","hasFormat":{"@id":"sfsd:1995/540/luku/7/pykala/1/momentti/2/kohta/7/alkup/fin/txt","content_fi":"7) kiinteistöön kohdistuvista oikeuksista ja rasituksista, jos ne ovat tämän tai muun lain nojalla ilmoitettava kirjaamisviranomaiselle tai jos ne ovat tarpeellisia rekisterin käyttötarkoituksen kannalta."}}}],"isMemberOf":"sfsd:1995/540/luku/7/pykala/1/momentti/2","version":"sfl:Original","versionDate":"2017-06-01"},{"@id":"sfsd:1995/540/luku/7/pykala/1/momentti/3/alkup","@type":"sfl:SubsectionVersion","cites":"sfsd:1999/523","isMemberOf":"sfsd:1995/540/luku/7/pykala/1/momentti/3","version":"sfl:Original","versionDate":"2017-06-01","languageVersion":{"@id":"sfsd:1995/540/luku/7/pykala/1/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1995/540/luku/7/pykala/1/momentti/3/alkup/fin/txt","content_fi":"Lainhuuto- ja kiinnitysrekisteriin merkitystä hakijasta tai oikeudenhaltijasta talletetaan henkilötietoina tunniste- ja yhteystietoja sekä tieto henkilön kuolinajasta rekisteröityjen yksiselitteiseksi yksilöimiseksi. Rekisteriin talletetaan lisäksi näitä tietoja koskevat muutokset. Lainhuuto- ja kiinnitysrekisterin henkilötietojen käsittelyyn sovelletaan henkilötietolakia (523/1999), jollei tässä laissa toisin säädetä."}}},{"@id":"sfsd:1995/540/luku/7/pykala/1/momentti/4/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1995/540/luku/7/pykala/1/momentti/4","version":"sfl:Original","versionDa