ion":{"@id":"sfsd:2008/284/pykala/13/alkup/fin","title_fi":"Voimaantulo"}}],"isAbout":"http://data.finlex.fi/voc/finlex/kw2558","isMemberOf":"sfsd:2008/284","relatedTo":"http://data.finlex.fi/eli/he/2007/161","version":"sfl:Consolidated","versionDate":"2015-01-01","languageVersion":{"@id":"sfsd:2008/284/ajantasa/20150101/fin","title_fi":"Laki maaseutuelinkeinohallinnon tietojärjestelmästä"}},"@context":{"languageVersion":{"@id":"eli:is_realized_by","@type":"@id"},"temporalVersions":{"@id":"eli:has_member","@type":"@id"},"temporalVersion":{"@id":"eli:has_member","@type":"@id"},"hasFormat":{"@id":"eli:is_embodied_by","@type":"@id"},"idLocal":"eli:id_local","dateDocument":{"@id":"eli:date_document","@type":"xsd:date"},"hasPart":{"@id":"eli:has_part","@type":"@id"},"isAbout":{"@id":"eli:is_about","@type":"@id"},"isMemberOf":{"@id":"eli:is_member_of","@type":"@id"},"relatedTo":{"@id":"eli:related_to","@type":"@id"},"version":{"@id":"eli:version","@type":"@id"},"versionDate":{"@id":"eli:version_date","@type":"xsd:date"},"title_fi":{"@id":"eli:title","@language":"fi"},"content_fi":{"@id":"sfl:text","@language":"fi"},"amendedBy":{"@id":"eli:amended_by","@type":"@id"},"cites":{"@id":"eli:cites","@type":"@id"},"inForce":{"@id":"eli:in_force","@type":"@id"},"typeDocument":{"@id":"eli:type_document","@type":"@id"},"dcterms":"http://purl.org/dc/terms/","xsd":"http://www.w3.org/2001/XMLSchema#","common":"http://data.finlex.fi/common/","rdf":"http://www.w3.org/1999/02/22-rdf-syntax-ns#","sfcl":"http://data.finlex.fi/schema/scfl/","sfl":"http://data.finlex.fi/schema/sfl/","sfecli":"http://data.finlex.fi/ecli/","sfsd":"http://data.finlex.fi/eli/sd/","skos":"http://www.w3.org/2004/02/skos/core#","text":"http://jena.apache.org/text#","eli":"http://data.europa.eu/eli/ontology#"}}