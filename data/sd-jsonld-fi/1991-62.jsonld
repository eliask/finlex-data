ersion":{"@id":"sfsd:1991/62/pykala/3/momentti/1/kohta/12/alkup/fin","hasFormat":{"@id":"sfsd:1991/62/pykala/3/momentti/1/kohta/12/alkup/fin/txt","content_fi":"12)Tuntsan alue, johon kuuluu noin 21 000 hehtaaria valtion omistamia alueita Sallan ja Savukosken kunnissa (liite 12)."}}}],"isMemberOf":"sfsd:1991/62/pykala/3/momentti/1","version":"sfl:Original","versionDate":"2011-07-01"},{"@id":"sfsd:1991/62/pykala/3/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/62/pykala/3/momentti/2","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/3/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1991/62/pykala/3/momentti/2/alkup/fin/txt","content_fi":"Erämaa-alueiden rajat on merkitty liitteinä oleviin karttoihin punaisella katkoviivalla."}}}],"isMemberOf":"sfsd:1991/62/pykala/3","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/3/alkup/fin","title_fi":"Erämaa-alueet"}},{"@id":"sfsd:1991/62/pykala/4/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1991/62/pykala/4/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/62/pykala/4/momentti/1","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/4/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/62/pykala/4/momentti/1/alkup/fin/txt","content_fi":"Erämaa-alueeseen kuuluvaa valtion omistamaa maa-aluetta tai siihen kohdistuvaa käyttöoikeutta ei saa luovuttaa eikä antaa vuokralle ilman valtioneuvoston lupaa."}}},{"@id":"sfsd:1991/62/pykala/4/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/62/pykala/4/momentti/2","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/4/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1991/62/pykala/4/momentti/2/alkup/fin/txt","content_fi":"Lupaa ei kuitenkaan tarvita käyttöoikeuden luovuttamiseen porotalouden, kalastuksen, metsästyksen tai keräilyn tarpeisiin 7§:ssä mainitun hoito- ja käyttösuunnitelman mukaisesti eikä alueen luovuttamiseen sellaista hanketta varten, joka on tarpeen puolustusvalmiuden tai rajavartiolaitoksen toiminnan kannalta."}}},{"@id":"sfsd:1991/62/pykala/4/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/62/pykala/4/momentti/3","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/4/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1991/62/pykala/4/momentti/3/alkup/fin/txt","content_fi":"Maa-alueen tai siihen kohdistuvan käyttöoikeuden luovuttamiseen ja vuokraamiseen sovelletaan muutoin, mitä oikeudesta luovuttaa valtion maaomaisuutta ja tuloatuottavia oikeuksia annetussa laissa (687/78) säädetään."}}}],"isMemberOf":"sfsd:1991/62/pykala/4","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/4/alkup/fin","title_fi":"Alueen luovuttaminen"}},{"@id":"sfsd:1991/62/pykala/5/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1991/62/pykala/5/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/62/pykala/5/momentti/1","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/5/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/62/pykala/5/momentti/1/alkup/fin/txt","content_fi":"Erämaa-alueille ei saa rakentaa pysyviä teitä."}}},{"@id":"sfsd:1991/62/pykala/5/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/62/pykala/5/momentti/2","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/5/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1991/62/pykala/5/momentti/2/alkup/fin/txt","content_fi":"Valtioneuvosto voi kuitenkin antaa luvan sellaisen pysyvän tien rakentamiseen, jolla on yleisen edun tai luontaiselinkeinojen harjoittamisen kannalta huomattava merkitys."}}},{"@id":"sfsd:1991/62/pykala/5/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/62/pykala/5/momentti/3","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/5/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1991/62/pykala/5/momentti/3/alkup/fin/txt","content_fi":"Edellä 1 momentissa tarkoitettu kielto ei koske tietä, joka rakennetaan ennen tämän lain voimaantuloa saadun oikeuden perusteella."}}}],"isMemberOf":"sfsd:1991/62/pykala/5","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/5/alkup/fin","title_fi":"Teiden rakentaminen"}},{"@id":"sfsd:1991/62/pykala/6/ajantasa/20110701","@type":"sfl:SectionVersion","amendedBy":"sfsd:2011/625/alkup","hasPart":{"@id":"sfsd:1991/62/pykala/6/momentti/1/ajantasa/20110701","@type":"sfl:SubsectionVersion","cites":"sfsd:2011/621","isMemberOf":"sfsd:1991/62/pykala/6/momentti/1","version":"sfl:Consolidated","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/6/momentti/1/ajantasa/20110701/fin","hasFormat":{"@id":"sfsd:1991/62/pykala/6/momentti/1/ajantasa/20110701/fin/txt","content_fi":"Kaivoslain (621/2011) mukaista kaivoslupaa ei saa myöntää erämaa-alueelle, ellei valtioneuvosto ole antanut siihen lupaa."}}},"isMemberOf":"sfsd:1991/62/pykala/6","version":"sfl:Consolidated","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/6/ajantasa/20110701/fin","title_fi":"Kaivostoiminta"}},{"@id":"sfsd:1991/62/pykala/7/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1991/62/pykala/7/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/62/pykala/7/momentti/1","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/7/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/62/pykala/7/momentti/1/alkup/fin/txt","content_fi":"Erämaa-alueen hoidossa ja käytössä on noudatettava hoito- ja käyttösuunnitelmaa, jonka laatii metsähallitus ja vahvistaa ympäristöministeriö."}}},{"@id":"sfsd:1991/62/pykala/7/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/62/pykala/7/momentti/2","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/7/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1991/62/pykala/7/momentti/2/alkup/fin/txt","content_fi":"Erämaa-alueiden metsät säilytetään luonnontilaisina tai niissä harjoitetaan luonnonmukaista metsänhoitoa. Metsähallitus laatii erämaa-alueiden luonnonmukaisesti hoidettavien metsien käsittelyohjeet, jotka otetaan huomioon alueen hoito- ja käyttösuunnitelmaa laadittaessa."}}}],"isMemberOf":"sfsd:1991/62/pykala/7","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/7/alkup/fin","title_fi":"Hoito- ja käyttösuunnitelma"}},{"@id":"sfsd:1991/62/pykala/8/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1991/62/pykala/8/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/62/pykala/8/momentti/1","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/8/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/62/pykala/8/momentti/1/alkup/fin/txt","content_fi":"Valtioneuvosto voi antaa luvan yksityistä tietä varten perustetun rasiteoikeuden tai käyttöoikeuden lunastamiseen, jos erämaa-alueella on oikeuden käyttämiseksi vireillä sellaisen pysyvän tien rakentamista koskeva hanke, josta on haittaa alueen erämaaluonteen säilyttämiselle. Jos erämaa-alueen sisällä olevalle yksityiselle alueelle ei rasiteoikeuden tai käyttöoikeuden lunastamisen vuoksi voida järjestää tyydyttävää kulkuyhteyttä ja tästä aiheutuu omistajalle huomattavaa haittaa, valtio on velvollinen omistajan vaatimuksesta lunastamaan alueen."}}},{"@id":"sfsd:1991/62/pykala/8/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/62/pykala/8/momentti/2","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/8/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1991/62/pykala/8/momentti/2/alkup/fin/txt","content_fi":"Lunastamisessa on muutoin noudatettava, mitä kiinteän omaisuuden ja erityisten oikeuksien lunastuksesta annetussa laissa (603/77) säädetään."}}}],"isMemberOf":"sfsd:1991/62/pykala/8","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/8/alkup/fin","title_fi":"Rasiteoikeuden lunastaminen"}},{"@id":"sfsd:1991/62/pykala/9/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1991/62/pykala/9/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/62/pykala/9/momentti/1","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/9/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/62/pykala/9/momentti/1/alkup/fin/txt","content_fi":"Jos alue, joka ei ole valtion omistuksessa, mutta joka sijaitsee erämaa-alueen rajojen sisällä, siirtyy valtion omistukseen, se luetaan erämaa-alueeseen kuuluvaksi."}}},"isMemberOf":"sfsd:1991/62/pykala/9","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/9/alkup/fin","title_fi":"Alueen liittäminen erämaa-alueeseen"}},{"@id":"sfsd:1991/62/pykala/10/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1991/62/pykala/10/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/62/pykala/10/momentti/1","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/10/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/62/pykala/10/momentti/1/alkup/fin/txt","content_fi":"Erämaa-alueet ovat metsähallituksen hallinnassa ja hoidossa."}}},"isMemberOf":"sfsd:1991/62/pykala/10","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/10/alkup/fin","title_fi":"Erämaa-alueiden hallinta"}},{"@id":"sfsd:1991/62/pykala/11/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1991/62/pykala/11/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/62/pykala/11/momentti/1","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/11/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/62/pykala/11/momentti/1/alkup/fin/txt","content_fi":"Erämaa-alueet merkitään metsähallituksen maakirjaan ja metsätalouskarttoihin tai niitä vastaaviin karttoihin."}}},"isMemberOf":"sfsd:1991/62/pykala/11","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/11/alkup/fin","title_fi":"Erämaa-alueiden merkintä"}},{"@id":"sfsd:1991/62/pykala/12/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1991/62/pykala/12/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/62/pykala/12/momentti/1","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/12/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/62/pykala/12/momentti/1/alkup/fin/txt","content_fi":"Tämä laki tulee voimaan 1 päivänä helmikuuta 1991."}}},{"@id":"sfsd:1991/62/pykala/12/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/62/pykala/12/momentti/2","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/12/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1991/62/pykala/12/momentti/2/alkup/fin/txt","content_fi":"Erämaa-alueisiin tämän lain voimaan tullessa kohdistuvat rasiteoikeudet ja niihin rinnastettavat käyttöoikeudet sekä vuokrasopimukset jäävät edelleen voimaan. Metsähallitus voi jatkaa sopimukseen perustuvien oikeuksien voimassaoloaikaa vahvistetun hoito- ja käyttösuunnitelman mukaisesti."}}},{"@id":"sfsd:1991/62/pykala/12/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/62/pykala/12/momentti/3","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/12/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1991/62/pykala/12/momentti/3/alkup/fin/txt","content_fi":"Sen estämättä, mitä 5§:ssä säädetään, saa Hammastunturin erämaa-alueelle rakentaa tien Inarin ja Pokan väliseltä maantieltä Kutturan kylään."}}}],"isMemberOf":"sfsd:1991/62/pykala/12","version":"sfl:Original","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/pykala/12/alkup/fin","title_fi":"Voimaantulo- ja siirtymäsäännökset"}}],"isAbout":["http://data.finlex.fi/voc/finlex/kw2485","http://data.finlex.fi/voc/finlex/kw1375"],"isMemberOf":"sfsd:1991/62","version":"sfl:Consolidated","versionDate":"2011-07-01","languageVersion":{"@id":"sfsd:1991/62/ajantasa/20110701/fin","title_fi":"Erämaalaki"}},"@context":{"languageVersion":{"@id":"eli:is_realized_by","@type":"@id"},"temporalVersions":{"@id":"eli:has_member","@type":"@id"},"temporalVersion":{"@id":"eli:has_member","@type":"@id"},"hasFormat":{"@id":"eli:is_embodied_by","@type":"@id"},"idLocal":"eli:id_local","dateDocument":{"@id":"eli:date_document","@type":"xsd:date"},"hasPart":{"@id":"eli:has_part","@type":"@id"},"isAbout":{"@id":"eli:is_about","@type":"@id"},"isMemberOf":{"@id":"eli:is_member_of","@type":"@id"},"version":{"@id":"eli:version","@type":"@id"},"versionDate":{"@id":"eli:version_date","@type":"xsd:date"},"title_fi":{"@id":"eli:title","@language":"fi"},"content_fi":{"@id":"sfl:text","@language":"fi"},"amendedBy":{"@id":"eli:amended_by","@type":"@id"},"cites":{"@id":"eli:cites","@type":"@id"},"inForce":{"@id":"eli:in_force","@type":"@id"},"typeDocument":{"@id":"eli:type_document","@type":"@id"},"dcterms":"http://purl.org/dc/terms/","xsd":"http://www.w3.org/2001/XMLSchema#","common":"http://data.finlex.fi/common/","rdf":"http://www.w3.org/1999/02/22-rdf-syntax-ns#","sfcl":"http://data.finlex.fi/schema/scfl/","sfl":"http://data.finlex.fi/schema/sfl/","sfecli":"http://data.finlex.fi/ecli/","sfsd":"http://data.finlex.fi/eli/sd/","skos":"http://www.w3.org/2004/02/skos/core#","text":"http://jena.apache.org/text#","eli":"http://data.europa.eu/eli/ontology#"}}