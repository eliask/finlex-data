{"@id":"sfsd:1969/1","@type":["http://data.finlex.fi/schema/sfl/NewStatute"],"idLocal":["1/1969"],"inForce":["http://data.europa.eu/eli/ontology#InForce-inForce"],"temporalVersions":["http://data.finlex.fi/eli/sd/1969/1/alkup"],"typeDocument":["http://data.finlex.fi/schema/sfl/Act"],"temporalVersion":{"@id":"sfsd:1969/1/alkup","@type":"sfl:StatuteVersion","dateDocument":"1969-01-10","hasPart":[{"@id":"sfsd:1969/1/johtolause/alkup","@type":"sfl:EnactingClauseVersion","isMemberOf":"sfsd:1969/1/johtolause","version":"sfl:Original","versionDate":"1969-01-10","languageVersion":{"@id":"sfsd:1969/1/johtolause/alkup/fin","hasFormat":{"@id":"sfsd:1969/1/johtolause/alkup/fin/txt","content_fi":"Eduskunnan päätöksen mukaisesti säädetään:"}}},{"@id":"sfsd:1969/1/pykala/1/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1969/1/pykala/1/momentti/1/alkup","@type":"sfl:SubsectionVersion","cites":"sfsd:1889/39/luku/2","isMemberOf":"sfsd:1969/1/pykala/1/momentti/1","version":"sfl:Original","versionDate":"1969-01-10","languageVersion":{"@id":"sfsd:1969/1/pykala/1/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1969/1/pykala/1/momentti/1/alkup/fin/txt","content_fi":"Rangaistusjärjestelmästä poistetaan kansalaisluottamuksen menettäminen, kelvottomuus maan palvelukseen ja kelpaamattomuus ajamaan toisen asiaa oikeuden tai muun julkisen viranomaisen edessä sekä siitä johtuen kumotaan rikoslain 2 luvun 11 ja 14 §, viimeksi mainitun pykälän 3 momentti sellaisena kuin se on 11 päivänä huhtikuuta 1958 annetussa laissa (149/58). Samalla lakkaavat olemasta voimassa eri lakeihin sisältyvät rangaistussäännökset niiltä osin kuin rikoksentekijä niiden mukaan on tuomittava tai voidaan tuomita edellä mainittuihin lisärangaistuksiin."}}},"isMemberOf":"sfsd:1969/1/pykala/1","version":"sfl:Original","versionDate":"1969-01-10"},{"@id":"sfsd:1969/1/pykala/2/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1969/1/pykala/2/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1969/1/pykala/2/momentti/1","version":"sfl:Original","versionDate":"1969-01-10","languageVersion":{"@id":"sfsd:1969/1/pykala/2/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1969/1/pykala/2/momentti/1/alkup/fin/txt","content_fi":"Edellä 1 §:ssä mainittujen lisärangaistusten poistamisen johdosta lakkaavat myös olemasta voimassa ne jonkun henkilön oikeuksia ja etuja koskevat rajoitukset, jotka ovat seurauksena siitä, että hänet on tuomittu tällaiseen lisärangaistukseen tai että hänellä ei ole hyvää mainetta."}}},"isMemberOf":"sfsd:1969/1/pykala/2","version":"sfl:Original","versionDate":"1969-01-10"}],"isAbout":"http://data.finlex.fi/voc/finlex/kw3314","isMemberOf":"sfsd:1969/1","repeals":["sfsd:1889/39/luku/2/pykala/11/ajantasa/19690110","sfsd:1889/39/luku/2/pykala/14/ajantasa/19690110"],"version":"sfl:Original","versionDate":"1969-01-10","languageVersion":{"@id":"sfsd:1969/1/alkup/fin","title_fi":"Laki eräiden lisärangaistusten poistamisesta"}},"@context":{"languageVersion":{"@id":"eli:is_realized_by","@type":"@id"},"temporalVersions":{"@id":"eli:has_member","@type":"@id"},"temporalVersion":{"@id":"eli:has_member","@type":"@id"},"hasFormat":{"@id":"eli:is_embodied_by","@type":"@id"},"idLocal":"eli:id_local","dateDocument":{"@id":"eli:date_document","@type":"xsd:date"},"hasPart":{"@id":"eli:has_part","@type":"@id"},"isAbout":{"@id":"eli:is_about","@type":"@id"},"isMemberOf":{"@id":"eli:is_member_of","@type":"@id"},"repeals":{"@id":"eli:repeals","@type":"@id"},"version":{"@id":"eli:version","@type":"@id"},"versionDate":{"@id":"eli:version_date","@type":"xsd:date"},"title_fi":{"@id":"eli:title","@language":"fi"},"cites":{"@id":"eli:cites","@type":"@id"},"content_fi":{"@id":"sfl:text","@language":"fi"},"inForce":{"@id":"eli:in_force","@type":"@id"},"typeDocument":{"@id":"eli:type_document","@type":"@id"},"dcterms":"http://purl.org/dc/terms/","xsd":"http://www.w3.org/2001/XMLSchema#","common":"http://data.finlex.fi/common/","rdf":"http://www.w3.org/1999/02/22-rdf-syntax-