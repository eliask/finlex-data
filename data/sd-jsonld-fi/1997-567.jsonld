sfsd:1997/567/pykala/4/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1997/567/pykala/4/momentti/2/alkup/fin/txt","content_fi":"Työjärjestyksen vahvistaa työvoimatoimisto."}}}],"isMemberOf":"sfsd:1997/567/pykala/4","version":"sfl:Original","versionDate":"1997-06-19","languageVersion":{"@id":"sfsd:1997/567/pykala/4/alkup/fin","title_fi":"Työjärjestys"}},{"@id":"sfsd:1997/567/pykala/5/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1997/567/pykala/5/momentti/1/alkup","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1997/567/pykala/5/momentti/1/johdanto/alkup","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1997/567/pykala/5/momentti/1/johdanto","version":"sfl:Original","versionDate":"1997-06-19","languageVersion":{"@id":"sfsd:1997/567/pykala/5/momentti/1/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:1997/567/pykala/5/momentti/1/johdanto/alkup/fin/txt","content_fi":"Kelpoisuusvaatimuksena on:"}}},{"@id":"sfsd:1997/567/pykala/5/momentti/1/kohta/1/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1997/567/pykala/5/momentti/1/kohta/1","version":"sfl:Original","versionDate":"1997-06-19","languageVersion":{"@id":"sfsd:1997/567/pykala/5/momentti/1/kohta/1/alkup/fin","hasFormat":{"@id":"sfsd:1997/567/pykala/5/momentti/1/kohta/1/alkup/fin/txt","content_fi":"1) toimistonjohtajan virkaan korkeakoulututkinto tai korkeakoulun työministeriön toimeksiannosta järjestämän 85 opintoviikon kelpoisuuskoulutusohjelman hyväksytty suorittaminen;"}}},{"@id":"sfsd:1997/567/pykala/5/momentti/1/kohta/2/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1997/567/pykala/5/momentti/1/kohta/2","version":"sfl:Original","versionDate":"1997-06-19","languageVersion":{"@id":"sfsd:1997/567/pykala/5/momentti/1/kohta/2/alkup/fin","hasFormat":{"@id":"sfsd:1997/567/pykala/5/momentti/1/kohta/2/alkup/fin/txt","content_fi":"2) ammatinvalintapsykologin virkaan psykologian maisterin tutkinto taikka ylempi korkeakoulututkinto ja siihen sisältyvä tai erikseen suoritettu ylin arvosana psykologiassa."}}}],"isMemberOf":"sfsd:1997/567/pykala/5/momentti/1","version":"sfl:Original","versionDate":"1997-06-19"},"isMemberOf":"sfsd:1997/567/pykala/5","version":"sfl:Original","versionDate":"1997-06-19","languageVersion":{"@id":"sfsd:1997/567/pykala/5/alkup/fin","title_fi":"Kelpoisuusvaatimukset"}},{"@id":"sfsd:1997/567/pykala/6/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1997/567/pykala/6/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/567/pykala/6/momentti/1","version":"sfl:Original","versionDate":"1997-06-19","languageVersion":{"@id":"sfsd:1997/567/pykala/6/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1997/567/pykala/6/momentti/1/alkup/fin/txt","content_fi":"Työvoimatoimiston päällikön nimittää työvoimaosasto. Työvoimatoimiston muut virkamiehet nimittää työvoimatoimisto."}}},{"@id":"sfsd:1997/567/pykala/6/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/567/pykala/6/momentti/2","version":"sfl:Original","versionDate":"1997-06-19","languageVersion":{"@id":"sfsd:1997/567/pykala/6/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1997/567/pykala/6/momentti/2/alkup/fin/txt","content_fi":"Nimittämisestä virkamieheksi määräaikaiseen virkasuhteeseen ja ottamisesta työsopimussuhteeseen samoin kuin tällöin maksettavasta palkasta päättää sama viranomainen, joka päättää nimittämisestä vastaavaan virkaan."}}}],"isMemberOf":"sfsd:1997/567/pykala/6","version":"sfl:Original","versionDate":"1997-06-19","languageVersion":{"@id":"sfsd:1997/567/pykala/6/alkup/fin","title_fi":"Virkojen täyttäminen"}},{"@id":"sfsd:1997/567/pykala/7/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1997/567/pykala/7/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/567/pykala/7/momentti/1","version":"sfl:Original","versionDate":"1997-06-19","languageVersion":{"@id":"sfsd:1997/567/pykala/7/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1997/567/pykala/7/momentti/1/alkup/fin/txt","content_fi":"Työvoimatoimiston päällikölle myöntää virkavapauden työvoimatoimisto, jos kysymys on virkavapaudesta, jonka saamiseen virkamiehellä on lain tai virkaehtosopimuksen nojalla oikeus, ja muissa tapauksissa työvoimaosasto. Työvoimatoimiston muille virkamiehille virkavapauden myöntää työvoimatoimisto."}}},"isMemberOf":"sfsd:1997/567/pykala/7","version":"sfl:Original","versionDate":"1997-06-19","languageVersion":{"@id":"sfsd:1997/567/pykala/7/alkup/fin","title_fi":"Virkavapauden myöntäminen"}},{"@id":"sfsd:1997/567/pykala/8/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1997/567/pykala/8/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/567/pykala/8/momentti/1","version":"sfl:Original","versionDate":"1997-06-19","languageVersion":{"@id":"sfsd:1997/567/pykala/8/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1997/567/pykala/8/momentti/1/alkup/fin/txt","content_fi":"Työministeriö päättää työvoimatoimistojen palkkauksiin käytettävissä olevien määrärahojen jakamisesta työvoimaosastoille. Työvoimaosasto päättää palkkauksiin käytettävissä olevien määrärahojen jakamisesta työvoimatoimistoille."}}},{"@id":"sfsd:1997/567/pykala/8/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1997/567/pykala/8/momentti/2","version":"sfl:Original","versionDate":"1997-06-19","languageVersion":{"@id":"sfsd:1997/567/pykala/8/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1997/567/pykala/8/momentti/2/alkup/fin/txt","content_fi":"Työvoimaosasto voi avoimien virkojen uudelleensijoittelua varten kieltää työvoimatoimistoa ryhtymästä toimenpiteisiin viran täyttämiseksi."}}}],"isMemberOf":"sfsd:1997/567/pykala/8","version":"sfl:Original","versionDate":"1997-06-19","languageVersion":{"@id":"sfsd:1997/567/pykala/8/alkup/fin","title_fi":"Palkkausmäärärahat"}},{"@id":"sfsd:1997/567/pykala/9/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1997/567/pykala/9/momentti/1/alkup","@type":"sfl:SubsectionVersion","cites":"sfsd:1988/651","isMemberOf":"sfsd:19