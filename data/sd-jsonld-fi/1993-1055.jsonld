rsionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/15/momentti/2/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/15/momentti/2/johdanto/alkup/fin/txt","content_fi":"Kirkkoherran tai kappalaisen virkaan kelpoinen ei ole hakija:"}}},{"@id":"sfsd:1993/1055/luku/6/pykala/15/momentti/2/kohta/1/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/15/momentti/2/kohta/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/15/momentti/2/kohta/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/15/momentti/2/kohta/1/alkup/fin/txt","content_fi":"1) joka ei täytä 10 §:ssä säädettyjä kelpoisuusvaatimuksia;"}}},{"@id":"sfsd:1993/1055/luku/6/pykala/15/momentti/2/kohta/2/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/15/momentti/2/kohta/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/15/momentti/2/kohta/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/15/momentti/2/kohta/2/alkup/fin/txt","content_fi":"2) joka on pidätetty pappisviran toimittamisesta;"}}},{"@id":"sfsd:1993/1055/luku/6/pykala/15/momentti/2/kohta/3/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/15/momentti/2/kohta/3","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/15/momentti/2/kohta/3/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/15/momentti/2/kohta/3/alkup/fin/txt","content_fi":"3) joka on hakijana välittömällä vaalilla valittavaan kirkkoherran virkaan paitsi 18 §:n 2 ja 3 momentissa tarkoitetuissa tapauksissa;"}}},{"@id":"sfsd:1993/1055/luku/6/pykala/15/momentti/2/kohta/4/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/15/momentti/2/kohta/4","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/15/momentti/2/kohta/4/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/15/momentti/2/kohta/4/alkup/fin/txt","content_fi":"4) jolta ilmeisesti puuttuvat viran hoitoon tarvittavat edellytykset."}}}],"isMemberOf":"sfsd:1993/1055/luku/6/pykala/15/momentti/2","version":"sfl:Original","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/6/pykala/15/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/15/momentti/3","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/15/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/15/momentti/3/alkup/fin/txt","content_fi":"Tuomiokapituli voi perustellusta syystä päättää viran hakuajan jatkamisesta, uudesta hakumenettelystä tai viran täyttämättä jättämisestä."}}}],"isMemberOf":"sfsd:1993/1055/luku/6/pykala/15","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/6/pykala/16/ajantasa/19911108","@type":"sfl:SectionVersion","amendedBy":"sfsd:2013/415/alkup","hasPart":[{"@id":"sfsd:1993/1055/luku/6/pykala/16/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/16/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/16/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/16/momentti/1/alkup/fin/txt","content_fi":"Kirkkoherran välitöntä vaalia varten tuomiokapituli tekee vaaliehdotuksen. Tuomiokapituli voi ennen vaaliehdotuksen tekemistä pyytää kirkkoneuvostolta tai seurakuntaneuvostolta lausunnon virkaan kelpoisista hakijoista. Vaaliehdotuksessa tuomiokapituli asettaa vaaliehdokkaiksi kolme kelpoisuusvaatimukset täyttävää hakijaa vaalisijoille siinä järjestyksessä kuin heidät katsotaan taitaviksi ja kykeneviksi virkaan ottaen huomioon viran erityiset tarpeet."}}},{"@id":"sfsd:1993/1055/luku/6/pykala/16/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/16/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/16/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/16/momentti/2/alkup/fin/txt","content_fi":"Kun kirkkoherra valitaan välittömällä vaalilla, hakija saa peruuttaa hakemuksensa ennen kuin vaaliehdotus on tehty. Tuomiokapituli voi erityisestä syystä hyväksyä myöhemmin tehdyn peruutuksen."}}},{"@id":"sfsd:1993/1055/luku/6/pykala/16/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/16/momentti/3","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/16/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/16/momentti/3/alkup/fin/txt","content_fi":"Kirkkoherran välillistä vaalia ja kappalaisen vaalia varten tuomiokapituli antaa hakijoista lausunnon seurakunnalle. Lausunnossa todetaan hakijoiden kelpoisuus ja arvioidaan heidän taitonsa ja kykynsä haettavana olevaan virkaan."}}}],"isMemberOf":"sfsd:1993/1055/luku/6/pykala/16","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/6/pykala/17/ajantasa/19911108","@type":"sfl:SectionVersion","amendedBy":"sfsd:2013/415/alkup","hasPart":[{"@id":"sfsd:1993/1055/luku/6/pykala/17/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/17/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/17/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/17/momentti/1/alkup/fin/txt","content_fi":"Tuomiokapituli voi julistaa kirkkoherran tai kappalaisen viran uudelleen haettavaksi kirkkovaltuuston, seurakuntaneuvoston tai kappelineuvoston pyynnöstä, jos virkaa on hakenut vain yksi kelpoinen hakija tai jos seurakunnan etu sitä vaatii."}}},{"@id":"sfsd:1993/1055/luku/6/pykala/17/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/17/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/17/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/17/momentti/2/alkup/fin/txt","content_fi":"Jos kirkkoherran tai kappalaisen virkaan ei uutena hakuaikana tule uutta kelpoisuusvaatimukset täyttävää hakijaa, tuomiokapituli antaa kirkkovaltuuston, seurakuntaneuvoston tai kappelineuvoston pyynnöstä kelpoiselle hakijalle viranhoitomääräyksen virkaan. Muussa tapauksessa tuomiokapituli voi perustellusta syystä päättää, että viran täyttämismenettely raukeaa."}}},{"@id":"sfsd:1993/1055/luku/6/pykala/17/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/17/momentti/3","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/17/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/17/momentti/3/alkup/fin/txt","content_fi":"Viran täyttämismenettelyn rauettua virka on julistettava haettavaksi vuoden kuluessa hakuajan päättymisestä lukien."}}}],"isMemberOf":"sfsd:1993/1055/luku/6/pykala/17","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/6/pykala/18/ajantasa/19911108","@type":"sfl:SectionVersion","amendedBy":"sfsd:2013/415/alkup","hasPart":[{"@id":"sfsd:1993/1055/luku/6/pykala/18/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/18/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/18/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/18/momentti/1/alkup/fin/txt","content_fi":"Kun kirkkoherran välitön vaali on saanut lainvoiman, tuomiokapituli antaa viranhoitomääräyksen kirkkoherran virkaan sille, joka on saanut vaalissa enimmät äänet."}}},{"@id":"sfsd:1993/1055/luku/6/pykala/18/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/18/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/18/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/18/momentti/2/alkup/fin/txt","content_fi":"Vaaliehdokkaalla, joka vaalin tuloksen mukaan on saamassa viranhoitomääräyksen kirkkoherran virkaan, on oikeus hakea toista kirkkoherran tai kappalaisen virkaa, jos viranhoitomääräystä ei ole annettu kolmen kuukauden kuluessa vaalin toimittamisesta. Jos hän on saamassa viranhoitomääräyksen useampaan virkaan, hänen on viivytyksettä ilmoitettava tuomiokapitulille, minkä viran hän ottaa vastaan."}}},{"@id":"sfsd:1993/1055/luku/6/pykala/18/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/18/momentti/3","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/18/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/18/momentti/3/alkup/fin/txt","content_fi":"Muulla vaaliehdokkaalla on oikeus hakea toista kirkkoherran tai kappalaisen virkaa, kun vaali on toimitettu. Hän pysyy kuitenkin ehdolla ensiksi hakemaansa virkaan, kunnes asia on lopullisesti ratkaistu. Jos hän on saamassa viranhoitomääräyksen useampaan virkaan, asiassa menetellään siten kuin 2 momentissa säädetään."}}}],"isMemberOf":"sfsd:1993/1055/luku/6/pykala/18","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/6/valiotsikko/4/alkup","@type":"sfl:SubheadingVersion","isMemberOf":"sfsd:1993/1055/luku/6/valiotsikko/4","version":"sfl:Original","versionDate":"2017-01-01","followedBy":"sfsd:1993/1055/luku/6/pykala/19/ajantasa/19911108","languageVersion":{"@id":"sfsd:1993/1055/luku/6/valiotsikko/4/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/valiotsikko/4/alkup/fin/txt","content_fi":"Otsikko on kumottu  P:llä 8.11.2013/415 v. 2014.\n(8.11.2013/415 v. 2014)"}}},{"@id":"sfsd:1993/1055/luku/6/pykala/19/ajantasa/19911108","@type":"sfl:SectionVersion","amendedBy":"sfsd:2013/415/alkup","hasPart":[{"@id":"sfsd:1993/1055/luku/6/pykala/19/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/19/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/19/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/19/momentti/1/alkup/fin/txt","content_fi":"Kirkkoherran välillistä vaalia ja kappalaisen vaalia varten tuomiokapituli lähettää seurakunnalle hakijoiden hakemukset ja lausuntonsa hakijoista. Tuomiokapituli määrää henkilön, jonka tehtävänä on kirkkoherran välillisen vaalin valmisteleminen seurakunnassa."}}},{"@id":"sfsd:1993/1055/luku/6/pykala/19/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/19/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/19/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/19/momentti/2/alkup/fin/txt","content_fi":"Kirkkovaltuusto tai seurakuntaneuvosto valitsee virkaan jonkun niistä hakijoista, jotka tuomiokapituli on todennut kelpoisiksi virkaan. Kappeliseurakunnan kappalaisen valitsee kuitenkin kappelineuvosto."}}},{"@id":"sfsd:1993/1055/luku/6/pykala/19/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/19/momentti/3","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/19/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/19/momentti/3/alkup/fin/txt","content_fi":"Jollei kirkkoherran välillisessä vaalissa kukaan ole saanut yli puolta annetuista äänistä, toimitetaan samassa kokouksessa uusi vaali kahden eniten ääniä saaneen välillä. Tässä vaalissa eniten ääniä saanut tulee valituksi."}}},{"@id":"sfsd:1993/1055/luku/6/pykala/19/momentti/4/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/19/momentti/4","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/19/momentti/4/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/19/momentti/4/alkup/fin/txt","content_fi":"Jos äänet kappalaisen vaalissa tai kirkkoherran välillisen vaalin ratkaisevassa äänestyksessä ovat menneet tasan, tuomiokapituli antaa viranhoitomääräyksen jollekin eniten ääniä saaneista hakijoista ottaen huomioon 16 §:n 3 momentissa mainitut seikat."}}}],"isMemberOf":"sfsd:1993/1055/luku/6/pykala/19","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/6/pykala/20/ajantasa/19911108","@type":"sfl:SectionVersion","amendedBy":"sfsd:2013/415/alkup","hasPart":[{"@id":"sfsd:1993/1055/luku/6/pykala/20/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/20/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/20/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/20/momentti/1/alkup/fin/txt","content_fi":"Kun kirkkovaltuusto, seurakuntaneuvosto tai kappelineuvosto on toimittanut kirkkoherran välillisen vaalin tai kappalaisen vaalin, sen on lähetettävä kirkkoherran tai kappalaisen viran hakemusasiakirjat ja ote pöytäkirjasta tuomiokapitulille. Pöytäkirjan otteeseen on liitettävä ilmoitus siitä, milloin päätös valitusosoituksineen on annettu tiedoksi hakijoille."}}},{"@id":"sfsd:1993/1055/luku/6/pykala/20/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/20/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/20/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/20/momentti/2/alkup/fin/txt","content_fi":"Jos virkaan valittu on saamassa viranhoitomääräyksen useampaan virkaan, hänen on viivytyksettä ilmoitettava tuomiokapitulille, minkä viran hän ottaa vastaan. Jos hän peruuttaa hakemuksensa, virka on julistettava uudelleen haettavaksi, jollei kirkkovaltuusto, seurakuntaneuvosto tai kappelineuvosto menettele kirkkolain 6 luvun 11 §:n 1 momentin 5 kohdan mukaisesti."}}},{"@id":"sfsd:1993/1055/luku/6/pykala/20/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/20/momentti/3","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/20/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/20/momentti/3/alkup/fin/txt","content_fi":"Tuomiokapituli antaa virkaan valitulle viranhoitomääräyksen kirkkovaltuuston tai seurakuntaneuvoston taikka kappelineuvoston päätöksen tultua lainvoimaiseksi."}}}],"isMemberOf":"sfsd:1993/1055/luku/6/pykala/20","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/6/pykala/21/ajantasa/19911108","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/21","repealedBy":"sfsd:2013/415/alkup","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/21/ajantasa/19911108/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/21/ajantasa/19911108/fin/txt","content_fi":"21–27 §\n\n\n21–27 § on kumottu  P:llä 8.11.2013/415 v. 2014."}}},{"@id":"sfsd:1993/1055/luku/6/pykala/21/alkup","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/21","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/21/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/21/alkup/fin/txt","content_fi":"21–27 §\n\n\n21–27 § on kumottu  P:llä 8.11.2013/415 v. 2014."}}},{"@id":"sfsd:1993/1055/luku/6/valiotsikko/5/alkup","@type":"sfl:SubheadingVersion","isMemberOf":"sfsd:1993/1055/luku/6/valiotsikko/5","version":"sfl:Original","versionDate":"2017-01-01","followedBy":"sfsd:1993/1055/luku/6/pykala/28/ajantasa/19911108","languageVersion":{"@id":"sfsd:1993/1055/luku/6/valiotsikko/5/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/valiotsikko/5/alkup/fin/txt","content_fi":"E. Kanttorin virka\n(12.11.2010/1009 v. 2012)"}}},{"@id":"sfsd:1993/1055/luku/6/pykala/28/ajantasa/19911108","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/1009/alkup","hasPart":{"@id":"sfsd:1993/1055/luku/6/pykala/28/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/28/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/28/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/28/momentti/1/alkup/fin/txt","content_fi":"Kanttorin tehtävänä on johtaa seurakunnan musiikkitoimintaa. Hän vastaa musiikista seurakunnan jumalanpalveluksissa sekä muissa kirkollisissa toimituksissa ja seurakunnan tilaisuuksissa, opettaa rippikoulussa, hoitaa muutenkin musiikkikasvatustyötä ja edistää musiikin käyttöä seurakunnan toiminnassa. Kanttori vastaa myös seurakunnan soittimien hoidosta ja huollosta."}}},"isMemberOf":"sfsd:1993/1055/luku/6/pykala/28","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/6/pykala/29/ajantasa/19911108","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/1009/alkup","hasPart":{"@id":"sfsd:1993/1055/luku/6/pykala/29/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/29/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/29/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/29/momentti/1/alkup/fin/txt","content_fi":"Kirkkohallitus voi antaa kelpoisuuden kanttorin virkoihin henkilölle, jolla on virkaan vaadittavaa tutkintoa vastaavat tiedot ja taidot."}}},"isMemberOf":"sfsd:1993/1055/luku/6/pykala/29","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/6/valiotsikko/6/alkup","@type":"sfl:SubheadingVersion","isMemberOf":"sfsd:1993/1055/luku/6/valiotsikko/6","version":"sfl:Original","versionDate":"2017-01-01","followedBy":"sfsd:1993/1055/luku/6/pykala/30/ajantasa/19911108","languageVersion":{"@id":"sfsd:1993/1055/luku/6/valiotsikko/6/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/valiotsikko/6/alkup/fin/txt","content_fi":"F. Lehtorin virka\n(12.11.2010/1009 v. 2012)"}}},{"@id":"sfsd:1993/1055/luku/6/pykala/30/ajantasa/19911108","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/1009/alkup","hasPart":[{"@id":"sfsd:1993/1055/luku/6/pykala/30/momentti/1/ajantasa/19911108","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2015/1606/alkup","isMemberOf":"sfsd:1993/1055/luku/6/pykala/30/momentti/1","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/30/momentti/1/ajantasa/19911108/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/30/momentti/1/ajantasa/19911108/fin/txt","content_fi":"Tuomiokapituli voi hakemuksesta myöntää oikeuden toimia lehtorina henkilölle, joka on yliopistossa suorittanut sellaisen teologisen tutkinnon, jonka piispainkokous on hyväksynyt lehtorin viran kelpoisuusvaatimukseksi.\n(8.5.2015/1606)"}}},{"@id":"sfsd:1993/1055/luku/6/pykala/30/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/30/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/30/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/30/momentti/2/alkup/fin/txt","content_fi":"Tuomiokapitulin tulee tutkia hänen sopivuutensa seurakunnan palvelukseen noudattaen soveltuvin osin, mitä papiksi vihittävästä on määrätty."}}},{"@id":"sfsd:1993/1055/luku/6/pykala/30/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/30/momentti/3","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/30/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/30/momentti/3/alkup/fin/txt","content_fi":"Lehtorina pidetään henkilöä, joka tuomiokapitulin antaman viranhoitomääräyksen perusteella on seurakunnan tai seurakuntayhtymän taikka kirkkolain 6 luvun 8 §:ssä tarkoitetun lehtorin viran haltija tai tuomiokapitulin oikeuttamana on tämän luvun 38 §:ssä tarkoitetun yhteisön palveluksessa."}}}],"isMemberOf":"sfsd:1993/1055/luku/6/pykala/30","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/6/pykala/31/ajantasa/19911108","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/1009/alkup","hasPart":{"@id":"sfsd:1993/1055/luku/6/pykala/31/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/31/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/31/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/31/momentti/1/alkup/fin/txt","content_fi":"Lehtorin tehtävänä on hoitaa seurakunnassa kristillistä kasvatus-, opetus- ja sielunhoitotyötä sekä osallistua muuhun seurakuntatyöhön. Lehtorin oikeudesta saarnata jumalanpalveluksessa, jakaa ehtoollista ja toimittaa konfirmaatio määrätään 2 luvun 6 ja 12 §:ssä sekä 3 luvun 5 §:ssä."}}},"isMemberOf":"sfsd:1993/1055/luku/6/pykala/31","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/6/pykala/32/ajantasa/19911108","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/1009/alkup","hasPart":[{"@id":"sfsd:1993/1055/luku/6/pykala/32/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/32/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/32/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/32/momentti/1/alkup/fin/txt","content_fi":"Seurakunnassa voi olla vakinaisia ja ylimääräisiä lehtorin virkoja. Ylimääräisiä lehtorin virkoja voi olla myös seurakuntayhtymässä."}}},{"@id":"sfsd:1993/1055/luku/6/pykala/32/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/32/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/32/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/32/momentti/2/alkup/fin/txt","content_fi":"Vakinaisen lehtorin viran hakijalta vaaditaan, että hän on suorittanut tämän luvun 10 §:ssä mainitun tutkinnon. Tutkinnon suorittamisen edellytyksistä on soveltuvin osin voimassa mitä tämän luvun 11 §:ssä säädetään."}}}],"isMemberOf":"sfsd:1993/1055/luku/6/pykala/32","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/6/pykala/33/ajantasa/19911108","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/1009/alkup","hasPart":[{"@id":"sfsd:1993/1055/luku/6/pykala/33/momentti/1/ajantasa/19911108","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2013/415/alkup","isMemberOf":"sfsd:1993/1055/luku/6/pykala/33/momentti/1","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/33/momentti/1/ajantasa/19911108/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/33/momentti/1/ajantasa/19911108/fin/txt","content_fi":"Vakinaisen lehtorin viran täyttämisestä on soveltuvin osin voimassa, mitä kappalaisen viran täyttämisestä 14–17, 19 ja 20 §:ssä säädetään.\n(8.11.2013/415 v. 2014)"}}},{"@id":"sfsd:1993/1055/luku/6/pykala/33/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/33/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/33/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/33/momentti/2/alkup/fin/txt","content_fi":"Kirkkoneuvosto tai seurakuntaneuvosto täyttää ylimääräisen lehtorin viran ja pyytää valitulle tuomiokapitulilta viranhoitomääräyksen."}}}],"isMemberOf":"sfsd:1993/1055/luku/6/pykala/33","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/6/pykala/34/ajantasa/19911108","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/1009/alkup","hasPart":{"@id":"sfsd:1993/1055/luku/6/pykala/34/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/34/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/34/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/34/momentti/1/alkup/fin/txt","content_fi":"Lehtori kuuluu hiippakuntaan, jonka tuomiokapitulilta hän on saanut nimityksen tai viranhoitomääräyksen taikka oikeuden toimia kristillisen yhdistyksen tai muun yhteisön palveluksessa. Seurakuntayhtymän palveluksessa olevan lehtorin kuulumisesta hiippakuntaan sekä lehtorin kuulumisesta rovastikuntaan on voimassa, mitä 5 luvun 10 §:n 2 ja 3 momentissa on papista säädetty."}}},"isMemberOf":"sfsd:1993/1055/luku/6/pykala/34","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/6/valiotsikko/7/alkup","@type":"sfl:SubheadingVersion","isMemberOf":"sfsd:1993/1055/luku/6/valiotsikko/7","version":"sfl:Original","versionDate":"2017-01-01","followedBy":"sfsd:1993/1055/luku/6/pykala/35/ajantasa/19911108","languageVersion":{"@id":"sfsd:1993/1055/luku/6/valiotsikko/7/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/valiotsikko/7/alkup/fin/txt","content_fi":"G. Seurakuntien yhteiset virat\n(12.11.2010/1009 v. 2012)"}}},{"@id":"sfsd:1993/1055/luku/6/pykala/35/ajantasa/19911108","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/1009/alkup","hasPart":[{"@id":"sfsd:1993/1055/luku/6/pykala/35/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/35/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/35/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/35/momentti/1/alkup/fin/txt","content_fi":"Aloitteen yhteisen kirkkoherran, kanttorin, diakonian tai seurakunnan taloushallinnon johtotehtäviä koskevan viran perustamisesta voi tehdä kirkkovaltuusto, yhteinen kirkkovaltuusto, seurakuntaneuvosto tai tuomiokapituli."}}},{"@id":"sfsd:1993/1055/luku/6/pykala/35/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/35/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/35/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/35/momentti/2/alkup/fin/txt","content_fi":"Kustannusten jakoperusteet vahvistetaan viran perustamisesta päätettäessä."}}}],"isMemberOf":"sfsd:1993/1055/luku/6/pykala/35","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/6/pykala/36/ajantasa/19911108","@type":"sfl:SectionVersion","amendedBy":"sfsd:2013/415/alkup","hasPart":{"@id":"sfsd:1993/1055/luku/6/pykala/36/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/36/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/36/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/36/momentti/1/alkup/fin/txt","content_fi":"Yhteisen kirkkoherran välillinen vaali toimitetaan seurakuntien kirkkovaltuustojen ja seurakuntaneuvostojen yhteisessä kokouksessa noudattaen soveltuvin osin mitä 19 ja 20 §:ssä säädetään."}}},"isMemberOf":"sfsd:1993/1055/luku/6/pykala/36","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/6/pykala/37/ajantasa/19911108","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/1009/alkup","hasPart":{"@id":"sfsd:1993/1055/luku/6/pykala/37/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/37/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/37/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/37/momentti/1/alkup/fin/txt","content_fi":"Seurakuntien kirkkoneuvostot tai seurakuntaneuvostot päättävät yhteisessä kokouksessa yhteistä kirkkoherran, kanttorin, diakonian tai seurakunnan taloushallinnon johtotehtäviä koskevan viran täyttöä ja virkaa koskevista asioista, jollei erikseen toisin säädetä tai määrätä. Jos seurakuntien kirkkoherran virka ei ole yhteinen, puheenjohtajana toimii väkiluvultaan suurimman seurakunnan kirkkoherra."}}},"isMemberOf":"sfsd:1993/1055/luku/6/pykala/37","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/6/valiotsikko/8/alkup","@type":"sfl:SubheadingVersion","isMemberOf":"sfsd:1993/1055/luku/6/valiotsikko/8","version":"sfl:Original","versionDate":"2017-01-01","followedBy":"sfsd:1993/1055/luku/6/pykala/38/ajantasa/19911108","languageVersion":{"@id":"sfsd:1993/1055/luku/6/valiotsikko/8/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/valiotsikko/8/alkup/fin/txt","content_fi":"H. Muissa yhteisöissä toimivat papit ja lehtorit\n(12.11.2010/1009 v. 2012)"}}},{"@id":"sfsd:1993/1055/luku/6/pykala/38/ajantasa/19911108","@type":"sfl:SectionVersion","amendedBy":"sfsd:2010/1009/alkup","hasPart":{"@id":"sfsd:1993/1055/luku/6/pykala/38/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/6/pykala/38/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/pykala/38/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/6/pykala/38/momentti/1/alkup/fin/txt","content_fi":"Tuomiokapituli voi hakemuksesta antaa papille tai lehtorille oikeuden toimia pappina tai lehtorina kristillisen yhdistyksen, yhteisön, säätiön tai laitoksen palveluksessa. Papille annettavan oikeuden edellytyksenä on lisäksi, että hän on pappina palvellut seurakunnassa tai seurakuntayhtymässä vähintään kaksi vuotta, jollei hakemuksessa ole kysymys palvelusta kirkon lähetysjärjestössä tai jollei muutoin ole olemassa painavia syitä."}}},"isMemberOf":"sfsd:1993/1055/luku/6/pykala/38","version":"sfl:Consolidated","versionDate":"2017-01-01"}],"isMemberOf":"sfsd:1993/1055/luku/6","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/6/ajantasa/19911108/fin","title_fi":"Henkilöstö"}}],"isMemberOf":"sfsd:1993/1055/osa/3","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/osa/3/ajantasa/20061101/fin","title_fi":"Pappisvirka, viranhaltijat ja työntekijät"}},{"@id":"sfsd:1993/1055/osa/4/ajantasa/20081107","@type":"sfl:PartVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/7/ajantasa/20050101","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/7/pykala/1/ajantasa/20050101","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/7/pykala/1/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/7/pykala/1/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/pykala/1/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/7/pykala/1/momentti/1/alkup/fin/txt","content_fi":"Kirkkolaissa tai kirkkojärjestyksessä säädettyjen alistettavien ohjesääntöjen lisäksi voi seurakunta ja seurakuntayhtymä hyväksyä johtosääntöjä toimielimien, viranhaltijoiden ja työntekijöiden toiminnan sekä seurakunnan muun hallinnon järjestämiseksi."}}},{"@id":"sfsd:1993/1055/luku/7/pykala/1/momentti/2/ajantasa/20050101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2003/822/alkup","isMemberOf":"sfsd:1993/1055/luku/7/pykala/1/momentti/2","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/pykala/1/momentti/2/ajantasa/20050101/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/7/pykala/1/momentti/2/ajantasa/20050101/fin/txt","content_fi":"Ohjesäännössä tai johtosäännössä voidaan määrätä, että toimielimen tai viranhaltijan päätös on tehtävä esittelystä. Kirkkoherra esittelee kirkkoneuvostolle tai seurakuntaneuvostolle taikka muulle toimielimelle toimialaansa koskevat asiat, jollei hän ole määrännyt esittelyä muulle viranhaltijalle.\n(9.5.2003/822 v. 2004)"}}}],"isMemberOf":"sfsd:1993/1055/luku/7/pykala/1","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/7/pykala/2/ajantasa/19911108","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1993/1055/luku/7/pykala/2","repealedBy":"sfsd:2013/415/alkup","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/pykala/2/ajantasa/19911108/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/7/pykala/2/ajantasa/19911108/fin/txt","content_fi":"2 §(8.11.2013/415 v. 2014)\n\n2 § on kumottu  P:llä 8.11.2013/415 v. 2014."}}},{"@id":"sfsd:1993/1055/luku/7/pykala/3/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/7/pykala/3/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/7/pykala/3/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/pykala/3/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/7/pykala/3/momentti/1/alkup/fin/txt","content_fi":"Milloin seurakuntavaaleilla valitun toimielimen jäsen on tilapäisesti estynyt saapumasta kokoukseen, hänen on viivytyksettä ilmoitettava siitä toimielimen puheenjohtajalle. Saatuaan jäseneltä tai muuten luotettavan tiedon esteestä, puheenjohtajan tulee kutsua varajäsen hänen sijaansa."}}},{"@id":"sfsd:1993/1055/luku/7/pykala/3/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/7/pykala/3/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/pykala/3/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/7/pykala/3/momentti/2/alkup/fin/txt","content_fi":"Jos muu kuin 1 momentissa mainittu luottamushenkilö on tilapäisesti estynyt saapumasta kokoukseen, hänen on, jos hänellä on varajäsen, kutsuttava tämä sijaansa. Myös puheenjohtaja voi toimittaa kutsun varajäsenelle."}}},{"@id":"sfsd:1993/1055/luku/7/pykala/3/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/7/pykala/3/momentti/3","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/pykala/3/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/7/pykala/3/momentti/3/alkup/fin/txt","content_fi":"Milloin sekä puheenjohtaja että varapuheenjohtaja ovat poissa tai esteellisiä jossakin asiassa, valitaan kokousta tai sanotun asian käsittelyä varten tilapäinen puheenjohtaja."}}}],"isMemberOf":"sfsd:1993/1055/luku/7/pykala/3","version":"sfl:Original","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/7/pykala/4/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/7/pykala/4/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/7/pykala/4/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/pykala/4/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/7/pykala/4/momentti/1/alkup/fin/txt","content_fi":"Toimielimen kokouksessa käsiteltävän asian esittelyn jälkeen on varattava tilaisuus keskustella asiasta. Kun pyydetyt puheenvuorot on käytetty, puheenjohtajan on julistettava keskustelu päättyneeksi."}}},{"@id":"sfsd:1993/1055/luku/7/pykala/4/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/7/pykala/4/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/pykala/4/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/7/pykala/4/momentti/2/alkup/fin/txt","content_fi":"Milloin päätös on yksimielinen tai tehtyä vastaehdotusta ei ole kannatettu, puheenjohtajan on todettava päätös. Muussa tapauksessa puheenjohtajan on todettava ehdotukset, joita ei kannatuksen puuttuessa oteta äänestettäviksi, ja ehdotukset, joista on äänestettävä. Tämän jälkeen hänen on saatettava toimielimen hyväksyttäväksi äänestystapa ja, jos äänestyksiä on toimitettava useampia, äänestysjärjestys sekä tehtävä äänestysesitys siten, että vastaus \"\"jaa\"\" tai \"\"ei\"\" ilmaisee kannanoton ehdotukseen."}}},{"@id":"sfsd:1993/1055/luku/7/pykala/4/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/7/pykala/4/momentti/3","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/pykala/4/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/7/pykala/4/momentti/3/alkup/fin/txt","content_fi":"Äänestys on toimitettava julkisesti joko nimenhuutoäänestyksellä tai puheenjohtajan määräämällä tavalla. Milloin äänestys on toimitettu muulla tavalla kuin nimenhuudon mukaan, on vaadittaessa tai, jos äänestys puheenjohtajan mielestä ei ole antanut selvää vastausta, toimitettava nimenhuutoäänestys."}}},{"@id":"sfsd:1993/1055/luku/7/pykala/4/momentti/4/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/7/pykala/4/momentti/4","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/pykala/4/momentti/4/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/7/pykala/4/momentti/4/alkup/fin/txt","content_fi":"Äänestyksen perusteella puheenjohtajan on kirkkolain 7 luvun 4 §:n 2 momentin ja 25 luvun 10 §:n mukaan todettava päätökseksi tullut mielipide."}}},{"@id":"sfsd:1993/1055/luku/7/pykala/4/momentti/5/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/7/pykala/4/momentti/5","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/pykala/4/momentti/5/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/7/pykala/4/momentti/5/alkup/fin/txt","content_fi":"Jos puheenjohtaja katsoo, että päätöksen tekemiseen vaaditaan määräenemmistön kannatus, hänen on ilmoitettava siitä ennen äänestyksen toimittamista ja otettava se huomioon äänestyksen tuloksen todetessaan."}}}],"isMemberOf":"sfsd:1993/1055/luku/7/pykala/4","version":"sfl:Original","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/7/pykala/5/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/7/pykala/5/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/7/pykala/5/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/pykala/5/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/7/pykala/5/momentti/1/alkup/fin/txt","content_fi":"Kirkkolain 7 luvun 4 §:n 3 momentissa tarkoitettua suhteellista vaalia toimitettaessa noudatetaan soveltuvin osin seurakuntavaalien suhteellista vaalitapaa koskevia säännöksiä."}}},{"@id":"sfsd:1993/1055/luku/7/pykala/5/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/7/pykala/5/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/pykala/5/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/7/pykala/5/momentti/2/alkup/fin/txt","content_fi":"Suhteellinen vaali ja vaadittaessa myös enemmistövaali on toimitettava suljetuin lipuin."}}}],"isMemberOf":"sfsd:1993/1055/luku/7/pykala/5","version":"sfl:Original","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/7/pykala/6/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/7/pykala/6/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/7/pykala/6/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/pykala/6/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/7/pykala/6/momentti/1/alkup/fin/txt","content_fi":"Toimielimen kokouksesta on pidettävä pöytäkirjaa, jonka puheenjohtaja allekirjoittaa ja sihteeri varmentaa."}}},{"@id":"sfsd:1993/1055/luku/7/pykala/6/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/7/pykala/6/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/pykala/6/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/7/pykala/6/momentti/2/alkup/fin/txt","content_fi":"Pöytäkirja tarkastetaan toimielimen päättämällä tavalla, jollei tarkastamisesta ole määrätty ohje- tai johtosäännössä."}}},{"@id":"sfsd:1993/1055/luku/7/pykala/6/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/7/pykala/6/momentti/3","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/pykala/6/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/7/pykala/6/momentti/3/alkup/fin/txt","content_fi":"Sillä, joka on ollut kokouksessa asiaa päättämässä tai esitellyt asian, on oikeus esittää päätöksestä eriävä mielipiteensä. Ilmoitus on tehtävä heti, kun päätös on tehty. Jos eriävän mielipiteen esittäjä tahtoo saada perustelunsa pöytäkirjaan liitetyiksi, hänen on annettava ne kirjallisina sihteerille viimeistään pöytäkirjaa tarkastettaessa."}}}],"isMemberOf":"sfsd:1993/1055/luku/7/pykala/6","version":"sfl:Original","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/7/pykala/7/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/7/pykala/7/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/7/pykala/7/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/pykala/7/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/7/pykala/7/momentti/1/alkup/fin/txt","content_fi":"Toimielimen toimituskirjan allekirjoittaa puheenjohtaja ja varmentaa sihteeri, jollei ohje- tai johtosäännössä ole määrätty toisin."}}},{"@id":"sfsd:1993/1055/luku/7/pykala/7/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/7/pykala/7/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/pykala/7/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/7/pykala/7/momentti/2/alkup/fin/txt","content_fi":"Pöytäkirjanotteen todistaa oikeaksi puheenjohtaja tai sihteeri taikka muu ohje- tai johtosäännössä määrätty henkilö."}}}],"isMemberOf":"sfsd:1993/1055/luku/7/pykala/7","version":"sfl:Original","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/7/pykala/8/ajantasa/20040101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1993/1055/luku/7/pykala/8","repealedBy":"sfsd:2003/1276/alkup","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/pykala/8/ajantasa/20040101/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/7/pykala/8/ajantasa/20040101/fin/txt","content_fi":"8 §(9.5.2003/1276)\n\n8 § on kumottu P:llä 9.5.2003/1276."}}},{"@id":"sfsd:1993/1055/luku/7/pykala/9/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/7/pykala/9/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/7/pykala/9/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/pykala/9/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/7/pykala/9/momentti/1/alkup/fin/txt","content_fi":"Seurakunnan jäsenellä on oikeus tehdä aloitteita seurakunnan ja seurakuntayhtymän toimintaa ja hallintoa koskevissa asioissa."}}},{"@id":"sfsd:1993/1055/luku/7/pykala/9/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/7/pykala/9/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/pykala/9/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/7/pykala/9/momentti/2/alkup/fin/txt","content_fi":"Seurakunnan jäsenille on sopivin tavoin annettava riittävät tiedot yleistä mielenkiintoa herättävistä vireillä olevista seurakunnan ja seurakuntayhtymän asioista, niitä koskevista suunnitelmista, asioiden käsittelystä ja tehdyistä ratkaisuista. Kirkkoneuvoston tehtävänä on johtaa ja valvoa tätä seurakunnan tiedotustoimintaa."}}}],"isMemberOf":"sfsd:1993/1055/luku/7/pykala/9","version":"sfl:Original","versionDate":"2017-01-01"}],"isMemberOf":"sfsd:1993/1055/luku/7","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/7/ajantasa/20050101/fin","title_fi":"Yleisiä määräyksiä"}},{"@id":"sfsd:1993/1055/luku/8/ajantasa/20050101","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/8/pykala/1/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1993/1055/luku/8/pykala/1/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/8/pykala/1/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/8/pykala/1/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/8/pykala/1/momentti/1/alkup/fin/txt","content_fi":"Kirkkovaltuustoon valitaan jäseniä seurakunnan väkiluvun ollessa  \n\n\n1 000 tai vähemmän\n11,\n\n1 001 -  4 000\n15,\n\n4 001 -  7 000 \n19,\n\n7 001 - 10 000\n23,\n\n10 001 - 20 000\n27,\n\n20 001 - 50 000\n33 ja\n\nyli      50 000\n39."}}},"isMemberOf":"sfsd:1993/1055/luku/8/pykala/1","version":"sfl:Original","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/8/pykala/2/ajantasa/20050101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2003/822/alkup","hasPart":{"@id":"sfsd:1993/1055/luku/8/pykala/2/momentti/1/ajantasa/20050101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/8/pykala/2/momentti/1","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/8/pykala/2/momentti/1/ajantasa/20050101/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/8/pykala/2/momentti/1/ajantasa/20050101/fin/txt","content_fi":"Kirkkovaltuusto valitsee keskuudestaan puheenjohtajan ja varapuheenjohtajan toimikautensa ensimmäisen ja kolmannen vuoden ensimmäisessä kokouksessa."}}},"isMemberOf":"sfsd:1993/1055/luku/8/pykala/2","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/8/pykala/3/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/8/pykala/3/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/8/pykala/3/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/8/pykala/3/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/8/pykala/3/momentti/1/alkup/fin/txt","content_fi":"Kirkkovaltuusto kokoontuu, kun kokoontuminen määräaikana käsiteltäväksi säädettyjen asioiden takia on tarpeen tai kun valtuuston puheenjohtaja muutoin katsoo sen tarpeelliseksi."}}},{"@id":"sfsd:1993/1055/luku/8/pykala/3/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/8/pykala/3/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/8/pykala/3/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/8/pykala/3/momentti/2/alkup/fin/txt","content_fi":"Valtuusto on kutsuttava koolle, jos kirkkohallitus, piispa, tuomiokapituli tai kirkkoneuvosto sitä vaatii taikka jos vähintään neljännes valtuutetuista on ilmoittamansa asian käsittelemistä varten sitä kirjallisesti esittänyt."}}}],"isMemberOf":"sfsd:1993/1055/luku/8/pykala/3","version":"sfl:Original","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/8/pykala/4/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1993/1055/luku/8/pykala/4/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/8/pykala/4/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/8/pykala/4/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/8/pykala/4/momentti/1/alkup/fin/txt","content_fi":"Jos kirkkovaltuuston jäsen haluaa saattaa jonkin asian valtuuston käsiteltäväksi, on hänen tehtävä siitä kirjallinen esitys valtuuston puheenjohtajalle, jonka on viivytyksettä toimitettava se kirkkoneuvoston valmisteltavaksi."}}},"isMemberOf":"sfsd:1993/1055/luku/8/pykala/4","version":"sfl:Original","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/8/pykala/5/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/8/pykala/5/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/8/pykala/5/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/8/pykala/5/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/8/pykala/5/momentti/1/alkup/fin/txt","content_fi":"Kirkkovaltuuston kutsuu koolle puheenjohtaja tai hänen estyneenä ollessaan varapuheenjohtaja. Jos varapuheenjohtajallakin on este, kutsun antaa kirkkoneuvoston puheenjohtaja. Kutsun valtuuston ensimmäiseen kokoukseen antaa kirkkoneuvoston puheenjohtaja ja kokouksen avaa iältään vanhin valtuutettu, joka johtaa puhetta, kunnes valtuuston puheenjohtaja ja varapuheenjohtaja on valittu."}}},{"@id":"sfsd:1993/1055/luku/8/pykala/5/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/8/pykala/5/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/8/pykala/5/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/8/pykala/5/momentti/2/alkup/fin/txt","content_fi":"Kokouskutsussa on ilmoitettava kokouksen aika ja paikka ja siihen on liitettävä luettelo käsiteltävistä asioista. Kutsu asialuetteloineen on lähetettävä kirkkovaltuuston ja kirkkoneuvoston jäsenille viimeistään viikkoa ennen kokousta sekä pidettävä seurakunnan ilmoitustaululla viikon ajan ennen kokousta."}}},{"@id":"sfsd:1993/1055/luku/8/pykala/5/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/8/pykala/5/momentti/3","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/8/pykala/5/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/8/pykala/5/momentti/3/alkup/fin/txt","content_fi":"Jos asia on kiireellinen eikä tarkoita uutta määrärahaa tai entisen korottamista, kirkkovaltuusto voi päättää ottaa sen käsiteltäväksi, vaikka sitä ei ole mainittu kokouskutsussa."}}},{"@id":"sfsd:1993/1055/luku/8/pykala/5/momentti/4/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/8/pykala/5/momentti/4","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/8/pykala/5/momentti/4/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/8/pykala/5/momentti/4/alkup/fin/txt","content_fi":"Jos jotakin asiaa ei ole saatu käsitellyksi kokouksessa ja se on päätetty siirtää käsiteltäväksi jatkokokouksessa, ei tähän tarvitse antaa eri kutsua."}}}],"isMemberOf":"sfsd:1993/1055/luku/8/pykala/5","version":"sfl:Original","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/8/pykala/6/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1993/1055/luku/8/pykala/6/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/8/pykala/6/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/8/pykala/6/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/8/pykala/6/momentti/1/alkup/fin/txt","content_fi":"Asiaa, joka on kirkkoneuvoston valmisteltava, ei saa ottaa päätettäväksi, ellei kirkkoneuvostolla ole ollut tilaisuutta sitä käsitellä."}}},"isMemberOf":"sfsd:1993/1055/luku/8/pykala/6","version":"sfl:Original","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/8/pykala/7/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/8/pykala/7/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/8/pykala/7/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/8/pykala/7/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/8/pykala/7/momentti/1/alkup/fin/txt","content_fi":"Kirkkoneuvoston puheenjohtajan ja varapuheenjohtajan tai heidän molempien ollessa estyneenä kirkkoneuvoston määräämän jäsenen on oltava saapuvilla kirkkovaltuuston kokouksessa. Heidän poissaolonsa ei kuitenkaan estä asioiden päättämistä. Kirkkoneuvoston jäsenillä on oikeus olla kokouksessa läsnä."}}},{"@id":"sfsd:1993/1055/luku/8/pykala/7/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/8/pykala/7/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/8/pykala/7/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/8/pykala/7/momentti/2/alkup/fin/txt","content_fi":"Edellä 1 momentissa mainittu henkilö saa valtuuston kokouksessa ottaa osaa keskusteluun, mutta ei päätöksen tekemiseen, ellei hän ole valtuutettu."}}},{"@id":"sfsd:1993/1055/luku/8/pykala/7/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/8/pykala/7/momentti/3","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/8/pykala/7/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/8/pykala/7/momentti/3/alkup/fin/txt","content_fi":"Valtuuston määräämien viranhaltijoiden tulee olla saapuvilla valtuuston kokouksissa antamassa tarvittavia tietoja."}}}],"isMemberOf":"sfsd:1993/1055/luku/8/pykala/7","version":"sfl:Original","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/8/pykala/8/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1993/1055/luku/8/pykala/8/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/8/pykala/8/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/8/pykala/8/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/8/pykala/8/momentti/1/alkup/fin/txt","content_fi":"Kirkkovaltuusto vahvistaa itselleen työjärjestyksen."}}},"isMemberOf":"sfsd:1993/1055/luku/8/pykala/8","version":"sfl:Original","versionDate":"2017-01-01"}],"isMemberOf":"sfsd:1993/1055/luku/8","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/8/ajantasa/20050101/fin","title_fi":"Kirkkovaltuusto"}},{"@id":"sfsd:1993/1055/luku/9/ajantasa/20050101","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/9/pykala/1/ajantasa/20040101","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/9/pykala/1/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/9/pykala/1/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/9/pykala/1/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/9/pykala/1/momentti/1/alkup/fin/txt","content_fi":"Kirkkoneuvosto päättää kokoontumisensa ajan ja paikan. Kirkkoneuvosto kokoontuu myös, milloin puheenjohtaja katsoo kokouksen tarpeelliseksi tai vähintään neljäsosa jäsenistä sitä kirjallisesti pyytää ilmoittamansa asian käsittelemistä varten."}}},{"@id":"sfsd:1993/1055/luku/9/pykala/1/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/9/pykala/1/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/9/pykala/1/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/9/pykala/1/momentti/2/alkup/fin/txt","content_fi":"Kokouksen kutsuu koolle puheenjohtaja kirkkoneuvoston päättämällä tavalla. Samalla on kokouksesta ilmoitettava myös kirkkovaltuuston puheenjohtajalle ja varapuheenjohtajalle."}}},{"@id":"sfsd:1993/1055/luku/9/pykala/1/momentti/3/ajantasa/20040101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2002/843/alkup","hasPart":[{"@id":"sfsd:1993/1055/luku/9/pykala/1/momentti/3/johdanto/alkup","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1993/1055/luku/9/pykala/1/momentti/3/johdanto","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/9/pykala/1/momentti/3/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/9/pykala/1/momentti/3/johdanto/alkup/fin/txt","content_fi":"Varapuheenjohtaja toimii puheenjohtajana kirkkoneuvoston kokouksessa"}}},{"@id":"sfsd:1993/1055/luku/9/pykala/1/momentti/3/kohta/1/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1993/1055/luku/9/pykala/1/momentti/3/kohta/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/9/pykala/1/momentti/3/kohta/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/9/pykala/1/momentti/3/kohta/1/alkup/fin/txt","content_fi":"1) kirkkoherran ollessa estynyt tai esteellinen; sekä"}}},{"@id":"sfsd:1993/1055/luku/9/pykala/1/momentti/3/kohta/2/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1993/1055/luku/9/pykala/1/momentti/3/kohta/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/9/pykala/1/momentti/3/kohta/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/9/pykala/1/momentti/3/kohta/2/alkup/fin/txt","content_fi":"2) muutoinkin kirkkoherran ja kirkkoneuvoston yhteisellä päätöksellä varapuheenjohtajan annettua siihen suostumuksensa. Päätös voidaan tehdä myös määräajaksi, kuitenkin enintään kirkkoneuvoston toimikaudeksi tai siihen saakka, kun kirkkoherra taikka varapuheenjohtaja vaihtuu."}}}],"isMemberOf":"sfsd:1993/1055/luku/9/pykala/1/momentti/3","version":"sfl:Consolidated","versionDate":"2017-01-01"}],"isMemberOf":"sfsd:1993/1055/luku/9/pykala/1","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/9/pykala/2/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1993/1055/luku/9/pykala/2/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/9/pykala/2/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/9/pykala/2/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/9/pykala/2/momentti/1/alkup/fin/txt","content_fi":"Jos kirkkoneuvoston jäsen haluaa saattaa jonkin asian neuvoston käsiteltäväksi, hänen on annettava asiasta kirjallinen esitys puheenjohtajalle, jonka tulee ottaa asia esille neuvoston seuraavassa kokouksessa. Kirkkoneuvoston kokouksessa voi aloitteen tehdä suullisestikin."}}},"isMemberOf":"sfsd:1993/1055/luku/9/pykala/2","version":"sfl:Original","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/9/pykala/3/ajantasa/20050101","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/9/pykala/3/momentti/1/ajantasa/20050101","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/9/pykala/3/momentti/1/johdanto/alkup","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1993/1055/luku/9/pykala/3/momentti/1/johdanto","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/9/pykala/3/momentti/1/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/9/pykala/3/momentti/1/johdanto/alkup/fin/txt","content_fi":"Oikeus olla kokouksessa läsnä ja ottaa osaa keskusteluun mutta ei päätöksen tekemiseen on"}}},{"@id":"sfsd:1993/1055/luku/9/pykala/3/momentti/1/kohta/1/ajantasa/20050101","@type":"sfl:ParagraphVersion","amendedBy":"sfsd:2003/822/alkup","isMemberOf":"sfsd:1993/1055/luku/9/pykala/3/momentti/1/kohta/1","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/9/pykala/3/momentti/1/kohta/1/ajantasa/20050101/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/9/pykala/3/momentti/1/kohta/1/ajantasa/20050101/fin/txt","content_fi":"1) kirkkovaltuuston puheenjohtajalla ja varapuheenjohtajalla sekä kappeliseurakunnan kappalaisella tai muulla kappeliseurakunnan papilla kirkkoneuvostossa;\n(9.5.2003/822 v. 2004)"}}},{"@id":"sfsd:1993/1055/luku/9/pykala/3/momentti/1/kohta/2/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1993/1055/luku/9/pykala/3/momentti/1/kohta/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/9/pykala/3/momentti/1/kohta/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/9/pykala/3/momentti/1/kohta/2/alkup/fin/txt","content_fi":"2) kirkkoneuvoston puheenjohtajalla ja varapuheenjohtajalla kirkkoneuvoston jaostoissa; ja"}}},{"@id":"sfsd:1993/1055/luku/9/pykala/3/momentti/1/kohta/3/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1993/1055/luku/9/pykala/3/momentti/1/kohta/3","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/9/pykala/3/momentti/1/kohta/3/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/9/pykala/3/momentti/1/kohta/3/alkup/fin/txt","content_fi":"3) kirkkoneuvoston puheenjohtajalla ja kirkkoneuvoston siihen tehtävään valitsemalla jäsenellä johtokunnissa."}}}],"isMemberOf":"sfsd:1993/1055/luku/9/pykala/3/momentti/1","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/9/pykala/3/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/9/pykala/3/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/9/pykala/3/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/9/pykala/3/momentti/2/alkup/fin/txt","content_fi":"Viranhaltijalle voidaan kirkkoneuvoston ohjesäännössä antaa 1 momentin 1 ja 2 kohdassa mainitut oikeudet."}}}],"isMemberOf":"sfsd:1993/1055/luku/9/pykala/3","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/9/pykala/4/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1993/1055/luku/9/pykala/4/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/9/pykala/4/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/9/pykala/4/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/9/pykala/4/momentti/1/alkup/fin/txt","content_fi":"Tilapäisiä tehtäviä varten kirkkoneuvosto voi asettaa toimikuntia, joiden tehtävän ja toiminta-ajan se määrää. Toimikunnassa käsiteltävä asia ja päätös voidaan siirtää kirkkoneuvoston ratkaistavaksi."}}},"isMemberOf":"sfsd:1993/1055/luku/9/pykala/4","version":"sfl:Original","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/9/pykala/5/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1993/1055/luku/9/pykala/5/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/9/pykala/5/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/9/pykala/5/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/9/pykala/5/momentti/1/alkup/fin/txt","content_fi":"Johtokuntien, toimikuntien sekä kirkkolain 10 luvun 4 §:n 2 momentissa mainittujen luottamushenkilöiden ja viranhaltijoiden päätökset on lähetettävä kirkkoneuvostolle sen ohjesäännössä määrätyllä tavalla."}}},"isMemberOf":"sfsd:1993/1055/luku/9/pykala/5","version":"sfl:Original","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/9/pykala/6/ajantasa/19911108","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1993/1055/luku/9/pykala/6","repealedBy":"sfsd:2011/1014/alkup","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/9/pykala/6/ajantasa/19911108/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/9/pykala/6/ajantasa/19911108/fin/txt","content_fi":"6 §(6.5.2011/1014 v. 2012)\n\n6 § on kumottu P:llä 6.5.2011/1014 v. 2012."}}},{"@id":"sfsd:1993/1055/luku/9/pykala/7/ajantasa/20030101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2002/1202/alkup","hasPart":{"@id":"sfsd:1993/1055/luku/9/pykala/7/momentti/1/ajantasa/20030101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/9/pykala/7/momentti/1","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/9/pykala/7/momentti/1/ajantasa/20030101/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/9/pykala/7/momentti/1/ajantasa/20030101/fin/txt","content_fi":"Jos kirkkoherra ja kirkkoneuvosto ovat eri mieltä jumalanpalvelusta, kirkollista toimitusta, kirkon, siunauskappelin tai kappelin käyttöä taikka seurakunnan päätettävissä olevia kolehteja koskevasta kysymyksestä, asia on saatettava tuomiokapitulin ratkaistavaksi."}}},"isMemberOf":"sfsd:1993/1055/luku/9/pykala/7","version":"sfl:Consolidated","versionDate":"2017-01-01"}],"isMemberOf":"sfsd:1993/1055/luku/9","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/9/ajantasa/20050101/fin","title_fi":"Kirkkoneuvosto"}},{"@id":"sfsd:1993/1055/luku/10/ajantasa/20060410","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/10/valiotsikko/1/alkup","@type":"sfl:SubheadingVersion","isMemberOf":"sfsd:1993/1055/luku/10/valiotsikko/1","version":"sfl:Original","versionDate":"2017-01-01","followedBy":"sfsd:1993/1055/luku/10/pykala/1/alkup","languageVersion":{"@id":"sfsd:1993/1055/luku/10/valiotsikko/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/valiotsikko/1/alkup/fin/txt","content_fi":"A. Seurakuntayhtymän perustaminen"}}},{"@id":"sfsd:1993/1055/luku/10/pykala/1/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1993/1055/luku/10/pykala/1/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/1/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/1/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/1/momentti/1/alkup/fin/txt","content_fi":"Kun seurakuntien on kirkkolain 11 luvun 1 §:n mukaan muodostettava seurakuntayhtymä, on kirkkoneuvostojen, väkiluvultaan suurimman seurakunnan kirkkoneuvoston aloitteesta, valmistettava ehdotus yhtymän perussäännöksi. Yhteinen ehdotus tai tuomiokapitulin asettaman selvitysmiehen ehdotus on lähetettävä seurakunnissa käsiteltäväksi."}}},"isMemberOf":"sfsd:1993/1055/luku/10/pykala/1","version":"sfl:Original","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/10/pykala/2/ajantasa/20050101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2003/822/alkup","hasPart":[{"@id":"sfsd:1993/1055/luku/10/pykala/2/momentti/1/ajantasa/20050101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/2/momentti/1","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/2/momentti/1/ajantasa/20050101/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/2/momentti/1/ajantasa/20050101/fin/txt","content_fi":"Perussäännössä on mainittava seurakuntayhtymän nimi, kotipaikka ja siihen kuuluvat seurakunnat. Nimen tulee sisältää sana seurakuntayhtymä."}}},{"@id":"sfsd:1993/1055/luku/10/pykala/2/momentti/2/ajantasa/20050101","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/10/pykala/2/momentti/2/johdanto/ajantasa/20050101","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/2/momentti/2/johdanto","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/2/momentti/2/johdanto/ajantasa/20050101/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/2/momentti/2/johdanto/ajantasa/20050101/fin/txt","content_fi":"Perussäännössä on määrättävä"}}},{"@id":"sfsd:1993/1055/luku/10/pykala/2/momentti/2/kohta/1/ajantasa/20050101","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/2/momentti/2/kohta/1","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/2/momentti/2/kohta/1/ajantasa/20050101/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/2/momentti/2/kohta/1/ajantasa/20050101/fin/txt","content_fi":"1) mitkä hallintoon ja talouteen liittyvät asiat on yhtymän hoidettava;"}}},{"@id":"sfsd:1993/1055/luku/10/pykala/2/momentti/2/kohta/2/ajantasa/20050101","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/2/momentti/2/kohta/2","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/2/momentti/2/kohta/2/ajantasa/20050101/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/2/momentti/2/kohta/2/ajantasa/20050101/fin/txt","content_fi":"2) niistä seurakunnalliseen toimintaan kuuluvista tehtävistä ja työmuodoista, jotka kirkkolain 11 luvun 2 §:n 5 momentin mukaan annetaan yhtymän hoidettavaksi; sekä"}}},{"@id":"sfsd:1993/1055/luku/10/pykala/2/momentti/2/kohta/3/ajantasa/20050101","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/2/momentti/2/kohta/3","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/2/momentti/2/kohta/3/ajantasa/20050101/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/2/momentti/2/kohta/3/ajantasa/20050101/fin/txt","content_fi":"3) niistä muista asioista, jotka ovat tarpeen seurakuntayhtymän ja siihen kuuluvien seurakuntien välisten asioiden hoitamiseksi."}}}],"isMemberOf":"sfsd:1993/1055/luku/10/pykala/2/momentti/2","version":"sfl:Consolidated","versionDate":"2017-01-01"}],"isMemberOf":"sfsd:1993/1055/luku/10/pykala/2","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/10/pykala/3/ajantasa/20050101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2003/822/alkup","hasPart":{"@id":"sfsd:1993/1055/luku/10/pykala/3/momentti/1/ajantasa/20050101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/3/momentti/1","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/3/momentti/1/ajantasa/20050101/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/3/momentti/1/ajantasa/20050101/fin/txt","content_fi":"Jos perussääntöä ei ehditä vahvistaa tai seurakuntavaaleja toimittaa, ennen kuin seurakuntayhtymän on kuntajaossa tapahtuvien muutosten takia aloitettava toimintansa, seurakuntien on kuntajaon muutoksen voimaantulosta lukien väliaikaisesti seurakuntayhtymänä huolehdittava kirkkolain 11 luvun 2 §:ssä mainituista välttämättä hoidettavista asioista."}}},"isMemberOf":"sfsd:1993/1055/luku/10/pykala/3","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/10/pykala/4/ajantasa/20050101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2003/822/alkup","hasPart":[{"@id":"sfsd:1993/1055/luku/10/pykala/4/momentti/1/ajantasa/20050101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/4/momentti/1","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/4/momentti/1/ajantasa/20050101/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/4/momentti/1/ajantasa/20050101/fin/txt","content_fi":"Seurakuntien kirkkovaltuustot ja kirkkoneuvostot sekä seurakuntaneuvostot jatkavat edelleen toimintaansa 3 §:ssä tarkoitetussa tilanteessa, kunnes uudet toimielimet on muodostettu tai valittu."}}},{"@id":"sfsd:1993/1055/luku/10/pykala/4/momentti/2/ajantasa/20050101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/4/momentti/2","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/4/momentti/2/ajantasa/20050101/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/4/momentti/2/ajantasa/20050101/fin/txt","content_fi":"Tuomiokapituli vahvistaa väliaikaisen yhteisen kirkkovaltuuston paikkojen jakautumisen seurakuntien kesken kirkkolain 11 luvun 7 §:n mukaisesti ja kirkkovaltuustot tai seurakuntaneuvostot valitsevat sen jäsenet ja henkilökohtaiset varajäsenet. Väliaikainen yhteinen kirkkovaltuusto valitsee jäsenet ja henkilökohtaiset varajäsenet väliaikaiseen yhteiseen kirkkoneuvostoon, johon kuuluu tämän luvun 10 §:n mukaan määrätyn puheenjohtajan lisäksi kahdeksan muuta jäsentä."}}}],"isMemberOf":"sfsd:1993/1055/luku/10/pykala/4","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/10/pykala/5/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/10/pykala/5/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/5/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/5/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/5/momentti/1/alkup/fin/txt","content_fi":"Tuomiokapituli voi määrätä, että seurakuntayhtymän väliaikaiset toimielimet on valittava ja niiden on ryhdyttävä hoitamaan välttämättömiä tehtäviä jo ennen kuin seurakuntayhtymän on kuntajaon muutoksen takia aloitettava toimintansa. Tuomiokapitulin asiana on antaa muutenkin määräykset seurakuntayhtymän muodostamisen yhteydessä tarvittavista väliaikaisista toimenpiteistä."}}},{"@id":"sfsd:1993/1055/luku/10/pykala/5/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/5/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/5/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/5/momentti/2/alkup/fin/txt","content_fi":"Seurakuntayhtymän perustamisen tai muuttamisen yhteydessä muodostetuista väliaikaisista toimielimistä on voimassa, mitä vastaavista seurakuntayhtymän toimielimistä on säädetty tai määrätty."}}}],"isMemberOf":"sfsd:1993/1055/luku/10/pykala/5","version":"sfl:Original","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/10/pykala/6/ajantasa/20050101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/6","repealedBy":"sfsd:2003/822/alkup","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/6/ajantasa/20050101/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/6/ajantasa/20050101/fin/txt","content_fi":"6 §(9.5.2003/822 v. 2004)\n\n6 § on kumottu kirkolliskokouksen päätöksellä 9.5.2003/822 v. 2004."}}},{"@id":"sfsd:1993/1055/luku/10/pykala/7/ajantasa/20050101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2003/822/alkup","hasPart":[{"@id":"sfsd:1993/1055/luku/10/pykala/7/momentti/1/ajantasa/20050101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/7/momentti/1","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/7/momentti/1/ajantasa/20050101/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/7/momentti/1/ajantasa/20050101/fin/txt","content_fi":"Väliaikaisen yhteisen kirkkovaltuuston on vahvistettava ensimmäistä vuotta varten tuloveroprosentti sekä hyväksyttävä yhtymän talousarvio."}}},{"@id":"sfsd:1993/1055/luku/10/pykala/7/momentti/2/ajantasa/20050101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/7/momentti/2","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/7/momentti/2/ajantasa/20050101/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/7/momentti/2/ajantasa/20050101/fin/txt","content_fi":"Väliaikaisesta yhteisestä kirkkovaltuustosta ja väliaikaisesta yhteisestä kirkkoneuvostosta on soveltuvin osin voimassa, mitä yhteisestä kirkkovaltuustosta ja yhteisestä kirkkoneuvostosta säädetään."}}}],"isMemberOf":"sfsd:1993/1055/luku/10/pykala/7","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/10/pykala/8/ajantasa/20050101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2003/822/alkup","hasPart":{"@id":"sfsd:1993/1055/luku/10/pykala/8/momentti/1/ajantasa/20050101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/8/momentti/1","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/8/momentti/1/ajantasa/20050101/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/8/momentti/1/ajantasa/20050101/fin/txt","content_fi":"Yhteinen kirkkovaltuusto vahvistaa yhtymän seurakuntien tilinpäätökset liittymistä edeltäneeltä vuodelta sekä päättää vastuuvapaudesta."}}},"isMemberOf":"sfsd:1993/1055/luku/10/pykala/8","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/10/valiotsikko/2/alkup","@type":"sfl:SubheadingVersion","isMemberOf":"sfsd:1993/1055/luku/10/valiotsikko/2","version":"sfl:Original","versionDate":"2017-01-01","followedBy":"sfsd:1993/1055/luku/10/pykala/9/ajantasa/20050101","languageVersion":{"@id":"sfsd:1993/1055/luku/10/valiotsikko/2/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/valiotsikko/2/alkup/fin/txt","content_fi":"B. Yhteinen kirkkovaltuusto"}}},{"@id":"sfsd:1993/1055/luku/10/pykala/9/ajantasa/20050101","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/10/pykala/9/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/9/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/9/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/9/momentti/1/alkup/fin/txt","content_fi":"Yhteiseen kirkkovaltuustoon valitaan jäseniä seurakuntien väkiluvun ollessa\n\n\n10 000 tai vähemmän \n21,\n\n10 001 -  20 000\n31,\n\n20 001 -  40 000\n41,\n\n40 001 - 150 000\n51,\n\n150 001 - 300 000\n61 ja\n\nyli       300 000\n91."}}},{"@id":"sfsd:1993/1055/luku/10/pykala/9/momentti/2/ajantasa/20050101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2003/822/alkup","isMemberOf":"sfsd:1993/1055/luku/10/pykala/9/momentti/2","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/9/momentti/2/ajantasa/20050101/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/9/momentti/2/ajantasa/20050101/fin/txt","content_fi":"Yhteinen kirkkovaltuusto vahvistaa viimeistään vaalivuoden kesäkuussa kirkkolain 11 luvun 7 §:n 2 momentin mukaan seurakuntayhtymän yhteisen kirkkovaltuuston jäsenten paikkojen jaon yhtymään kuuluvien seurakuntien lukumäärän sekä seurakunnissa vuoden alussa olleiden väkilukujen perusteella.\n(9.5.2003/822 v. 2004)"}}}],"isMemberOf":"sfsd:1993/1055/luku/10/pykala/9","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/10/valiotsikko/3/alkup","@type":"sfl:SubheadingVersion","isMemberOf":"sfsd:1993/1055/luku/10/valiotsikko/3","version":"sfl:Original","versionDate":"2017-01-01","followedBy":"sfsd:1993/1055/luku/10/pykala/10/ajantasa/20050101","languageVersion":{"@id":"sfsd:1993/1055/luku/10/valiotsikko/3/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/valiotsikko/3/alkup/fin/txt","content_fi":"C. Yhteinen kirkkoneuvosto"}}},{"@id":"sfsd:1993/1055/luku/10/pykala/10/ajantasa/20050101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2003/822/alkup","hasPart":[{"@id":"sfsd:1993/1055/luku/10/pykala/10/momentti/1/ajantasa/20050101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/10/momentti/1","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/10/momentti/1/ajantasa/20050101/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/10/momentti/1/ajantasa/20050101/fin/txt","content_fi":"Yhteisen kirkkoneuvoston jäseninä ovat tuomiokapitulin seurakuntayhtymän seurakuntien kirkkoherroista määräämä puheenjohtaja ja yhteisen kirkkovaltuuston valitsemat varapuheenjohtaja sekä vähintään 5 ja enintään 15 muuta jäsentä sen mukaan kuin ohjesäännössä määrätään. Kirkkoherraa lukuun ottamatta kullakin jäsenellä on henkilökohtainen varajäsen. Jäseneksi ja varajäseneksi ei saa valita saman seurakuntayhtymän tai siihen kuuluvan seurakunnan viranhaltijaa eikä työsopimussuhteessa olevaa työntekijää."}}},{"@id":"sfsd:1993/1055/luku/10/pykala/10/momentti/2/ajantasa/20050101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/10/momentti/2","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/10/momentti/2/ajantasa/20050101/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/10/momentti/2/ajantasa/20050101/fin/txt","content_fi":"Yhteisen kirkkoneuvoston päätöksen noudattamista ja laillisuutta valvoo sen puheenjohtaja, jollei ohje- tai johtosäännössä ole toisin määrätty."}}}],"isMemberOf":"sfsd:1993/1055/luku/10/pykala/10","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/10/pykala/11/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1993/1055/luku/10/pykala/11/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/11/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/11/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/11/momentti/1/alkup/fin/txt","content_fi":"Seurakuntayhtymään kuuluvien seurakuntien kirkkoherroilla sekä, sen mukaan kuin ohjesäännössä määrätään, seurakuntayhtymän johtavilla viranhaltijoilla on oikeus olla läsnä yhteisen kirkkoneuvoston kokouksissa ja ottaa osaa keskusteluun mutta ei päätöksen tekemiseen."}}},"isMemberOf":"sfsd:1993/1055/luku/10/pykala/11","version":"sfl:Original","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/10/valiotsikko/4/alkup","@type":"sfl:SubheadingVersion","isMemberOf":"sfsd:1993/1055/luku/10/valiotsikko/4","version":"sfl:Original","versionDate":"2017-01-01","followedBy":"sfsd:1993/1055/luku/10/pykala/12/ajantasa/20050101","languageVersion":{"@id":"sfsd:1993/1055/luku/10/valiotsikko/4/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/valiotsikko/4/alkup/fin/txt","content_fi":"D. Seurakuntaneuvosto"}}},{"@id":"sfsd:1993/1055/luku/10/pykala/12/ajantasa/20050101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2003/822/alkup","hasPart":{"@id":"sfsd:1993/1055/luku/10/pykala/12/momentti/1/ajantasa/20050101","@type":"sfl:SubsectionVersion","hasPart":{"@id":"sfsd:1993/1055/luku/10/pykala/12/momentti/1/johdanto/ajantasa/20050101","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/12/momentti/1/johdanto","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/12/momentti/1/johdanto/ajantasa/20050101/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/12/momentti/1/johdanto/ajantasa/20050101/fin/txt","content_fi":"Seurakuntaneuvoston puheenjohtajana on seurakunnan kirkkoherra ja varapuheenjohtajana neuvoston toimikauden ensimmäisen ja kolmannen vuoden ensimmäisessä kokouksessa keskuudestaan valitsema jäsen. Jäsenet valitaan seurakuntavaaleilla seurakunnan väkiluvun mukaan seuraavasti:"}}},"isMemberOf":"sfsd:1993/1055/luku/10/pykala/12/momentti/1","version":"sfl:Consolidated","versionDate":"2017-01-01"},"isMemberOf":"sfsd:1993/1055/luku/10/pykala/12","version":"sfl:Consolidated","versionDate":"2017-01-01"},{"@id":"sfsd:1993/1055/luku/10/pykala/13/ajantasa/20060410","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1993/1055/luku/10/pykala/13/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/13/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/13/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/13/momentti/1/alkup/fin/txt","content_fi":"1 momentti on kumottu P:llä 12.5.1995/864."}}},{"@id":"sfsd:1993/1055/luku/10/pykala/13/momentti/1/ajantasa/19950701","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1993/1055/luku/10/pykala/13/momentti/1","repealedBy":"sfsd:1995/864/alkup","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:1993/1055/luku/10/pykala/13/momentti/1/ajantasa/19950701/fin","hasFormat":{"@id":"sfsd:1993/1055/luku/10/pykala/13/momentti/1/ajantasa/19950701/fin/txt","content_fi":"1 momentti on kumottu P:ll