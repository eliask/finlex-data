isMemberOf":"sfsd:1991/1419/pykala/17a/momentti/2","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1991/1419/pykala/17a/momentti/3/ajantasa/20081201","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/1419/pykala/17a/momentti/3","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1991/1419/pykala/17a/momentti/3/ajantasa/20081201/fin","hasFormat":{"@id":"sfsd:1991/1419/pykala/17a/momentti/3/ajantasa/20081201/fin/txt","content_fi":"Hyödyllisyysmallioikeudesta annetun lain 21 §:n 2 momentissa tarkoitetussa suojavaatimusjulkaisussa on oltava 2 momentissa mainittujen tietojen lisäksi tieto siitä, milloin rekisteriviranomainen on tehnyt lainvoimaisen päätöksen osamitätöinnistä."}}}],"isMemberOf":"sfsd:1991/1419/pykala/17a","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1991/1419/pykala/18/ajantasa/19960101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1995/1701/alkup","hasPart":{"@id":"sfsd:1991/1419/pykala/18/momentti/1/ajantasa/19960101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/1419/pykala/18/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1991/1419/pykala/18/momentti/1/ajantasa/19960101/fin","hasFormat":{"@id":"sfsd:1991/1419/pykala/18/momentti/1/ajantasa/19960101/fin/txt","content_fi":"Hyödyllisyysmallioikeudesta annetun lain 17 §:ssä tarkoitetun hyödyllisyysmallin rekisteröintiä koskevan kuulutuksen tulee sisältää hyödyllisyysmallin rekisterinumero ja kansainvälisen patenttiluokituksen mukaiset luokat, keksinnön nimitys ja tarpeelliset kuvat, hyödyllisyysmallioikeuden haltijan nimi sekä hakemuksen päiväkirjanumero ja merkintä siitä, onko hakemuksesta tehty mainitun lain 12 §:n mukainen tutkimus."}}},"isMemberOf":"sfsd:1991/1419/pykala/18","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1991/1419/pykala/19/ajantasa/20130901","@type":"sfl:SectionVersion","amendedBy":"sfsd:2013/581/alkup","hasPart":[{"@id":"sfsd:1991/1419/pykala/19/momentti/1/ajantasa/20130901","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/1419/pykala/19/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1991/1419/pykala/19/momentti/1/ajantasa/20130901/fin","hasFormat":{"@id":"sfsd:1991/1419/pykala/19/momentti/1/ajantasa/20130901/fin/txt","content_fi":"Kun rekisteriviranomaiselle on tehty rekisteröinnin mitättömäksi julistamista koskeva vaatimus taikka ilmoitettu hyödyllisyysmallioikeuden siirtämistä tai pakkoluvan myöntämistä koskevan kanteen nostamisesta, siitä tehdään merkintä hyödyllisyysmallirekisteriin."}}},{"@id":"sfsd:1991/1419/pykala/19/momentti/2/ajantasa/20130901","@type":"sfl:SubsectionVersion","cites":"sfsd:2013/100/luku/4/pykala/23","isMemberOf":"sfsd:1991/1419/pykala/19/momentti/2","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1991/1419/pykala/19/momentti/2/ajantasa/20130901/fin","hasFormat":{"@id":"sfsd:1991/1419/pykala/19/momentti/2/ajantasa/20130901/fin/txt","content_fi":"Kun rekisteriviranomaiselle on toimitettu jäljennös asian ratkaisusta oikeudenkäynnistä markkinaoikeudessa annetun lain (100/2013) 4 luvun 23 §:n tai hyödyllisyysmallioikeudesta annetun lain 44 c §:n mukaisesti tai kun rekisteriviranomainen on 1 momentissa tarkoitetussa asiassa ratkaissut rekisteröinnin mitättömäksi julistamista koskevan vaatimuksen, siitä tehdään merkintä hyödyllisyysmallirekisteriin. Ratkaisun saatua lainvoiman, sen pääasiallisesta sisällöstä on lisäksi tehtävä merkintä rekisteriin."}}}],"isMemberOf":"sfsd:1991/1419/pykala/19","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1991/1419/pykala/20/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1991/1419/pykala/20/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/1419/pykala/20/momentti/1","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1991/1419/pykala/20/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/1419/pykala/20/momentti/1/alkup/fin/txt","content_fi":"Hyödyllisyysmallioikeudesta annetun lain 28 ja 29 §:ssä tarkoitetun merkinnän tulee sisältää oikeuden haltijan nimi, kotipaikka ja osoite sekä hyödyllisyysmallioikeuden siirtymisen, käyttöluvan luovutuksen tai panttauksen päivämäärä. Käyttöluvan osalta on pyynnöstä merkittävä, onko hyödyllisyysmallioikeuden haltijan oikeutta myöntää muita käyttölupia rajoitettu."}}},{"@id":"sfsd:1991/1419/pykala/20/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/1419/pykala/20/momentti/2","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1991/1419/pykala/20/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1991/1419/pykala/20/momentti/2/alkup/fin/txt","content_fi":"Jollei kysymystä merkinnän tekemisestä välittömästi voida ratkaista, on rekisteriin kuitenkin merkittävä, että merkintää on pyydetty."}}},{"@id":"sfsd:1991/1419/pykala/20/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/1419/pykala/20/momentti/3","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1991/1419/pykala/20/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1991/1419/pykala/20/momentti/3/alkup/fin/txt","content_fi":"Edellä olevia 1 momentin säännöksiä on vastaavasti sovellettava pakkoluvan ja hyödyllisyysmallioikeudesta annetun lain 32 §:n 1 momentissa tarkoitetun oikeuden merkitsemiseen."}}}],"isMemberOf":"sfsd:1991/1419/pykala/20","version":"sfl:Original","versionDate":"2013-09-01"},{"@id":"sfsd:1991/1419/pykala/21/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1991/1419/pykala/21/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/1419/pykala/21/momentti/1","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1991/1419/pykala/21/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/1419/pykala/21/momentti/1/alkup/fin/txt","content_fi":"Milloin hyödyllisyysmallioikeus on ulosmitattu, tehdään siitä ilmoituksen perusteella merkintä hyödyllisyysmallirekisteriin."}}},{"@id":"sfsd:1991/1419/pykala/21/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/1419/pykala/21/momentti/2","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1991/1419/pykala/21/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1991/1419/pykala/21/momentti/2/alkup/fin/txt","content_fi":"Rekisteriin merkitään myös ilmoitus asiamiestä koskevasta muutoksesta."}}}],"isMemberOf":"sfsd:1991/1419/pykala/21","version":"sfl:Original","versionDate":"2013-09-01"},{"@id":"sfsd:1991/1419/pykala/22/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1991/1419/pykala/22/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/1419/pykala/22/momentti/1","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1991/1419/pykala/22/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/1419/pykala/22/momentti/1/alkup/fin/txt","content_fi":"Jos hyödyllisyysmallioikeuden haltija ilmoittaa hyödyllisyysmallioikeudesta annetun lain 33 §:n 1 momentin mukaan luopuvansa hyödyllisyysmallinsa rekisteröinnistä ja jos tätä mallia koskeva käyttölupa on merkitty rekisteriin, on käyttöluvan haltijalle ilmoitettava siitä ja varattava hänelle kohtuullinen aika oikeutensa valvomiseksi asiassa ennen kuin hyödyllisyysmalli poistetaan rekisteristä."}}},"isMemberOf":"sfsd:1991/1419/pykala/22","version":"sfl:Original","versionDate":"2013-09-01"},{"@id":"sfsd:1991/1419/pykala/23/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1991/1419/pykala/23/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/1419/pykala/23/momentti/1","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1991/1419/pykala/23/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/1419/pykala/23/momentti/1/alkup/fin/txt","content_fi":"Rekisteröinnin uudistamisesta tehdään merkintä hyödyllisyysmallirekisteriin."}}},{"@id":"sfsd:1991/1419/pykala/23/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/1419/pykala/23/momentti/2","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1991/1419/pykala/23/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1991/1419/pykala/23/momentti/2/alkup/fin/txt","content_fi":"Uudistamista koskevassa kuulutuksessa on ilmoitettava hyödyllisyysmallin rekisterinumero, uudistetun suoja-ajan alkamispäivä sekä hyödyllisyysmallioikeuden haltijan nimi ja osoite."}}}],"isMemberOf":"sfsd:1991/1419/pykala/23","version":"sfl:Original","versionDate":"2013-09-01"},{"@id":"sfsd:1991/1419/pykala/24/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1991/1419/pykala/24/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/1419/pykala/24/momentti/1","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1991/1419/pykala/24/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/1419/pykala/24/momentti/1/alkup/fin/txt","content_fi":"Kun rekisteröinti on lakannut, on rekisteriviranomaisen poistettava hyödyllisyysmalli rekisteristä."}}},{"@id":"sfsd:1991/1419/pykala/24/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/1419/pykala/24/momentti/2","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1991/1419/pykala/24/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1991/1419/pykala/24/momentti/2/alkup/fin/txt","content_fi":"Kun hyödyllisyysmalli on poistettu rekisteristä tai kun hyödyllisyysmallioikeus on lainvoimaisella tuomioistuimen päätöksellä siirretty toiselle, on rekisteriviranomaisen kuulutettava siitä."}}}],"isMemberOf":"sfsd:1991/1419/pykala/24","version":"sfl:Original","versionDate":"2013-09-01"},{"@id":"sfsd:1991/1419/valiotsikko/8/alkup","@type":"sfl:SubheadingVersion","isMemberOf":"sfsd:1991/1419/valiotsikko/8","version":"sfl:Original","versionDate":"2013-09-01","followedBy":"sfsd:1991/1419/pykala/24a/ajantasa/19960101","languageVersion":{"@id":"sfsd:1991/1419/valiotsikko/8/alkup/fin","hasFormat":{"@id":"sfsd:1991/1419/valiotsikko/8/alkup/fin/txt","content_fi":"Rekisteriviranomaisen tutkimus\n\n(22.12.1995/1701)"}}},{"@id":"sfsd:1991/1419/pykala/24a/ajantasa/19960101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1995/1701/alkup","hasPart":{"@id":"sfsd:1991/1419/pykala/24a/momentti/1/ajantasa/19960101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/1419/pykala/24a/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1991/1419/pykala/24a/momentti/1/ajantasa/19960101/fin","hasFormat":{"@id":"sfsd:1991/1419/pykala/24a/momentti/1/ajantasa/19960101/fin/txt","content_fi":"Rekisteriviranomainen ottaa huomioon kaikki seikat, jotka se saa tietoonsa tutkiessaan keksinnön hyödyllisyysmallioikeudesta annetun lain 12 §:n 3 momentin mukaisesti. Rekisteriviranomaisen tutkimus suoritetaan Suomen julkisten hyödyllisyysmalli- ja mallihakemusten sekä Suomen, Norjan, Ruotsin, Tanskan, Amerikan Yhdysvaltojen, Ison-Britannian, Ranskan, Saksan ja Euroopan patenttiviraston patentti- ja kuulutusjulkaisujen sekä kansainvälisten julkisten patenttihakemusten perusteella. Lisäksi voidaan tutkia muitakin saatavissa olevia julkaisuja, jos se katsotaan aiheelliseksi. Rekisteriviranomainen ilmoittaa tutkimustuloksesta ja antaa siitä lausunnon, jos lausuntoa on erikseen pyydetty."}}},"isMemberOf":"sfsd:1991/1419/pykala/24a","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1991/1419/valiotsikko/9/alkup","@type":"sfl:SubheadingVersion","isMemberOf":"sfsd:1991/1419/valiotsikko/9","version":"sfl:Original","versionDate":"2013-09-01","followedBy":"sfsd:1991/1419/pykala/24b/ajantasa/19960101","languageVersion":{"@id":"sfsd:1991/1419/valiotsikko/9/alkup/fin","hasFormat":{"@id":"sfsd:1991/1419/valiotsikko/9/alkup/fin/txt","content_fi":"Kansainvälisiä hakemuksia vastaanottava viranomainen\n\n(22.12.1995/1701)"}}},{"@id":"sfsd:1991/1419/pykala/24b/ajantasa/19960101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1995/1701/alkup","hasPart":[{"@id":"sfsd:1991/1419/pykala/24b/momentti/1/ajantasa/19960101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/1419/pykala/24b/momentti/1","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1991/1419/pykala/24b/momentti/1/ajantasa/19960101/fin","hasFormat":{"@id":"sfsd:1991/1419/pykala/24b/momentti/1/ajantasa/19960101/fin/txt","content_fi":"Patentti- ja rekisterihallitus on hyödyllisyysmallia koskevia kansainvälisiä hakemuksia vastaanottava viranomainen, kun hakija tai joku hakijoista on Suomen kansalainen, luonnollinen henkilö, jolla on kotipaikka Suomessa, Suomen lain mukaan muodostettu oikeushenkilö tai joku, joka harjoittaa liikettä Suomessa."}}},{"@id":"sfsd:1991/1419/pykala/24b/momentti/2/ajantasa/19960101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/1419/pykala/24b/momentti/2","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1991/1419/pykala/24b/momentti/2/ajantasa/19960101/fin","hasFormat":{"@id":"sfsd:1991/1419/pykala/24b/momentti/2/ajantasa/19960101/fin/txt","content_fi":"Hyödyllisyysmallia koskevan kansainvälisen hakemuksen vastaanottamisessa viranomaisessa noudatetaan soveltuvin osin, mitä patenttiasetuksen 46–51 ja 51 a §:ssä säädetään. Suomen käsittävän hyödyllisyysmallia koskevan kansainvälisen hakemuksen jatkamisessa noudatetaan soveltuvin osin, mitä patenttiasetuksen 52 ja 52 a–52 c §:ssä säädetään."}}}],"isMemberOf":"sfsd:1991/1419/pykala/24b","version":"sfl:Consolidated","versionDate":"2013-09-01"},{"@id":"sfsd:1991/1419/valiotsikko/10/alkup","@type":"sfl:SubheadingVersion","isMemberOf":"sfsd:1991/1419/valiotsikko/10","version":"sfl:Original","versionDate":"2013-09-01","followedBy":"sfsd:1991/1419/pykala/25/alkup","languageVersion":{"@id":"sfsd:1991/1419/valiotsikko/10/alkup/fin","hasFormat":{"@id":"sfsd:1991/1419/valiotsikko/10/alkup/fin/txt","content_fi":"Erinäisiä säännöksiä"}}},{"@id":"sfsd:1991/1419/pykala/25/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1991/1419/pykala/25/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/1419/pykala/25/momentti/1","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1991/1419/pykala/25/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/1419/pykala/25/momentti/1/alkup/fin/txt","content_fi":"Hyödyllisyysmallihakemus luokitellaan kansainvälisen patenttiluokitusjärjestelmän mukaisesti."}}},"isMemberOf":"sfsd:1991/1419/pykala/25","version":"sfl:Original","versionDate":"2013-09-01"},{"@id":"sfsd:1991/1419/pykala/26/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1991/1419/pykala/26/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/1419/pykala/26/momentti/1","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1991/1419/pykala/26/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/1419/pykala/26/momentti/1/alkup/fin/txt","content_fi":"Kuulutukset hyödyllisyysmalliasioissa julkaistaan rekisteriviranomaisen toimittamassa julkaisussa."}}},"isMemberOf":"sfsd:1991/1419/pykala/26","version":"sfl:Original","versionDate":"2013-09-01"},{"@id":"sfsd:1991/1419/valiotsikko/11/alkup","@type":"sfl:SubheadingVersion","isMemberOf":"sfsd:1991/1419/valiotsikko/11","version":"sfl:Original","versionDate":"2013-09-01","followedBy":"sfsd:1991/1419/pykala/27/alkup","languageVersion":{"@id":"sfsd:1991/1419/valiotsikko/11/alkup/fin","hasFormat":{"@id":"sfsd:1991/1419/valiotsikko/11/alkup/fin/txt","content_fi":"Voimaantulosäännös"}}},{"@id":"sfsd:1991/1419/pykala/27/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1991/1419/pykala/27/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1991/1419/pykala/27/momentti/1","version":"sfl:Original","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1991/1419/pykala/27/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1991/1419/pykala/27/momentti/1/alkup/fin/txt","content_fi":"Tämä asetus tulee voimaan 1 päivänä tammikuuta 1992."}}},"isMemberOf":"sfsd:1991/1419/pykala/27","version":"sfl:Original","versionDate":"2013-09-01"}],"isAbout":"http://data.finlex.fi/voc/finlex/kw1617","isMemberOf":"sfsd:1991/1419","version":"sfl:Consolidated","versionDate":"2013-09-01","languageVersion":{"@id":"sfsd:1991/1419/ajantasa/20130901/fin","title_fi":"Asetus hyödyllisyysmallioikeudesta"}},"@context":{"languageVersion":{"@id":"eli:is_realized_by","@type":"@id"},"temporalVersions":{"@id":"eli:has_member","@type":"@id"},"temporalVersion":{"@id":"eli:has_member","@type":"@id"},"hasFormat":{"@id":"eli:is_embodied_by","@type":"@id"},"idLocal":"eli:id_local","dateDocument":{"@id":"eli:date_document","@type":"xsd:date"},"hasPart":{"@id":"eli:has_part","@type":"@id"},"isAbout":{"@id":"eli:is_about","@type":"@id"},"isMemberOf":{"@id":"eli:is_member_of","@type":"@id"},"version":{"@id":"eli:version","@type":"@id"},"versionDate":{"@id":"eli:version_date","@type":"xsd:date"},"title_fi":{"@id":"eli:title","@language":"fi"},"content_fi":{"@id":"sfl:text","@language":"fi"},"followedBy":{"@id":"sfl:followedBy","@type":"@id"},"amendedBy":{"@id":"eli:amended_by","@type":"@id"},"repealedBy":{"@id":"eli:repealed_by","@type":"@id"},"cites":{"@id":"eli:cites","@type":"@id"},"inForce":{"@id":"eli:in_force","@type":"@id"},"typeDocument":{"@id":"eli:type_document","@type":"@id"},"dcterms":"http://purl.org/dc/terms/","xsd":"http://www.w3.org/2001/XMLSchema#","common":"http://data.finlex.fi/common/","rdf":"http://www.w3.org/1999/02/22-rdf-syntax-ns#","sfcl":"http://data.finlex.fi/schema/scfl/","sfl":"http://data.finlex.fi/schema/sfl/","sfecli":"http://data.finlex.fi/ecli/","sfsd":"http://data.finlex.fi/eli/sd/","skos":"http://www.w3.org/2004/02/skos/core#","text":"http://jena.apache.org/text#","eli":"http://data.europa.eu/eli/ontology#"}}