guageVersion":{"@id":"sfsd:2002/388/pykala/10/momentti/1/kohta/6/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/10/momentti/1/kohta/6/alkup/fin/txt","content_fi":"6) tapaturmavakuutuksen päiväraha ja elinkorko;"}}},{"@id":"sfsd:2002/388/pykala/10/momentti/1/kohta/7/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2002/388/pykala/10/momentti/1/kohta/7","version":"sfl:Original","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/10/momentti/1/kohta/7/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/10/momentti/1/kohta/7/alkup/fin/txt","content_fi":"7) sairausvakuutuslain mukainen päivä- ja äitiysraha;"}}},{"@id":"sfsd:2002/388/pykala/10/momentti/1/kohta/8/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2002/388/pykala/10/momentti/1/kohta/8","version":"sfl:Original","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/10/momentti/1/kohta/8/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/10/momentti/1/kohta/8/alkup/fin/txt","content_fi":"8) kansaneläke ja ansioeläke;"}}},{"@id":"sfsd:2002/388/pykala/10/momentti/1/kohta/9/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2002/388/pykala/10/momentti/1/kohta/9","version":"sfl:Original","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/10/momentti/1/kohta/9/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/10/momentti/1/kohta/9/alkup/fin/txt","content_fi":"9) opintotuki;"}}},{"@id":"sfsd:2002/388/pykala/10/momentti/1/kohta/10/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2002/388/pykala/10/momentti/1/kohta/10","version":"sfl:Original","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/10/momentti/1/kohta/10/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/10/momentti/1/kohta/10/alkup/fin/txt","content_fi":"10) lapsilisä ja kotihoidontuki."}}}],"isMemberOf":"sfsd:2002/388/pykala/10/momentti/1","version":"sfl:Original","versionDate":"2010-01-01"},{"@id":"sfsd:2002/388/pykala/10/momentti/2/ajantasa/20100101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2009/1008/alkup","isMemberOf":"sfsd:2002/388/pykala/10/momentti/2","version":"sfl:Consolidated","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/10/momentti/2/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/10/momentti/2/ajantasa/20100101/fin/txt","content_fi":"Tuloista yleensä riippumatonta tiettyyn tarkoitukseen myönnettyä sosiaalista etuutta ei oteta huomioon.\n(3.12.2009/1008)"}}}],"isMemberOf":"sfsd:2002/388/pykala/10","version":"sfl:Consolidated","versionDate":"2010-01-01"},{"@id":"sfsd:2002/388/pykala/11/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2002/388/pykala/11/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2002/388/pykala/11/momentti/1","version":"sfl:Original","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/11/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/11/momentti/1/alkup/fin/txt","content_fi":"Mikäli palkansaajan tulot vaihtelevat, käyttövaran laskemisen perusteeksi otetaan viimeisten kuuden kuukauden tulot. Yrittäjän kuukausittaiset käyttövarat lasketaan lopullisessa verotuksessa vahvistettujen vuosittaisten ansio- ja pääomatulojen perusteella. Jos toiminnassa on tapahtunut olennaisia muutoksia viimeksi toimitetun verotuksen jälkeen, käyttövarojen laskennan perusteeksi voidaan ottaa ennakonkannon mukaiset tulot."}}},"isMemberOf":"sfsd:2002/388/pykala/11","version":"sfl:Original","versionDate":"2010-01-01"},{"@id":"sfsd:2002/388/pykala/12/ajantasa/20080101","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2002/388/pykala/12/momentti/1/ajantasa/20080101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2007/867/alkup","isMemberOf":"sfsd:2002/388/pykala/12/momentti/1","version":"sfl:Consolidated","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/12/momentti/1/ajantasa/20080101/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/12/momentti/1/ajantasa/20080101/fin/txt","content_fi":"Asumismenoina hyväksytään kohtuullinen vuokra, vastike ja omakotitalon hoitomenot. Ilman selvitystä omakotitalon kuukausittaisina hoitomenoina hyväksytään enintään 250 euroa. Omistusasunnon asumismenona hyväksytään asuntolainan kuukausittaiset korot. Asumismenot voidaan jättää hyväksymättä siltä osin kuin asunto on perheen kokoon nähden ylimitoitettu. Asumistuki ja opintorahan asumislisä vähennetään asumismenoista.\n(27.9.2007/867)"}}},{"@id":"sfsd:2002/388/pykala/12/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2002/388/pykala/12/momentti/2","version":"sfl:Original","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/12/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/12/momentti/2/alkup/fin/txt","content_fi":"Asumismenoina käyttövaralaskelmassa voidaan huomioida myös laitoshoidossa olevan hakijan tai hänen puolisonsa hoitomaksut."}}}],"isMemberOf":"sfsd:2002/388/pykala/12","version":"sfl:Consolidated","versionDate":"2010-01-01"},{"@id":"sfsd:2002/388/pykala/13/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2002/388/pykala/13/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2002/388/pykala/13/momentti/1","version":"sfl:Original","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/13/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/13/momentti/1/alkup/fin/txt","content_fi":"Elatusapuna hyväksytään hakijan ja hänen puolisonsa tuomioistuimen päätöksen tai vahvistetun sopimuksen mukaiset kuukausittaiset suoritukset. Hyväksyä voidaan myös luotettavan selvityksen mukaan säännönmukaisesti ilman tuomiosta tai sopimusta maksettu elatusapu."}}},"isMemberOf":"sfsd:2002/388/pykala/13","version":"sfl:Original","versionDate":"2010-01-01"},{"@id":"sfsd:2002/388/pykala/14/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2002/388/pykala/14/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2002/388/pykala/14/momentti/1","version":"sfl:Original","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/14/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/14/momentti/1/alkup/fin/txt","content_fi":"Ulosottosuorituksena hyväksytään ulosottoon kuukausittain säännöllisesti maksettu suoritus."}}},{"@id":"sfsd:2002/388/pykala/14/momentti/2/alkup","@type":"sfl:SubsectionVersion","cites":"sfsd:1993/57","isMemberOf":"sfsd:2002/388/pykala/14/momentti/2","version":"sfl:Original","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/14/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/14/momentti/2/alkup/fin/txt","content_fi":"Yksityishenkilön velkajärjestelystä annetun lain (57/1993) mukaisessa maksuohjelmassa vahvistetut suoritukset velkojille hyväksytään edellyttäen, että velallinen on noudattanut maksuohjelmaa. Samoin edellytyksin hyväksytään suoritukset, jotka perustuvat yksityishenkilön velkajärjestelystä annetussa laissa tarkoitettuun velallisen kaikki velat kattavaan kirjalliseen sovintoratkaisuun."}}},{"@id":"sfsd:2002/388/pykala/14/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2002/388/pykala/14/momentti/3","version":"sfl:Original","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/14/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/14/momentti/3/alkup/fin/txt","content_fi":"Kun oikeusapua haetaan yksityishenkilön velkajärjestelystä annetun lain mukaisessa asiassa, vähennetään käyttövaralaskelmassa myös ne varat, jotka velallinen tosiasiallisesti käyttää velanhoitomenoihin."}}}],"isMemberOf":"sfsd:2002/388/pykala/14","version":"sfl:Original","versionDate":"2010-01-01"},{"@id":"sfsd:2002/388/pykala/15/ajantasa/20100101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:2002/388/pykala/15","repealedBy":"sfsd:2009/1008/alkup","version":"sfl:Consolidated","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/15/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/15/ajantasa/20100101/fin/txt","content_fi":"15-16 §\n15-16 § on kumottu A:lla 3.12.2009/1008."}}},{"@id":"sfsd:2002/388/pykala/17/ajantasa/20100101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2009/1008/alkup","hasPart":{"@id":"sfsd:2002/388/pykala/17/momentti/1/ajantasa/20100101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2002/388/pykala/17/momentti/1","version":"sfl:Consolidated","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/17/momentti/1/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/17/momentti/1/ajantasa/20100101/fin/txt","content_fi":"Jos oikeusapua haetaan perunkirjoitusta varten, 7 §:n mukaiseen lisäomavastuuosuuteen lisätään täysimääräisesti vainajan varat ennen velkojen kattamista. Varattoman pesän perunkirjoitusasiassa Valtiokonttorille tai muulle perunkirjoitusvelvolliselle voidaan erityisistä syistä antaa korvauksetta oikeusapua."}}},"isMemberOf":"sfsd:2002/388/pykala/17","version":"sfl:Consolidated","versionDate":"2010-01-01"},{"@id":"sfsd:2002/388/pykala/18/ajantasa/20100101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2009/1008/alkup","hasPart":{"@id":"sfsd:2002/388/pykala/18/momentti/1/ajantasa/20100101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2002/388/pykala/18/momentti/1","version":"sfl:Consolidated","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/18/momentti/1/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/18/momentti/1/ajantasa/20100101/fin/txt","content_fi":"Kun oikeusapu on myönnetty perinnönjakoa tai ositusta koskevassa asiassa, asian päätyttyä lasketaan hakijan saama perintö tai tasinko hänen varoikseen ja vahvistetaan oikeusavun saajan lopullinen lisäomavastuuosuus siten kuin 7 §:ssä säädetään. Samoin menetellään, kun oikeusavun saaja saa rahanarvoisen hyödyn muussa asiassa, jossa oikeusapu on myönnetty."}}},"isMemberOf":"sfsd:2002/388/pykala/18","version":"sfl:Consolidated","versionDate":"2010-01-01"},{"@id":"sfsd:2002/388/pykala/19/ajantasa/20041130","@type":"sfl:SectionVersion","amendedBy":"sfsd:2004/997/alkup","hasPart":[{"@id":"sfsd:2002/388/pykala/19/momentti/1/ajantasa/20041130","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2002/388/pykala/19/momentti/1","version":"sfl:Consolidated","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/19/momentti/1/ajantasa/20041130/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/19/momentti/1/ajantasa/20041130/fin/txt","content_fi":"Oikeussuojakeinojen parantamisesta rajat ylittävissä riita-asioissa vahvistamalla oikeusapuun kyseisissä riita-asioissa liittyvät yhteiset vähimmäisvaatimukset annetussa neuvoston direktiivissä  2003/8/EY tarkoitettuina lähettävinä viranomaisina toimivat oikeusaputoimistot. Perheoikeudellisissa asioissa lähettävänä viranomaisena voi toimia myös oikeusministeriö. Mainitussa direktiivissä tarkoitettuna vastaanottavana viranomaisena toimii Helsingin oikeusaputoimisto."}}},{"@id":"sfsd:2002/388/pykala/19/momentti/2/ajantasa/20041130","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2002/388/pykala/19/momentti/2","version":"sfl:Consolidated","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/19/momentti/2/ajantasa/20041130/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/19/momentti/2/ajantasa/20041130/fin/txt","content_fi":"Oikeusapuhakemus ja sen liitteet voidaan toimittaa 1 momentissa tarkoitetulle vastaanottavalle viranomaiselle myös englanninkielisinä."}}}],"isMemberOf":"sfsd:2002/388/pykala/19","version":"sfl:Consolidated","versionDate":"2010-01-01"},{"@id":"sfsd:2002/388/pykala/20/ajantasa/20080101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2007/867/alkup","hasPart":{"@id":"sfsd:2002/388/pykala/20/momentti/1/ajantasa/20080101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2002/388/pykala/20/momentti/1","version":"sfl:Consolidated","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/20/momentti/1/ajantasa/20080101/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/20/momentti/1/ajantasa/20080101/fin/txt","content_fi":"Oikeusapumaksu on 70 euroa. Oikeusapumaksua ei peritä, jos hakija saa korvauksetonta oikeusapua."}}},"isMemberOf":"sfsd:2002/388/pykala/20","version":"sfl:Consolidated","versionDate":"2010-01-01"},{"@id":"sfsd:2002/388/pykala/21/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2002/388/pykala/21/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2002/388/pykala/21/momentti/1","version":"sfl:Original","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/21/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/21/momentti/1/alkup/fin/txt","content_fi":"Oikeusapu voidaan oikeusapupäätöksessä rajata määrättyihin toimenpiteisiin ja määrättyyn tuntimäärään, jos se on perusteltua ottaen huomioon asian laatu ja merkitys, riidan kohteen arvo sekä olosuhteet kokonaisuudessaan."}}},"isMemberOf":"sfsd:2002/388/pykala/21","version":"sfl:Original","versionDate":"2010-01-01"},{"@id":"sfsd:2002/388/pykala/22/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2002/388/pykala/22/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2002/388/pykala/22/momentti/1","version":"sfl:Original","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/22/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/22/momentti/1/alkup/fin/txt","content_fi":"Tuomioistuimen on toimitettava ratkaisupyynnön perusteella tekemänsä päätös tiedoksi oikeusaputoimistolle."}}},{"@id":"sfsd:2002/388/pykala/22/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2002/388/pykala/22/momentti/2","version":"sfl:Original","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/22/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/22/momentti/2/alkup/fin/txt","content_fi":"Oikeusaputoimiston on toimitettava päätöksensä oikeusavun muuttamisesta tai lakkauttamisesta tiedoksi pääasiaa käsittelevälle tuomioistuimelle."}}}],"isMemberOf":"sfsd:2002/388/pykala/22","version":"sfl:Original","versionDate":"2010-01-01"},{"@id":"sfsd:2002/388/pykala/23/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2002/388/pykala/23/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2002/388/pykala/23/momentti/1","version":"sfl:Original","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/23/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/23/momentti/1/alkup/fin/txt","content_fi":"Muutoksenhakua tai vastauksen antamista varten säädettynä aikana käsitellyn ratkaisupyynnön johdosta annettu päätös oikeusavun myöntämisestä on viran puolesta toimitettava muutoksenhakutuomioistuimeen."}}},"isMemberOf":"sfsd:2002/388/pykala/23","version":"sfl:Original","versionDate":"2010-01-01"},{"@id":"sfsd:2002/388/pykala/24/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2002/388/pykala/24/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2002/388/pykala/24/momentti/1","version":"sfl:Original","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/24/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/24/momentti/1/alkup/fin/txt","content_fi":"Kun muutoksenhakutuomioistuin myöntää oikeusapua taannehtivasti koskemaan sellaisia yksityisen avustajan palkkio- ja kulukorvauksia ja muita kustannuksia, jotka oikeusavun saajan vastapuoli on alemman oikeusasteen lainvoimaisella tuomiolla velvoitettu oikeudenkäyntikulujen korvauksena suorittamaan oikeusavun saajalle, sen tulee ilmoittaa päätöksestään oikeusrekisterikeskukselle. Oikeusrekisterikeskus perii saatavan valtiolle."}}},"isMemberOf":"sfsd:2002/388/pykala/24","version":"sfl:Original","versionDate":"2010-01-01"},{"@id":"sfsd:2002/388/pykala/25/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2002/388/pykala/25/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2002/388/pykala/25/momentti/1","version":"sfl:Original","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/25/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/25/momentti/1/alkup/fin/txt","content_fi":"Kun asiassa, johon on myönnetty oikeusapua, on myönnetty vakuutusetu sen jälkeen kun pääasia on lainvoimaisesti ratkaistu, oikeusaputoimisto perii vakuutusyhtiöltä valtion varoista oikeusapuna suoritettavaksi määrätyn tai oikeusaputoimiston palkkiota ja kuluja vastaavan korvauksen."}}},"isMemberOf":"sfsd:2002/388/pykala/25","version":"sfl:Original","versionDate":"2010-01-01"},{"@id":"sfsd:2002/388/pykala/26/ajantasa/20060101","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2002/388/pykala/26/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2002/388/pykala/26/momentti/1","version":"sfl:Original","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/26/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/26/momentti/1/alkup/fin/txt","content_fi":"Oikeusaputoimisto antaa epäillyn pyynnöstä tämän asetuksen mukaisista oikeusavun taloudellisista edellytyksistä selvityksen, jonka perusteella tuomioistuin voi määrätä oikeudenkäynnistä rikosasioissa annetun lain 2 luvun 11 §:ssä tarkoitetun korvauksen rikoksesta tuomitun puolustajalle valtion varoista maksetuista korvauksista."}}},{"@id":"sfsd:2002/388/pykala/26/momentti/2/ajantasa/20060101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2005/1208/alkup","cites":"sfsd:2005/1204/pykala/29","isMemberOf":"sfsd:2002/388/pykala/26/momentti/2","version":"sfl:Consolidated","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/26/momentti/2/ajantasa/20060101/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/26/momentti/2/ajantasa/20060101/fin/txt","content_fi":"Oikeusaputoimisto antaa korvauksenhakijan pyynnöstä tämän asetuksen mukaisista oikeusavun taloudellisista edellytyksistä selvityksen, jonka perusteella Valtiokonttori voi ratkaista hakijan oikeuden korvaukseen hakemuskuluista rikosvahinkolain (1204/2005) 29 §:n 1 momentin nojalla.\n(29.12.2005/1208)"}}}],"isMemberOf":"sfsd:2002/388/pykala/26","version":"sfl:Consolidated","versionDate":"2010-01-01"},{"@id":"sfsd:2002/388/pykala/27/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2002/388/pykala/27/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2002/388/pykala/27/momentti/1","version":"sfl:Original","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/27/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/27/momentti/1/alkup/fin/txt","content_fi":"Oikeusapuhakemus, 26 §:n mukainen selvitys, päätös oikeusavun myöntämisestä sekä ratkaisupyyntö tulee mahdollisuuksien mukaan laatia oikeusministeriön näitä asiakirjoja varten vahvistamien kaavojen mukaisesti."}}},"isMemberOf":"sfsd:2002/388/pykala/27","version":"sfl:Original","versionDate":"2010-01-01"},{"@id":"sfsd:2002/388/pykala/28/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2002/388/pykala/28/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2002/388/pykala/28/momentti/1","version":"sfl:Original","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/28/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/28/momentti/1/alkup/fin/txt","content_fi":"Tarkempia määräyksiä tämän asetuksen soveltamisesta antaa tarvittaessa oikeusministeriö."}}},"isMemberOf":"sfsd:2002/388/pykala/28","version":"sfl:Original","versionDate":"2010-01-01"},{"@id":"sfsd:2002/388/pykala/29/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2002/388/pykala/29/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2002/388/pykala/29/momentti/1","version":"sfl:Original","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/29/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/29/momentti/1/alkup/fin/txt","content_fi":"Tämä asetus tulee voimaan 1 päivänä kesäkuuta 2002."}}},{"@id":"sfsd:2002/388/pykala/29/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2002/388/pykala/29/momentti/2","version":"sfl:Original","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/pykala/29/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2002/388/pykala/29/momentti/2/alkup/fin/txt","content_fi":"Asetusta sovelletaan myönnettäessä oikeusapua mainitun päivämäärän jälkeen."}}}],"isMemberOf":"sfsd:2002/388/pykala/29","version":"sfl:Original","versionDate":"2010-01-01"}],"isAbout":"http://data.finlex.fi/voc/finlex/kw2864","isMemberOf":"sfsd:2002/388","version":"sfl:Consolidated","versionDate":"2010-01-01","languageVersion":{"@id":"sfsd:2002/388/ajantasa/20100101/fin","title_fi":"Valtioneuvoston asetus oikeusavusta"}},"@context":{"languageVersion":{"@id":"eli:is_realized_by","@type":"@id"},"temporalVersions":{"@id":"eli:has_member","@type":"@id"},"temporalVersion":{"@id":"eli:has_member","@type":"@id"},"hasFormat":{"@id":"eli:is_embodied_by","@type":"@id"},"idLocal":"eli:id_local","dateDocument":{"@id":"eli:date_document","@type":"xsd:date"},"hasPart":{"@id":"eli:has_part","@type":"@id"},"isAbout":{"@id":"eli:is_about","@type":"@id"},"isMemberOf":{"@id":"eli:is_member_of","@type":"@id"},"version":{"@id":"eli:version","@type":"@id"},"versionDate":{"@id":"eli:version_date","@type":"xsd:date"},"title_fi":{"@id":"eli:title","@language":"fi"},"cites":{"@id":"eli:cites","@type":"@id"},"content_fi":{"@id":"sfl:text","@language":"fi"},"amendedBy":{"@id":"eli:amended_by","@type":"@id"},"repealedBy":{"@id":"eli:repealed_by","@type":"@id"},"inForce":{"@id":"eli:in_force","@type":"@id"},"typeDocument":{"@id":"eli:type_document","@type":"@id"},"dcterms":"http://purl.org/dc/terms/","xsd":"http://www.w3.org/2001/XMLSchema#","common":"http://data.finlex.fi/common/","rdf":"http://www.w3.org/1999/02/22-rdf-syntax-ns#","sfcl":"http://data.finlex.fi/schema/scfl/","sfl":"http://data.finlex.fi/schema/