n":{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/2/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/2/johdanto/alkup/fin/txt","content_fi":"2) isä on Suomen kansalainen; ja"}}}],"isMemberOf":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/2","version":"sfl:Original","versionDate":"2017-01-01"},{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/3/alkup","@type":"sfl:ParagraphVersion","hasPart":[{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/3/alakohta/i/alkup","@type":"sfl:SubparagraphVersion","isMemberOf":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/3/alakohta/i","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/3/alakohta/i/alkup/fin","hasFormat":{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/3/alakohta/i/alkup/fin/txt","content_fi":"3) isä on kuollut, mutta oli kuollessaan Suomen kansalainen; ja"}}},{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/3/alakohta/a/alkup","@type":"sfl:SubparagraphVersion","isMemberOf":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/3/alakohta/a","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/3/alakohta/a/alkup/fin","hasFormat":{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/3/alakohta/a/alkup/fin/txt","content_fi":"a) avioliitossa lapsen äidin kanssa; tai"}}},{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/3/alakohta/b/alkup","@type":"sfl:SubparagraphVersion","isMemberOf":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/3/alakohta/b","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/3/alakohta/b/alkup/fin","hasFormat":{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/3/alakohta/b/alkup/fin/txt","content_fi":"b) lapsi syntyy Suomessa ja miehen isyys lapseen vahvistetaan; tai"}}},{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/3/johdanto/alkup","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/3/johdanto","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/3/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/3/johdanto/alkup/fin/txt","content_fi":"3) isä on kuollut, mutta oli kuollessaan Suomen kansalainen; ja"}}}],"isMemberOf":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/3","version":"sfl:Original","versionDate":"2017-01-01"},{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/4/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/4","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/4/alkup/fin","hasFormat":{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/kohta/4/alkup/fin/txt","content_fi":"4) lapsi syntyy Suomessa eikä saa syntyessään minkään vieraan valtion kansalaisuutta, eikä hänellä syntymänsä perusteella ole edes toissijaisesti oikeutta saada minkään vieraan valtion kansalaisuutta."}}}],"isMemberOf":"sfsd:2003/359/luku/2/pykala/9/momentti/1","version":"sfl:Original","versionDate":"2017-01-01"},{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/359/luku/2/pykala/9/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/2/alkup/fin/txt","content_fi":"Suomessa syntyvä lapsi saa syntymäpaikan perusteella Suomen kansalaisuuden, jos lapsen vanhemmilla on Suomessa pakolaisen asema tai jos heille muutoin on annettu suojelua kansalaisuusvaltionsa viranomaisia vastaan. Lisäksi edellytetään, että lapsi ei saa kummankaan vanhempansa kansalaisuutta muutoin kuin rekisteröimällä lapsen syntymä vanhemman kansalaisuusvaltion viranomaisessa tai muulla tämän valtion viranomaisen myötävaikutusta edellyttävällä tavalla. Jos edellä tarkoitettua suojelua on annettu vain toiselle vanhemmista, edellytetään myös, että lapsi ei saa syntyessään eikä hänellä syntymänsä perusteella ole edes toissijaisesti oikeutta saada toisen vanhempansa kansalaisuutta."}}}],"isMemberOf":"sfsd:2003/359/luku/2/pykala/9","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/359/luku/2/pykala/9/alkup/fin","title_fi":"Lapsi"}},{"@id":"sfsd:2003/359/luku/2/pykala/10/ajantasa/20150101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2014/971/alkup","hasPart":{"@id":"sfsd:2003/359/luku/2/pykala/10/momentti/1/ajantasa/20150101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/359/luku/2/pykala/10/momentti/1","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/359/luku/2/pykala/10/momentti/1/ajantasa/20150101/fin","hasFormat":{"@id":"sfsd:2003/359/luku/2/pykala/10/momentti/1/ajantasa/20150101/fin/txt","content_fi":"Ulkomaalainen alle 12-vuotias lapsi, joka on adoptoitu ja jonka adoptiovanhemmista ainakin toinen on Suomen kansalainen, saa Suomen kansalaisuuden adoption perusteella siitä ajankohdasta lukien, jolloin adoptio on Suomessa pätevä."}}},"isMemberOf":"sfsd:2003/359/luku/2/pykala/10","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/359/luku/2/pykala/10/ajantasa/20150101/fin","title_fi":"Adoptiolapsi"}},{"@id":"sfsd:2003/359/luku/2/pykala/11/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2003/359/luku/2/pykala/11/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/359/luku/2/pykala/11/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/359/luku/2/pykala/11/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2003/359/luku/2/pykala/11/momentti/1/alkup/fin/txt","content_fi":"Lapsi, joka ei saa Suomen kansalaisuutta 9 §:n nojalla tai joka ei ole saanut Suomen kansalaisuutta ilmoituksesta, saa Suomen kansalaisuuden vanhempiensa keskinäisen avioliiton perusteella avioliiton solmimisesta lukien, jos mies oli lapsen syntyessä ja on siitä lähtien ollut Suomen kansalainen ja miehen isyys lapseen on vahvistettu ennen avioliiton solmimista."}}},{"@id":"sfsd:2003/359/luku/2/pykala/11/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/359/luku/2/pykala/11/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/359/luku/2/pykala/11/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2003/359/luku/2/pykala/11/momentti/2/alkup/fin/txt","content_fi":"Jos isyys kuitenkin vahvistetaan avioliiton solmimisen jälkeen ja isä on edelleen Suomen kansalainen, lapsi saa Suomen kansalaisuuden vasta isyyden vahvistamispäivästä lukien. Jos isä on kuollut avioliiton solmimisen jälkeen, lapsi saa Suomen kansalaisuuden isyyden vahvistamispäivästä lukien, jos isä oli kuollessaan Suomen kansalainen."}}}],"isMemberOf":"sfsd:2003/359/luku/2/pykala/11","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/359/luku/2/pykala/11/alkup/fin","title_fi":"Vanhempien avioliitto"}},{"@id":"sfsd:2003/359/luku/2/pykala/12/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2003/359/luku/2/pykala/12/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/359/luku/2/pykala/12/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/359/luku/2/pykala/12/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2003/359/luku/2/pykala/12/momentti/1/alkup/fin/txt","content_fi":"Suomessa tavattavaa löytölasta pidetään Suomen kansalaisena niin kauan kuin hänen ei ole todettu olevan vieraan valtion kansalainen. Jos lapsi on todettu vieraan valtion kansalaiseksi vasta sen jälkeen, kun hän on täyttänyt viisi vuotta, lapsen Suomen kansalaisuus kuitenkin säilyy."}}},{"@id":"sfsd:2003/359/luku/2/pykala/12/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/359/luku/2/pykala/12/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/359/luku/2/pykala/12/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2003/359/luku/2/pykala/12/momentti/2/alkup/fin/txt","content_fi":"Suomessa syntynyttä lasta, jonka vanhempien kansalaisuus on tuntematon, pidetään Suomen kansalaisena niin kauan kuin hänen ei ole todettu olevan vieraan valtion kansalainen. Sama koskee Suomessa avioliiton ulkopuolella syntynyttä lasta, jonka äidin kansalaisuus on tuntematon. Jos lapsi on todettu vieraan valtion kansalaiseksi vasta sen jälkeen, kun hän on täyttänyt viisi vuotta, lapsen Suomen kansalaisuus kuitenkin säilyy."}}}],"isMemberOf":"sfsd:2003/359/luku/2/pykala/12","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/359/luku/2/pykala/12/alkup/fin","title_fi":"Löytölapsi ja kansalaisuudeltaan tuntemattomien vanhempien lapsi"}}],"isMemberOf":"sfsd:2003/359/luku/2","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/359/luku/2/ajantasa/20150101/fin","title_fi":"Suomen kansalaisuus syntymän perusteella"}},{"@id":"sfsd:2003/359/luku/2/alkup","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:2003/359/luku/2/pykala/9/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/alkup","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/johdanto/alkup","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:2003/359/luku/2/pykala/9/momentti/1/johdanto","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:2003/359/luku/2/pykala/9/momentti/1/johdanto/alkup/fin/txt","content_fi":"Lapsi saa syntyessään Suomen kansalaisuuden, jos:"}}},{"@id":"sfsd:2003/359/luku/2/pykal