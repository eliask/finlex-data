u/7/pykala/36/momentti/3/ajantasa/20100101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/7/pykala/36/momentti/3","version":"sfl:Consolidated","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/7/pykala/36/momentti/3/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:2007/568/luku/7/pykala/36/momentti/3/ajantasa/20100101/fin/txt","content_fi":"Jos 2 momentissa tarkoitettu muutos on tapahtunut kuukauden ensimmäisenä päivänä, täydennysmäärä tarkistetaan sen kuukauden alusta. Muussa tapauksessa täydennysmäärä tarkistetaan muutosta seuraavan kuukauden alusta."}}}],"isMemberOf":"sfsd:2007/568/luku/7/pykala/36","version":"sfl:Consolidated","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/7/pykala/36/ajantasa/20100101/fin","title_fi":"Jatkoeläkkeen täydennysmäärän tarkistaminen"}},{"@id":"sfsd:2007/568/luku/7/pykala/37/ajantasa/20100101","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2007/568/luku/7/pykala/37/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/7/pykala/37/momentti/1","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/7/pykala/37/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/7/pykala/37/momentti/1/alkup/fin/txt","content_fi":"Lesken jatkoeläke lakkaa sitä seuraavan kuukauden alusta, jona leski täyttää 65 vuotta."}}},{"@id":"sfsd:2007/568/luku/7/pykala/37/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/7/pykala/37/momentti/2","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/7/pykala/37/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/7/pykala/37/momentti/2/alkup/fin/txt","content_fi":"Lesken jatkoeläke lakkaa sitä seuraavan kuukauden alusta, jona leski solmii uuden avioliiton ennen kuin hän on täyttänyt 50 vuotta."}}},{"@id":"sfsd:2007/568/luku/7/pykala/37/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/7/pykala/37/momentti/3","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/7/pykala/37/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/7/pykala/37/momentti/3/alkup/fin/txt","content_fi":"Lesken jatkoeläke lakkaa, kun leskelle myönnetään 29 §:n 1 momentissa tarkoitettu eläke. Jos eläke myönnetään kuukauden ensimmäisenä päivänä, jatkoeläke lakkaa sen kuukauden alusta. Muussa tapauksessa jatkoeläke lakkaa eläkkeen myöntämistä seuraavan kuukauden alusta."}}},{"@id":"sfsd:2007/568/luku/7/pykala/37/momentti/4/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/7/pykala/37/momentti/4","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/7/pykala/37/momentti/4/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/7/pykala/37/momentti/4/alkup/fin/txt","content_fi":"Jatkoeläkkeen perusmäärä lakkaa sitä seuraavan kuukauden alusta, jona lesken kanssa samassa taloudessa asunut lesken tai edunjättäjän lapsi, joka on asunut samassa taloudessa lesken ja edunjättäjän kanssa jo edunjättäjän kuollessa, muuttaa pois lesken taloudesta. Perusmäärä lakkaa aina viimeistään sitä seuraavan kuukauden alusta, jona nuorin edellä mainituista lapsista tai lesken ja edunjättäjän yhteisistä lapsista täyttää 18 vuotta."}}},{"@id":"sfsd:2007/568/luku/7/pykala/37/momentti/5/ajantasa/20100101","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:2009/1223/alkup","isMemberOf":"sfsd:2007/568/luku/7/pykala/37/momentti/5","version":"sfl:Consolidated","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/7/pykala/37/momentti/5/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:2007/568/luku/7/pykala/37/momentti/5/ajantasa/20100101/fin/txt","content_fi":"Jatkoeläkkeen täydennysmäärä lakkaa ilman eri päätöstä, jos 73 §:n 3 momentin 3 kohdan perusteella keskeytettyä täydennysmäärää ei ole kahden vuoden kuluessa keskeyttämisestä lukien vaadittu maksettavaksi.\n(22.12.2009/1223)"}}}],"isMemberOf":"sfsd:2007/568/luku/7/pykala/37","version":"sfl:Consolidated","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/7/pykala/37/ajantasa/20100101/fin","title_fi":"Jatkoeläkkeen lakkaaminen"}},{"@id":"sfsd:2007/568/luku/7/pykala/38/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2007/568/luku/7/pykala/38/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/7/pykala/38/momentti/1","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/7/pykala/38/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/7/pykala/38/momentti/1/alkup/fin/txt","content_fi":"Jos leskeneläkettä on maksettu vähintään vuoden ajalta ja se lakkautetaan sillä perusteella, että leski solmii uuden avioliiton ennen kuin on täyttänyt 50 vuotta, leskelle maksetaan kertasuorituksena se jatkoeläkkeen määrä, joka hänellä olisi ollut oikeus saada seuraavalta kolmelta vuodelta."}}},{"@id":"sfsd:2007/568/luku/7/pykala/38/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/7/pykala/38/momentti/2","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/7/pykala/38/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/7/pykala/38/momentti/2/alkup/fin/txt","content_fi":"Kertasuoritus lasketaan sen suuruisen kuukausieläkkeen perusteella, jota leski viimeksi sai ennen avioliiton solmimista."}}},{"@id":"sfsd:2007/568/luku/7/pykala/38/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/7/pykala/38/momentti/3","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/7/pykala/38/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/7/pykala/38/momentti/3/alkup/fin/txt","content_fi":"Jos lesken uusi avioliitto purkautuu, oikeus aikaisemman avioliiton perusteella saatuun jatkoeläkkeeseen ei palaudu."}}}],"isMemberOf":"sfsd:2007/568/luku/7/pykala/38","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/7/pykala/38/alkup/fin","title_fi":"Lakkautettavan jatkoeläkkeen myöntäminen kertasuorituksena"}},{"@id":"sfsd:2007/568/luku/7/pykala/39/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2007/568/luku/7/pykala/39/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/7/pykala/39/momentti/1","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/7/pykala/39/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/7/pykala/39/momentti/1/alkup/fin/txt","content_fi":"Jatkoeläkkeen perusmäärää ja täydennysmäärää ei makseta, jos niiden yhteismäärä on pienempi kuin 5,38 euroa kuukaudessa."}}},"isMemberOf":"sfsd:2007/568/luku/7/pykala/39","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/7/pykala/39/alkup/fin","title_fi":"Jatkoeläkkeen vähimmäismäärä"}}],"isMemberOf":"sfsd:2007/568/luku/7","version":"sfl:Consolidated","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/7/ajantasa/20100101/fin","title_fi":"Jatkoeläkkeen tarkistaminen ja lakkaaminen"}},{"@id":"sfsd:2007/568/luku/8/alkup","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:2007/568/luku/8/pykala/40/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2007/568/luku/8/pykala/40/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/8/pykala/40/momentti/1","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/40/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/8/pykala/40/momentti/1/alkup/fin/txt","content_fi":"Oikeus lapseneläkkeeseen on edunjättäjän alle 18-vuotiaalla lapsella ja alle 21-vuotiaalla päätoimisesti opiskelevalla lapsella."}}},{"@id":"sfsd:2007/568/luku/8/pykala/40/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/8/pykala/40/momentti/2","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/40/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/8/pykala/40/momentti/2/alkup/fin/txt","content_fi":"Oikeus lapseneläkkeeseen on yhden edunjättäjän jälkeen. Edunjättäjällä tarkoitetaan lapsen omaa vanhempaa tai muuta henkilöä, jonka kanssa lapsi asui samassa taloudessa tämän kuollessa ja joka vastasi lapsen elatuksesta."}}},{"@id":"sfsd:2007/568/luku/8/pykala/40/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/8/pykala/40/momentti/3","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/40/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/8/pykala/40/momentti/3/alkup/fin/txt","content_fi":"Täysorvolla lapsella on oikeus lapseneläkkeeseen samanaikaisesti kahden edunjättäjän jälkeen, joista toinen voi olla muukin kuin lapsen oma vanhempi. Lapsi on täysorpo, kun hänen molemmat omat vanhempansa ovat kuolleet. Täysorpona pidetään myös lasta, jonka äiti on kuollut ja lapsen isän isyyttä ei ole vahvistettu."}}},{"@id":"sfsd:2007/568/luku/8/pykala/40/momentti/4/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/8/pykala/40/momentti/4","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/40/momentti/4/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/8/pykala/40/momentti/4/alkup/fin/txt","content_fi":"Lapsella ei ole oikeutta lapseneläkkeeseen, jos hän saa tämän lain mukaista työkyvyttömyyseläkettä tai leskeneläkettä taikka ulkomailta maksettavaa vastaavaa eläkettä."}}}],"isMemberOf":"sfsd:2007/568/luku/8/pykala/40","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/40/alkup/fin","title_fi":"Oikeus lapseneläkkeeseen"}},{"@id":"sfsd:2007/568/luku/8/pykala/41/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2007/568/luku/8/pykala/41/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/8/pykala/41/momentti/1","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/41/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/8/pykala/41/momentti/1/alkup/fin/txt","content_fi":"Lapseneläkkeenä maksetaan perusmäärä. Alle 18-vuotiaan lapsen eläkkeeseen maksetaan lisäksi tulosidonnainen täydennysmäärä."}}},"isMemberOf":"sfsd:2007/568/luku/8/pykala/41","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/41/alkup/fin","title_fi":"Lapseneläkkeen osat"}},{"@id":"sfsd:2007/568/luku/8/pykala/42/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2007/568/luku/8/pykala/42/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/8/pykala/42/momentti/1","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/42/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/8/pykala/42/momentti/1/alkup/fin/txt","content_fi":"Lapseneläkkeen perusmäärä on 48,05 euroa kuukaudessa."}}},{"@id":"sfsd:2007/568/luku/8/pykala/42/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/8/pykala/42/momentti/2","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/42/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/8/pykala/42/momentti/2/alkup/fin/txt","content_fi":"Lapseneläkkeen täydennysmäärä on täysimääräisenä 72,68 euroa kuukaudessa."}}}],"isMemberOf":"sfsd:2007/568/luku/8/pykala/42","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/42/alkup/fin","title_fi":"Lapseneläkkeen määrä"}},{"@id":"sfsd:2007/568/luku/8/pykala/43/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2007/568/luku/8/pykala/43/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/8/pykala/43/momentti/1","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/43/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/8/pykala/43/momentti/1/alkup/fin/txt","content_fi":"Täysimääräisestä tai 44 §:n mukaisesti lasketusta lapseneläkkeen täydennysmäärästä vähennetään 50 prosenttia siitä 22 §:n 1 momentin 1, 3, 4 ja 6 kohdassa mainittujen perhe- ja huoltoeläkkeiden sekä näitä ja tämän lain mukaista perhe-eläkettä vastaavien ulkomailta maksettavien eläketulojen yhteismäärästä, joka ylittää 536 euroa vuodessa."}}},{"@id":"sfsd:2007/568/luku/8/pykala/43/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/8/pykala/43/momentti/2","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/43/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/8/pykala/43/momentti/2/alkup/fin/txt","content_fi":"Täysorvon lapsen eläkkeestä ei vähennetä sellaisia perhe- ja huoltoeläkkeitä, jotka on jo otettu huomioon aikaisemmin kuolleen edunjättäjän jälkeen myönnetyssä lapseneläkkeessä."}}}],"isMemberOf":"sfsd:2007/568/luku/8/pykala/43","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/43/alkup/fin","title_fi":"Eläketulojen vaikutus lapseneläkkeen täydennysmäärään"}},{"@id":"sfsd:2007/568/luku/8/pykala/44/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2007/568/luku/8/pykala/44/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/8/pykala/44/momentti/1","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/44/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/8/pykala/44/momentti/1/alkup/fin/txt","content_fi":"Jos lapseneläke on laskettava sosiaaliturva-asetuksen perusteella edunjättäjän Suomessa asumaan aikaan suhteutettuna, suhteutuskerroin määrätään 32 §:n mukaisesti vastaavasti kuin leskeneläkkeessä."}}},{"@id":"sfsd:2007/568/luku/8/pykala/44/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/8/pykala/44/momentti/2","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/44/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/8/pykala/44/momentti/2/alkup/fin/txt","content_fi":"Lapseneläkettä ei muutoin kuin 1 momentissa mainituissa tilanteissa suhteuteta edunjättäjän Suomessa asumaan aikaan."}}}],"isMemberOf":"sfsd:2007/568/luku/8/pykala/44","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/44/alkup/fin","title_fi":"Suomessa asutun ajan vaikutus lapseneläkkeen määrään"}},{"@id":"sfsd:2007/568/luku/8/pykala/45/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2007/568/luku/8/pykala/45/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/8/pykala/45/momentti/1","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/45/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/8/pykala/45/momentti/1/alkup/fin/txt","content_fi":"Kansaneläkelaitoksella on oikeus tarkistaa lapseneläkkeen täydennysmäärä ilman hakemusta, kun tulona huomioon otettava 43 §:n 1 momentissa tarkoitettu perhe- ja huoltoeläke myönnetään tai tällaisessa tulona huomioon otetussa eläkkeessä on tapahtunut muu kuin indeksisidonnaisuudesta johtuva muutos."}}},{"@id":"sfsd:2007/568/luku/8/pykala/45/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/8/pykala/45/momentti/2","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/45/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/8/pykala/45/momentti/2/alkup/fin/txt","content_fi":"Jos 1 momentissa tarkoitettu muutos on tapahtunut kuukauden ensimmäisenä päivänä, lapseneläke tarkistetaan sen kuukauden alusta. Muussa tapauksessa lapseneläkkeen täydennysmäärä tarkistetaan muutosta seuraavan kuukauden alusta."}}}],"isMemberOf":"sfsd:2007/568/luku/8/pykala/45","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/45/alkup/fin","title_fi":"Lapseneläkkeen täydennysmäärän tarkistaminen"}},{"@id":"sfsd:2007/568/luku/8/pykala/46/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/alkup","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/johdanto/alkup","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:2007/568/luku/8/pykala/46/momentti/1/johdanto","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/johdanto/alkup/fin/txt","content_fi":"Lapseneläke lakkaa sitä seuraavan kuukauden alusta, jona:"}}},{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/1/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/1","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/1/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/1/alkup/fin/txt","content_fi":"1) lapsi täyttää 18 vuotta;"}}},{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/2/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/2","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/2/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/2/alkup/fin/txt","content_fi":"2) 18 vuotta täyttänyt lapsi ei enää opiskele päätoimisesti;"}}},{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/3/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/3","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/3/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/3/alkup/fin/txt","content_fi":"3) päätoimisesti opiskeleva lapsi täyttää 21 vuotta;"}}},{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/4/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/4","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/4/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/4/alkup/fin/txt","content_fi":"4) lapsi annetaan ottolapseksi muulle kuin edunjättäjän leskelle tai tämän puolisolle;"}}},{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/5/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/5","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/5/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/5/alkup/fin/txt","content_fi":"5) lapselle myönnetään tämän lain mukainen työkyvyttömyyseläke tai leskeneläke tai näitä vastaava ulkomailta maksettava eläke; tai"}}},{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/6/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/6","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/6/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/8/pykala/46/momentti/1/kohta/6/alkup/fin/txt","content_fi":"6) lapsi, joka edunjättäjän kuollessa asui tämän kanssa samassa taloudessa ja jonka elatuksesta tämä vastasi, siirtyy omien vanhempiensa huollettavaksi."}}}],"isMemberOf":"sfsd:2007/568/luku/8/pykala/46/momentti/1","version":"sfl:Original","versionDate":"2017-03-01"},"isMemberOf":"sfsd:2007/568/luku/8/pykala/46","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/46/alkup/fin","title_fi":"Lapseneläkkeen lakkaaminen"}},{"@id":"sfsd:2007/568/luku/8/pykala/47/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2007/568/luku/8/pykala/47/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/8/pykala/47/momentti/1","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/47/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/8/pykala/47/momentti/1/alkup/fin/txt","content_fi":"Lapseneläkkeen perusmäärää ja täydennysmäärää ei makseta, jos niiden yhteismäärä on pienempi kuin 5,38 euroa kuukaudessa."}}},"isMemberOf":"sfsd:2007/568/luku/8/pykala/47","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/pykala/47/alkup/fin","title_fi":"Lapseneläkkeen vähimmäismäärä"}}],"isMemberOf":"sfsd:2007/568/luku/8","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/8/alkup/fin","title_fi":"Lapseneläke"}},{"@id":"sfsd:2007/568/luku/9/alkup","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:2007/568/luku/9/pykala/48/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2007/568/luku/9/pykala/48/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/9/pykala/48/momentti/1","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/9/pykala/48/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/9/pykala/48/momentti/1/alkup/fin/txt","content_fi":"Lesken alkueläke ja lapseneläke myönnetään edunjättäjän kuolinpäivää seuraavan kuukauden alusta. Lesken alkueläke yhteisen lapsen perusteella myönnetään edellä mainitusta ajankohdasta myös silloin, kun lapsi syntyy edunjättäjän kuoleman jälkeen. Lesken jatkoeläke myönnetään edunjättäjän kuolinpäivää seuraavan seitsemännen kuukauden alusta."}}},"isMemberOf":"sfsd:2007/568/luku/9/pykala/48","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/9/pykala/48/alkup/fin","title_fi":"Perhe-eläkkeen alkamisaika"}},{"@id":"sfsd:2007/568/luku/9/pykala/49/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2007/568/luku/9/pykala/49/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/9/pykala/49/momentti/1","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/9/pykala/49/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/9/pykala/49/momentti/1/alkup/fin/txt","content_fi":"Jos edunjättäjän kuolemasta ei voida esittää selvitystä, mutta on todennäköistä, että hän on kuollut hukkumisen, muun onnettomuuden tai muun niihin rinnastettavan syyn vuoksi, perhe-eläke voidaan myöntää määräajaksi."}}},{"@id":"sfsd:2007/568/luku/9/pykala/49/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/9/pykala/49/momentti/2","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/9/pykala/49/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/9/pykala/49/momentti/2/alkup/fin/txt","content_fi":"Kun perhe-eläke myönnetään määräajaksi 1 momentin mukaisesti, Kansaneläkelaitoksen maksama edunjättäjän eläke tai muu etuus keskeytetään perhe-eläkkeen alkamispäivästä."}}}],"isMemberOf":"sfsd:2007/568/luku/9/pykala/49","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/9/pykala/49/alkup/fin","title_fi":"Perhe-eläkkeen myöntäminen määräajaksi"}},{"@id":"sfsd:2007/568/luku/9/pykala/50/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2007/568/luku/9/pykala/50/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2007/568/luku/9/pykala/50/momentti/1","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/9/pykala/50/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/9/pykala/50/momentti/1/alkup/fin/txt","content_fi":"Perhe-eläkeoikeutta ei ole henkilöllä, joka on rikoksella tahallisesti aiheuttanut edunjättäjän kuoleman."}}},"isMemberOf":"sfsd:2007/568/luku/9/pykala/50","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/9/pykala/50/alkup/fin","title_fi":"Perhe-eläkeoikeuden menetys"}}],"isMemberOf":"sfsd:2007/568/luku/9","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/9/alkup/fin","title_fi":"Perhe-eläkettä koskevat yhteiset säännökset"}}],"isMemberOf":"sfsd:2007/568/osa/3","version":"sfl:Consolidated","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/osa/3/ajantasa/20170101/fin","title_fi":"PERHE-ELÄKE"}},{"@id":"sfsd:2007/568/osa/4/ajantasa/20170101","@type":"sfl:PartVersion","hasPart":{"@id":"sfsd:2007/568/luku/10/ajantasa/20170101","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:2007/568/luku/10/pykala/51/ajantasa/20170101","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2007/568/luku/10/pykala/51/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:2007/568/luku/10/pykala/51/momentti/1/johdanto/alkup","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:2007/568/luku/10/pykala/51/momentti/1/johdanto","version":"sfl:Original","versionDate":"2017-03-01","languageVersion":{"@id":"sfsd:2007/568/luku/10/pykala/51/momentti/1/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:2007/568/luku/10/pykala/51/momentti/1/johdanto/alkup/fin/txt","content_fi":"Oikeus lapsikorotukseen on henkilöllä, joka saa:"}}},{"@id":"sfsd:2007/568/luku/10/pykala/51/momentti/1/kohta/1/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2007/568/luku/10/pykala/51/momentti/1/kohta/1","version":"sfl:Original","