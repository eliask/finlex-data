Of":"sfsd:2003/375/pykala/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/375/pykala/1/alkup/fin","title_fi":"Ministeriön tehtävät"}},{"@id":"sfsd:2003/375/pykala/2/ajantasa/20100101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2009/1088/alkup","hasPart":{"@id":"sfsd:2003/375/pykala/2/momentti/1/ajantasa/20100101","@type":"sfl:SubsectionVersion","hasPart":{"@id":"sfsd:2003/375/pykala/2/momentti/1/johdanto/ajantasa/20100101","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:2003/375/pykala/2/momentti/1/johdanto","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/375/pykala/2/momentti/1/johdanto/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:2003/375/pykala/2/momentti/1/johdanto/ajantasa/20100101/fin/txt","content_fi":"Puolustusministeriön toimialaan kuuluvat sen lisäksi, mitä valtioneuvoston ohjesäännössä (262/2003) säädetään:"}}},"isMemberOf":"sfsd:2003/375/pykala/2/momentti/1","version":"sfl:Consolidated","versionDate":"2017-01-01"},"isMemberOf":"sfsd:2003/375/pykala/2","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/375/pykala/2/ajantasa/20100101/fin","title_fi":"Ministeriön toimialaan kuuluvat virastot, laitokset ja toimielimet"}},{"@id":"sfsd:2003/375/pykala/3/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/981/alkup","hasPart":[{"@id":"sfsd:2003/375/pykala/3/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/375/pykala/3/momentti/1","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/375/pykala/3/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:2003/375/pykala/3/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Ministeriössä on puolustuspoliittinen osasto, hallintopoliittinen osasto ja resurssipoliittinen osasto. Osastoihin kuuluvista yksiköistä säädetään ministeriön työjärjestyksessä."}}},{"@id":"sfsd:2003/375/pykala/3/momentti/2/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/375/pykala/3/momentti/2","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/375/pykala/3/momentti/2/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:2003/375/pykala/3/momentti/2/ajantasa/20170101/fin/txt","content_fi":"Ministeriössä on lisäksi kansliapäällikön alaisuudessa hallintoyksikkö ja viestintäyksikkö sekä Turvallisuuskomitean sihteeristö. Viestintäyksikön yhteydessä on maanpuolustustiedotuksen suunnittelukunnan sihteeristö ja puolustuspoliittisen osaston tutkimusyksikön johtajan alaisuudessa maanpuolustuksen tieteellisen neuvottelukunnan sihteeristö."}}},{"@id":"sfsd:2003/375/pykala/3/momentti/3/ajantasa/20170101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/375/pykala/3/momentti/3","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/375/pykala/3/momentti/3/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:2003/375/pykala/3/momentti/3/ajantasa/20170101/fin/txt","content_fi":"Osastoon kuuluvan yksikön ja erillisen toimintayksikön johtajana on ministerin tehtävään määräämä virkamies."}}}],"isMemberOf":"sfsd:2003/375/pykala/3","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/375/pykala/3/ajantasa/20170101/fin","title_fi":"Organisaatio"}},{"@id":"sfsd:2003/375/pykala/4/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2003/375/pykala/4/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/375/pykala/4/momentti/1","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/375/pykala/4/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2003/375/pykala/4/momentti/1/alkup/fin/txt","content_fi":"Työjärjestyksessä säädetään ministeriön hallinnonalan ohjauksesta, ministeriön johtamisesta ja johtoryhmistä, osastojen ja erillisten toimintayksiköiden toimialasta, johtavien virkamiesten tehtävistä ja sijaisista, asioiden valmistelusta sekä siitä, kenen virkamiehen ratkaistavaksi asia kuuluu."}}},{"@id":"sfsd:2003/375/pykala/4/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/375/pykala/4/momentti/2","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/375/pykala/4/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2003/375/pykala/4/momentti/2/alkup/fin/txt","content_fi":"Osaston ja erillisen toimintayksikön sisäisestä organisaatiosta ja työnjaosta säädetään tarkemmin työjärjestyksessä tai määrätään tarvittaessa osastopäällikön tai erillisen toimintayksikön johtajan antamassa ministeriön sisäisessä määräyksessä."}}}],"isMemberOf":"sfsd:2003/375/pykala/4","version":"sfl:Original","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/375/pykala/4/alkup/fin","title_fi":"Työjärjestys"}},{"@id":"sfsd:2003/375/pykala/5/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/981/alkup","hasPart":{"@id":"sfsd:2003/375/pykala/5/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","cites":"sfsd:2003/175/pykala/16","isMemberOf":"sfsd:2003/375/pykala/5/momentti/1","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/375/pykala/5/momentti/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:2003/375/pykala/5/momentti/1/ajantasa/20170101/fin/txt","content_fi":"Sen lisäksi, mitä valtioneuvostosta annetun lain (175/2003) 16 §:ssä ja valtioneuvoston ohjesäännön 37 §:ssä säädetään ratkaisuvallan pidättämisestä, voi erillisen toimintayksikön johtaja yksittäistapauksessa pidättää itselleen päätösvallan asiassa, jonka hänen alaisensa muuten saisi ratkaista."}}},"isMemberOf":"sfsd:2003/375/pykala/5","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/375/pykala/5/ajantasa/20170101/fin","title_fi":"Oikeus päätösvallan pidättämiseen"}},{"@id":"sfsd:2003/375/pykala/6/ajantasa/20170101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2016/981/alkup","hasPart":{"@id":"sfsd:2003/375/pykala/6/momentti/1/ajantasa/20170101","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:2003/375/pykala/6/momentti/1/johdanto/ajantasa/20170101","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:2003/375/pykala/6/momentti/1/johdanto","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/375/pykala/6/momentti/1/johdanto/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:2003/375/pykala/6/momentti/1/johdanto/ajantasa/20170101/fin/txt","content_fi":"Kelpoisuusvaatimuksena on, sen lisäksi, mitä valtioneuvoston ohjesäännön 43 §:ssä virkojen kelpoisuusvaatimuksista säädetään:"}}},{"@id":"sfsd:2003/375/pykala/6/momentti/1/kohta/1/ajantasa/20170101","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2003/375/pykala/6/momentti/1/kohta/1","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/375/pykala/6/momentti/1/kohta/1/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:2003/375/pykala/6/momentti/1/kohta/1/ajantasa/20170101/fin/txt","content_fi":"1) hallintojohtajalla käytännössä osoitettu johtamistaito ja johtamiskokemus;"}}},{"@id":"sfsd:2003/375/pykala/6/momentti/1/kohta/2/ajantasa/20170101","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2003/375/pykala/6/momentti/1/kohta/2","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/375/pykala/6/momentti/1/kohta/2/ajantasa/20170101/fin","hasFormat":{"@id":"sfsd:2003/375/pykala/6/momentti/1/kohta/2/ajantasa/20170101/fin/txt","content_fi":"2) erikoistutkijalla, neuvottelevalla virkamiehellä, yli-insinöörillä ja ylitarkastajalla ylempi korkeakoulututkinto sekä perehtyneisyys viran tehtäväalaan;"}}},{"@id":"sfsd:2003/375/pykala/6/momentti/1/kohta/3/ajantasa/20170101","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2003/375/pykala/6/momentti/1/kohta/3","version":"sfl:Consolidated","versionDate":"2017-01-01","languageVersion":{"@id":"sfsd:2003/375/pykala/6/momentti/1/ko