kala/77/momentti/1/kohta/73/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/1/kohta/73/alkup/fin/txt","content_fi":"hukkaan joutunutta vekseliä (73 §); sekä"}}},{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/1/kohta/74/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1932/242/osa/2/pykala/77/momentti/1/kohta/74","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/1/kohta/74/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/1/kohta/74/alkup/fin/txt","content_fi":"vekselinhaltijan oikeutta ajaa aiheetonta hyötymistä koskevaa kannetta (74 §)."}}}],"isMemberOf":"sfsd:1932/242/osa/2/pykala/77/momentti/1","version":"sfl:Original","versionDate":"1993-12-01"},{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/alkup","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/johdanto/alkup","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1932/242/osa/2/pykala/77/momentti/2/johdanto","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/johdanto/alkup/fin/txt","content_fi":"Omaan vekseliin on niin ikään sovellettava säännöksiä, jotka koskevat:"}}},{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/4/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/4","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/4/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/4/alkup/fin/txt","content_fi":"asetetta, joka on maksettava kolmannen henkilön luona tai muualla kuin maksavan asuinpaikkakunnalla (4 ja 27 §);"}}},{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/5/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/5","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/5/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/5/alkup/fin/txt","content_fi":"koron määräämistä (5 §);"}}},{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/6/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/6","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/6/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/6/alkup/fin/txt","content_fi":"eroavaisuuksia vekselin määrän merkitsemisessä (6 §);"}}},{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/7/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/7","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/7/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/7/alkup/fin/txt","content_fi":"7 §:ssä mainituissa olosuhteissa toimitetun nimen kirjoittamisen vaikutuksia;"}}},{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/8/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/8","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/8/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/8/alkup/fin/txt","content_fi":"sellaisen henkilön toimittaman nimen kirjoittamisen vaikutuksia, joka toimii ilman valtuutta tai valtuutensa ylittäen (8 §); sekä"}}},{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/10/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/10","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/10/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/2/kohta/10/alkup/fin/txt","content_fi":"vekseliä, joka on annettu epätäydellisenä (10 §)."}}}],"isMemberOf":"sfsd:1932/242/osa/2/pykala/77/momentti/2","version":"sfl:Original","versionDate":"1993-12-01"},{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/2/pykala/77/momentti/3","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/2/pykala/77/momentti/3/alkup/fin/txt","content_fi":"Niin ikään sovellettakoon omaan vekseliin vekselitakausta koskevia säännöksiä (30― 32 §). Jos 31 §:n 4 momentissa mainitussa tapauksessa sellainen takaus ei osoita, kenen hyväksi se on annettu, katsotaan se annetuksi oman vekselin asettajan hyväksi."}}}],"isMemberOf":"sfsd:1932/242/osa/2/pykala/77","version":"sfl:Original","versionDate":"1993-12-01"},{"@id":"sfsd:1932/242/osa/2/pykala/78/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1932/242/osa/2/pykala/78/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/2/pykala/78/momentti/1","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/2/pykala/78/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/2/pykala/78/momentti/1/alkup/fin/txt","content_fi":"Oman vekselin asettaja on vastuussa samalla tavoin kuin asetteen hyväksyjä."}}},{"@id":"sfsd:1932/242/osa/2/pykala/78/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/2/pykala/78/momentti/2","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/2/pykala/78/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/2/pykala/78/momentti/2/alkup/fin/txt","content_fi":"Milloin oma vekseli on maksettava määräaikana näyttämisen jälkeen, se on 23 §:ssä mainitun määräajan kuluessa esitettävä asettajalle siihen näyttämisestä tehtävää merkintää varten. Erääntymisaikaa laskettaessa on alkukohtana oleva se päivä joksi asettajan allekirjoittama merkintä on päivätty. Jos asettaja kieltäytyy tekemästä päivättyä merkintää, tämä on selvitettävä protestilla (25 §) ja erääntymisaikaa määrättäessä laskeminen aloitettava protestinottopäivästä."}}}],"isMemberOf":"sfsd:1932/242/osa/2/pykala/78","version":"sfl:Original","versionDate":"1993-12-01"}],"isMemberOf":"sfsd:1932/242/osa/2","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/2/alkup/fin","title_fi":"Omat vekselit"}},{"@id":"sfsd:1932/242/osa/3/alkup","@type":"sfl:PartVersion","hasPart":[{"@id":"sfsd:1932/242/osa/3/pykala/79/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1932/242/osa/3/pykala/79/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/3/pykala/79/momentti/1","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/3/pykala/79/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/3/pykala/79/momentti/1/alkup/fin/txt","content_fi":"Suomen kansalaisen oikeus mennä vekselisitoumukseen määräytyy Suomen lain mukaan silloinkin, kun hänellä ei ollut kotipaikkaa tässä valtakunnassa tai kun velvoittautuminen on tapahtunut valtakunnan ulkopuolella."}}},{"@id":"sfsd:1932/242/osa/3/pykala/79/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/3/pykala/79/momentti/2","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/3/pykala/79/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/3/pykala/79/momentti/2/alkup/fin/txt","content_fi":"Ulkomaan kansalaisen oikeus mennä vekselisitoumukseen määräytyy sen valtion lain mukaan, johon hän kuuluu. Jos sanotun lain mukaan muun maan lakia on sovellettava, noudatettakoon sitä."}}},{"@id":"sfsd:1932/242/osa/3/pykala/79/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/3/pykala/79/momentti/3","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/3/pykala/79/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/3/pykala/79/momentti/3/alkup/fin/txt","content_fi":"Jos sen lain mukaan, jota täten on noudatettava, ulkomaan kansalainen ei saa mennä vekselisitoumukseen, olkoon kuitenkin tässä valtakunnassa sellaisesta sitoumuksesta vastuussa, milloin velvoittautuminen on tapahtunut sellaisen valtion alueella, jonka lain mukaan hänellä oli siihen oikeus."}}}],"isMemberOf":"sfsd:1932/242/osa/3/pykala/79","version":"sfl:Original","versionDate":"1993-12-01"},{"@id":"sfsd:1932/242/osa/3/pykala/80/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1932/242/osa/3/pykala/80/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/3/pykala/80/momentti/1","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/3/pykala/80/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/3/pykala/80/momentti/1/alkup/fin/txt","content_fi":"Vekselisitoumuksen muotoon nähden on noudatettava sen valtion lakia, missä sitoumus on allekirjoitettu."}}},{"@id":"sfsd:1932/242/osa/3/pykala/80/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/3/pykala/80/momentti/2","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/3/pykala/80/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/3/pykala/80/momentti/2/alkup/fin/txt","content_fi":"Milloin vekselisitoumus, sen johdosta, mitä tässä on sanottu, on mitätön, mutta olisi ollut pätevä, jos se olisi syntynyt jossakin toisessa valtiossa, missä vekseliin on merkitty myöhempi sitoumus, älköön aikaisemman sitoumuksen mitättömyydestä johtuko, että myöskin myöhempi sitoumus on mitätön."}}},{"@id":"sfsd:1932/242/osa/3/pykala/80/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/3/pykala/80/momentti/3","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/3/pykala/80/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/3/pykala/80/momentti/3/alkup/fin/txt","content_fi":"Vekselisitoumus, johon Suomen kansalainen on mennyt ulkomailla ja joka muodon puolesta täyttää Suomen lain vaatimukset, olkoon tässä valtakunnassa toiseen Suomen kansalaiseen nähden pätevä myöskin silloin, kun siltä 1 momentin säännöksen perusteella puuttuisi pätevyys."}}}],"isMemberOf":"sfsd:1932/242/osa/3/pykala/80","version":"sfl:Original","versionDate":"1993-12-01"},{"@id":"sfsd:1932/242/osa/3/pykala/81/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1932/242/osa/3/pykala/81/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/3/pykala/81/momentti/1","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/3/pykala/81/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/3/pykala/81/momentti/1/alkup/fin/txt","content_fi":"Asetteen hyväksymisen ja oman vekselin asettamisen oikeusvaikutukset määräytyvät sen valtion lain mukaan, missä vekseli on maksettava."}}},{"@id":"sfsd:1932/242/osa/3/pykala/81/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/3/pykala/81/momentti/2","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/3/pykala/81/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/3/pykala/81/momentti/2/alkup/fin/txt","content_fi":"Muunlaisen vekselisitoumuksen vaikutukset määräytyvät, mikäli ei jäljempänä toisin säädetä, sen valtion lain mukaan, missä sitoumus on allekirjoitettu."}}}],"isMemberOf":"sfsd:1932/242/osa/3/pykala/81","version":"sfl:Original","versionDate":"1993-12-01"},{"@id":"sfsd:1932/242/osa/3/pykala/82/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1932/242/osa/3/pykala/82/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/3/pykala/82/momentti/1","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/3/pykala/82/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/3/pykala/82/momentti/1/alkup/fin/txt","content_fi":"Takautumiskanteen määräajat on kaikkien vekselivelallisten osalta laskettava sen valtion lain mukaan, missä vekseli on asetettu."}}},"isMemberOf":"sfsd:1932/242/osa/3/pykala/82","version":"sfl:Original","versionDate":"1993-12-01"},{"@id":"sfsd:1932/242/osa/3/pykala/83/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1932/242/osa/3/pykala/83/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/3/pykala/83/momentti/1","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/3/pykala/83/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/3/pykala/83/momentti/1/alkup/fin/txt","content_fi":"Jos sen valtion lain mukaan, missä vekseli on asetettu, vekselin haltijaa pidetään myös sen saamisen haltijana, joka on ollut aiheena vekselin asettamiseen, sanottu laki olkoon siinä kohden noudatettavana."}}},"isMemberOf":"sfsd:1932/242/osa/3/pykala/83","version":"sfl:Original","versionDate":"1993-12-01"},{"@id":"sfsd:1932/242/osa/3/pykala/84/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1932/242/osa/3/pykala/84/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/3/pykala/84/momentti/1","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/3/pykala/84/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/3/pykala/84/momentti/1/alkup/fin/txt","content_fi":"Voidaanko vekselin hyväksyminen rajoittaa vain osaan vekselin määrästä ja saako vekselin haltija kieltäytyä ottamasta vastaan osamaksua, arvostellaan sen valtion lain mukaan, missä vekseli on maksettava."}}},"isMemberOf":"sfsd:1932/242/osa/3/pykala/84","version":"sfl:Original","versionDate":"1993-12-01"},{"@id":"sfsd:1932/242/osa/3/pykala/85/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1932/242/osa/3/pykala/85/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/3/pykala/85/momentti/1","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/3/pykala/85/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/3/pykala/85/momentti/1/alkup/fin/txt","content_fi":"Siitä, minä aikana ja millä tavoin protesti on toimitettava, niin myös siitä, millä tavoin sellaiset muut toimet on suoritettava, jotka ovat vekseliin perustuvan oikeuden käyttämisen tai säilyttämisen edellytyksenä olkoon voimassa sen valtion laki, missä protesti on toimitettava tai muu kysymykseen tuleva toimenpide suoritettava."}}},"isMemberOf":"sfsd:1932/242/osa/3/pykala/85","version":"sfl:Original","versionDate":"1993-12-01"},{"@id":"sfsd:1932/242/osa/3/pykala/86/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1932/242/osa/3/pykala/86/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/3/pykala/86/momentti/1","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/3/pykala/86/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/3/pykala/86/momentti/1/alkup/fin/txt","content_fi":"Toimenpiteistä, joihin on ryhdyttävä silloin, kun vekseli on oikeudettomasti viety sen haltijalta tai joutunut hukkaan, olkoon voimassa sen valtion laki, missä vekseli on maksettava."}}},"isMemberOf":"sfsd:1932/242/osa/3/pykala/86","version":"sfl:Original","versionDate":"1993-12-01"},{"@id":"sfsd:1932/242/osa/3/pykala/87/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1932/242/osa/3/pykala/87/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/3/pykala/87/momentti/1","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/3/pykala/87/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/3/pykala/87/momentti/1/alkup/fin/txt","content_fi":"Jos sen mukaan, mitä edellä on sanottu, jossakin asiassa on sovellettava vieraan valtion lakia, oikeuden tulee, ellei se tunne lain sisällystä tai asianosainen sitä näytä toteen, hankkia siitä selvitys ulkoasiainministeriöltä. Ellei selvitystä voida saada, Suomen lakia on sovellettava."}}},"isMemberOf":"sfsd:1932/242/osa/3/pykala/87","version":"sfl:Original","versionDate":"1993-12-01"}],"isMemberOf":"sfsd:1932/242/osa/3","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/3/alkup/fin","title_fi":"Suhtautuminen ulkomaan lakiin"}},{"@id":"sfsd:1932/242/osa/4/ajantasa/19931201","@type":"sfl:PartVersion","hasPart":[{"@id":"sfsd:1932/242/osa/4/pykala/88/ajantasa/19610101","@type":"sfl:SectionVersion","amendedBy":"sfsd:1960/289/alkup","hasPart":{"@id":"sfsd:1932/242/osa/4/pykala/88/momentti/1/ajantasa/19610101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/4/pykala/88/momentti/1","version":"sfl:Consolidated","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/4/pykala/88/momentti/1/ajantasa/19610101/fin","hasFormat":{"@id":"sfsd:1932/242/osa/4/pykala/88/momentti/1/ajantasa/19610101/fin/txt","content_fi":"Protesti on kaupungissa julkisen notaarin ja maalla paikkakunnan tai lähimmän kaupungin julkisen notaarin toimitettava."}}},"isMemberOf":"sfsd:1932/242/osa/4/pykala/88","version":"sfl:Consolidated","versionDate":"1993-12-01"},{"@id":"sfsd:1932/242/osa/4/pykala/89/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1932/242/osa/4/pykala/89/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/4/pykala/89/momentti/1","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/4/pykala/89/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/4/pykala/89/momentti/1/alkup/fin/txt","content_fi":"Protesti on toimitettava vekselin esittämispaikkakunnalla kello 9 ja 19 välillä. sen liikehuoneistossa, johon protesti kohdistuu, tai ellei hänellä paikkakunnalla ole tällaista huoneistoa, hänen asunnossaan. Jollei häntä tavata tai hänen liikehuoneistoaan tahi asuntoaan tiedetä eikä siitä voida saada tietoa paikkakunnan poliisiviranomaiselta protesti on kuitenkin toimitettava."}}},{"@id":"sfsd:1932/242/osa/4/pykala/89/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/4/pykala/89/momentti/2","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/4/pykala/89/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/4/pykala/89/momentti/2/alkup/fin/txt","content_fi":"Vekselin hyväksyttäväksi tai maksettavaksi esittämiseen on sovellettava, mitä 1 momentissa on protestista sanottu, ja sen vuoksi on, jollei asianomaista tavata, esittämisen kuitenkin katsottava tapahtuneen, milloin vekselin haltija tai, kun on kysymys hyväksyttäväksi esittämisestä, se, jolla on vekseli hallussaan, on esittääkseen vekselin käynyt asianomaisen liikehuoneistossa tai asunnossa tahi koettanut sinne päästä."}}}],"isMemberOf":"sfsd:1932/242/osa/4/pykala/89","version":"sfl:Original","versionDate":"1993-12-01"},{"@id":"sfsd:1932/242/osa/4/pykala/90/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1932/242/osa/4/pykala/90/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/4/pykala/90/momentti/1","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/4/pykala/90/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/4/pykala/90/momentti/1/alkup/fin/txt","content_fi":"Toimitettaessa protestia vekselin hyväksymättä tai maksamatta jättämisen vuoksi toimitusmiehellä tai jollakulla hänen mukanaan olevalla henkilölle tulee olla muassaan alkuperäinen vekseli; toimitusmiehen on protestista tehtävä merkintä vekseliin. Jos vekselin siirto tai takaus on merkitty jäljennökseen, myöskin jäljennös on tuotava toimitukseen ja siihen tehtävä protestista merkintä."}}},"isMemberOf":"sfsd:1932/242/osa/4/pykala/90","version":"sfl:Original","versionDate":"1993-12-01"},{"@id":"sfsd:1932/242/osa/4/pykala/91/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1932/242/osa/4/pykala/91/momentti/1/alkup","@type":"sfl:SubsectionVersion","hasPart":{"@id":"sfsd:1932/242/osa/4/pykala/91/momentti/1/johdanto/alkup","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:1932/242/osa/4/pykala/91/momentti/1/johdanto","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/4/pykala/91/momentti/1/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/4/pykala/91/momentti/1/johdanto/alkup/fin/txt","content_fi":"Protestista on pidettävä pöytäkirjaa. Siihen otettakoon:"}}},"isMemberOf":"sfsd:1932/242/osa/4/pykala/91/momentti/1","version":"sfl:Original","versionDate":"1993-12-01"},{"@id":"sfsd:1932/242/osa/4/pykala/91/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/4/pykala/91/momentti/2","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/4/pykala/91/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/4/pykala/91/momentti/2/alkup/fin/txt","content_fi":"Jäljennös, jota 90 §:ssä tarkoitetaan, on siihen tehtyine merkintöineen myötä otettava pöytäkirjaan."}}},{"@id":"sfsd:1932/242/osa/4/pykala/91/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/4/pykala/91/momentti/3","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/4/pykala/91/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/4/pykala/91/momentti/3/alkup/fin/txt","content_fi":"Toimitusmiehen tulee säilyttää protestia toimitettaessa pidetty pöytäkirja, jotta jokainen, joka haluaa, voi saada siitä tietoja."}}}],"isMemberOf":"sfsd:1932/242/osa/4/pykala/91","version":"sfl:Original","versionDate":"1993-12-01"},{"@id":"sfsd:1932/242/osa/4/pykala/92/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:1932/242/osa/4/pykala/92/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/4/pykala/92/momentti/1","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/4/pykala/92/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/4/pykala/92/momentti/1/alkup/fin/txt","content_fi":"Toimitusmies, jolla, protestia maksamatta jättämisen vuoksi toimitettaessa, on alkuperäinen vekseli hallussaan, on katsottava oikeutetuksi kantamaan maksun."}}},"isMemberOf":"sfsd:1932/242/osa/4/pykala/92","version":"sfl:Original","versionDate":"1993-12-01"},{"@id":"sfsd:1932/242/osa/4/pykala/93/ajantasa/19931201","@type":"sfl:SectionVersion","amendedBy":"sfsd:1993/603/alkup","hasPart":{"@id":"sfsd:1932/242/osa/4/pykala/93/momentti/1/ajantasa/19931201","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/4/pykala/93/momentti/1","version":"sfl:Consolidated","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/4/pykala/93/momentti/1/ajantasa/19931201/fin","hasFormat":{"@id":"sfsd:1932/242/osa/4/pykala/93/momentti/1/ajantasa/19931201/fin/txt","content_fi":"Vekseliasiain tuomioistuin on sen paikkakunnan käräjäoikeus, missä velallinen asuu tai tavataan taikka missä vekseli on asetettu maksettavaksi."}}},"isMemberOf":"sfsd:1932/242/osa/4/pykala/93","version":"sfl:Consolidated","versionDate":"1993-12-01"},{"@id":"sfsd:1932/242/osa/4/pykala/94/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1932/242/osa/4/pykala/94/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/4/pykala/94/momentti/1","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/4/pykala/94/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/4/pykala/94/momentti/1/alkup/fin/txt","content_fi":"Vekseliasiassa on haaste, milloin hakija vaatii, annettava samaksi päiväksi, jona sitä pyydetään, jos oikeudella silloin on istunto ja vastaaja on niin lähellä, että hän voi tulla saapuville ennen oikeudenkäyntiajan loppua."}}},{"@id":"sfsd:1932/242/osa/4/pykala/94/momentti/1/ajantasa/19320714","@type":"sfl:SubsectionVersion","amendedBy":"sfsd:1991/1059/alkup","isMemberOf":"sfsd:1932/242/osa/4/pykala/94/momentti/1","version":"sfl:Consolidated","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/4/pykala/94/momentti/1/ajantasa/19320714/fin","hasFormat":{"@id":"sfsd:1932/242/osa/4/pykala/94/momentti/1/ajantasa/19320714/fin/txt","content_fi":"Vekseliasiassa kantaja saa, ilmoitettuaan siitä tuomioistuimelle, itse huolehtia haasteen tiedoksiantamisesta. Jos vastaajaa kehotetaan haasteessa vastaamaan kanteeseen suullisesti istunnossa, haaste on kantajan vaatimuksesta annettava samaksi päiväksi, jona sitä pyydetään, jos tuomioistuimella silloin on istunto ja vastaaja on niin lähellä, että hän voi tulla saapuville ennen oikeudenkäyntiajan loppua.\n(22.7.1991/1059)"}}},{"@id":"sfsd:1932/242/osa/4/pykala/94/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/4/pykala/94/momentti/2","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/4/pykala/94/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/4/pykala/94/momentti/2/alkup/fin/txt","content_fi":"Päätöksestä, joka on annettu väitteeseen vekseliasiassa, saa erikseen valittaa vain, jos asian käsitteleminen tuomioistuimessa on siihen päättänyt."}}}],"isMemberOf":"sfsd:1932/242/osa/4/pykala/94","version":"sfl:Original","versionDate":"1993-12-01"},{"@id":"sfsd:1932/242/osa/4/pykala/95/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1932/242/osa/4/pykala/95/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/4/pykala/95/momentti/1","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/4/pykala/95/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/4/pykala/95/momentti/1/alkup/fin/txt","content_fi":"Vekseliasiassa annettu tuomio pantakoon täytäntöön sen estämättä, ettei se ole saanut lainvoimaa. Täytäntöönpanoa älköön viivytettäkö sen johdosta, että tappiolle joutunut asianosainen tarjoaa vakuutta siitä, mitä on tuomittu."}}},{"@id":"sfsd:1932/242/osa/4/pykala/95/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:1932/242/osa/4/pykala/95/momentti/2","version":"sfl:Original","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/4/pykala/95/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:1932/242/osa/4/pykala/95/momentti/2/alkup/fin/txt","content_fi":"Joka on voittanut asian, älköön kuitenkaan, ennenkuin tuomio on lainvoimainen, nostako, mitä hänelle on tuomittu, ellei hän aseta panttia tai takausta takaisinmaksamisesta, jos tuomio muuttuu."}}}],"isMemberOf":"sfsd:1932/242/osa/4/pykala/95","version":"sfl:Original","versionDate":"1993-12-01"},{"@id":"sfsd:1932/242/osa/4/pykala/96/ajantasa/19931201","@type":"sfl:SectionVersion","isMemberOf":"sfsd:1932/242/osa/4/pykala/96","repealedBy":"sfsd:1993/603/alkup","version":"sfl:Consolidated","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/4/pykala/96/ajantasa/19931201/fin","hasFormat":{"@id":"sfsd:1932/242/osa/4/pykala/96/ajantasa/19931201/fin/txt","content_fi":"96 §(28.6.1993/603)\n96 § on kumottu L:lla 28.6.1993/603."}}}],"isMemberOf":"sfsd:1932/242/osa/4","version":"sfl:Consolidated","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/osa/4/ajantasa/19931201/fin","title_fi":"Protestin toimittaminen ja oikeudenkäynti vekselijutuissa"}}],"isAbout":"http://data.finlex.fi/voc/finlex/kw4359","isMemberOf":"sfsd:1932/242","version":"sfl:Consolidated","versionDate":"1993-12-01","languageVersion":{"@id":"sfsd:1932/242/ajantasa/19931201/fin","title_fi":"Vekselilaki"}},"@context":{"languageVersion":{"@id":"eli:is_realized_by","@type":"@id"},"temporalVersions":{"@id":"eli:has_member","@type":"@id"},"temporalVersion":{"@id":"eli:has_member","@type":"@id"},"hasFormat":{"@id":"eli:is_embodied_by","@type":"@id"},"idLocal":"eli:id_local","dateDocument":{"@id":"eli:date_document","@type":"xsd:date"},"hasPart":{"@id":"eli:has_part","@type":"@id"},"isAbout":{"@id":"eli:is_about","@type":"@id"},"isMemberOf":{"@id":"eli:is_member_of","@type":"@id"},"version":{"@id":"eli:version","@type":"@id"},"versionDate":{"@id":"eli:version_date","@type":"xsd:date"},"title_fi":{"@id":"eli:title","@language":"fi"},"content_fi":{"@id":"sfl:text","@language":"fi"},"amendedBy":{"@id":"eli:amended_by","@type":"@id"},"repealedBy":{"@id":"eli:repealed_by","@type":"@id"},"followedBy":{"@id":"sfl:followedBy","@type":"@id"},"inForce":{"@id":"eli:in_force","@type":"@id"},"typeDocument":{"@id":"eli:type_document","@type":"@id"},"dcterms":"http://purl.org/dc/terms/","xsd":"http://www.w3.org/2001/XMLSchema#","common":"http://data.finlex.fi/common/","rdf":"http://www.w3.org/1999/02/22-rdf-syntax-ns#","sfcl":"http://data.finlex.fi/schema/scfl/","sfl":"http://data.finlex.fi/schema/sfl/","sfecli":"http://data.finlex.fi/ecli/","sfsd":"http://data.finlex.fi/eli/sd/","skos":"http://www.w3.org/2004/02/skos/core#","text":"http://jena.apache.org/text#","eli":"http://data.europa.eu/eli/ontology#"}}