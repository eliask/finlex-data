fsd:2003/357/luku/1/pykala/4/momentti/1/alkup","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:2003/357/luku/1/pykala/4/momentti/1/johdanto/alkup","@type":"sfl:PreambleVersion","isMemberOf":"sfsd:2003/357/luku/1/pykala/4/momentti/1/johdanto","version":"sfl:Original","versionDate":"2005-01-01","languageVersion":{"@id":"sfsd:2003/357/luku/1/pykala/4/momentti/1/johdanto/alkup/fin","hasFormat":{"@id":"sfsd:2003/357/luku/1/pykala/4/momentti/1/johdanto/alkup/fin/txt","content_fi":"Opettajankoulutuksen tavoitteena on antaa opiskelijalle:"}}},{"@id":"sfsd:2003/357/luku/1/pykala/4/momentti/1/kohta/1/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2003/357/luku/1/pykala/4/momentti/1/kohta/1","version":"sfl:Original","versionDate":"2005-01-01","languageVersion":{"@id":"sfsd:2003/357/luku/1/pykala/4/momentti/1/kohta/1/alkup/fin","hasFormat":{"@id":"sfsd:2003/357/luku/1/pykala/4/momentti/1/kohta/1/alkup/fin/txt","content_fi":"1) tiedot ja taidot ohjata erilaisten opiskelijoiden oppimista; sekä"}}},{"@id":"sfsd:2003/357/luku/1/pykala/4/momentti/1/kohta/2/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2003/357/luku/1/pykala/4/momentti/1/kohta/2","version":"sfl:Original","versionDate":"2005-01-01","languageVersion":{"@id":"sfsd:2003/357/luku/1/pykala/4/momentti/1/kohta/2/alkup/fin","hasFormat":{"@id":"sfsd:2003/357/luku/1/pykala/4/momentti/1/kohta/2/alkup/fin/txt","content_fi":"2) valmiudet kehittää opetusalaansa ottaen huomioon työelämän ja ammattien kehittyminen."}}}],"isMemberOf":"sfsd:2003/357/luku/1/pykala/4/momentti/1","version":"sfl:Original","versionDate":"2005-01-01"},"isMemberOf":"sfsd:2003/357/luku/1/pykala/4","version":"sfl:Original","versionDate":"2005-01-01","languageVersion":{"@id":"sfsd:2003/357/luku/1/pykala/4/alkup/fin","title_fi":"Opintojen tavoitteet"}},{"@id":"sfsd:2003/357/luku/1/pykala/5/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2003/357/luku/1/pykala/5/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/357/luku/1/pykala/5/momentti/1","version":"sfl:Original","versionDate":"2005-01-01","languageVersion":{"@id":"sfsd:2003/357/luku/1/pykala/5/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2003/357/luku/1/pykala/5/momentti/1/alkup/fin/txt","content_fi":"Ammatillisen opettajakorkeakoulun on järjestettävä ammatilliset opettajankoulutusopinnot niin, että kokopäiväinen opiskelija voi suorittaa opinnot niiden laajuutta vastaavassa ajassa."}}},"isMemberOf":"sfsd:2003/357/luku/1/pykala/5","version":"sfl:Original","versionDate":"2005-01-01","languageVersion":{"@id":"sfsd:2003/357/luku/1/pykala/5/alkup/fin","title_fi":"Opiskeluaika"}}],"isMemberOf":"sfsd:2003/357/luku/1","version":"sfl:Consolidated","versionDate":"2005-01-01","languageVersion":{"@id":"sfsd:2003/357/luku/1/ajantasa/20050101/fin","title_fi":"Opettajankoulutusopinnot"}},{"@id":"sfsd:2003/357/luku/2/alkup","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:2003/357/luku/2/pykala/6/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2003/357/luku/2/pykala/6/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/357/luku/2/pykala/6/momentti/1","version":"sfl:Original","versionDate":"2005-01-01","languageVersion":{"@id":"sfsd:2003/357/luku/2/pykala/6/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2003/357/luku/2/pykala/6/momentti/1/alkup/fin/txt","content_fi":"Johtajalta ja opettajalta vaaditaan, että hän hallitsee opettajankoulutuksen opetuskielen tai sen kielen, jolla hänet on määrätty antamaan opetusta."}}},"isMemberOf":"sfsd:2003/357/luku/2/pykala/6","version":"sfl:Original","versionDate":"2005-01-01","languageVersion":{"@id":"sfsd:2003/357/luku/2/pykala/6/alkup/fin","title_fi":"Kielitaitovaatimukset"}},{"@id":"sfsd:2003/357/luku/2/pykala/7/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2003/357/luku/2/pykala/7/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/357/luku/2/pykala/7/momentti/1","version":"sfl:Original","versionDate":"2005-01-01","languageVersion":{"@id":"sfsd:2003/357/luku/2/pykala/7/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2003/357/luku/2/pykala/7/momentti/1/alkup/fin/txt","content_fi":"Johtajalta vaaditaan soveltuva jatkotutkintona suoritettu lisensiaatin tutkinto tai tohtorin tutkinto ja perehtyneisyyttä ammatilliseen opettajankoulutukseen."}}},{"@id":"sfsd:2003/357/luku/2/pykala/7/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/357/luku/2/pykala/7/momentti/2","version":"sfl:Original","versionDate":"2005-01-01","languageVersion":{"@id":"sfsd:2003/357/luku/2/pykala/7/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2003/357/luku/2/pykala/7/momentti/2/alkup/fin/txt","content_fi":"Yliopettajalta vaaditaan soveltuva jatkotutkintona suoritettu lisensiaatin tutkinto tai tohtorin tutkinto ja lehtorilta soveltuva ylempi korkeakoulututkinto."}}},{"@id":"sfsd:2003/357/luku/2/pykala/7/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/357/luku/2/pykala/7/momentti/3","version":"sfl:Original","versionDate":"2005-01-01","languageVersion":{"@id":"sfsd:2003/357/luku/2/pykala/7/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:2003/357/luku/2/pykala/7/momentti/3/alkup/fin/txt","content_fi":"Erityisestä syystä johtajan tai yliopettajan virkaan tai toimeen voidaan 1 ja 2 momentin estämättä nimittää ylemmän korkeakoulututkinnon suorittanut henkilö ja lehtorin virkaan tai toimeen henkilö, joka ei ole suorittanut ylempää korkeakoulututkintoa, jos nimitettävä henkilö on erittäin hyvin perehtynyt viran tai toimen tehtäväalaan."}}},{"@id":"sfsd:2003/357/luku/2/pykala/7/momentti/4/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/357/luku/2/pykala/7/momentti/4","version":"sfl:Original","versionDate":"2005-01-01","languageVersion":{"@id":"sfsd:2003/357/luku/2/pykala/7/momentti/4/alkup/fin","hasFormat":{"@id":"sfsd:2003/357/luku/2/pykala/7/momentti/4/alkup/fin/txt","content_fi":"Yliopettajalta ja lehtorilta vaaditaan lisäksi ammattikorkeakoulun tai ammatillisen oppilaitoksen opettajan kelpoisuus ja vähintään kolmen vuoden opettajakokemus ammattikorkeakoulussa tai ammatillisessa oppilaitoksessa."}}},{"@id":"sfsd:2003/357/luku/2/pykala/7/momentti/5/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/357/luku/2/pykala/7/momentti/5","version":"sfl:Original","versionDate":"2005-01-01","languageVersion":{"@id":"sfsd:2003/357/luku/2/pykala/7/momentti/5/alkup/fin","hasFormat":{"@id":"sfsd:2003/357/luku/2/pykala/7/momentti/5/alkup/fin/txt","content_fi":"Tuntiopettajalta vaaditaan, mikäli mahdollista, vastaava kelpoisuus kuin viran tai toimen haltijalta."}}}],"isMemberOf":"sfsd:2003/357/luku/2/pykala/7","version":"sfl:Original","versionDate":"2005-01-01","languageVersion":{"@id":"sfsd:2003/357/luku/2/pykala/7/alkup/fin","title_fi":"Kelpoisuusvaatimukset"}}],"isMemberOf":"sfsd:2003/357/luku/2","version":"sfl:Original","versionDate":"2005-01-01","languageVersion":{"@id":"sfsd:2003/357/luku/2/alkup/fin","title_fi":"Opettajankoulutuksen henkilöstö"}},{"@id":"sfsd:2003/357/luku/3/alkup","@type":"sfl:ChapterVersion","hasPart":{"@id":"sfsd:2003/357/luku/3/pykala/8/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2003/357/luku/3/pykala/8/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/357/luku/3/pykala/8/momentti/1","version":"sfl:Original","versionDate":"2005-01-01","languageVersion":{"@id":"sfsd:2003/357/luku/3/pykala/8/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2003/357/luku/3/pykala/8/momentti/1/alkup/fin/txt","content_fi":"Tämä asetus tulee voimaan 1 päivänä elokuuta 2003."}}},{"@id":"sfsd:2003/357/luku/3/pykala/8/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/357/luku/3/pykala/8/momentti/2","version":"sfl:Original","versionDate":"2005-01-01","languageVersion":{"@id":"sfsd:2003/357/luku/3/pykala/8/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2003/357/luku/3/pykala/8/momentti/2/alkup/fin/txt","content_fi":"Ennen tämän asetuksen voimaantuloa voidaan ryhtyä sen täytäntöönpanon edellyttämiin toimenpiteisiin."}}}],"isMemberOf":"sfsd:2003/357/luku/3/pykala/8","version":"sfl:Original","versionDate":"2005-01-01","languageVersion":{"@id":"sfsd:2003/357/luku/3/pykala/8/alkup/fin","title_fi":"Voimaantulo"}},"isMemberOf":"sfsd:2003/357/luku/3","version":"sfl:Original","versionDate":"2005-01-01","languageVersion":{"@id":"sfsd:2003/357/luku/3/alkup/fin","title_fi":"Voimaantulosäännökset"}}],"isAbout":["http://data.finlex.fi/voc/finlex/kw2904","http://data.finlex.fi/voc/finlex/kw1090"],"isMemberOf":"sfsd:2003/357","version":"sfl:Consolidated","versionDate":"2005-01-01","languageVersion":{"@id":"sfsd:2003/357/ajantasa/20050101/fin","title_fi":"Valtioneuvoston asetus ammatillisesta opettajankoulutuksesta"}},"@context":{"languageVersion":{"@id":"eli:is_realized_by","@type":"@id"},"temporalVersions":{"@id":"eli:has_member","@type":"@id"},"temporalVersion":{"@id":"eli:has_member","@type":"@id"},"hasFormat":{"@id":"eli:is_embodied_by","@type":"@id"},"idLocal":"eli:id_local","dateDocument":{"@id":"eli:date_document","@type":"xsd:date"},"hasPart":{"@id":"eli:has_part","@type":"@id"},"isAbout":{"@id":"eli:is_about","@type":"@id"},"isMemberOf":{"@id":"eli:is_member_of","@type":"@id"},"version":{"@id":"eli:version","@type":"@id"},"versionDate":{"@id":"eli:version_date","@type":"xsd:date"},"title_fi":{"@id":"eli:title","@language":"fi"},"content_fi":{"@id":"sfl:text","@language":"fi"},"amendedBy":{"@id":"eli:amended_by","@type":"@id"},"cites":{"@id":"eli:cites","@type":"@id"},"inForce":{"@id":"eli:in_force","@type":"@id"},"typeDocument":{"@id":"eli:type_document","@type":"@id"},"dcterms":"http://purl.org/dc/terms/","xsd":"http://www.w3.org/2001/XMLSchema#","common":"http://data.finlex.fi/common/","rdf":"http://www.w3.org/1999/02/22-rdf-syntax-ns#","sfcl":"http://data.finlex.fi/schema/scfl/","sfl":"http://data.finlex.fi/schema/sfl/","sfecli":"http://data.finlex.fi/ecli/","sfsd":"http://data.finlex.fi/eli/sd/","skos":"http://www.w3