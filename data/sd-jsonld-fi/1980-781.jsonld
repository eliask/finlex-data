{"@id":"sfsd:1980/781","@type":["http://data.finlex.fi/schema/sfl/NewStatute"],"idLocal":["781/1980"],"inForce":["http://data.europa.eu/eli/ontology#InForce-inForce"],"temporalVersions":["http://data.finlex.fi/eli/sd/1980/781/alkup","http://data.finlex.fi/eli/sd/1980/781/ajantasa/20150101","http://data.finlex.fi/eli/sd/1980/781/ajantasa/20170701"],"typeDocument":["http://data.finlex.fi/schema/sfl/Act"],"temporalVersion":{"@id":"sfsd:1980/781/ajantasa/20170701","@type":"sfl:StatuteVersion","dateDocument":"1980-12-05","hasPart":[{"@id":"sfsd:1980/781/johtolause/alkup","@type":"sfl:EnactingClauseVersion","isMemberOf":"sfsd:1980/781/johtolause","version":"sfl:Original","versionDate":"2017-07-01","languageVersion":{"@id":"sfsd:1980/781/johtolause/alkup/fin","hasFormat":{"@id":"sfsd:1980/781/johtolause/alkup/fin/txt","content_fi":"Eduskunnan päätöksen mukaisesti säädetään:"}}},{"@id":"sfsd:1980/781/pykala/1/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:1980/781/pykala/1/momentti/1/alkup","@type":"sfl:SubsectionVersion","hasPart":[{"@id":"sfsd:1980/781/pykala/1/momentti/1/kohta/1/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1980/781/pykala/1/momentti/1/kohta/1","version":"sfl:Original","versionDate":"2017-07-01","languageVersion":{"@id":"sfsd:1980/781/pykala/1/momentti/1/kohta/1/alkup/fin","hasFormat":{"@id":"sfsd:1980/781/pykala/1/momentti/1/kohta/1/alkup/fin/txt","content_fi":"1) henkilön etsimiseksi ja kiinni ottamiseksi;"}}},{"@id":"sfsd:1980/781/pykala/1/momentti/1/kohta/2/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1980/781/pykala/1/momentti/1/kohta/2","version":"sfl:Original","versionDate":"2017-07-01","languageVersion":{"@id":"sfsd:1980/781/pykala/1/momentti/1/kohta/2/alkup/fin","hasFormat":{"@id":"sfsd:1980/781/pykala/1/momentti/1/kohta/2/alkup/fin/txt","content_fi":"2) alueen eristämiseksi ja tutkimiseksi;"}}},{"@id":"sfsd:1980/781/pykala/1/momentti/1/kohta/3/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:1980/781/