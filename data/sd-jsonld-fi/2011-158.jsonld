ate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/3/pykala/32/ajantasa/20180101/fin","title_fi":"Toiminnan rahoitus"}},{"@id":"sfsd:2011/158/luku/3/pykala/33/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2011/158/luku/3/pykala/33/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2011/158/luku/3/pykala/33/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/3/pykala/33/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2011/158/luku/3/pykala/33/momentti/1/alkup/fin/txt","content_fi":"Suomen riistakeskuksen ja riistanhoitoyhdistysten suoritteista perittäviin maksuihin sovelletaan, mitä valtion maksuperustelaissa (150/1992) säädetään."}}},"isMemberOf":"sfsd:2011/158/luku/3/pykala/33","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/3/pykala/33/alkup/fin","title_fi":"Suoritteista perittävät maksut"}},{"@id":"sfsd:2011/158/luku/3/pykala/34/ajantasa/20180101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2017/968/alkup","hasPart":[{"@id":"sfsd:2011/158/luku/3/pykala/34/momentti/1/ajantasa/20180101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2011/158/luku/3/pykala/34/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/3/pykala/34/momentti/1/ajantasa/20180101/fin","hasFormat":{"@id":"sfsd:2011/158/luku/3/pykala/34/momentti/1/ajantasa/20180101/fin/txt","content_fi":"Maa- ja metsätalousministeriö valvoo Suomen riistakeskuksen varojen käyttöä."}}},{"@id":"sfsd:2011/158/luku/3/pykala/34/momentti/2/ajantasa/20180101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2011/158/luku/3/pykala/34/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/3/pykala/34/momentti/2/ajantasa/20180101/fin","hasFormat":{"@id":"sfsd:2011/158/luku/3/pykala/34/momentti/2/ajantasa/20180101/fin/txt","content_fi":"Suomen riistakeskus valvoo riistanhoitoyhdistysten varojen käyttöä."}}}],"isMemberOf":"sfsd:2011/158/luku/3/pykala/34","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/3/pykala/34/ajantasa/20180101/fin","title_fi":"Varojen käytön valvonta"}},{"@id":"sfsd:2011/158/luku/3/pykala/34a/ajantasa/20180101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2017/968/alkup","hasPart":[{"@id":"sfsd:2011/158/luku/3/pykala/34a/momentti/1/ajantasa/20180101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2011/158/luku/3/pykala/34a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/3/pykala/34a/momentti/1/ajantasa/20180101/fin","hasFormat":{"@id":"sfsd:2011/158/luku/3/pykala/34a/momentti/1/ajantasa/20180101/fin/txt","content_fi":"Suomen riistakeskuksen on salassapitosäännösten estämättä annettava maa- ja metsätalousministeriölle ne välttämättömät tiedot ja asiakirjat, joita ministeriö sille säädettyjen tehtävien hoitamiseksi tarvitsee."}}},{"@id":"sfsd:2011/158/luku/3/pykala/34a/momentti/2/ajantasa/20180101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2011/158/luku/3/pykala/34a/momentti/2","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/3/pykala/34a/momentti/2/ajantasa/20180101/fin","hasFormat":{"@id":"sfsd:2011/158/luku/3/pykala/34a/momentti/2/ajantasa/20180101/fin/txt","content_fi":"Riistanhoitoyhdistyksen on salassapitosäännösten estämättä annettava Suomen riistakeskukselle ja maa- ja metsätalousministeriölle ne välttämättömät tiedot ja asiakirjat, joita Suomen riistakeskus ja ministeriö niille säädettyjen tehtävien hoitamiseksi tarvitsevat."}}}],"isMemberOf":"sfsd:2011/158/luku/3/pykala/34a","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/3/pykala/34a/ajantasa/20180101/fin","title_fi":"Tiedonantovelvollisuus"}},{"@id":"sfsd:2011/158/luku/3/pykala/35/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2011/158/luku/3/pykala/35/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2011/158/luku/3/pykala/35/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/3/pykala/35/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2011/158/luku/3/pykala/35/momentti/1/alkup/fin/txt","content_fi":"Suomen riistakeskus ja riistanhoitoyhdistykset saavat harjoittaa sellaista elinkeinotoimintaa, joka tukee niiden tarkoituksen toteutumista eikä vaaranna niiden puolueettomuutta julkisten hallintotehtävien hoitamisessa."}}},"isMemberOf":"sfsd:2011/158/luku/3/pykala/35","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/3/pykala/35/alkup/fin","title_fi":"Elinkeinon harjoittaminen"}}],"isMemberOf":"sfsd:2011/158/luku/3","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/3/ajantasa/20180101/fin","title_fi":"Erinäisiä säännöksiä"}},{"@id":"sfsd:2011/158/luku/4/alkup","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:2011/158/luku/4/pykala/36/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2011/158/luku/4/pykala/36/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2011/158/luku/4/pykala/36/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/pykala/36/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2011/158/luku/4/pykala/36/momentti/1/alkup/fin/txt","content_fi":"Tämä laki tulee voimaan 1 päivänä maaliskuuta 2011."}}},{"@id":"sfsd:2011/158/luku/4/pykala/36/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2011/158/luku/4/pykala/36/momentti/2","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/pykala/36/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2011/158/luku/4/pykala/36/momentti/2/alkup/fin/txt","content_fi":"Maa- ja metsätalousministeriö voi ryhtyä ennen tämän lain voimaantuloa täytäntöönpanon edellyttämiin toimiin."}}}],"isMemberOf":"sfsd:2011/158/luku/4/pykala/36","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/pykala/36/alkup/fin","title_fi":"Voimaantulo"}},{"@id":"sfsd:2011/158/luku/4/pykala/37/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2011/158/luku/4/pykala/37/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2011/158/luku/4/pykala/37/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/pykala/37/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2011/158/luku/4/pykala/37/momentti/1/alkup/fin/txt","content_fi":"Suomen riistakeskuksen käsiteltäviksi ja ratkaistaviksi siirtyvät tämän lain tullessa voimaan Metsästäjäin keskusjärjestössä ja riistanhoitopiireissä vireillä olevat asiat. Suomen riistakeskukselle siirtyvät Metsästäjäin keskusjärjestöä ja riistanhoitopiirejä koskevat oikeudet, sopimukset, sitoumukset ja velvoitteet."}}},{"@id":"sfsd:2011/158/luku/4/pykala/37/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2011/158/luku/4/pykala/37/momentti/2","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/pykala/37/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2011/158/luku/4/pykala/37/momentti/2/alkup/fin/txt","content_fi":"Tämän lain voimaan tullessa voimassaolevat riistanhoitopiirien antamat luvat, hyväksynnät, kiellot, rajoitukset ja päätökset jäävät voimaan niissä mainituilla ehdoilla."}}}],"isMemberOf":"sfsd:2011/158/luku/4/pykala/37","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/pykala/37/alkup/fin","title_fi":"Tehtäviä, oikeuksia ja velvoitteita koskevat siirtymäsäännökset"}},{"@id":"sfsd:2011/158/luku/4/pykala/38/alkup","@type":"sfl:SectionVersion","hasPart":{"@id":"sfsd:2011/158/luku/4/pykala/38/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2011/158/luku/4/pykala/38/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/pykala/38/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2011/158/luku/4/pykala/38/momentti/1/alkup/fin/txt","content_fi":"Tämän lain voimaan tullessa toimivat riistanhoitoyhdistykset jatkavat toimintaansa toiminta-alueillaan. Suomen riistakeskuksen tulee viimeistään 31 joulukuuta 2013 vahvistaa riistanhoitoyhdistysten toiminta-alueet 11 §:n 1 momentin mukaisiksi asianomaisen riistanhoitoyhdistyksen tai asianomaisten riistanhoitoyhdistysten esityksestä."}}},"isMemberOf":"sfsd:2011/158/luku/4/pykala/38","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/pykala/38/alkup/fin","title_fi":"Riistanhoitoyhdistyksiä koskevat siirtymäsäännökset"}},{"@id":"sfsd:2011/158/luku/4/pykala/39/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2011/158/luku/4/pykala/39/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2011/158/luku/4/pykala/39/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/pykala/39/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2011/158/luku/4/pykala/39/momentti/1/alkup/fin/txt","content_fi":"Metsästäjäin keskusjärjestön ja riistanhoitopiirien työsopimussuhteinen henkilöstö ja tehtävät siirtyvät tämän lain voimaan tullessa Suomen riistakeskukseen työsopimuslain (55/2001) 1 luvun 10 §:ssä liikkeen luovutuksesta säädettyjen periaatteiden mukaisesti. Siirtyvä henkilöstö säilyttää siirtymähetkellä palvelussuhteeseen liittyvät oikeudet ja velvollisuudet sekä euromääräisen palkkansa."}}},{"@id":"sfsd:2011/158/luku/4/pykala/39/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2011/158/luku/4/pykala/39/momentti/2","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/pykala/39/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2011/158/luku/4/pykala/39/momentti/2/alkup/fin/txt","content_fi":"Suomen riistakeskuksella ei ole oikeutta irtisanoa työntekijän työsopimusta pelkästään työsopimuslain 1 luvun 10 §:ssä tarkoitetun liikkeen luovutuksen perusteella."}}},{"@id":"sfsd:2011/158/luku/4/pykala/39/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2011/158/luku/4/pykala/39/momentti/3","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/pykala/39/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:2011/158/luku/4/pykala/39/momentti/3/alkup/fin/txt","content_fi":"Suomen riistakeskuksen tulee sijoittaa henkilön tehtävä kolmen vuoden aikana tämän lain voimaantulosta samalle työttömyysturvalain (1290/2002) 1 luvun 9 §:ssä tarkoitetulle työssäkäyntialueelle, missä henkilö on hoitanut tehtäväänsä ennen tämän lain voimaantuloa, jollei muuta sovita. Suomen riistakeskuksen avoimeksi tulevat tehtävät tai perustettavat uudet toimet tai tehtävät, lukuun ottamatta Suomen riistakeskuksen johtajan ja julkisten hallintotehtävien päällikön tehtäviä, voidaan täyttää haettavaksi julistamatta siirtyvällä henkilöstöllä, jos henkilö antaa toimen täyttöön suostumuksensa."}}},{"@id":"sfsd:2011/158/luku/4/pykala/39/momentti/4/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2011/158/luku/4/pykala/39/momentti/4","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/pykala/39/momentti/4/alkup/fin","hasFormat":{"@id":"sfsd:2011/158/luku/4/pykala/39/momentti/4/alkup/fin/txt","content_fi":"Uudistuksesta aiheutuvat mahdolliset toimipaikkamuutokset toteutetaan kolmen vuoden siirtymäajan jälkeen, jollei työntekijä anna tätä ennen kirjallista suostumusta muutoksiin. Toimipaikkaa voidaan kuitenkin muuttaa työnantajan päätöksellä työssäkäyntialueella."}}}],"isMemberOf":"sfsd:2011/158/luku/4/pykala/39","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/pykala/39/alkup/fin","title_fi":"Henkilöstöä koskevat siirtymäsäännökset"}},{"@id":"sfsd:2011/158/luku/4/pykala/40/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2011/158/luku/4/pykala/40/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2011/158/luku/4/pykala/40/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/pykala/40/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2011/158/luku/4/pykala/40/momentti/1/alkup/fin/txt","content_fi":"Metsästäjäin keskusjärjestön hallitus toimii Suomen riistakeskuksen hallituksena, kunnes tämän lain mukainen hallitus on nimetty. Metsästäjäin keskusjärjestön toiminnanjohtaja toimii Suomen riistakeskuksen johtajana ja apulaistoiminnanjohtaja julkisten hallintotehtävien päällikkönä, kunnes johtaja ja julkisten hallintotehtävien päällikkö on tämän lain nojalla nimitetty. Suomen riistakeskuksen johtaja ja julkisten hallintotehtävien päällikkö on nimitettävä viimeistään 30 syyskuuta 2011."}}},{"@id":"sfsd:2011/158/luku/4/pykala/40/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2011/158/luku/4/pykala/40/momentti/2","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/pykala/40/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2011/158/luku/4/pykala/40/momentti/2/alkup/fin/txt","content_fi":"Riistanhoitopiirien toimistot ja riistanhoitopiirien toiminta-alueet muodostavat Suomen riistakeskuksen aluetoimistot ja toiminta-alueet, kunnes Suomen riistakeskuksen työjärjestys on tämän lain nojalla vahvistettu. Työjärjestys on vahvistettava viimeistään 31 joulukuuta 2011."}}},{"@id":"sfsd:2011/158/luku/4/pykala/40/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2011/158/luku/4/pykala/40/momentti/3","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/pykala/40/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:2011/158/luku/4/pykala/40/momentti/3/alkup/fin/txt","content_fi":"Riistanhoitopiirien metsästyslain 9 luvun mukaiset kokoukset toimivat 31 päivään maaliskuuta 2011 asti Suomen riistakeskuksen aluekokouksina. Kokouksissa tehdään esitykset sekä alueelliseen riistaneuvostoon että valtakunnalliseen riistaneuvostoon nimitettävistä riistanhoitomaksun suorittaneista jäsenistä. Maa- ja metsätalousministeriö nimittää sekä alueelliset riistaneuvostot että valtakunnallisen riistaneuvoston viimeistään 30 huhtikuuta 2011. Suomen riistakeskuksen hallitus nimitetään viimeistään 31 toukokuuta 2011."}}},{"@id":"sfsd:2011/158/luku/4/pykala/40/momentti/4/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2011/158/luku/4/pykala/40/momentti/4","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/pykala/40/momentti/4/alkup/fin","hasFormat":{"@id":"sfsd:2011/158/luku/4/pykala/40/momentti/4/alkup/fin/txt","content_fi":"Edellä 17 §:n 2 momentissa tarkoitetun riistanhoitoyhdistyksen toimialueen merkityksellisten maanomistajajärjestöjen jäsenen riistanhoitoyhdistyksen hallitukseen valitsee 15 huhtikuuta 2011 mennessä Suomen riistakeskus kuultuaan riistanhoitoyhdistyksiä sekä näiden toimialueiden merkityksellisiä maanomistajajärjestöjä. Näin valittu edustaja toimii hallituksessa vuoden 2012 riistanhoitoyhdistyksen vuosikokoukseen asti."}}}],"isMemberOf":"sfsd:2011/158/luku/4/pykala/40","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/pykala/40/alkup/fin","title_fi":"Suomen riistakeskuksen perustamista koskevat siirtymäsäännökset"}},{"@id":"sfsd:2011/158/luku/4/pykala/41/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2011/158/luku/4/pykala/41/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2011/158/luku/4/pykala/41/momentti/1","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/pykala/41/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2011/158/luku/4/pykala/41/momentti/1/alkup/fin/txt","content_fi":"Metsästyksenvalvojan peruskoulutusta vastaavan aikaisemman koulutuksen suorittanut voidaan 21 §:n 1 momentin 2 kohdan estämättä nimittää metsästyksenvalvojaksi uudelleen hänen hakiessaan metsästyksenvalvojaksi nimittämistä kuuden kuukauden kuluessa aikaisemman metsästyksenvalvojaksi nimittämisen päättymisestä. Vastaavasti tämän lain voimaan tullessa metsästäjätutkinnon ja ampumakokeen vastaanottajina toimivat voivat hakea nimittämistä uudelleen. Nimittämisen edellytyksenä on tällöinkin, että hakija on suorittanut hyväksytysti säädetyn kertauskoulutuksen."}}},{"@id":"sfsd:2011/158/luku/4/pykala/41/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2011/158/luku/4/pykala/41/momentti/2","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/pykala/41/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2011/158/luku/4/pykala/41/momentti/2/alkup/fin/txt","content_fi":"Metsästyksenvalvojaksi nimittämisen johdosta aikaisempi voimassa oleva metsästyksenvalvojaksi hyväksyminen raukeaa."}}}],"isMemberOf":"sfsd:2011/158/luku/4/pykala/41","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/pykala/41/alkup/fin","title_fi":"Metsästyksenvalvojia sekä ampumakokeen ja metsästäjätutkinnon vastaanottajia koskevat siirtymäsäännökset"}}],"isMemberOf":"sfsd:2011/158/luku/4","version":"sfl:Original","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/luku/4/alkup/fin","title_fi":"Voimaantulo"}}],"isAbout":"http://data.finlex.fi/voc/finlex/kw5878","isMemberOf":"sfsd:2011/158","relatedTo":"http://data.finlex.fi/eli/he/2010/237","version":"sfl:Consolidated","versionDate":"2018-01-01","languageVersion":{"@id":"sfsd:2011/158/ajantasa/20180101/fin","title_fi":"Riistahallintolaki"}},"@context":{"languageVersion":{"@id":"eli:is_realized_by","@type":"@id"},"temporalVersions":{"@id":"eli:has_member","@type":"@