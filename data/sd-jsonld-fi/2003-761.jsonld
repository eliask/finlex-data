ion":"sfl:Original","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2003/761/luku/7/pykala/45/momentti/1/kohta/5/alkup/fin","hasFormat":{"@id":"sfsd:2003/761/luku/7/pykala/45/momentti/1/kohta/5/alkup/fin/txt","content_fi":"5) muihin poliisin henkilörekistereihin sisältyviin henkilöä tai tekoa koskeviin luokitus-, tarkkailu- tai tekotapatietoihin."}}},{"@id":"sfsd:2003/761/luku/7/pykala/45/momentti/1/kohta/6/alkup","@type":"sfl:ParagraphVersion","isMemberOf":"sfsd:2003/761/luku/7/pykala/45/momentti/1/kohta/6","version":"sfl:Original","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2003/761/luku/7/pykala/45/momentti/1/kohta/6/alkup/fin","hasFormat":{"@id":"sfsd:2003/761/luku/7/pykala/45/momentti/1/kohta/6/alkup/fin/txt","content_fi":"6) 2 §:n 3 momentin 11 kohdassa tarkoitettuihin havaintotietoihin."}}}],"isMemberOf":"sfsd:2003/761/luku/7/pykala/45/momentti/1","version":"sfl:Consolidated","versionDate":"2018-07-01"},{"@id":"sfsd:2003/761/luku/7/pykala/45/momentti/2/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/761/luku/7/pykala/45/momentti/2","version":"sfl:Original","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2003/761/luku/7/pykala/45/momentti/2/alkup/fin","hasFormat":{"@id":"sfsd:2003/761/luku/7/pykala/45/momentti/2/alkup/fin/txt","content_fi":"Tietosuojavaltuutettu voi rekisteröidyn pyynnöstä tarkastaa 1 momentissa tarkoitettujen, rekisteröityä koskevien tietojen lainmukaisuuden."}}},{"@id":"sfsd:2003/761/luku/7/pykala/45/momentti/3/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/761/luku/7/pykala/45/momentti/3","version":"sfl:Original","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2003/761/luku/7/pykala/45/momentti/3/alkup/fin","hasFormat":{"@id":"sfsd:2003/761/luku/7/pykala/45/momentti/3/alkup/fin/txt","content_fi":"Tarkastusoikeuden rajoituksista on lisäksi voimassa, mitä henkilötietolain 27 §:ssä säädetään."}}}],"isMemberOf":"sfsd:2003/761/luku/7/pykala/45","version":"sfl:Consolidated","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2003/761/luku/7/pykala/45/ajantasa/20140101/fin","title_fi":"Tarkastusoikeuden rajoittaminen"}},{"@id":"sfsd:2003/761/luku/7/pykala/46/ajantasa/20140101","@type":"sfl:SectionVersion","isMemberOf":"sfsd:2003/761/luku/7/pykala/46","repealedBy":"sfsd:2013/1181/alkup","version":"sfl:Consolidated","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2003/761/luku/7/pykala/46/ajantasa/20140101/fin","hasFormat":{"@id":"sfsd:2003/761/luku/7/pykala/46/ajantasa/20140101/fin/txt","content_fi":"46 §(30.12.2013/1181)\n\n46 § on kumottu L:lla 30.12.2013/1181."}}},{"@id":"sfsd:2003/761/luku/7/pykala/47/ajantasa/20140101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2013/1181/alkup","hasPart":[{"@id":"sfsd:2003/761/luku/7/pykala/47/momentti/1/ajantasa/20140101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/761/luku/7/pykala/47/momentti/1","version":"sfl:Consolidated","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2003/761/luku/7/pykala/47/momentti/1/ajantasa/20140101/fin","hasFormat":{"@id":"sfsd:2003/761/luku/7/pykala/47/momentti/1/ajantasa/20140101/fin/txt","content_fi":"Jokaisella on oikeus pyytää Schengenin yleissopimuksen 115 artiklassa tarkoitettua valvontaviranomaista varmistamaan, että häntä koskevien henkilötietojen kerääminen, tallettaminen, käsittely ja käyttö Schengenin tietojärjestelmän teknisen tuen yksikön ylläpitämässä tiedostossa tapahtuu lainmukaisesti ja oikein. Tätä tarkoittava pyyntö on esitettävä tietosuojavaltuutetulle tai poliisilaitokselle. Poliisilaitokselle esitetty varmistuspyyntö on poliisin viipymättä toimitettava tietosuojavaltuutetulle."}}},{"@id":"sfsd:2003/761/luku/7/pykala/47/momentti/2/ajantasa/20140101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/761/luku/7/pykala/47/momentti/2","version":"sfl:Consolidated","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2003/761/luku/7/pykala/47/momentti/2/ajantasa/20140101/fin","hasFormat":{"@id":"sfsd:2003/761/luku/7/pykala/47/momentti/2/ajantasa/20140101/fin/txt","content_fi":"Edellä 1 momentissa tarkoitettu poliisilaitokselle esitettävä varmistuspyyntö on esitettävä henkilökohtaisesti poliisilaitokselle, ja pyynnön esittäjän on samalla todistettava henkilöllisyytensä."}}}],"isMemberOf":"sfsd:2003/761/luku/7/pykala/47","version":"sfl:Consolidated","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2003/761/luku/7/pykala/47/ajantasa/20140101/fin","title_fi":"Tarkastusoikeuden toteuttaminen Schengenin tietojärjestelmän teknisen tuen yksikön ylläpitämän tiedoston tietoihin"}}],"isMemberOf":"sfsd:2003/761/luku/7","version":"sfl:Consolidated","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2003/761/luku/7/ajantasa/20140101/fin","title_fi":"Rekisteröidyn oikeudet"}},{"@id":"sfsd:2003/761/luku/8/ajantasa/20150324","@type":"sfl:ChapterVersion","hasPart":[{"@id":"sfsd:2003/761/luku/8/pykala/47a/ajantasa/20150324","@type":"sfl:SectionVersion","amendedBy":"sfsd:2015/230/alkup","hasPart":{"@id":"sfsd:2003/761/luku/8/pykala/47a/momentti/1/ajantasa/20150324","@type":"sfl:SubsectionVersion","cites":"sfsd:2015/227/pykala/4","isMemberOf":"sfsd:2003/761/luku/8/pykala/47a/momentti/1","version":"sfl:Consolidated","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2003/761/luku/8/pykala/47a/momentti/1/ajantasa/20150324/fin","hasFormat":{"@id":"sfsd:2003/761/luku/8/pykala/47a/momentti/1/ajantasa/20150324/fin/txt","content_fi":"Mitä tässä laissa säädetään lähestymiskiellosta, sovelletaan suojelutoimenpiteeseen, joka on ilmoitettu poliisiasiain tietojärjestelmään yksityisoikeuden alalla määrättyjen suojelutoimenpiteiden vastavuoroisesta tunnustamisesta annetun Euroopan parlamentin ja neuvoston asetuksen soveltamisesta annetun lain (227/2015) 4 §:n nojalla."}}},"isMemberOf":"sfsd:2003/761/luku/8/pykala/47a","version":"sfl:Consolidated","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2003/761/luku/8/pykala/47a/ajantasa/20150324/fin","title_fi":"Yksityisoikeuden alalla määrätyt suojelutoimenpiteet"}},{"@id":"sfsd:2003/761/luku/8/pykala/48/ajantasa/20100101","@type":"sfl:SectionVersion","amendedBy":"sfsd:2009/1424/alkup","hasPart":[{"@id":"sfsd:2003/761/luku/8/pykala/48/momentti/1/ajantasa/20100101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/761/luku/8/pykala/48/momentti/1","version":"sfl:Consolidated","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2003/761/luku/8/pykala/48/momentti/1/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:2003/761/luku/8/pykala/48/momentti/1/ajantasa/20100101/fin/txt","content_fi":"Tarkempia säännöksiä menettelytavoista tämän lain 19 §:ssä tarkoitetussa teknisen käyttöyhteyden antamisessa, 20 §:ssä tarkoitetussa tietojen luovuttamisesta päättämisessä ja 44 §:ssä tarkoitetussa tarkastusoikeuden käyttämisessä voidaan antaa valtioneuvoston asetuksella."}}},{"@id":"sfsd:2003/761/luku/8/pykala/48/momentti/2/ajantasa/20100101","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/761/luku/8/pykala/48/momentti/2","version":"sfl:Consolidated","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2003/761/luku/8/pykala/48/momentti/2/ajantasa/20100101/fin","hasFormat":{"@id":"sfsd:2003/761/luku/8/pykala/48/momentti/2/ajantasa/20100101/fin/txt","content_fi":"Poliisihallitus vahvistaa poliisille kuuluvissa toimenpiteissä käytettävien lomakkeiden kaavat."}}}],"isMemberOf":"sfsd:2003/761/luku/8/pykala/48","version":"sfl:Consolidated","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2003/761/luku/8/pykala/48/ajantasa/20100101/fin","title_fi":"Tarkemmat säännökset ja ohjeet"}},{"@id":"sfsd:2003/761/luku/8/pykala/49/alkup","@type":"sfl:SectionVersion","hasPart":[{"@id":"sfsd:2003/761/luku/8/pykala/49/momentti/1/alkup","@type":"sfl:SubsectionVersion","isMemberOf":"sfsd:2003/761/luku/8/pykala/49/momentti/1","version":"sfl:Original","versionDate":"2018-07-01","languageVersion":{"@id":"sfsd:2003/761/luku/8/pykala/49/momentti/1/alkup/fin","hasFormat":{"@id":"sfsd:2003/761/