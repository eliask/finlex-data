    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>om ändring av 1 § lagen om bankinspektionen </met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>RP 267/92</sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
