                  <tau:tbody>
                    <tau:row>
                      <tau:entry>
                        <sis:KappaleKooste>Testperiod: 3 000 h, testtemperatur: 110 °C: medium: destillerat vatten</sis:KappaleKooste>
                      </tau:entry>
                      <tau:entry>
                        <sis:KappaleKooste>Krav</sis:KappaleKooste>
                      </tau:entry>
