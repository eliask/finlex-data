              <tau:colspec COLNUM="2" COLNAME="2" COLWIDTH=""/>
              <tau:tbody VALIGN="TOP">
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>
                      <sis1:LihavaTeksti>Höjning i procent</sis1:LihavaTeksti>
                    </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>
                      <sis1:LihavaTeksti> Kommuner</sis1:LihavaTeksti>
                    </sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>10</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>Helsingfors, Esbo, Vanda, Grankulla, Träskända, Kervo, Kyrkslätt, Sibbo, Tusby, Enontekis, Enare, Muonio och Utsjoki </sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>5</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>Hyvinge, Tavastehus, Lahtis, Lojo, Nurmijärvi, Borgå, Riihimäki, Vichtis, Dragsfjärd, Houtskär, Iniö, Korpo, Gustavs, Nagu, Velkua, Karlö, Kemijärvi, Kittilä, Kolari, Pelkosenniemi, Posio, Salla, Savukoski och Sodankylä.</sis:KappaleKooste>
                  </tau:entry>
