    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>om ändring av lagen om sjömanspensioner</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>RP 279/2014</sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
        <sis1:ViiteTeksti>ShUB 46/2014</sis1:ViiteTeksti>
      </met:Muu2Viite>
      <met:Muu3Viite>
