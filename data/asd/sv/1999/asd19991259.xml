    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>om ändring av lagen om stämningsmän</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>RP 111/1999</sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
        <sis1:ViiteTeksti>LaUB 8/1999</sis1:ViiteTeksti>
      </met:Muu2Viite>
      <met:Muu3Viite>
        <sis1:ViiteTeksti>RSv 92/1999</sis1:ViiteTeksti>
      </met:Muu3Viite>
    </met:AsiakirjaViitteet>
