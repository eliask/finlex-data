    <met:Nimeke>
      <met1:NimekeTeksti>om ändring av 15 § lagen om djursjukdomar</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>RP 49/1999</sis1:ViiteTeksti>
      </met:Muu1Viite>
