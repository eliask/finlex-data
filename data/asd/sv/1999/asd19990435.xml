    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>om ändring av kommunallagen</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>RP 114/1998</sis1:ViiteTeksti>
      </met:Muu1Viite>
