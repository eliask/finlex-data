  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="sv" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Kyrkomötets beslut</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>om ändring av kyrkoordningen</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>I enlighet med kyrkomötets beslut</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>ändras</saa:SaadosKursiiviKooste> i kyrkoordningen av den 8 november 1991 (1055/1993) 2 kap. 12 § 2 mom. som följer:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Luku saa1:identifiointiTunnus="2 kap.">
          <saa:LukuTunnusKooste>2 kap.</saa:LukuTunnusKooste>
          <saa:SaadosOtsikkoKooste>Kyrkans heliga handlingar</saa:SaadosOtsikkoKooste>
          <saa:SaadosValiotsikkoKooste>B. Nattvarden</saa:SaadosValiotsikkoKooste>
          <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="12 §">
            <saa:PykalaTunnusKooste>12 §</saa:PykalaTunnusKooste>
            <sis:Katkoviiva/>
            <saa:MomenttiKooste>Kyrkoherden kan bevilja en sådan medlem av evangelisk-lutherska kyrkan som är konfirmerad och känd för sin kristna övertygelse rätt att biträda vid nattvardens utdelning.</saa:MomenttiKooste>
            <sis:Katkoviiva/>
          </saa:Pykala>
        </saa:Luku>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Detta beslut träder i kraft den 1 januari 2000.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
