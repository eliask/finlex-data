  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>67</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1988</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>om inrättande och indragning av vissa tjänster vid justitiekanslersämbetet samt inom justitieministeriets förvaltningsområde</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="sv" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Förordning</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>om inrättande och indragning av vissa tjänster vid justitiekanslersämbetet samt inom justitieministeriets förvaltningsområde</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>På föredragning av justitieministern stadgas med stöd av 6 § 1 mom. och 10 § 1 mom. statstjänstemannalagen av den 24 oktober 1986 (755/86):</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Sedan riksdagen beviljat medel för ändamålet inrättas och indras nedan nämnda tjänster med grundlön.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Justitiekanslersämbetet</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Inrättas den 1 mars 1988:</saa:MomenttiKooste>
          <saa:MomenttiKooste>en föredragandetjänst i löneklass A 21.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Inrättas den 1 september 1988:</saa:MomenttiKooste>
          <saa:MomenttiKooste>en tjänst som konsultativ tjänsteman i löneklass A 30.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Justitieministeriet</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Inrättas den 1 mars 1988:</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Allmänna avdelningen</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:MomenttiKooste>tre byråfunktionärstjänster i löneklass A 8,</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Justitieförvaltningsavdelningen</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:MomenttiKooste>en överinspektörstjänst i löneklass A 24 och</saa:MomenttiKooste>
          <saa:MomenttiKooste>en kamrerstjänst i löneklass A 18,</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Lagberedningsavdelningen</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:MomenttiKooste>en lagstiftningsrådstjänst i löneklass A 28,</saa:MomenttiKooste>
          <saa:MomenttiKooste>sex lagstiftningsrådstjänster i löneklass A 27,</saa:MomenttiKooste>
          <saa:MomenttiKooste>tio lagstiftningsrådstjänster i löneklass A 26 och</saa:MomenttiKooste>
          <saa:MomenttiKooste>två lagstiftningsrådstjänster i löneklass A 25,</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Fångvårdsavdelningen</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:MomenttiKooste>en byråsekreterartjänst i löneklass A 12 samt</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Räkenskapsbyrån</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:MomenttiKooste>en överinspektörstjänst i löneklass A 23.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Inrättas den 1 juni 1988:</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Justitieförvaltningsavdelningen</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:MomenttiKooste>fyra byråsekreterartjänster i löneklass A 13.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Inrättas den 1 september 1988:</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Justitieförvaltningsavdelningen</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:MomenttiKooste>en utbildningsplanerartjänst i löneklass A 21,</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Lagberedningsavdelningen</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:MomenttiKooste>en lagstiftningsrådstjänst i löneklass A 26 samt</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Fångvårdsavdelningen</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:MomenttiKooste>en avdelningssekreterartjänst i löneklass A 16.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Indras den 1 mars 1988:</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Justitieförvaltningsavdelningen</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:MomenttiKooste>en föredragandetjänst i löneklass A 21.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>De högsta domstolarna</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Inrättas den 1 mars 1988:</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Högsta förvaltningsdomstolen</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:MomenttiKooste>en tjänst som administrativ sekreterare i löneklass A 24,</saa:MomenttiKooste>
          <saa:MomenttiKooste>en yngre förvaltningssekreterartjänst i löneklass A 23,</saa:MomenttiKooste>
          <saa:MomenttiKooste>en övervaktmästartjänst i löneklass A 12 och</saa:MomenttiKooste>
          <saa:MomenttiKooste>två byråsekreterartjänster i löneklass A 11.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Indras den 1 mars 1988:</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Högsta förvaltningsdomstolen</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:MomenttiKooste>en föredragandetjänst i löneklass A 21.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="5 §">
          <saa:PykalaTunnusKooste>5 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Hovrätterna</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Inrättas den 1 mars 1988:</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Åbo hovrätt</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:MomenttiKooste>tre fiskalstjänster i löneklass A 22,</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Vasa hovrätt</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:MomenttiKooste>två fiskalstjänster i löneklass A 22 och</saa:MomenttiKooste>
          <saa:MomenttiKooste>en byråfunktionärstjänst i löneklass A 5,</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Östra Finlands hovrätt</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:MomenttiKooste>två fiskalstjänster i löneklass A 22 samt</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Helsingfors hovrätt</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:MomenttiKooste>tre fiskalstjänster i löneklass A 22,</saa:MomenttiKooste>
          <saa:MomenttiKooste>en bibliotekarietjänst i löneklass A 21 och</saa:MomenttiKooste>
          <saa:MomenttiKooste>två byråfunktionärstjänster i löneklass A 5.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Indras den 1 mars 1988:</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Åbo hovrätt</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:MomenttiKooste>tre extraordinarie fiskalstjänster i löneklass A 19,</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Vasa hovrätt</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:MomenttiKooste>två extraordinarie fiskalstjänster i löneklass A 19,</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Östra Finlands hovrätt</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:MomenttiKooste>två extraordinarie fiskalstjänster i löneklass A 19 samt</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Helsingfors hovrätt</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:MomenttiKooste>tre extraordinarie fiskalstjänster i löneklass A 19.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="6 §">
          <saa:PykalaTunnusKooste>6 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Domsagorna</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Inrättas den 1 november 1988:</saa:MomenttiKooste>
          <saa:MomenttiKooste>en tingsdomartjänst i löneklass A 25 och</saa:MomenttiKooste>
          <saa:MomenttiKooste>en tingsdomartjänst i löneklass A 24.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="7 §">
          <saa:PykalaTunnusKooste>7 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Rådstuvurätterna och magistraterna</saa:SaadosOtsikkoKooste>
