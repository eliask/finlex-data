                  <tau:entry>
                    <sis:KappaleKooste><saa:SaadosKursiiviKooste>P</saa:SaadosKursiiviKooste> ≤ 70 kW 	</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>106 </sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>70 kW &lt; <saa:SaadosKursiiviKooste>P</saa:SaadosKursiiviKooste> ≤ 160 kW 	</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>108 </sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>160 kW &lt; <saa:SaadosKursiiviKooste>P</saa:SaadosKursiiviKooste> ≤ 350 kW </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                </tau:row>
                <tau:row>
