  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="sv" saa1:saadostyyppiNimi="Laki">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Lag</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>om ändring av vattenlagen </saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>I enlighet med riksdagens beslut</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>fogas</saa:SaadosKursiiviKooste> till 1 kap. vattenlagen av den 19 maj 1961 (264/61) en ny 21 a § som följer: </saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Luku saa1:identifiointiTunnus="1 kap.">
          <saa:LukuTunnusKooste>1 kap. </saa:LukuTunnusKooste>
          <saa:SaadosOtsikkoKooste>Allmänna stadganden </saa:SaadosOtsikkoKooste>
          <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="">
            <saa:PykalaTunnusKooste/>
            <saa:SaadosOtsikkoKooste>Allmänna inskränkningar i nyttjande </saa:SaadosOtsikkoKooste>
          </saa:Pykala>
          <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="21 a §">
            <saa:PykalaTunnusKooste>21 a § </saa:PykalaTunnusKooste>
            <saa:MomenttiKooste>Tillförseln av nitrater från jordbruket i vattnen begränsas så som bestäms i beslut av statsrådet.</saa:MomenttiKooste>
            <saa:MomenttiKooste>Med stöd av denna paragraf kan endast utfärdas sådana bestämmelser som gäller utsläpp som avses i Europeiska gemenskapens råds direktiv om skydd mot att vatten förorenas av nitrater från jordbruket (91/676/EEG).</saa:MomenttiKooste>
          </saa:Pykala>
        </saa:Luku>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Denna lag träder i kraft den 1 juli 1995. </saa:MomenttiKooste>
        </saa:Pykala>
