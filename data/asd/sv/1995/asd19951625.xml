                    <sis:KappaleKooste>210 000 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>fyrsiffrigt riktnummer 	</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
