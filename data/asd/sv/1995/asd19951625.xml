                    <sis:KappaleKooste>fyrsiffrigt fjärrprefix 	</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>58 500 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>tresiffrigt utlandsprefix 	</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>585 000 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>fyrsiffrigt utlandsprefix 	</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
