    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>om ändring av 6 a § lagen om marknadsdomstolen</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>Regeringens proposition 9/86</sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
