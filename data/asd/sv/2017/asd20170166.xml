    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>om ändring av lagen om penninginsamlingar</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>RP 264/2016</sis1:ViiteTeksti>
      </met:Muu1Viite>
