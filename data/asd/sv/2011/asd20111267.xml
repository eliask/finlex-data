                  <tau:entry>
                    <sis:KappaleKooste>Statsskatt</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>27,19 %</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
