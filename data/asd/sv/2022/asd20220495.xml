  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="sv" saa1:saadostyyppiNimi="Laki">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Lag</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>om ändring av 6 § i lagen om Enheten för utredning av grå ekonomi</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>I enlighet med riksdagens beslut</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>fogas</saa:SaadosKursiiviKooste> till 6 § 1 mom. i lagen om Enheten för utredning av grå ekonomi (1207/2010), sådant det lyder i lagarna 308/2016, 858/2016, 1159/2016, 1413/2016, 1419/2016, 324/2017, 454/2017, 1112/2017, 404/2018, 414/2018, 722/2019, 1399/2019, 624/2020, 1264/2020, 488/2021, 690/2021, 763/2021 och 1134/2021, en ny 14 a-punkt som följer:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="6 §">
          <saa:PykalaTunnusKooste>6 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Syftet med fullgöranderapporter</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Fullgöranderapporter utarbetas till stöd för</saa:MomenttiJohdantoKooste>
            <sis:Katkoviiva/>
            <saa:MomenttiKohtaKooste>14 a) utredning av verksamhetsutövarens tillförlitlighet enligt 95 § 2 mom. i avfallslagen (646/2011),</saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Denna lag träder i kraft den 1 januari 2023.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
