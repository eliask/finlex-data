        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="19 §">
          <saa:PykalaTunnusKooste>19 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Beloppet av och förskott på stöd för växthusproduktion</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Stöd för växthusproduktion beviljas högst till följande belopp:</saa:MomenttiKooste>
          <tau:table frame="all" colsep="1" rowsep="1">
            <tau:tgroup cols="2" colsep="1" rowsep="1">
              <tau:colspec colnum="1" colname="1" colwidth="50*"/>
              <tau:colspec colnum="2" colname="2" colwidth="50*"/>
              <tau:tbody>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Odlingssäsong</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>euro/m²</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Kort: minst 2 mån. men högst 7 mån.</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>6,6</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Lång: över 7 mån.</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>17,9</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
              </tau:tbody>
            </tau:tgroup>
          </tau:table>
