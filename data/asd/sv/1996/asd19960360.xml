  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>360</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1996 </asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>om ändring av handels- och industriministeriets beslut om processhjälpmedel i livsmedel </met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>Europaparlamentets och rådets direktiv 94/52/EEG, EGT nr L 331, 21.12.1994, s. 10 </sis1:ViiteTeksti>
      </met:Muu1Viite>
    </met:AsiakirjaViitteet>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="sv" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Handels- och industriministeriets beslut</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>om ändring av handels- och industriministeriets beslut om processhjälpmedel i livsmedel </saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Handels- och industriministeriet har fogat till punkt III i bilagan till sitt beslut av den 20 december 1993 om processhjälpmedel i livsmedel (1303/93) en ny punkt, cyklohexan, som följer: </saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <sis:Katkoviiva/>
        <te>III. Extraheringslösningsmedel som får användas på följande villkor: </te>
        <tau:table>
          <tau:tgroup cols="2" colsep="0" rowsep="0" align="left">
            <tau:colspec colnum="1" colname="1" colwidth=""/>
            <tau:colspec colnum="2" colname="2" colwidth=""/>
            <tau:tbody valign="top">
              <tau:row>
                <tau:entry>
                  <sis:KappaleKooste>Extraheringslösningsmedel</sis:KappaleKooste>
                </tau:entry>
                <tau:entry>
                  <sis:KappaleKooste>Extraheringslösningsmedlets maximimängd i ett livsmedel i vilket det har råkat med sådana aromer som har framställts ur naturliga aromkällor med användning av extraheringslösningsmedel </sis:KappaleKooste>
                </tau:entry>
              </tau:row>
              <tau:row>
                <tau:entry>
                  <sis:KappaleKooste>Cyklohexan</sis:KappaleKooste>
                </tau:entry>
                <tau:entry>
                  <sis:KappaleKooste>1 mg/kg</sis:KappaleKooste>
                </tau:entry>
              </tau:row>
            </tau:tbody>
          </tau:tgroup>
        </tau:table>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Detta beslut träder i kraft den 1 juni 1996. </saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
  <asi:AllekirjoitusOsa>
