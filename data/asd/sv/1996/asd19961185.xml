                  <tau:entry>
                    <sis:KappaleKooste>10 400 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>minst sexsiffrigt nationellt servicenummer </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>2 100 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>5) avgift för datanätskod (DNIC) </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>67 500 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
              </tau:tbody>
            </tau:tgroup>
          </tau:table>
