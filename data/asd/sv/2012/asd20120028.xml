  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>28</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>2012</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>om ändring av 22 och 25 kap. i strafflagen</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
