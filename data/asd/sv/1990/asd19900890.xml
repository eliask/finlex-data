  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>890</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1990</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>om lyxtelegramblanketter</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="sv" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Trafikministeriets beslut</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>om lyxtelegramblanketter</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Trafikministeriet har med stöd av 16 § teleförordningen den 3 april 1987 (347/87) beslutat:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>De samfund till vilkas förmån lyxtelegramblanketter kan användas är</saa:MomenttiKooste>
          <saa:MomenttiKooste>Mannerheims Barnskyddsförbund r.f.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Hjärtsjukdomsförbundet i Finland r.f.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Cancerföreningen i Finland r.f.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Avgiften för de lyxmappar som används i telegramtrafik får fördelas på följande sätt:</saa:MomenttiKooste>
          <tau:table>
            <tau:tgroup cols="2" colsep="0" rowsep="0" align="left">
              <tau:colspec colnum="1" colname="1" colwidth=""/>
              <tau:colspec colnum="2" colname="2" colwidth=""/>
              <tau:tbody valign="top">
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Post- och televerket</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>30,5 %</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Mannerheims Barnskyddsförbund r.f.</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>31   %</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Hjärtsjukdomsförbundet i Finland r.f.</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>28   %</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Cancerföreningen i Finland r.f.</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>10,5 %</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
              </tau:tbody>
            </tau:tgroup>
          </tau:table>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Avgiften för de lyxblanketter som används i telegramtrafik får fördelas på följande sätt:</saa:MomenttiKooste>
          <tau:table>
            <tau:tgroup cols="2" colsep="0" rowsep="0" align="left">
              <tau:colspec colnum="1" colname="1" colwidth=""/>
              <tau:colspec colnum="2" colname="2" colwidth=""/>
              <tau:tbody valign="top">
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Post- och televerket</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>46,5 %</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Mannerheims Barnskyddsförbund r.f.</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>24   %</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Hjärtsjukdomsförbundet i Finland r.f.</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>21,5 %</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Cancerföreningen i Finland r.f.</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>8   %</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
              </tau:tbody>
            </tau:tgroup>
          </tau:table>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Detta beslut träder i kraft den 1 oktober 1990. Genom detta beslut upphävs trafikministeriets beslut den 1 december 1990 om lyxtelegramblanketter (2168/01/89).</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
