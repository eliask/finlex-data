                    <sis:KappaleKooste>750 </sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>b) yrkesundervisningsanstalter 	</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>1 580 </sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
