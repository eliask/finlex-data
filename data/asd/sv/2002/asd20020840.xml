                  <tau:entry>
                    <sis:KappaleKooste>553,89 </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>1 375,74</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
