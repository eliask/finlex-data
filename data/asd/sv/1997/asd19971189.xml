              <tau:tbody valign="top">
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Region</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>mk/kg</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Yttre skärgården, regionerna A och B </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>0,52</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Åland, utom yttre skärgården </sis:KappaleKooste>
