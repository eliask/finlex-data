    <met:Nimeke>
      <met1:NimekeTeksti>om ändring av 6 och 25 § konkursstadgan</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>RP 216/2001</sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
