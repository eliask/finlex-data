    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>om ändring av 6 § lagen om säkerhetskontroller inom flygtrafiken</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>RP 44/2002</sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
        <sis1:ViiteTeksti>LaUB 28/2002</sis1:ViiteTeksti>
      </met:Muu2Viite>
      <met:Muu3Viite>
