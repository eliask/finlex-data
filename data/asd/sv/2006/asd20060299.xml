    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>om ändring av lagen om medicinsk behandling av djur</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>RP 203/2005</sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
        <sis1:ViiteTeksti>JsUB 2/2006</sis1:ViiteTeksti>
      </met:Muu2Viite>
      <met:Muu3Viite>
