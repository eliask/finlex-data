  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="sv" saa1:saadostyyppiNimi="Laki">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Lag</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>om ändring av 2 § i handelskammarlagen</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>I enlighet med riksdagens beslut</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>fogas</saa:SaadosKursiiviKooste> till 2 § i handelskammarlagen av den 1 november 2002 (878/2002) ett nytt 3 mom. som följer:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Handelskamrarnas och Centralhandelskammarens offentliga uppgifter</saa:SaadosOtsikkoKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Centralhandelskammaren har dessutom till uppgift att sköta de uppgifter som den har ålagts i denna lag och i värdepappersmarknadslagen (495/1989).</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Denna lag träder i kraft den 1 juli 2006.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
