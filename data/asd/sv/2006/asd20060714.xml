                  <tau:entry>
                    <sis:KappaleKooste>utdrag ur handelsregistret</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>10 €</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>registreringsintyg</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>15 €</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
