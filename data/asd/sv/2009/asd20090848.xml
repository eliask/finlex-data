    <met:Nimeke>
      <met1:NimekeTeksti>om ändring av 30 och 37 § i lagen om militär krishantering</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>RP 96/2009</sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
        <sis1:ViiteTeksti>FsUB 2/2009</sis1:ViiteTeksti>
      </met:Muu2Viite>
      <met:Muu3Viite>
