  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>9</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>2013</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>om energiformsfaktorerna för byggnader</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>Europaparlamentets och rådets direktiv 2010/31/EU (32010L0031); EGT nr L 153, 18.6.2010, s. 13</sis1:ViiteTeksti>
      </met:Muu1Viite>
    </met:AsiakirjaViitteet>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="sv" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Statsrådets förordning</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>om energiformsfaktorerna för byggnader</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>I enlighet med statsrådets beslut föreskrivs med stöd av 117 g § 4 mom. i markanvändnings- och bygglagen (132/1999), sådant det lyder i lag 958/2012:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Energiformsfaktorerna för byggnader är</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>1)  1,7 för el,</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>2)  0,7 för fjärrvärme</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>3)  0,4 för fjärrkyla</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>4)  1,0 för fossila bränslen, samt</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>5)  0,5 för förnybara bränslen som används i en byggnad.</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Denna förordning träder i kraft den 15 januari 2013.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
