<?xml version="1.0" encoding="utf-8"?>
<vah:VahvistettavaLaki xmlns:vah="http://www.vn.fi/skeemat/vahvistettavalaki/2010/04/27" xmlns:sdk="http://www.finlex.fi/skeemat/edita/sdk/2010/04/27" xmlns:saa="http://www.vn.fi/skeemat/saadoskooste/2010/04/27" xmlns:saa1="http://www.vn.fi/skeemat/saadoselementit/2010/04/27" xmlns:met="http://www.vn.fi/skeemat/metatietokooste/2010/04/27" xmlns:met1="http://www.vn.fi/skeemat/metatietoelementit/2010/04/27" xmlns:asi="http://www.vn.fi/skeemat/asiakirjakooste/2010/04/27" xmlns:asi1="http://www.vn.fi/skeemat/asiakirjaelementit/2010/04/27" xmlns:org="http://www.vn.fi/skeemat/organisaatiokooste/2010/02/15" xmlns:org1="http://www.vn.fi/skeemat/organisaatioelementit/2010/02/15" xmlns:tau="http://www.vn.fi/skeemat/taulukkokooste/2010/04/27" xmlns:sis="http://www.vn.fi/skeemat/sisaltokooste/2010/04/27" xmlns:sis1="http://www.vn.fi/skeemat/sisaltoelementit/2010/04/27" met1:kieliKoodi="sv" met1:julkisuusKoodi="julkinen" met1:tilaKoodi="Valmis" met1:identifiointiTunnus="0000000000000000" met1:versioTeksti="1.0" met1:asiakirjatyyppiNimi="Vahvistettava laki" met1:laadintaPvm="2100-01-01" met1:rakennemaarittelyNimi="vahvistettavalaki.xsd">
  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>604</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1987</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>om inrättande av vissa tjänster inom justitieministeriets förvaltningsområde</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="sv" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Förordning</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>om inrättande av vissa tjänster inom justitieministeriets förvaltningsområde</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>På föredragning av justitieministern stadgas:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Sedan riksdagen beviljat erforderliga medel för ändamålet inrättas nedan nämnda tjänster med grundlön eller avtalslön.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Specialdomstolarna</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Inrättas räknat från den 1 december 1987:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>Vattenöverdomstolen</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
tre tjänster som sekreterare för vattenärenden i avlöningsklassen A 21.
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Datasekretess</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Inrättas räknat från den 1 oktober 1987:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>Dataombudsmannens byrå</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
en dataombudsmannatjänst i avtalslöneklassen S 28,
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
en byråchefstjänst i avlöningsklassen A 25 och
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
en byråsekreterartjänst i avlöningsklassen A 12.
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Denna förordning träder i kraft den 1 juli 1987.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Åtgärder för besättande av de genom denna förordning inrättade tjänsterna får vidtas redan före den tidpunkt räknat från vilken tjänsten inrättas.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
  <asi:AllekirjoitusOsa>
    <asi:PaivaysKooste>Nådendal den 26 juni 1987</asi:PaivaysKooste>
    <asi:Allekirjoittaja>
      <org:Henkilo>
        <org1:AsemaTeksti>Republikens President </org1:AsemaTeksti>
        <org1:SukuNimi>Mauno Koivisto</org1:SukuNimi>
      </org:Henkilo>
    </asi:Allekirjoittaja>
    <asi:Allekirjoittaja>
      <org:Henkilo>
        <org1:AsemaTeksti>Justitieminister </org1:AsemaTeksti>
        <org1:SukuNimi>Matti Louekoski</org1:SukuNimi>
      </org:Henkilo>
    </asi:Allekirjoittaja>
  </asi:AllekirjoitusOsa>
</vah:VahvistettavaLaki>
