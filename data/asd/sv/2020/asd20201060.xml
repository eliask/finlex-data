      <met1:NimekeTeksti>om ändring av 3 § i fastighetsskattelagen</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>RP 169/2020</sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
