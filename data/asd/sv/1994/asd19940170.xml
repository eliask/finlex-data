              <tau:entry>
                <sis:KappaleKooste>100 mk </sis:KappaleKooste>
              </tau:entry>
            </tau:row>
            <tau:row>
              <tau:entry>
                <sis:KappaleKooste>4) beslut om övervakningslicens </sis:KappaleKooste>
              </tau:entry>
              <tau:entry>
                <sis:KappaleKooste>50 mk </sis:KappaleKooste>
