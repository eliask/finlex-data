    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>om ändring av 2 § lagen om företagssanering</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>RP 180/2001</sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
        <sis1:ViiteTeksti>EkUB 20/2001</sis1:ViiteTeksti>
      </met:Muu2Viite>
      <met:Muu3Viite>
