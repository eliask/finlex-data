    <asi:Allekirjoittaja>
      <org:Henkilo>
        <org1:AsemaTeksti>Yngre regeringssekreterare </org1:AsemaTeksti>
        <org1:SukuNimi>Anne Haikonen</org1:SukuNimi>
      </org:Henkilo>
    </asi:Allekirjoittaja>
  </asi:AllekirjoitusOsa>
