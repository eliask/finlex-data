                  <tau:entry>
                    <sis:KappaleKooste> 0― 6-åringar</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>24 192 mk</sis:KappaleKooste>
                  </tau:entry>
