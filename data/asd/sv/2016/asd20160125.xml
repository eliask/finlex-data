                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>Bockningsprov: böjning minst 180°</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>Återbockningsprov: böjning minst 90˚, åldring och böjningtillbaka minst 20 ˚</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
