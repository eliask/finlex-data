  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>656</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1989</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>om ändring av 2 och 4 §§ lagen om bränsleaccis</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>Regeringens proposition 68/89</sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
        <sis1:ViiteTeksti>Statsutsk. bet. 23/89</sis1:ViiteTeksti>
      </met:Muu2Viite>
      <met:Muu3Viite>
        <sis1:ViiteTeksti>Stora utsk. bet. 72/89</sis1:ViiteTeksti>
      </met:Muu3Viite>
    </met:AsiakirjaViitteet>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="sv" saa1:saadostyyppiNimi="Laki">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Lag</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>om ändring av 2 och 4 §§ lagen om bränsleaccis</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>I enlighet med riksdagens beslut</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>ändras</saa:SaadosKursiiviKooste> i lagen den 17 december 1982 om bränsleaccis (948/82) 4 § 1 mom., sådant det lyder i lag av den 29 december 1988 (1286/88), samt</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>fogas</saa:SaadosKursiiviKooste> till 2 §, sådan den lyder delvis ändrad genom lag av den 11 juli 1986 (548/86), en ny 5 punkt som följer:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>I denna lag avses</saa:MomenttiJohdantoKooste>
            <sis:Katkoviiva/>
            <saa:MomenttiKohtaKooste>
5) med blandning av motorbensin en blandning som innehåller minst hälften blyfri motorbensin.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>I bränsleaccis skall betalas</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1) för blyfri motorbensin 95,39 penni per liter,
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
2) för blandning av motorbensin 109,10 penni per liter,
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
3) för annan motorbensin 122,81 penni per liter samt
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
4) för dieselolja 84,49 penni per liter.

</saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Denna lag träder i kraft den 1 augusti 1989.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
