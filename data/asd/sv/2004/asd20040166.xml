  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="sv" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Statsrådets förordning</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>om ändring av statsrådets förordning om arbetsmarknadsregioner</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>I enlighet med statsrådets beslut, fattat på föredragning från arbetsministeriet,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>ändras</saa:SaadosKursiiviKooste> i förordningen av den 30 december 2002 om arbetsmarknadsregioner (1350/2002) 3, 8, 12 och 13 § som följer:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Egentliga Finlands arbetskrafts- och näringscentral</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Kimito:</saa:SaadosKursiiviKooste> Kimito, Dragsfjärd, Västanfjärd</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Loimaa:</saa:SaadosKursiiviKooste> Loimaa, Alastaro, Aura, Karinainen, Koski, Loimaa kommun, Marttila, Mellilä, Oripää, Pöytyä, Tarvasjoki, Yläne</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Pargas:</saa:SaadosKursiiviKooste> Pargas, Houtskär, Iniö, Korpo, Nagu</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Salo:</saa:SaadosKursiiviKooste> Salo, Halikko, Kiikala, Kisko, Kuusjoki, Muurla, Bjärnå, Pertteli, Somero, Suomusjärvi, Finby</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Åbo:</saa:SaadosKursiiviKooste> Åbo, Villnäs, S:t Karins, Lemu, Lundo, Masku, Merimasku, Nådendal, Nousis, Pemar, Pikis, Reso, Rusko, Rimito, Sagu, Vahto, Velkua</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Nystadsregionen:</saa:SaadosKursiiviKooste> Gustavs, Laitila, Mietoinen, Mynämäki, Pyhäranta, Tövsala, Nystad, Vehmaa</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="8 §">
          <saa:PykalaTunnusKooste>8 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Södra Savolax arbetskrafts- och näringscentral</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Heinävesi:</saa:SaadosKursiiviKooste> Heinävesi</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Jorois:</saa:SaadosKursiiviKooste> Jorois</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Juva:</saa:SaadosKursiiviKooste> Juva</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Kangasniemi:</saa:SaadosKursiiviKooste> Kangasniemi</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>S:t Michel:</saa:SaadosKursiiviKooste> S:t Michel, Hirvensalmi, Ristiina</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Mäntyharju:</saa:SaadosKursiiviKooste> Mäntyharju, Pertunmaa</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Pieksämäki:</saa:SaadosKursiiviKooste> Pieksämäki, Haukivuori, Pieksänmaa</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Puumala:</saa:SaadosKursiiviKooste> Puumala</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Rantasalmi:</saa:SaadosKursiiviKooste> Rantasalmi</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Nyslott:</saa:SaadosKursiiviKooste> Nyslott, Enonkoski, Kerimäki, Punkaharju, Savonranta, Sulkava</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="12 §">
          <saa:PykalaTunnusKooste>12 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Södra Österbottens arbetskrafts- och näringscentral</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Härmänmaa:</saa:SaadosKursiiviKooste> Alahärmä, Kauhava, Lappo, Ylihärmä</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Järviseutu:</saa:SaadosKursiiviKooste> Alajärvi, Evijärvi, Kortesjärvi, Lappajärvi, Vimpeli</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Kuusiokunnat:</saa:SaadosKursiiviKooste> Alavus, Kuortane, Lehtimäki, Soini, Töysä, Etseri</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Seinäjokiregionen:</saa:SaadosKursiiviKooste> Seinäjoki, Ilmajoki, Jalasjärvi, Kurikka, Nurmo, Peräseinäjoki, Ylistaro</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Sydösterbotten:</saa:SaadosKursiiviKooste> Storå, Jurva, Bötom, Kauhajoki, Östermark</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="13 §">
          <saa:PykalaTunnusKooste>13 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Österbottens arbetskrafts- och näringscentral</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Halso―Lestijärvi―Perho:</saa:SaadosKursiiviKooste> Halso, Lestijärvi, Perho</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Karleby:</saa:SaadosKursiiviKooste> Karleby, Himanka, Kannus, Kaustby, Kronoby, Kelviå, Lochteå, Toholampi, Ullava, Vetil</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Sydösterbottens kusttrakt:</saa:SaadosKursiiviKooste> Kaskö, Korsnäs, Kristinestad, Närpes</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Jakobstad:</saa:SaadosKursiiviKooste> Jakobstad, Larsmo, Pedersöre kommun, Nykarleby</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Vasa: </saa:SaadosKursiiviKooste>Vasa, Storkyro, Laihela, Malax, Maxmo, Korsholm, Oravais, Lillkyro, Vörå</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Denna förordning träder i kraft den 1 april 2004.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
