    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>om ändring av arkivlagen</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>RP 120/2003</sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
