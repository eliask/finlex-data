          <saa:SaadosOtsikkoKooste>Övriga prestationer</saa:SaadosOtsikkoKooste>
          <tau:table>
            <tau:tgroup COLS="2" COLSEP="0" ROWSEP="0" ALIGN="left" SECURITY="u">
              <tau:colspec COLNUM="2" COLNAME="2" COLWIDTH=""/>
              <tau:tbody VALIGN="TOP">
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste NAMEST="1" NAMEEND="2">Prestationer enligt förordningen om offentligt kontrollerade vattenforskningsinstitut (325/1962)</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>- behandling av en ansökan om godkännande som vattenforskningsinstitut </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>1 100</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
