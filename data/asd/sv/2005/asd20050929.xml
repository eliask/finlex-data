    <met:Nimeke>
      <met1:NimekeTeksti>om ändring av riksdagens räkenskapsstadga</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>TKF 1/2005</sis1:ViiteTeksti>
      </met:Muu1Viite>
