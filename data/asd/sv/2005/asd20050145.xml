  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>145</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>2005</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>om ändring av befolkningsdataförordningen</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="sv" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Statsrådets förordning</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>om ändring av befolkningsdataförordningen</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>I enlighet med statsrådets beslut, fattat på föredragning från inrikesministeriet,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>fogas</saa:SaadosKursiiviKooste> till befolkningsdataförordningen av den 22 oktober 1993 (886/1993) en ny 5 d § som följer:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Luku saa1:identifiointiTunnus="1 a kap.">
          <saa:LukuTunnusKooste>1 a kap.</saa:LukuTunnusKooste>
          <saa:SaadosOtsikkoKooste>Anmälning och registrering av uppgifter om födelser och dödsfall</saa:SaadosOtsikkoKooste>
          <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="5 d §">
            <saa:PykalaTunnusKooste>5 d §</saa:PykalaTunnusKooste>
            <saa:SaadosOtsikkoKooste>Registrering av uppgifter om dödförklaring</saa:SaadosOtsikkoKooste>
            <saa:MomenttiKooste>När magistraten har fattat beslut om dödförklaring skall den utan dröjsmål registrera uppgiften om beslutet i befolkningsdatasystemet.</saa:MomenttiKooste>
          </saa:Pykala>
        </saa:Luku>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Denna förordning träder i kraft den 15 mars 2005.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
  <asi:AllekirjoitusOsa>
