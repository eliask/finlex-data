    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>om ändring av lagen om ädelmetallarbeten</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>RP 236/2004</sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
