  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>756</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1998</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>om fördelning av riksdagsmandaten mellan valkretsarna</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="sv" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Statsrådets beslut</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>om fördelning av riksdagsmandaten mellan valkretsarna</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Statsrådet har vid föredragning från justitieministeriet med stöd av 6 § vallagen av den 2 oktober 1998 (714/1998) beslutat:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Vid de riksdagsval som förrättas den 21 mars 1999 skall i valkretsarna väljas följande antal riksdagsledamöter:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
I Helsingfors valkrets 20
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
I Nylands valkrets 32
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
I Egentliga Finlands valkrets 17
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
I Satakunta valkrets 10
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
I Tavastlands valkrets 13
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
I Birkalands valkrets 16
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
I Kymmene valkrets 13
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
I S:t Michels valkrets 8
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
I Kuopio valkrets 10
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
I Norra Karelens valkrets 7
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
I Vasa valkrets 17
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
I Mellersta Finlands valkrets 10
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
I Uleåborgs valkrets 18
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
I Lapplands valkrets 8
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>I landskapet Ålands valkrets väljs enligt vallagen (714/1998) en riksdagsledamot.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Detta beslut träder i kraft den 1 november 1998.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
