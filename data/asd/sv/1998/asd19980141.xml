          <tau:colspec colnum="1" colname="1" colwidth=""/>
          <tau:colspec colnum="2" colname="2" colwidth=""/>
          <tau:tbody valign="top">
            <tau:row>
              <tau:entry>
                <sis:KappaleKooste>1. Intyg</sis:KappaleKooste>
              </tau:entry>
              <tau:entry>
                <sis:KappaleKooste/>
              </tau:entry>
            </tau:row>
            <tau:row>
              <tau:entry>
                <sis:KappaleKooste>när en eller två handlingar beträffande en arkivbildare används </sis:KappaleKooste>
              </tau:entry>
              <tau:entry>
                <sis:KappaleKooste>85 mk/sida</sis:KappaleKooste>
              </tau:entry>
            </tau:row>
            <tau:row>
              <tau:entry>
                <sis:KappaleKooste>när fler än två handlingar beträffande en arkivbildare används </sis:KappaleKooste>
              </tau:entry>
              <tau:entry>
                <sis:KappaleKooste>100 mk/sida</sis:KappaleKooste>
              </tau:entry>
            </tau:row>
            <tau:row>
              <tau:entry>
                <sis:KappaleKooste>när flera arkivbildares handlingar används </sis:KappaleKooste>
              </tau:entry>
              <tau:entry>
                <sis:KappaleKooste>130 mk/sida</sis:KappaleKooste>
              </tau:entry>
            </tau:row>
            <tau:row>
