  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>796</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>2007</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>om ändring av kyrkoordningen</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="sv" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Kyrkomötets beslut</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>om ändring av kyrkoordningen</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>I enlighet med kyrkomötets beslut</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>ändras </saa:SaadosKursiiviKooste>i kyrkoordningen av den 8 november 1991 (1055/1993) 19 kap. 5 § 2 mom., sådant det lyder i kyrkomötets beslut 1275/2003, som följer:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Luku saa1:identifiointiTunnus="19 kap.">
          <saa:LukuTunnusKooste>19 kap.</saa:LukuTunnusKooste>
          <saa:SaadosOtsikkoKooste>Stiftets övriga förvaltning</saa:SaadosOtsikkoKooste>
          <saa:SaadosValiotsikkoKooste>A. Domkapitlet</saa:SaadosValiotsikkoKooste>
          <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="5 §">
            <saa:PykalaTunnusKooste>5 §</saa:PykalaTunnusKooste>
            <sis:Katkoviiva/>
            <saa:MomenttiKooste>Ordföranden för stiftsfullmäktige eller, om denna har förhinder, vice ordföranden har närvaro- och yttranderätt vid domkapitlets sammanträden, men inte rätt att delta i beslutsfattandet.</saa:MomenttiKooste>
          </saa:Pykala>
        </saa:Luku>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Detta beslut träder i kraft den 1 augusti 2007.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
  <asi:PaivaysKooste>Åbo den 11 maj 2007</asi:PaivaysKooste>
  <asi1:JohdantoTeksti>För kyrkomötet</asi1:JohdantoTeksti>
