  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>ajoneuvoasetuksen 49 §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Liikenneministerin esittelystä muutetaan 26 päivänä maaliskuuta 1982 annetun ajoneuvoasetuksen (233/82) 49§:n 5 momentti näin kuuluvaksi:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:SaadosValiotsikkoKooste>Henkilökuljetus</saa:SaadosValiotsikkoKooste>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="49 §">
          <saa:PykalaTunnusKooste>49 § </saa:PykalaTunnusKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>5.Ministeriö voi määrätä millä tavalla ja millä ehdoilla 1-3 momentin säännöksistä voidaan poiketa koulu- ja päivähoitokuljetuksissa sekä 4 momentista poiketen voidaan sallia henkilöiden kuljetus auton tavaratilassa.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 18 päivänä toukokuuta 1988.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
