  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>humanistisista tutkinnoista annetun asetuksen 6 §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Helsingin yliopiston konsistorin annettua Helsingin yliopiston osalta lausuntonsa opetusministerin esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan </saa:SaadosKursiiviKooste>humanistisista tutkinnoista 22 päivänä joulukuuta 1978 annetun asetuksen (1080/78) 6 §:n 1 momentti näin kuuluvaksi:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="6 §">
          <saa:PykalaTunnusKooste>6 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Koulutusohjelma suunnitellaan ja järjestetään siten, että sen laajuus on 160 opintoviikkoa tai, jos on kysymys aineenopettajien koulutuksesta tai logopedian koulutusohjelmasta, 180 opintoviikkoa.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä elokuuta 1988.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
