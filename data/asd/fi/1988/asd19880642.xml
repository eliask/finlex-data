<vah:VahvistettavaLaki xmlns:vah="http://www.vn.fi/skeemat/vahvistettavalaki/2010/04/27" xmlns:sdk="http://www.finlex.fi/skeemat/edita/sdk/2010/04/27" xmlns:saa="http://www.vn.fi/skeemat/saadoskooste/2010/04/27" xmlns:saa1="http://www.vn.fi/skeemat/saadoselementit/2010/04/27" xmlns:met="http://www.vn.fi/skeemat/metatietokooste/2010/04/27" xmlns:met1="http://www.vn.fi/skeemat/metatietoelementit/2010/04/27" xmlns:asi="http://www.vn.fi/skeemat/asiakirjakooste/2010/04/27" xmlns:asi1="http://www.vn.fi/skeemat/asiakirjaelementit/2010/04/27" xmlns:org="http://www.vn.fi/skeemat/organisaatiokooste/2010/02/15" xmlns:org1="http://www.vn.fi/skeemat/organisaatioelementit/2010/02/15" xmlns:tau="http://www.vn.fi/skeemat/taulukkokooste/2010/04/27" xmlns:sis="http://www.vn.fi/skeemat/sisaltokooste/2010/04/27" xmlns:sis1="http://www.vn.fi/skeemat/sisaltoelementit/2010/04/27" met1:kieliKoodi="fi" met1:julkisuusKoodi="julkinen" met1:tilaKoodi="Valmis" met1:identifiointiTunnus="0000000000000000" met1:versioTeksti="1.0" met1:asiakirjatyyppiNimi="Vahvistettava laki" met1:laadintaPvm="2100-01-01" met1:rakennemaarittelyNimi="vahvistettavalaki.xsd">
  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>642</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1988</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>humanistisen perustutkinnon järjestämisestä annetun asetuksen muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>humanistisen perustutkinnon järjestämisestä annetun asetuksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Helsingin yliopiston konsistorin annettua Helsingin yliopiston osalta lausuntonsa opetusministerin esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> humanistisen perustutkinnon järjestämisestä 21 päivänä syyskuuta 1979 annetun asetuksen (741/79) 1§, 3§ ja 6§,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste>näistä 1 ja 6§ sellaisina kuin ne ovat 29 päivänä huhtikuuta 1981 annetussa asetuksessa (311/81), näin kuuluviksi:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Helsingin yliopiston filosofisen tiedekunnan historiallis-kielitieteellisessä osastossa johtavat filosofian kandidaatin tutkintoon seuraavat koulutusohjelmat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1) suomen kielen ja sen sukukielten koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
2) ruotsin kielen ja kirjallisuuden koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
3) vieraiden kielten koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
4) klassillisen filologian koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
5) kääntämisen ja tulkinnan koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
6) historian koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
7) kulttuurien tutkimuksen koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
8) taiteiden tutkimuksen koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
9) filosofian koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
10) logopedian koulutusohjelma; sekä
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
11) kieliteorian ja kognitiotieteen koulutusohjelma.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Jyväskylän yliopiston humanistisessa tiedekunnassa johtavat filosofian kandidaatin tutkintoon seuraavat koulutusohjelmat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1) suomen kielen ja sen sukukielten koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
2) vieraiden kielten koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
3) klassillisten kielten koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
4) historian koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
5) taiteiden tutkimuksen koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
6) kulttuurien tutkimuksen koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
7) musiikkikasvatuksen ja musiikkiterapian koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
8) soveltavan kielitieteen ja puheentutkimuksen koulutusohjelma; sekä
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
9) journalistiikan koulutusohjelma.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="6 §">
          <saa:PykalaTunnusKooste>6 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Turun yliopiston humanistisessa tiedekunnassa johtavat filosofian kandidaatin tutkintoon seuraavat koulutusohjelmat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1) suomen kielen ja sen sukukielten koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
2) vieraiden kielten koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
3) klassillisten kielten koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
4) kääntämisen ja tulkinnan koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
5) historian koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
6) kulttuurien tutkimuksen koulutusohjelma;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
7) taiteentutkimuksen ja kulttuurihistorian koulutusohjelma; sekä
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
8) uskonnontutkimuksen koulutusohjelma.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:MomenttiKooste>Turun yliopiston humanistisessa tiedekunnassa on koulutusohjelmiin kuulumattomana tieteenalana filosofia.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä elokuuta 1988.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Ennen tämän asetuksen voimaantuloa voidaan ryhtyä asetuksen täytäntöönpanon edellyttämiin toimenpiteisiin.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
