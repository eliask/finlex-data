<vah:VahvistettavaLaki xmlns:vah="http://www.vn.fi/skeemat/vahvistettavalaki/2010/04/27" xmlns:sdk="http://www.finlex.fi/skeemat/edita/sdk/2010/04/27" xmlns:saa="http://www.vn.fi/skeemat/saadoskooste/2010/04/27" xmlns:saa1="http://www.vn.fi/skeemat/saadoselementit/2010/04/27" xmlns:met="http://www.vn.fi/skeemat/metatietokooste/2010/04/27" xmlns:met1="http://www.vn.fi/skeemat/metatietoelementit/2010/04/27" xmlns:asi="http://www.vn.fi/skeemat/asiakirjakooste/2010/04/27" xmlns:asi1="http://www.vn.fi/skeemat/asiakirjaelementit/2010/04/27" xmlns:org="http://www.vn.fi/skeemat/organisaatiokooste/2010/02/15" xmlns:org1="http://www.vn.fi/skeemat/organisaatioelementit/2010/02/15" xmlns:tau="http://www.vn.fi/skeemat/taulukkokooste/2010/04/27" xmlns:sis="http://www.vn.fi/skeemat/sisaltokooste/2010/04/27" xmlns:sis1="http://www.vn.fi/skeemat/sisaltoelementit/2010/04/27" met1:kieliKoodi="fi" met1:julkisuusKoodi="julkinen" met1:tilaKoodi="Valmis" met1:identifiointiTunnus="0000000000000000" met1:versioTeksti="1.0" met1:asiakirjatyyppiNimi="Vahvistettava laki" met1:laadintaPvm="2100-01-01" met1:rakennemaarittelyNimi="vahvistettavalaki.xsd">
  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>758</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1988</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>valtiovarainministeriön hallinnonalaan kuuluvien eräiden virkojen lakkauttamisesta ja perustamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>valtiovarainministeriön hallinnonalaan kuuluvien eräiden virkojen lakkauttamisesta ja perustamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtiovarainministeriön toimialaan kuuluvia asioita käsittelemään määrätyn ministerin esittelystä säädetään 24 päivänä lokakuuta 1986 annetun valtion virkamieslain (755/86) 6 §:n nojalla:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Valtiokonttori</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Lakkautetaan:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
yksi A 5 palkkausluokkaan kuuluva konekirjoittajan virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
neljä A 5 palkkausluokkaan kuuluvaa toimistovirkailijan virkaa; ja
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 3 palkkausluokkaan kuuluva toimistovirkailijan virka,
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Perustetaan:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
yksi A 25 palkkausluokkaan kuuluva toimistopäällikön virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 16 palkkausluokkaan kuuluva osastosihteerin virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 14 palkkausluokkaan kuuluva toimistosihteerin virka; ja
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 12 palkkausluokkaan kuuluva laskentasihteerin virka.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Verohallinto</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Lakkautetaan:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>
                <saa:SaadosKursiiviKooste>Läänin vero virastot</saa:SaadosKursiiviKooste>
              </saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 5 palkkausluokkaan kuuluva toimistoapulaisen virka; ja
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 4 palkkausluokkaan kuuluva toimistoapulaisen virka.
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>
                <saa:SaadosKursiiviKooste>Verotoimistot</saa:SaadosKursiiviKooste>
              </saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
kolme A 4 palkkausluokkaan kuuluvaa toimistoapulaisen virkaa.
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Perustetaan:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>
                <saa:SaadosKursiiviKooste>Lääninverovirastot</saa:SaadosKursiiviKooste>
              </saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 22 palkkausluokkaan kuuluva esittelijän virka.
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>
                <saa:SaadosKursiiviKooste>Verotoimistot</saa:SaadosKursiiviKooste>
              </saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
kaksi A 18 palkkausluokkaan kuuluvaa verosihteerin virkaa.
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä marraskuuta 1988.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
  <asi:AllekirjoitusOsa>
    <asi:PaivaysKooste>Helsingissä 26 päivänä elokuuta 1988</asi:PaivaysKooste>
    <asi:Allekirjoittaja>
      <org:Henkilo>
        <org1:AsemaTeksti>Tasavallan Presidentti </org1:AsemaTeksti>
        <org1:SukuNimi>MAUNO KOIVISTO</org1:SukuNimi>
      </org:Henkilo>
    </asi:Allekirjoittaja>
    <asi:Allekirjoittaja>
      <org:Henkilo>
        <org1:AsemaTeksti>Ministeri </org1:AsemaTeksti>
        <org1:SukuNimi>Ulla Puolanne</org1:SukuNimi>
      </org:Henkilo>
    </asi:Allekirjoittaja>
  </asi:AllekirjoitusOsa>
</vah:VahvistettavaLaki>
