  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>159</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1988</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>passintarkastuspaikoista</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Sisäasiainministeriön päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>passintarkastuspaikoista</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Sisäasiainministeriö on 26 päivänä huhtikuuta 1983 annetun ulkomaalaislain (400/83) 3 §:n nojalla päättänyt:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Ulkomaalaisen maahantulo ja maastalähtö on sallittu passintarkastuspaikoista annetussa asetuksessa (121/88) mainituissa passintarkastuspaikoissa.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Haettaessa lupaa maahantuloon tai maastalähtöön muualta kuin passintarkastuspaikan kautta on noudatettava, mitä passintarkastuspaikoista annetun asetuksen 3 §:ssä on säädetty.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä maaliskuuta 1988 ja sillä kumotaan passintarkastus- ja rajanylityspaikoista 2 päivänä toukokuuta 1984 annettu sisäasiainministeriön päätös (354/84).</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
