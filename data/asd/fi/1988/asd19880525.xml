  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>525</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1988</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>oikeusministeriön hallinnonalan eräiden virkojen perustamisesta ja lakkauttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>oikeusministeriön hallinnonalan eräiden virkojen perustamisesta ja lakkauttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Oikeusministerin esittelystä säädetään 24 päivänä lokakuuta 1986 annetun valtion virkamieslain (755/86) 6§:n 1 momentin ja 10§:n 1 momentin nojalla:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Eduskunnan myönnettyä tarkoitukseen tarvittavat varat perustetaan ja lakkautetaan jäljempänä mainitut peruspalkkaiset ja sopimuspalkkaiset virat.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Oikeusministeriö </saa:SaadosOtsikkoKooste>
          <ete>Perustetaan 1 päivästä syyskuuta 1988:</ete>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>
              <saa:SaadosKursiiviKooste>Oikeushallinto-osasto</saa:SaadosKursiiviKooste>
            </saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
yksi S 25 sopimuspalkkaluokkaan kuuluva ulosoton ylitarkastajan virka.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Ulosottovirastot </saa:SaadosOtsikkoKooste>
          <ete>Lakkautetaan 1 päivästä syyskuuta 1988:</ete>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>
              <saa:SaadosKursiiviKooste>Helsingin ulosottovirasto</saa:SaadosKursiiviKooste>
            </saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
yksi A 26 palkkausluokkaan kuuluva kaupunginvoudin virka.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Vankeinhoitolaitos </saa:SaadosOtsikkoKooste>
          <ete>Perustetaan 1 päivästä lokakuuta 1988:</ete>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>
              <saa:SaadosKursiiviKooste>Vankeinhoitolaitoksen yhteiset</saa:SaadosKursiiviKooste>
            </saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
yksi A 24 palkkausluokkaan kuuluva johtajan virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 21 palkkausluokkaan kuuluva sosiaalipäällikön virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 20 palkkausluokkaan kuuluva psykologin virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 19 palkkausluokkaan kuuluva apulaisjohtajan virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 18 palkkausluokkaan kuuluva talouspäällikön virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 16 palkkausluokkaan kuuluva notaarin virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 16 palkkausluokkaan kuuluva tilanhoitajan virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 16 palkkausluokkaan kuuluva vartiopäällikön virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 15 palkkausluokkaan kuuluva apulaiskamreerin virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 15 palkkausluokkaan kuuluva erikoissairaanhoitajan virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 15 palkkausluokkaan kuuluva sosiaaliohjaajan virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 15 palkkausluokkaan kuuluva talousmestarin virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 14 palkkausluokkaan kuuluva työnjohtajan virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 13 palkkausluokkaan kuuluva konemestarin virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 13 palkkausluokkaan kuuluva ylivartijan virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
kaksi A 12 palkkausluokkaan kuuluvaa keittiömestarin virkaa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
kaksi A 12 palkkausluokkaan kuuluvaa työmestarin virkaa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 10 palkkausluokkaan kuuluva toimistosihteerin virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
kaksi A 10 palkkausluokkaan kuuluvaa vanhemman vartijan virkaa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
kymmenen A 9 palkkausluokkaan kuuluvaa nuoremman vartijan virkaa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 8 palkkausluokkaan kuuluva toimistovirkailijan virka; ja
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
kolme A 5 palkkausluokkaan kuuluvaa keittäjän virkaa.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="5 §">
          <saa:PykalaTunnusKooste>5 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Tietosuoja </saa:SaadosOtsikkoKooste>
          <ete>Perustetaan 1 päivästä heinäkuuta 1988:</ete>
          <saa:MomenttiKooste>yksi A 18 palkkausluokkaan kuuluva apulaistarkastajan virka.</saa:MomenttiKooste>
          <ete>Lakkautetaan 1 päivästä heinäkuuta 1988:</ete>
          <saa:MomenttiKooste>yksi A 16 palkkausluokkaan kuuluva osastosihteerin virka.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="6 §">
          <saa:PykalaTunnusKooste>6 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä heinäkuuta 1988.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Toimenpiteisiin tällä asetuksella perustetun viran täyttämiseksi saadaan ryhtyä jo ennen sitä ajankohtaa, josta lukien virka on perustettu.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:identifiointiTunnus="7 §">
          <saa:PykalaTunnusKooste>7 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämän asetuksen 4 ja 5§:ssä perustetut virat voidaan ensimmäisellä kerralla täyttää niitä haettavaksi julistamatta.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Edellä 4§:ssä perustetut virat voidaan ensimmäisellä kerralla täyttää sen estämättä, mitä viran kelpoisuusvaatimuksista on säädetty.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
