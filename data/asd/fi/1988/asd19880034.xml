<?xml version="1.0" encoding="utf-8"?>
<vah:VahvistettavaLaki xmlns:vah="http://www.vn.fi/skeemat/vahvistettavalaki/2010/04/27" xmlns:sdk="http://www.finlex.fi/skeemat/edita/sdk/2010/04/27" xmlns:saa="http://www.vn.fi/skeemat/saadoskooste/2010/04/27" xmlns:saa1="http://www.vn.fi/skeemat/saadoselementit/2010/04/27" xmlns:met="http://www.vn.fi/skeemat/metatietokooste/2010/04/27" xmlns:met1="http://www.vn.fi/skeemat/metatietoelementit/2010/04/27" xmlns:asi="http://www.vn.fi/skeemat/asiakirjakooste/2010/04/27" xmlns:asi1="http://www.vn.fi/skeemat/asiakirjaelementit/2010/04/27" xmlns:org="http://www.vn.fi/skeemat/organisaatiokooste/2010/02/15" xmlns:org1="http://www.vn.fi/skeemat/organisaatioelementit/2010/02/15" xmlns:tau="http://www.vn.fi/skeemat/taulukkokooste/2010/04/27" xmlns:sis="http://www.vn.fi/skeemat/sisaltokooste/2010/04/27" xmlns:sis1="http://www.vn.fi/skeemat/sisaltoelementit/2010/04/27" met1:kieliKoodi="fi" met1:julkisuusKoodi="julkinen" met1:tilaKoodi="Valmis" met1:identifiointiTunnus="0000000000000000" met1:versioTeksti="1.0" met1:asiakirjatyyppiNimi="Vahvistettava laki" met1:laadintaPvm="2100-01-01" met1:rakennemaarittelyNimi="vahvistettavalaki.xsd">
  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>34</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1988</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>sisäasiainministeriön eräiden virkojen perustamisesta ja lakkauttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>sisäasiainministeriön eräiden virkojen perustamisesta ja lakkauttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Sisäasiainministerin esittelystä säädetään 24 päivänä lokakuuta 1986 annetun valtion virkamieslain (755/86) 6§:n 1 momentin ja 10§:n 1 momentin nojalla:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Eduskunnan myönnettyä tarkoitukseen tarvittavat varat perustetaan ja lakkautetaan sisäasiainministeriössä seuraavat sopimuspalkkaiset ja peruspalkkaiset virat:</saa:MomenttiJohdantoKooste>
            <ete>Perustetaan 1 päivästä helmikuuta 1988:</ete>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>
                <saa:SaadosKursiiviKooste>Poliisiosasto</saa:SaadosKursiiviKooste>
              </saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 21 palkkausluokkaan kuuluva esittelijän virka; ja
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 13 palkkausluokkaan kuuluva toimistosihteerin virka.
</saa:MomenttiAlakohtaKooste>
              <ete>Perustetaan 1 päivästä maaliskuuta 1988:</ete>
            </saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>
                <saa:SaadosKursiiviKooste>Yleinen osasto</saa:SaadosKursiiviKooste>
              </saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi S 27 sopimuspalkkaluokkaan kuuluva toimistopäällikön virka;
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>
                <saa:SaadosKursiiviKooste>Lääni- ja kuntaosasto</saa:SaadosKursiiviKooste>
              </saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 22 palkkausluokkaan kuuluva esittelijän virka;
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>
                <saa:SaadosKursiiviKooste>Aluepoliittinen osasto</saa:SaadosKursiiviKooste>
              </saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
kolme S 23 sopimuspalkkaluokkaan kuuluvaa erikoissuunnittelijan virkaa;
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
viisi S 21 sopimuspalkkaluokkaan kuuluvaa tutkijan virkaa;
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
kolme A 15 palkkausluokkaan kuuluvaa apulaistutkijan virkaa;
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 14 palkkausluokkaan kuuluva tutkimusapulaisen virka; ja
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 5 palkkausluokkaan kuuluva konekirjoittajan virka;
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>
                <saa:SaadosKursiiviKooste>Poliisiosasto</saa:SaadosKursiiviKooste>
              </saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
kaksi A 24 palkkausluokkaan kuuluvaa ylitarkastajan virkaa;
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 23 palkkausluokkaan kuuluva ylitarkastajan virka;
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 23 palkkausluokkaan kuuluva toimistoinsinöörin virka;
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
neljä A 22 palkkausluokkaan kuuluvaa erikoissuunnittelijan virkaa;
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 22 palkkausluokkaan kuuluva toimistoinsinöörin virka; ja
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 15 palkkausluokkaan kuuluva osastosihteerin virka.
</saa:MomenttiAlakohtaKooste>
              <ete>Perustetaan 1 päivästä syyskuuta 1988:</ete>
            </saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>
                <saa:SaadosKursiiviKooste>Poliisiosasto</saa:SaadosKursiiviKooste>
              </saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 24 palkkausluokkaan kuuluva ylitarkastajan virka.
</saa:MomenttiAlakohtaKooste>
              <ete>Lakkautetaan 1 päivästä maaliskuuta 1988:</ete>
            </saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>
                <saa:SaadosKursiiviKooste>Yleinen osasto</saa:SaadosKursiiviKooste>
              </saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 27 palkkausluokkaan kuuluva kamreerin, toimistopäällikkönä, virka.
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä helmikuuta 1988.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tällä asetuksella perustetut virat saadaan täyttää ensimmäisellä kerralla niitä haettaviksi julistamatta.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Toimenpiteisiin tällä asetuksella perustettujen virkojen täyttämiseksi saa ryhtyä jo ennen sitä ajankohtaa, josta lukien virka on perustettu.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
  <asi:AllekirjoitusOsa>
    <asi:PaivaysKooste>Helsingissä 15 päivänä tammikuuta 1988</asi:PaivaysKooste>
    <asi:Allekirjoittaja>
      <org:Henkilo>
        <org1:AsemaTeksti>Tasavallan Presidentti </org1:AsemaTeksti>
        <org1:SukuNimi>Mauno Koivisto</org1:SukuNimi>
      </org:Henkilo>
    </asi:Allekirjoittaja>
    <asi:Allekirjoittaja>
      <org:Henkilo>
        <org1:AsemaTeksti>Ministeri </org1:AsemaTeksti>
        <org1:SukuNimi>Erkki Liikanen  </org1:SukuNimi>
      </org:Henkilo>
    </asi:Allekirjoittaja>
  </asi:AllekirjoitusOsa>
</vah:VahvistettavaLaki>
