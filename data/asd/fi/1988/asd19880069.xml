<tau:table><tau:tgroup cols="3" colsep="0" rowsep="0" align="left"><tau:colspec colnum="1" colname="1" colwidth=""/><tau:colspec colnum="2" colname="2" colwidth=""/><tau:tbody valign="top"><tau:row><tau:entry><sis:KappaleKooste>Työnantajan sosiaaliturvamaksuprosentti</sis:KappaleKooste></tau:entry><tau:entry><sis:KappaleKooste>Kansaneläkemaksua %:a </sis:KappaleKooste></tau:entry><tau:entry><sis:KappaleKooste>Sairausvakuutusmaksua %:a</sis:KappaleKooste></tau:entry></tau:row><tau:row><tau:entry><sis:KappaleKooste>8,40 </sis:KappaleKooste></tau:entry><tau:entry><sis:KappaleKooste>52,976 </sis:KappaleKooste></tau:entry><tau:entry><sis:KappaleKooste>47,024</sis:KappaleKooste></tau:entry></tau:row></tau:tbody></tau:tgroup></tau:table>
  
