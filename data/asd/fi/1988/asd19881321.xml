<vah:VahvistettavaLaki xmlns:vah="http://www.vn.fi/skeemat/vahvistettavalaki/2010/04/27" xmlns:sdk="http://www.finlex.fi/skeemat/edita/sdk/2010/04/27" xmlns:saa="http://www.vn.fi/skeemat/saadoskooste/2010/04/27" xmlns:saa1="http://www.vn.fi/skeemat/saadoselementit/2010/04/27" xmlns:met="http://www.vn.fi/skeemat/metatietokooste/2010/04/27" xmlns:met1="http://www.vn.fi/skeemat/metatietoelementit/2010/04/27" xmlns:asi="http://www.vn.fi/skeemat/asiakirjakooste/2010/04/27" xmlns:asi1="http://www.vn.fi/skeemat/asiakirjaelementit/2010/04/27" xmlns:org="http://www.vn.fi/skeemat/organisaatiokooste/2010/02/15" xmlns:org1="http://www.vn.fi/skeemat/organisaatioelementit/2010/02/15" xmlns:tau="http://www.vn.fi/skeemat/taulukkokooste/2010/04/27" xmlns:sis="http://www.vn.fi/skeemat/sisaltokooste/2010/04/27" xmlns:sis1="http://www.vn.fi/skeemat/sisaltoelementit/2010/04/27" met1:kieliKoodi="fi" met1:julkisuusKoodi="julkinen" met1:tilaKoodi="Valmis" met1:identifiointiTunnus="0000000000000000" met1:versioTeksti="1.0" met1:asiakirjatyyppiNimi="Vahvistettava laki" met1:laadintaPvm="2100-01-01" met1:rakennemaarittelyNimi="vahvistettavalaki.xsd">
  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>1321</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1988</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>eräiden kauppa- ja teollisuusministeriön ja sen hallinnonalan virkojen perustamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>eräiden kauppa- ja teollisuusministeriön ja sen hallinnonalan virkojen perustamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Kauppa- ja teollisuusministerin esittelystä säädetään 24 päivänä lokakuuta 1986 annetun valtion virkamieslain (755/86) 6§:n 1 momentin ja 10§:n 1 momentin nojalla:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Eduskunnan myönnettyä tarkoitukseen tarvittavat varat perustetaan jäljempänä mainitut peruspalkkaiset virat.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Kauppa- ja teollisuusministeriö </saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Perustetaan:</saa:MomenttiJohdantoKooste>
            <ete>
              <saa:SaadosKursiiviKooste>Ministeriön yhteiset</saa:SaadosKursiiviKooste>
            </ete>
            <saa:MomenttiKohtaKooste>
yksi S 23 palkkausluokkaan kuuluva pääsihteerin virka.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Teollisuuspiirit </saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Perustetaan:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
yksi A 17 palkkausluokkaan kuuluva yritysneuvojan virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 7 palkkausluokkaan kuuluva toimistovirkailijan virka.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Merenkulkulaitos </saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Perustetaan:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
yksi A 7 palkkausluokkaan kuuluva väylänhoitajan virka.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="5 §">
          <saa:PykalaTunnusKooste>5 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Patentti- ja rekisterihallitus </saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Perustetaan:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
kolme A 23 palkkausluokkaan kuuluvaa jaostopäällikön virkaa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
kaksi A 16 palkkausluokkaan kuuluvaa toimistosihteerin virkaa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 14 palkkausluokkaan kuuluva toimistosihteerin virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 12 palkkausluokkaan kuuluva toimistosihteerin virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 11 palkkausluokkaan kuuluva toimistosihterin virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 10 palkkausluokkaan kuuluva toimistovirkailijan virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
viisi A 3 palkkausluokkaan kuuluvaa toimistovirkailijan virkaa.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="6 §">
          <saa:PykalaTunnusKooste>6 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä tammikuuta 1989.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Tällä asetuksella perustetut virat saadaan ensimmäisellä kerralla täyttää niitä haettavaksi julistamatta.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Toimenpiteisiin tällä asetuksella perustettujen virkojen täyttämiseksi saadaan ryhtyä ennen sitä ajankohtaa, josta lukien virka on perustettu.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
  <asi:AllekirjoitusOsa>
    <asi:PaivaysKooste>Helsingissä 29 päivänä joulukuuta 1988</asi:PaivaysKooste>
    <asi:Allekirjoittaja>
      <org:Henkilo>
        <org1:AsemaTeksti>Tasavallan Presidentti </org1:AsemaTeksti>
        <org1:SukuNimi>Mauno Koivisto</org1:SukuNimi>
      </org:Henkilo>
    </asi:Allekirjoittaja>
    <asi:Allekirjoittaja>
      <org:Henkilo>
        <org1:AsemaTeksti>Ministeri </org1:AsemaTeksti>
        <org1:SukuNimi>Pertti Salolainen</org1:SukuNimi>
      </org:Henkilo>
    </asi:Allekirjoittaja>
  </asi:AllekirjoitusOsa>
</vah:VahvistettavaLaki>
