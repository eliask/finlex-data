  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>passintarkastuspaikoista</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Sisäasiainministerin esittelystä säädetään 22 päivänä elokuuta 1986 annetun passilain (642/86) 11 ja 22§:n nojalla:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Passintarkastuspaikat </saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Passilain (642/86) 11§:ssä tarkoitettuja passintarkastuspaikkoja ovat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1) satamat:
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Eckerö, Hamina, Hanko, Haukipudas, Helsinki, Inkoo, Kalajoki, Karhula, Kaskinen, Kemi, Kokkola, Kotka, Kristiinankaupunki, Lappeenranta, Loviisa, Långnäs, Maarianhamina, Naantali, Nuijamaa, Oulu, Parainen, Pietarsaari, Pori, Porvoon maalaiskunta, Raahe, Rauma, Tammisaari, Tornio, Turku, Uusikaupunki ja Vaasa;</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
2) lentoasemat:
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>Helsinki-Malmi, Helsinki-Vantaa, Ivalo, Joensuu, Jyväskylä, Kemi, Kruunupyy, Kuopio, Lappeenranta, Maarianhamina, Oulu, Pori, Rovaniemi, Savonlinna, Tampere-Pirkkala, Turku, Vaasa ja Varkaus; sekä</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
3) maarajanylityspaikat:
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:MomenttiKooste>Aavasaksa, Karesuvanto, Karigasniemi, Keinovuopion kylän ja Kolmen valtakunnan pyykin välinen alue, Kilpisjärvi, Kivilompolo, Kolari, Kolmen valtakunnan pyykki - RM 294, Muonio, Nuijamaa, Nuorgam, Näätämö, Pello, Piilola (RM 353 K), Pättikkä, Raja-Jooseppi, Ropinsalmi, Saarijärvi (RM 296 B ja RM 297 A), Saarikoski, Tornio, Utsjoki, Vaalimaa ja Vainikkala.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Passintarkastus muualla kuin passintarkastuspaikassa </saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Sisäasiainministeriö päättää erikseen muualla kuin passintarkastuspaikassa tapahtuvasta passintarkastuksesta.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Suomen ja Norjan sekä Suomen ja Ruotsin hallitukset ovat päättäneet eräistä keskinäisistä maiden välisen maarajan ylittämistä koskevista helpotuksista.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Rajan ylittäminen sisäasiainministeriön luvalla </saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Passilain 11§:n 1 momentissa tarkoitettua rajanylityslupaa haetaan kirjallisesti sisäasiainministeriöltä. Hakemuksesta tulee käydä ilmi:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1) rajan ylittäjän henkilö-, kansalaisuus- ja osoitetiedot;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
2) rajanylityksen syy;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
3) rajanylitysaika ja -paikka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
4) päiväys ja hakijan allekirjoitus.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:MomenttiKooste>Sisäasiainministeriö voi asettaa rajan ylittämiseen liittyviä ehtoja.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="">
          <saa:PykalaTunnusKooste/>
          <saa:SaadosOtsikkoKooste>Voimaantulo </saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä maalis&gt;kuuta 1988.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
