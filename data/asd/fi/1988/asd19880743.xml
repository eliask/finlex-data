  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>743</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1988</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>kansainvälisluonteisten oikeudenkäyntien helpottamista koskevan yleissopimuksen voimaansaattamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>kansainvälisluonteisten oikeudenkäyntien helpottamista koskevan yleissopimuksen voimaansaattamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Ulkoasiainministerin esittelystä säädetään:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Haagissa 25 päivänä lokakuuta 1980 tehty kansainvälisluonteisten oikeudenkäyntien helpottamista koskeva yleissopimus, jonka tasavallan presidentti on hyväksynyt 27 päivänä toukokuuta 1988 ja jota koskeva hyväksymiskirja on talletettu Alankomaiden ulkoasiainministeriön huostaan 13 päivänä kesäkuuta 1988, tulee voimaan 1 päivänä syyskuuta 1988 niin kuin siitä on sovittu.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Edellä 1 §:ssä mainitun yleissopimuksen 3 ja 16 artiklassa tarkoitettuna keskusviranomaisena sekä 4 ja 16 artiklassa tarkoitettuna lähettävänä viranomaisena on Suomessa oikeusministeriö.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä syyskuuta 1988.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
      <saa:SaadosLiite>
        <saa:SaadosKappaleKooste>(Sopimusteksti on julkaistu Suomen säädöskokoelman sopimussarjan n:ossa 47/88)</saa:SaadosKappaleKooste>
      </saa:SaadosLiite>
    </saa:Saados>
  </saa:SaadosOsa>
