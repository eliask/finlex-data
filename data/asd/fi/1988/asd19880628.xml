  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>628</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1988</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>eräiden kauppa- ja teollisuusministeriön virkojen perustamisesta ja lakkauttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>eräiden kauppa- ja teollisuusministeriön virkojen perustamisesta ja lakkauttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Kauppa- ja teollisuusministerin esittelystä säädetään 24 päivänä lokakuuta 1986 annetun valtion virkamieslain (755/86) 6§:n 1 ja 4 momentin sekä 10§:n 1 momentin nojalla:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Kauppa- ja teollisuusministeriö </saa:SaadosOtsikkoKooste>
          <ete>Perustetaan:</ete>
          <saa:MomenttiKooste>yksi A 23 palkkausluokkaan kuuluva ylitarkastajan virka;</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 22 palkkausluokkaan kuuluva ylikielenkääntäjän virka;</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 20 palkkausluokkaan kuuluva tarkastajan virka;</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 15 palkkausluokkaan kuuluva osastosihteerin virka;</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 13 palkkausluokkaan kuuluva toimistosihteerin virka; ja</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 10 palkkausluokkaan kuuluva virastomestarin virka.</saa:MomenttiKooste>
          <ete>Lakkautetaan:</ete>
          <saa:MomenttiKooste>yksi A 22 palkkausluokkaan kuuluva esittelijän virka;</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 21 palkkausluokkaan kuuluva kielenkääntäjän virka;</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 17 palkkausluokkaan kuuluva osastosihteerin virka;</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 9 palkkausluokkaan kuuluva vahtimestarin virka; ja</saa:MomenttiKooste>
          <saa:MomenttiKooste>kolme A 5 palkkausluokkaan kuuluvaa konekirjoittajan virkaa.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä lokakuuta 1988.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Tällä asetuksella perustetut virat saadaan ensimmäisellä kerralla täyttää niitä haettavaksi julistamatta.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
