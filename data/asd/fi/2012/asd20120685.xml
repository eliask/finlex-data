  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>Afrikan Unionin kanssa Suomen tuesta AU Mediation Support Capacity 2012-2014 -hankkeelle tehdyn sopimuksen voimaansaattamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvoston päätöksen mukaisesti säädetään:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Suomen tuesta AU Mediation Support Capacity 2012–2014 -hankkeelle Addis Abebassa 19 päivänä marraskuuta 2012 Suomen tasavallan hallituksen ja Afrikan Unionin välillä tehty sopimus tulee voimaan 19 päivänä joulukuuta 2012, niin kuin siitä on sovittu.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Tasavallan presidentti on hyväksynyt sopimuksen 2 päivänä marraskuuta 2012.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Sopimuksen määräykset ovat asetuksena voimassa.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 5 päivänä joulukuuta 2012.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
      <saa:SaadosLiite>
        <saa:SaadosKappaleKooste>(Sopimus on nähtävänä ja saatavana ulkoasiainministeriön määräyskokoelmassa www.finlex.fi sekä ulkoasiainministeriössä, joka antaa siitä myös tietoja suomeksi ja ruotsiksi)</saa:SaadosKappaleKooste>
      </saa:SaadosLiite>
    </saa:Saados>
  </saa:SaadosOsa>
  <saa:SaadosLiite>
