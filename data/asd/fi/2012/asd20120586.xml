  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>kuluttajatutkimuskeskuksesta annetun valtioneuvoston asetuksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvoston päätöksen mukaisesti</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> kuluttajatutkimuskeskuksesta annetun valtioneuvoston asetuksen (456/2000) 2 § ja 7 §:n 2 momentti seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Johtokunta</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Valtioneuvosto nimittää kolmeksi vuodeksi kerrallaan Kuluttajatutkimuskeskukselle johtokunnan, johon kuuluu puheenjohtaja ja varapuheenjohtaja sekä vähintään kolme ja enintään viisi muuta jäsentä. Johtokunnan jäsenistä yksi edustaa työ- ja elinkeinoministeriötä. Johtokunnan jäseneksi ei voida valita henkilöä, joka on palvelussuhteessa Kuluttajatutkimuskeskukseen. Kuluttajatutkimuskeskuksen henkilöstö valitsee keskuudestaan edustajan, jolla on läsnäolo- ja puheoikeus johtokunnan kokouksissa.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Johtokunta ratkaisee asiat johtajan esittelystä. Johtajan ollessa estynyt asian esittelee johtajan sijainen.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Työ- ja elinkeinoministeriö vahvistaa johtokunnan jäsenten palkkiot.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="7 §">
          <saa:PykalaTunnusKooste>7 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Virkojen täyttäminen</saa:SaadosOtsikkoKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Johtokunta nimittää johtajan välittömässä alaisuudessa toimivan päällikön johtajan esittelystä.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä marraskuuta 2012.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Ennen asetuksen voimaantuloa voidaan ryhtyä asetuksen täytäntöönpanon edellyttämiin toimiin.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
