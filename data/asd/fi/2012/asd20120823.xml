  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>823</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>2012</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>yleisistä kielitutkinnoista perittävistä maksuista</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Opetus- ja kulttuuriministeriön asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>yleisistä kielitutkinnoista perittävistä maksuista</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Opetus- ja kulttuuriministeriön päätöksen mukaisesti säädetään yleisistä kielitutkinnoista annetun lain (964/2004) 11 §:n nojalla:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Yleisen kielitutkinnon järjestäjän perimän tutkintomaksun määrä on:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>1) perustason tutkinto 90 euroa;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>2) keskitason tutkinto 100 euroa; ja</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>3) ylimmän tason tutkinto 145 euroa.</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Ennen 1 päivää helmikuuta 2013 suoritettavien kielitutkintojen tutkintomaksu on kuitenkin:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>1) perustason tutkinto 65 euroa;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>2) keskitason tutkinto 95 euroa; ja</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>3) ylimmän tason tutkinto 140 euroa.</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä tammikuuta 2013 ja on voimassa 31 päivään joulukuuta 2015.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
