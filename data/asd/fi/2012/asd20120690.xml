  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Laki">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Laki</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>potilaan asemasta ja oikeuksista annetun lain muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
