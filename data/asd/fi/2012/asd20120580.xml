    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>jalometallituotteista annetun valtioneuvoston asetuksen 3 §:n 1 momentin 3 ja 4 kohdan muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>jalometallituotteista annetun valtioneuvoston asetuksen 3 §:n 1 momentin 3 ja 4 kohdan muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvoston päätöksen mukaisesti</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> jalometallituotteista annetun valtioneuvoston asetuksen (1148/2000) 3 §:n 1 momentin 3 ja 4 kohta, sellaisena kuin ne ovat asetuksessa 8/2009, seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Juottaminen</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Käytettävien juotteiden tulee olla samaa pitoisuutta ja jalometallia kuin varsinainen tuote seuraavin poikkeuksin:</saa:MomenttiJohdantoKooste>
            <sis:Katkoviiva/>
            <saa:MomenttiKohtaKooste>3) platinatuotteiden juotteiden jalometallipitoisuuden tulee olla vähintään 800 tuhannesosaa;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>4) palladiumtuotteiden juotteiden jalometallipitoisuuden tulee olla vähintään 700 tuhannesosaa.</saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä marraskuuta 2012.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
