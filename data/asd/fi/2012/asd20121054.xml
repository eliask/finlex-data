    <met:Nimeke>
      <met1:NimekeTeksti>televisio-ohjelmiin liitettävästä ääni- ja tekstityspalvelusta annetun valtioneuvoston asetuksen 2 ja 3 §:n muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>televisio-ohjelmiin liitettävästä ääni- ja tekstityspalvelusta annetun valtioneuvoston asetuksen 2 ja 3 §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvoston päätöksen mukaisesti</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> televisio-ohjelmiin liitettävästä ääni- ja tekstityspalvelusta annetun valtioneuvoston asetuksen (292/2011) 2 ja 3 § seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Televisio- ja radiotoiminnasta annetun lain 19 a § 2 momentissa tarkoitettuja ohjelmistoluvan haltijoita ja ohjelmistoja ovat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>1) MTV Oy, ohjelmisto MTV3;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>2) Sanoma Entertainment Oy, ohjelmisto Nelonen;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>3) Fox International Channels Netherlands Holding B.V, ohjelmisto Fox.</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Ääni- ja tekstityspalvelun toteuttamisesta aiheutuva ohjelmatuntikohtainen kustannus vuosina 2013 ja 2014 on:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>1) äänen liittämisestä ohjelmistoihin 70 euroa;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>2) tekstityksen liittämisestä suomen- tai ruotsinkielisiin ohjelmiin 500 euroa.</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä tammikuuta 2013.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
