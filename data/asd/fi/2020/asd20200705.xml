  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>705</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>2020</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>avaruustoiminnasta annetun työ- ja elinkeinoministeriön asetuksen 5 §:n muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Työ- ja elinkeinoministeriön asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>avaruustoiminnasta annetun työ- ja elinkeinoministeriön asetuksen 5 §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Työ- ja elinkeinoministeriön päätöksen mukaisesti</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> avaruustoiminnasta annetun työ- ja elinkeinoministeriön asetuksen (74/2018) 5 § 2 momentti seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="5 §">
          <saa:PykalaTunnusKooste>5 §</saa:PykalaTunnusKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Työ- ja elinkeinoministeriö voi olla vaatimatta avaruustoiminnasta annetun lain 8 §:n 1 momentissa tarkoitettua vakuutusta, jos koko avaruusesineen elinkaaren ajan sen riski törmäyksestä muun, läpimitaltaan vähintään kymmenen senttimetrin kokoisen, avaruusesineen kanssa kiertoradalla on alle 1/1 000 ja jos todennäköisyys sille, että avaruusesineen tai sen osien palautumisesta tai palauttamisesta maahan aiheutuisi henkilövahinkoja, on alle 1/10 000.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 15 päivänä lokakuuta 2020.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
  <asi:AllekirjoitusOsa>
