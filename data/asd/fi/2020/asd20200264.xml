  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>264</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>2020</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>riistahallinnosta annetun valtioneuvoston asetuksen väliaikaisesta muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>riistahallinnosta annetun valtioneuvoston asetuksen väliaikaisesta muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvoston päätöksen mukaisesti</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>lisätään</saa:SaadosKursiiviKooste> riistahallinnosta annettuun valtioneuvoston asetukseen (171/2011) väliaikaisesti uusi 1 a, 3 a ja 5 a § seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 a §">
          <saa:PykalaTunnusKooste>1 a §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Hallituksen toimikauden jatkaminen</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Suomen riistakeskuksen hallituksen nykyinen toimikausi jatkuu 1 §:n 1 ja 3 momentissa säädetystä poiketen, kunnes maa- ja metsätalousministeriö asettaa uuden hallituksen, kuitenkin enintään 31 päivään joulukuuta 2021.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 a §">
          <saa:PykalaTunnusKooste>3 a §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Valtakunnallisen riistaneuvoston toimikauden jatkaminen</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Valtakunnallisen riistaneuvoston nykyinen toimikausi jatkuu 3 §:n 1 ja 3 momentissa säädetystä poiketen, kunnes maa- ja metsätalousministeriö asettaa uuden valtakunnallisen riistaneuvoston, kuitenkin enintään 31 päivään heinäkuuta 2021.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="5 a §">
          <saa:PykalaTunnusKooste>5 a §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Alueellisen riistaneuvoston toimikauden jatkaminen</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Alueellisen riistaneuvoston nykyinen toimikausi jatkuu 5 §:n 1 ja 3 momentissa säädetystä poiketen, kunnes maa- ja metsätalousministeriö asettaa uuden alueellisen riistaneuvoston, kuitenkin enintään 31 päivään heinäkuuta 2021.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä toukokuuta 2020 ja on voimassa 31 päivään joulukuuta 2021.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
