    <met:Nimeke>
      <met1:NimekeTeksti>kaupunginviskaaleista annetun asetuksen muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>HE 101/92</sis1:ViiteTeksti>
