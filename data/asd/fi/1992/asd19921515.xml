  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>1515</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1992</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>alueellisen teknologiatoiminnan edistämiseen myönnettävien avustusten ehdoista annetun valtioneuvoston päätöksen muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>alueellisen teknologiatoiminnan edistämiseen myönnettävien avustusten ehdoista annetun valtioneuvoston päätöksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvosto on kauppa- ja teollisuusministeriön esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>kumonnut</saa:SaadosKursiiviKooste> alueellisen teknologiatoiminnan edistämiseen myönnettävien avustusten ehdoista 31 päivänä lokakuuta 1991 antamansa päätöksen (1310/91) 3 §:n 3 momentin, sekä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muuttanut</saa:SaadosKursiiviKooste> 3 §:n 2 momentin ja 9 §:n 1 momentin, seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Avustusta voidaan myöntää enintään 50 prosenttia sekä niissä tapauksissa, joissa avustuksen saajana on pieni tai keskisuuri yritys, enintään 60 prosenttia tutkimus- ja kehityshankkeen hyväksyttävistä kustannuksista.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="9 §">
          <saa:PykalaTunnusKooste>9 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 6 päivänä marraskuuta 1991 ja se on voimassa vuoden 1993 loppuun.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä tammikuuta 1993.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
