  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>299</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1992</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>veropiireistä annetun valtioneuvoston päätöksen muuttamisesta </met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston päätös </saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>veropiireistä annetun valtioneuvoston päätöksen muuttamisesta </saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvosto on valtiovarainministeriön esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>kumonnut</saa:SaadosKursiiviKooste> veropiireistä 25 päivänä helmikuuta 1960 annetun valtioneuvoston päätöksen (122/60) Hollolan veropiiriä, Puolangan veropiiriä ja Pedersören veropiiriä koskevat kohdat,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste>näistä Pedersören veropiiriä koskeva kohta sellaisena kuin se on 21 päivänä joulukuuta 1989 annetussa valtioneuvoston päätöksessä (1181/89), sekä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muuttanut</saa:SaadosKursiiviKooste> Lahden veropiiriä, Hyrynsalmen veropiiriä ja Pietarsaaren kaupungin veropiiriä koskevat kohdat sekä osasta <saa:SaadosKursiiviKooste>A. Kahdesta tai useammasta kunnasta muodostetut veropiirit</saa:SaadosKursiiviKooste> Vaasan lääniä koskevan kohdan,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste>näistä Lahden veropiiriä koskeva kohta sellaisena kuin se on 19 päivänä joulukuuta 1991 annetussa valtioneuvoston päätöksessä (1624/91), seuraavasti: </saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <te>
          <pl>Hämeen lääni: </pl>
        </te>
        <sis:Katkoviiva/>
        <te><saa:SaadosKursiiviKooste>Lahden veropiiri:</saa:SaadosKursiiviKooste> Lahden kaupunki sekä Hollolan, Kärkölän ja Nastolan kunnat;</te>
        <sis:Katkoviiva/>
        <te>
          <pl>Vaasan lääni: </pl>
        </te>
        <sis:Katkoviiva/>
        <te><saa:SaadosKursiiviKooste>Pietarsaaren veropiiri:</saa:SaadosKursiiviKooste> Pietarsaaren kaupunki sekä Luodon ja Pedersören kunnat; </te>
        <sis:Katkoviiva/>
        <te>
          <pl>Oulun lääni: </pl>
        </te>
        <sis:Katkoviiva/>
        <te><saa:SaadosKursiiviKooste>Hyrynsalmen veropiiri:</saa:SaadosKursiiviKooste> Hyrynsalmen, Puolangan ja Ristijärven kunnat; </te>
        <sis:Katkoviiva/>
        <te>Samalla valtioneuvosto on määrännyt, että veropiirit ovat suomen- tai ruotsinkielisiä tai kaksikielisiä seuraavasti:<saa:SaadosKursiiviKooste>A. Kahdesta tai useammasta kunnasta muodostetut veropiirit</saa:SaadosKursiiviKooste> </te>
        <sis:Katkoviiva/>
        <te>
          <pl>Vaasan läänissä: </pl>
        </te>
        <te>1) Ruotsinkielisiä ruotsinkielisten ja kaksikielisiä kaksikielisten kuntien osalta, <saa:SaadosKursiiviKooste>ruotsi</saa:SaadosKursiiviKooste> enemmistön kielenä, ovat Närpiön, Oravaisten ja Pietarsaaren veropiirit.</te>
        <te>2) Maalahden veropiiri on <saa:SaadosKursiiviKooste>ruotsinkielinen</saa:SaadosKursiiviKooste>.</te>
        <te>3) Muut veropiirit ovat <saa:SaadosKursiiviKooste>suomenkielisiä</saa:SaadosKursiiviKooste>. </te>
        <sis:Katkoviiva/>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä toukokuuta 1992. </saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
