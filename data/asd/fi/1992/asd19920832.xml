  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>832</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1992</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>sosiaali- ja terveyshallituksesta annetun asetuksen muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus </saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>sosiaali- ja terveyshallituksesta annetun asetuksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Sosiaali- ja terveysministerin esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>kumotaan</saa:SaadosKursiiviKooste> sosiaali- ja terveyshallituksesta 25 päivänä tammikuuta 1991 annetun asetuksen (142/91) 18 § sekä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> 1 §:n 2 momentti ja 10 §:n 1 momentti, seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:SaadosValiotsikkoKooste>Sosiaali- ja terveyshallituksen organisaatio</saa:SaadosValiotsikkoKooste>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Elinympäristöosastolle kuuluvat terveyden ja sosiaalisen turvallisuuden edistämistä sekä sosiaalisten olojen ja elinympäristön kehittämistä koskevat asiat.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <saa:SaadosValiotsikkoKooste>Erityisistunnot</saa:SaadosValiotsikkoKooste>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="10 §">
          <saa:PykalaTunnusKooste>10 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Erityisistunnossa ratkaistaan raskauden keskeyttämistä, steriloimista, kastroimista, oikeuspsykiatriaa, terveydenhuollon ammatinharjoittamista sekä lääkehuoltoa koskevat asiat siten kuin jäljempänä säädetään.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä syyskuuta 1992.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
