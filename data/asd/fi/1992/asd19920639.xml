  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>639</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1992</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>eräiden tilojen, erään alueen ja pakkolunastusyksikön siirtämisestä Hirvensalmen, Kangasniemen ja Mäntyharjun kunnista Mikkelin maalaiskuntaan</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Sisäasiainministeriön päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>eräiden tilojen, erään alueen ja pakkolunastusyksikön siirtämisestä Hirvensalmen, Kangasniemen ja Mäntyharjun kunnista Mikkelin maalaiskuntaan</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Sisäasiainministeriö on kuntajaosta 21 päivänä tammikuuta 1977 annetun lain (73/77) 14 §:n 2 momentin nojalla päättänyt:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Hirvensalmen kunnasta siirretään Mikkelin maalaiskuntaan Lahnaniemen kylästä Valtion rautatiealue 6:3.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Kangasniemen kunnasta siirretään Mikkelin maalaiskuntaan Rauhajärven kylästä kokonaan tilat Niittula 1:9, Ketola 1:11, Pöllänpalsta 1:26, Metsäpuro II 4:12 ja Kielorinne 5:11 sekä edellä mainittujen tilojen 1:11, 1:26 ja 4:12 alueella oleva Harjumaan-Luusniemen maantie.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Mäntyharjun kunnasta siirretään Mikkelin maalaiskuntaan Tiilikkalan kylästä kokonaan tila Harjurinne 2:42.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Siirrot eivät aiheuta muutosta kuntien kielelliseen jaotukseen.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="5 §">
          <saa:PykalaTunnusKooste>5 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä tammikuuta 1993.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
