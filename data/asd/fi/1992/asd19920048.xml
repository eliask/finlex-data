    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>Hallituksen esitys 92/91</sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
        <sis1:ViiteTeksti>Hallintovaliok. miet. 5/91</sis1:ViiteTeksti>
