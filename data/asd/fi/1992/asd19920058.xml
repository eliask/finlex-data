  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>58</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1992</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>lomittajatoimintaa koskevasta ostopalvelukokeilusta annetun valtioneuvoston päätöksen muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>lomittajatoimintaa koskevasta ostopalvelukokeilusta annetun valtioneuvoston päätöksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvosto on sosiaali- ja terveysministeriön esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muuttanut</saa:SaadosKursiiviKooste> lomittajatoimintaa koskevasta ostopalvelukokeilusta 20 päivänä kesäkuuta 1990 annetun valtioneuvoston päätöksen (585/90) 6 §:n 1 momentin, 7 §:n ja 8 §:n 1 momentin, näistä 8 §:n 1 momentin sellaisena kuin se on 3 päivänä tammikuuta 1991 annetussa valtioneuvoston päätöksessä (6/91), seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="6 §">
          <saa:PykalaTunnusKooste>6 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Kokeilun yleinen johto, ohjaus ja valvonta kuuluu sosiaali- ja terveysministeriölle.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="7 §">
          <saa:PykalaTunnusKooste>7 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tarkempia määräyksiä kokeilun toimeenpanosta ja ohjeita tämän päätöksen soveltamisesta antaa tarvittaessa sosiaali- ja terveysministeriö.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="8 §">
          <saa:PykalaTunnusKooste>8 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä heinäkuuta 1990. Kokeilua voidaan tämän päätöksen nojalla jatkaa 31 päivään joulukuuta 1992 niissä kunnissa, jotka ovat olleet mukana kokeilussa 31 päivänä joulukuuta 1991.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä helmikuuta 1992.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
