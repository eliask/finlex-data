  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>526</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1992</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>tupakkatuotteista suoritettavasta lisäverosta </met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>tupakkatuotteista suoritettavasta lisäverosta </saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvosto on tupakkaverosta 7 päivänä joulukuuta 1979 annetun lain (875/79) 5 §:n nojalla valtiovarainministeriön esittelystä päättänyt:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tupakkatuotteista on suoritettava lisäveroa seuraavasti:</saa:MomenttiKooste>
          <te/>
          <te>Tuote         Tuote-                 Lisävero</te>
          <te>              ryhmä             vähittäis-  enintään</te>
          <te>                      mk/1 000  myynti-</te>
          <te>                      kpl       hinnasta</te>
          <te>Savukkeet          1    50,00    10,00 %    160 mk/1 000 kpl</te>
          <te>Sikarit</te>
          <te>- sikarisavukkeet  2    50,00    10,00 %    160 mk/1 000 kpl</te>
          <te>- muut sikarit     3    -         0,60 %    500 mk/1 000 kpl</te>
          <te>Piippu- ja savuke-</te>
          <te>  tupakka          4    -        14,00 %     95 mk/kg</te>
          <te>Nuuska- ja puru-</te>
          <te>  tupakka          5    -        24,00 %     95 mk/kg</te>
          <te>Savukepaperi, määräkokoon leikattu, myös lehtiöinä tai</te>
          <te>  hylsyinä, vähittäis-</te>
          <te>  myyntimuodossa   6    -        17,00 %     15 mk/1 000</te>
          <te>                                             savuketta vas-</te>
          <te>                                             taavalta kap-</te>
          <te>                                             paleelta tai</te>
          <te>                                             paperin osalta</te>
          <te/>
          <saa:MomenttiKooste>Savukkeista on suoritettava lisäveroa vähintään 420 markkaa 1 000 kappaleelta vähennettynä perusveron määrällä.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä syyskuuta 1992.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Tällä päätöksellä kumotaan 10 päivänä lokakuuta 1991 annettu valtioneuvoston päätös tupakkatuotteista suoritettavasta lisäverosta (1251/91).</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
