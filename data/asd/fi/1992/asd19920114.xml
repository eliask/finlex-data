  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus </saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>poliisin eräiden virkojen perustamisesta ja lakkauttamisesta </saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Sisäasiainministerin esittelystä säädetään 24 päivänä lokakuuta 1986 annetun valtion virkamieslain (755/86) 6 §:n 1 ja 4 momentin ja 10 §:n 1 momentin nojalla, sellaisena kuin niistä 6 §:n 4 momentti on 3 päivänä elokuuta 1990 annetussa laissa (673/90): </saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Eduskunnan myönnettyä tarkoitukseen tarvittavat varat perustetaan ja lakkautetaan jäljempänä mainitut peruspalkkaiset virat: </saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 § </saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Poliisivarikot </saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Perustetaan 1 päivänä maaliskuuta 1992: </saa:MomenttiKooste>
          <te/>
          <te>Luku-   Viran nimi                      Palkkaus- </te>
          <te>määrä                                   luokka </te>
          <te>1       varikonpäällikkö                A 26 </te>
          <te>3       linjapäällikkö                  A 23 </te>
          <te>1       tarkastaja                      A 20 </te>
          <te/>
          <saa:MomenttiKooste>Lakkautetaan 1 päivänä maaliskuuta 1992: </saa:MomenttiKooste>
          <te>1       varikonpäällikkö                A 24 </te>
          <te>2       varikonpäällikkö                A 21 </te>
          <te>1       varikonpäällikkö                A 20 </te>
          <te>1       autovaraston esimies            A 20</te>
          <te> </te>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 § </saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Paikallispoliisi </saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Perustetaan 1 päivästä marraskuuta 1992: </saa:MomenttiKooste>
          <te/>
          <te>1       nimismies                       A 28 </te>
          <te>1       osastopäällikkö                 A 26 </te>
          <te>2       apulaisnimismies                A 25 </te>
          <te>1       osastosihteeri                  A 15 </te>
          <te>3       avustava ulosottomies           A 12 </te>
          <te>1       toimistosihteeri                A 12 </te>
          <te>2       haastemies                      A 11</te>
          <te>1       avustava ulosottomies           A 11 </te>
          <te>1       toimistosihteeri                A 11 </te>
          <te>1       toimistovirkailija              A  9   </te>
          <te/>
          <saa:MomenttiKooste>Lakkautetaan 1 päivästä marraskuuta 1992: </saa:MomenttiKooste>
          <te>1       nimismies                       A 27 </te>
          <te>1       poliisimestari                  A 24 </te>
          <te>1       apulaisnimismies                A 24</te>
          <te> </te>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä maaliskuuta 1992. </saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:identifiointiTunnus="5 §">
          <saa:PykalaTunnusKooste>5 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tällä asetuksella perustetut virat voidaan täyttää ensimmäisellä kerralla niitä haettavaksi julistamatta.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Toimenpiteisiin tällä asetuksella perustettujen virkojen täyttämiseksi saadaan ryhtyä jo ennen sitä ajankohtaa, josta lukien virat on perustettu. </saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
