  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>1458</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1992</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>kotieläinten arvon määräämisessä noudatettavista perusteista</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Verohallituksen päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>kotieläinten arvon määräämisessä noudatettavista perusteista</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Verohallitus on 29 päivänä joulukuuta 1988 annetun tulo- ja varallisuusverolain (1240/88) 128 §:n 2 momentin nojalla määrännyt, että kotieläinten käyvän arvon määräämisessä on noudatettava seuraavia perusteita:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Kotieläinten arvoksi katsotaan seuraavat keskimääräiset arvot eläintä kohti:</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Suomenhevoset</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <te>1-2 v:n ikäiset                  3 300-4 200 mk</te>
          <te>3-4 v:n ikäiset                  5 200-5 900 mk</te>
          <te>5-14 v:n ikäiset                 5 900-6 500 mk</te>
          <te>yli 14 v:n ikäiset               5 200-6 000 mk</te>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Lämminveriset urheiluhevoset</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <te>1-2 v:n ikäiset                  4 300-5 500 mk</te>
          <te>3-4 v:n ikäiset                  6 200-7 100 mk</te>
          <te>5-10 v:n ikäiset                 6 500-7 400 mk</te>
          <te>yli 10 v:n ikäiset               5 300-6 300 mk</te>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Nautaeläimet</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <te>Sonnit, yli 2 v:n ikäiset        5 900-6 300 mk</te>
          <te>Nuoret sonnit, 1-2 v:n  ikäiset  3 600-3 800 mk</te>
          <te>Lehmät                           4 270-4 650 mk</te>
          <te>Hiehot                           3 900-4 200 mk</te>
          <te>Mullit                           2 450-2 650 mk</te>
          <te>Vasikat                            570-  640 mk</te>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Siat</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <te>Karjut, yli 9 kk:n ikäiset       1 400-1 600 mk</te>
          <te>Emakot                           1 600-1 850 mk</te>
          <te>Lihotussiat, 6-9 kk:n ikäiset    1 050-1 200 mk</te>
          <te>Siat, 3-6 kk:n ikäiset             550-  850 mk</te>
          <te>Siat, 2-3 kk:n ikäiset               410-510 mk</te>
          <te>Porsaat, alle 2 kk:n ikäiset             330 mk</te>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Lampaat</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <te>yli 6 kk:n ikäiset                       420 mk</te>
          <te>alle 6 kk:n ikäiset                      245 mk</te>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Porot</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <te>yli 1 v:n ikäiset                        790 mk</te>
          <te>alle 1 v:n ikäiset                       460 mk</te>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Siipikarja</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <te>Munivat kanat, yli 6 kk:n ikäiset         20 mk</te>
          <te>Munivat kanat, alle 6 kk:n ikäiset        30 mk</te>
          <te>Teuraaksi kasvatettavat kanat              7 mk</te>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Keskimääräistä arvokkaammat eläimet on arvioitava täsä päätöksessä mainittuja arvoja korkeampiin arvoihin.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Kotieläimet, joita tässä päätöksessä ei mainita, arvioidaan niiden todennäköiseen luovutushintaan.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä tammikuuta 1993, ja sitä sovelletaan vuodelta 1992 toimitettavassa tulo- ja varallisuusverotuksessa.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
