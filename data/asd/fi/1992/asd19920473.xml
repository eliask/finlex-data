  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>473</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1992</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>eräiden tilojen ja alueiden siirtämisestä Mietoisten kunnasta Mynämäen kuntaan</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>eräiden tilojen ja alueiden siirtämisestä Mietoisten kunnasta Mynämäen kuntaan</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvosto on kuntajaosta 21 päivänä tammikuuta 1977 annetun lain (73/77) 14 §:n 1 momentin nojalla sisäasiainministeriön esittelystä päättänyt:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Mietoisten kunnasta siirretään Mynämäen kuntaan seuraavat tilat ja alueet:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1) Haraisen yksinäistaloa tila Mattila 1:3;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
2) Katavaisen kylästä tila Kivilä 2:1;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
3) Pursilan kylästä tila Mantere 4;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
4) Pyhön kylästä tilat Korpela 1:27 ja Korpelanlisä 2:32;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
5) Ruonkallion kylästä tilat Lehtonen 2:1, Lehtoranta II 2:5 ja Lehtoranta 3:4;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
6) Sukoisen kylästä tila Mikala 2:4;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
7) Tuokilan yksinäistaloa tila Jokela 1:19,
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
8) Valaskallion kylästä tilat Yliheikkilä 1:19; Palmunen 2:2, Reuna II 4:18, Reuna 4:20 ja Kulmala 6:11; sekä
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
9) edellämainittujen tilojen Mattila 1:3, Kivilä 2:1, Jokela 1:19 ja Palmunen 2:2 alueella olevat yleiset alueet.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Siirrot eivät aiheuta muutosta kuntien kielelliseen jaotukseen.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä tammikuuta 1993.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
