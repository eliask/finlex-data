  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Laki">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Laki</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>vapaakuntakokeilusta annetun lain muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Eduskunnan päätöksen mukaisesti</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>kumotaan</saa:SaadosKursiiviKooste> vapaakuntakokeilusta 29 päivänä heinäkuuta 1988 annetun lain (718/88) 5 §, 6 §:n 2 momentin 2 ja 4-6 kohta, 8-10 § ja 9 ja 10 §:n väliotsikko,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste>sellaisena kuin näistä on 5 § osittain muutettuna 13 päivänä joulukuuta 1991 annetulla lailla (1442/91), sekä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> 2 §:n 2 momentti ja 15 § seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Soveltamisala</saa:SaadosOtsikkoKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste> Vuoden 1993 alusta lukien tässä laissa vapaakunnalle säädettyä kokeilua voi harjoittaa myös muu kunta (hallinnon kokeilukunta) ilmoitettuaan kokeilusuunnitelmasta sisäasiainministeriölle.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="15 §">
          <saa:PykalaTunnusKooste>15 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Voimassaoloaika</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Tämä laki tulee voimaan 10 päivänä elokuuta 1988 ja on voimassa 31 päivään joulukuuta 1996.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä laki tulee voimaan 1 päivänä tammikuuta 1993.</saa:MomenttiKooste>
        </saa:Pykala>
