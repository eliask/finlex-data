  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>1295</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1992</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>eräiden poliisipiirien uudelleen järjestämisestä</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>eräiden poliisipiirien uudelleen järjestämisestä</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvosto on poliisin hallinnosta 14 päivänä helmikuuta 1992 annetun lain (110/92) 6 §:n 2 momentin nojalla sisäasiainministeriön esittelystä päättänyt:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Kalannin nimismiespiiri lakkautetaan 1 päivästä tammikuuta 1993 lukien.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Ikaalisten, Kankaanpään, Parkanon, Rauman, Korpilahden ja Äänekosken nimismiespiirit muodostetaan uudelleen.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Ikaalisten nimismiespiiriin kuuluu Ikaalisten kaupunki.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Kankaanpään nimismiespiiriin kuuluvat Kankaanpään kaupunki sekä Honkajoen, Jämijärven ja Karvian kunnat.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Parkanon nimismiespiiriin kuuluvat Parkanon kaupunki ja Kihniön kunta.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Rauman nimismiespiiriin kuuluvat Eurajoen ja Lapin kunnat.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Korpilahden nimismiespiiriin kuuluvat Korpilahden ja Muuramen kunnat.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Äänekosken nimismiespiiriin kuuluvat Äänekosken kaupunki ja Uuraisten kunta.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä päätös ei aiheuta muutoksia hallintoalueiden kielelliseen jaotukseen.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä tammikuuta 1993.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Ennen päätöksen voimaantuloa voidaan ryhtyä päätöksen täytäntöönpanon edellyttämiin toimenpiteisiin.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
