  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>661</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1996</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>maatilatalouden investointeihin ja tilanpidon aloittamiseen myönnetyn ja myönnettävänvaltionlainan korosta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Maa- ja metsätalousministeriön päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>maatilatalouden investointeihin ja tilanpidon aloittamiseen myönnetyn ja myönnettävänvaltionlainan korosta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Maa- ja metsätalousministeriö on maa- ja metsätalouden rakennepoliittisista toimenpiteistä 22 päivänä joulukuuta 1994 annetun lain (1303/94) 52 §:n 2 momentin sekä maatiloille myönnettävästä investointituesta 16 päivänä marraskuuta 1995 annetun valtioneuvoston päätöksen (53/96) 32 §:n nojalla päättänyt:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Maatilatalouden investointeihin ja tilanpidon aloittamiseen myönnetyn ja myönnettävän valtionlainan vuotuinen korko on 8,30 prosenttia 1 päivänä marraskuuta 1996 alkavalla ja 31 päivänä lokakuuta 1997 päättyvällä korkojaksolla.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 4 päivänä syyskuuta 1996.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
