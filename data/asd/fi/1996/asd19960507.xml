  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>507</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1996</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>rajoitetun käyttäjäryhmän teletoiminnasta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Liikenneministeriön päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>rajoitetun käyttäjäryhmän teletoiminnasta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Liikenneministeriö on 20 päivänä helmikuuta 1987 annetun teletoimintalain (183/87) 7 §:n 1 momentin 3 kohdan nojalla, sellaisena kuin se on 24 päivänä toukokuuta 1996 annetussa laissa (343/96), päättänyt:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Soveltamisala</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Rajoitetun käyttäjäryhmän tarvetta varten harjoitetussa teletoiminnassa noudatetaan tämän päätöksen ehtoja ja määräyksiä.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Määritelmiä</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Tässä päätöksessä tarkoitetaan<saa:SaadosKursiiviKooste>rajoitetulla käyttäjäryhmällä</saa:SaadosKursiiviKooste> sellaista käyttäjien ryhmää, jota yhdistää taloudellinen, ammatillinen tai liiketoiminnallinen yhteenkuuluvaisuus ja joilla on tarvetta ryhmän sisäiseen yhteydenpitoon.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Teletoiminnan harjoittamisen aloittaminen</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Ennen rajoitetun käyttäjäryhmän tarvetta varten harjoitetun teletoiminnan aloittamista teletoiminnan harjoittajan on tehtävä siitä teletoimintailmoitus liikenneministeriöön, jollei teletoimintalain soveltamisalaan kuulumattomasta teletoiminnasta annetusta liikenneministeriön päätöksestä (501/96) muuta johdu.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Verkon liittäminen yleiseen televerkkoon</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Rajoitetun käyttäjäryhmän tarvetta varten rakennettu televerkko voidaan liittää muihin televerkkoihin. Yhteenliittämisessä on noudatettava teletoimintalain 20 §:n nojalla annettuja määräyksiä.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="5 §">
          <saa:PykalaTunnusKooste>5 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Lisämääräykset</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Liikenneministeriö ja Telehallintokeskus voivat antaa lisämääräyksiä rajoitetun käyttäjäryhmän tarvetta varten harjoitetusta teletoiminnasta ottaen huomioon tätä toimintaa koskevat Euroopan yhteisöjen säädökset.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="6 §">
          <saa:PykalaTunnusKooste>6 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Voimaantulo</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä elokuuta 1996.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
