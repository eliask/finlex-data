  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>1276</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1996</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>valtionosuutta saavista liikunnan koulutuskeskuksista annetun asetuksen muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>valtionosuutta saavista liikunnan koulutuskeskuksista annetun asetuksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Opetusministerin esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>kumotaan</saa:SaadosKursiiviKooste> valtionosuutta saavista liikunnan koulutuskeskuksista 13 päivänä marraskuuta 1992 annetun asetuksen (1019/1992) 13―16 §,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste>sellaisina kuin niistä ovat 13 ja 14 § asetuksessa 1476/1993, sekä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> 17 ja 18 §,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste>sellaisena kuin niistä on 17 § mainitussa asetuksessa 1476/1993, seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="17 §">
          <saa:PykalaTunnusKooste>17 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Opiskelijamäärien ilmoittaminen</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Koulutuskeskuksen ylläpitäjän on toimitettava asianomaiselle ministeriölle ammatilliseen peruskoulutukseen osallistuvien opiskelijoiden määrät tammikuun 20 ja syyskuun 20 päivän tilanteen mukaan kymmenen päivän kuluessa mainituista päivistä.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="18 §">
          <saa:PykalaTunnusKooste>18 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Kokeilu- ja kehittämistoiminta</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Koulutuksen kehittämiseksi järjestettävässä kokeilussa voidaan asianomaisen ministeriön luvalla poiketa valtionosuutta saavista liikunnan koulutuskeskuksista annetun lain ja tämän asetuksen muista kuin valtionosuutta koskevista säännöksistä.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä tammikuuta 1997.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
