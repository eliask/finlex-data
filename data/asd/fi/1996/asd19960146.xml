  <saa:SaadosOsa>
    <saa:MuuSaados>
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Sosiaali- ja terveysministeriön päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>sosiaali- ja terveydenhuollon tuotevalvontakeskuksen maksullisista suoritteista annetun sosiaali- ja terveysministeriön päätöksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:SisaltoMuuSaados>
        <saa:SaadosKappaleKooste>Sosiaali- ja terveysministeriö on</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muuttanut</saa:SaadosKursiiviKooste> sosiaali- ja terveydenhuollon tuotevalvontakeskuksen maksullisista suoritteista joulukuun 29 päivänä 1994 annetun sosiaali- ja terveysministeriön päätöksen (4/95) liitteen 1 kohdan 2, seuraavasti:</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste>Liite 1</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste>2) Tukkumyyntiä koskevat päätökset</saa:SaadosKappaleKooste>
        <tau:table>
          <tau:tgroup cols="2" colsep="0" rowsep="0" align="left">
            <tau:colspec colnum="1" colname="col1" colwidth="23003*"/>
            <tau:colspec colnum="2" colname="col2" colwidth="11520*"/>
            <tau:tbody valign="top">
              <tau:row>
                <tau:entry>
                  <sis:KappaleKooste>― alkoholijuoman ja väkiviinan tukkumyyntilupa</sis:KappaleKooste>
                </tau:entry>
                <tau:entry>
                  <sis:KappaleKooste>10 000 markkaa</sis:KappaleKooste>
                </tau:entry>
              </tau:row>
              <tau:row>
                <tau:entry>
                  <sis:KappaleKooste>― tukkumyyntiluvan haltijan verottoman varaston tai sen muutoksen hyväksyminen</sis:KappaleKooste>
                </tau:entry>
                <tau:entry>
                  <sis:KappaleKooste>3 000 markkaa</sis:KappaleKooste>
                </tau:entry>
              </tau:row>
              <tau:row>
                <tau:entry>
                  <sis:KappaleKooste>― denaturoidun etanolin ja alkoholijuomamaisen alkoholivalmisteen tukkumyyntilupa</sis:KappaleKooste>
                </tau:entry>
                <tau:entry>
                  <sis:KappaleKooste>4 000 markkaa</sis:KappaleKooste>
                </tau:entry>
              </tau:row>
              <tau:row>
                <tau:entry>
                  <sis:KappaleKooste>― tukkumyyntiluvan muutos</sis:KappaleKooste>
                </tau:entry>
                <tau:entry>
                  <sis:KappaleKooste>1 000 markkaa</sis:KappaleKooste>
                </tau:entry>
              </tau:row>
              <tau:row>
                <tau:entry>
                  <sis:KappaleKooste>― ajallisesti tai muutoin rajoitettu tukkumyyntilupa (alkoholijuomien tuote-esittely, muun alkoholivalmisteen tukkumyynti)</sis:KappaleKooste>
                </tau:entry>
                <tau:entry>
                  <sis:KappaleKooste>500 markkaa</sis:KappaleKooste>
                </tau:entry>
              </tau:row>
            </tau:tbody>
          </tau:tgroup>
        </tau:table>
