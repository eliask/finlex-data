  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>715</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1982</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>korvauksen suorittamisesta toisesta pohjoismaasta Suomen tuomioistuimeen kutsutulle henkilölle annetun asetuksen muuttamisesta </met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>korvauksen suorittamisesta toisesta pohjoismaasta Suomen tuomioistuimeen kutsutulle henkilölle annetun asetuksen muuttamisesta </saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Oikeusministerin esittelystä muutetaan korvauksen suorittamisesta toisesta pohjoismaasta Suomen tuomioistuimeen kutsutulle henkilölle 13 päivänä kesäkuuta 1975 annetun asetuksen (446/75) 3-5 § näin kuuluviksi: </saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3§">
          <saa:PykalaTunnusKooste>3§</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Päivärahan määrä, joka lasketaan valtion varoista maksettavista todistelukustannuksista annetun asetuksen 5 §:ssä säädetyllä tavalla, on matkavuorokaudelta 100 markkaa, jos matka sen aikana kestää enintään 12 tuntia, ja 200 markkaa, jos matka sen aikana kestää yli 12 tuntia. </saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="4§">
          <saa:PykalaTunnusKooste>4§</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Majoittumiskorvaus määrätään valtion varoista maksettavista todistelukustannuksista annetun asetuksen 7 §:ssä säädetyllä tavalla. </saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="5§">
          <saa:PykalaTunnusKooste>5§</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Korvauksena taloudellisesta menetyksestä, joka lasketaan valtion varoista maksettavista todistelukustannuksista annetun asetuksen 8 §:ssä säädetyllä tavalla, suoritetaan enintään 120 markkaa vuorokaudelta. </saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä lokakuuta 1982. </saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
