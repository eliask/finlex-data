  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Kirje">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Tasavallan Presidentin avoin kirje</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>valtioneuvostossa toimeenpannuista muutoksista</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <sis:Tyhja/>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Tasavallan Presidentti vapauttaa pyynnöstä</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>kansanedustaja Pekka Olavi Haaviston valtioneuvoston jäsenyydestä ja kehitysministerin tehtävästä sekä käsittelemästä valtioneuvoston kanslian toimialaan kuuluvia asioita sekä</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>puolueen puheenjohtaja, tutkija, valtiotieteen maisteri, kansanedustaja Ville Matti Niinistön valtioneuvoston jäsenyydestä ja ympäristöministerin tehtävästä.</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala>
          <saa:MomenttiKooste>Lisäksi Tasavallan Presidentti vapauttaa oikeustieteen kandidaatti, kansanedustaja Jan Pellervo Vapaavuoren käsittelemästä valtiovarainministeriön toimialaan kuuluvia asioita.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Samalla Tasavallan Presidentti nimittää ja määrää</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>ehkäisevän päihdetyön ohjaaja, kansanedustaja Sirpa Hannele Paateron valtioneuvoston jäseneksi ja kehitysministeriksi ulkoasiainministeriöön sekä käsittelemään valtioneuvoston kanslian toimialaan kuuluvia asioita sekä</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>valtiotieteiden maisteri, kansanedustaja Sanni Kaisa Grahn-Laasosen valtioneuvoston jäseneksi ja ympäristöministeriksi.</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Lisäksi Tasavallan Presidentti määrää</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>puolueen puheenjohtaja, lääkäri, lääketieteen lisensiaatti, kansanedustaja Päivi Maria Räsäsen käsittelemään myös valtiovarainministeriön toimialaan kuuluvia asioita sekä</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>puolueen puheenjohtaja, kauppatieteen maisteri Carl Christoffer Haglundin käsittelemään myös opetus- ja kulttuuriministeriön toimialaan kuuluvia asioita.</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
