  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Laki">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Laki</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>työttömyysturvalain 22 ja 24 §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Eduskunnan päätöksen mukaisesti</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> 24 päivänä elokuuta 1984 annetun työttömyysturvalain (602/84) 22 §:n 1 momentti ja 24 §, sellaisina kuin ne ovat 28 päivänä joulukuuta 1990 annetussa laissa (1365/90), seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="22 §">
          <saa:PykalaTunnusKooste>22 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Peruspäivärahan suuruus</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Peruspäivärahan täysi määrä on 118 markkaa päivältä.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="24 §">
          <saa:PykalaTunnusKooste>24 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Lapsikorotus</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Työttömyyspäivärahan saajalle, jolla on huollettavanaan 18 vuotta nuorempi lapsi, maksetaan päiväraha korotettuna lapsikorotuksella, jonka suuruus yhdestä lapsesta on 24 markkaa, kahdesta lapsesta yhteensä 35 markkaa ja kolmesta tai useammasta lapsesta yhteensä 45 markkaa.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä laki tulee voimaan 1 päivänä syyskuuta 1995. Laissa säädetyt markkamäärät vastaavat vuoden 1995 ensimmäisen neljänneksen tasoa.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
