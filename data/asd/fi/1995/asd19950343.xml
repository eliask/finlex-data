    <met:Nimeke>
      <met1:NimekeTeksti>eduskunnan vaalisäännön muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>PNE 5/94 </sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
        <sis1:ViiteTeksti>PeVM 20/94</sis1:ViiteTeksti>
      </met:Muu2Viite>
