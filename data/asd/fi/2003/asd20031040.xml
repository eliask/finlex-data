  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Eduskunnan päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>suostumuksen antamisesta toimenpiteille, joiden seurauksena valtio voi luopua enemmistöomistuksesta Fortum Oyj:n öljyliiketoiminnassa</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <sis:Tyhja/>
      </saa:Johtolause>
      <saa:Pykalisto>
        <sis:Katkoviiva/>
        <te> Hallitus on antanut eduskunnalle esityksensä suostumuksen antamisesta toimenpiteille, joiden seurauksena valtio voi luopua enemmistöomistuksesta Fortum Oyj:n öljyliiketoiminnassa (HE 94/2003 vp).</te>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala">
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Eduskunta on </saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
              <saa:SaadosKursiiviKooste>päättänyt antaa suostumuksensa toimenpiteille, joiden seurauksena valtio voi luopua enemmistöomistuksesta Fortum Oyj -konsernin öljyliiketoiminnassa siten, että määräysvalta, yli puolet yhtiön kaikkien osakkeiden yhteenlasketusta äänimäärästä, öljyliiketoimintaa harjoittavassa osakeyhtiössä säilyy valtiolla yhdessä valtionyhtiö Fortum Oyj:n kanssa.</saa:SaadosKursiiviKooste>
            </saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
