          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Lisäkilpi 850. Tunneliluokka</saa:MomenttiJohdantoKooste>
            <sis:Katkoviiva/>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <sis:Palstaviiva/>
