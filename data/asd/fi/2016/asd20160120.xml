            <tau:tgroup colsep="0" cols="2" rowsep="0">
              <tau:colspec colnum="1" colname="1" colwidth="50*"/>
              <tau:colspec colnum="2" colname="2" colwidth="50*"/>
              <tau:tbody>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Valtion verojen osuus</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>29,58 %</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
