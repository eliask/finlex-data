    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>kiinteistöverolain muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>HE 174/2016</sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
        <sis1:ViiteTeksti>VaVM 12/2016</sis1:ViiteTeksti>
      </met:Muu2Viite>
      <met:Muu3Viite>
