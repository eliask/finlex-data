  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Laki">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Laki</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>oma-aloitteisten verojen verotusmenettelystä annetun lain 66 ja 72 §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Eduskunnan päätöksen mukaisesti</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> oma-aloitteisten verojen verotusmenettelystä annetun lain (768/2016) 66 §:n 2 momentti ja 72 § seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="66 §">
          <saa:PykalaTunnusKooste>66 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Kuuleminen valitusta käsiteltäessä</saa:SaadosOtsikkoKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Sen lisäksi, mitä oikeudenkäynnistä hallintoasioissa annetun lain (808/2019) 44 §:n 2 momentissa säädetään asian ratkaisemisesta asianosaista kuulematta, hallintotuomioistuin voi ratkaista valituksen kuulematta Veronsaajien oikeudenvalvontayksikköä, jos veron määrä voi verovelvollisen vaatimuksesta muuttua enintään 6 000 euroa, eikä asia ole tulkinnanvarainen tai epäselvä.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="72 §">
          <saa:PykalaTunnusKooste>72 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Oikeudenkäyntikulut</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Oikeudenkäyntikulujen korvauksesta säädetään oikeudenkäynnistä hallintoasioissa annetun lain 10 luvussa.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä laki tulee voimaan 1 päivänä tammikuuta 2020.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
