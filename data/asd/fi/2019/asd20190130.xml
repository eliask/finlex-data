    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>HE 173/2018</sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
        <sis1:ViiteTeksti>LiVM 33/2018</sis1:ViiteTeksti>
      </met:Muu2Viite>
      <met:Muu3Viite>
