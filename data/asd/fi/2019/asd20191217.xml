  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Laki">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Laki</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>työttömyysturvalain 6 ja 14 luvun muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Eduskunnan päätöksen mukaisesti</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> työttömyysturvalain (1290/2002) 6 luvun 1 §:n 1 ja 2 momentti ja 6 § sekä 14 luvun 1 §:n 2 momentti, sellaisina kuin ne ovat laissa 1049/2013, seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Luku saa1:identifiointiTunnus="6 luku">
          <saa:LukuTunnusKooste>6 luku</saa:LukuTunnusKooste>
          <saa:SaadosOtsikkoKooste>Työttömyyspäivärahan määrä ja kesto</saa:SaadosOtsikkoKooste>
          <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
            <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
            <saa:SaadosOtsikkoKooste>Työttömyyspäivärahan määrä</saa:SaadosOtsikkoKooste>
            <saa:MomenttiKooste>Peruspäiväraha on 33,33 euroa päivältä.</saa:MomenttiKooste>
            <saa:MomenttiKooste>Peruspäivärahan korotusosa on 4,74 euroa päivältä.</saa:MomenttiKooste>
            <sis:Katkoviiva/>
          </saa:Pykala>
          <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="6 §">
            <saa:PykalaTunnusKooste>6 §</saa:PykalaTunnusKooste>
            <saa:SaadosOtsikkoKooste>Lapsikorotus</saa:SaadosOtsikkoKooste>
            <saa:MomenttiKooste>Työttömyyspäivärahan saajalle, jolla on huollettavanaan 18 vuotta nuorempi lapsi, maksetaan päiväraha korotettuna lapsikorotuksella, jonka suuruus yhdestä lapsesta on 5,23 euroa, kahdesta lapsesta yhteensä 7,68 euroa ja kolmesta tai useammasta lapsesta yhteensä 9,90 euroa.</saa:MomenttiKooste>
          </saa:Pykala>
        </saa:Luku>
        <saa:Luku saa1:identifiointiTunnus="14 luku">
          <saa:LukuTunnusKooste>14 luku</saa:LukuTunnusKooste>
          <saa:SaadosOtsikkoKooste>Erinäisiä säännöksiä</saa:SaadosOtsikkoKooste>
          <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
            <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
            <saa:SaadosOtsikkoKooste>Etuuksien korotus</saa:SaadosOtsikkoKooste>
            <sis:Katkoviiva/>
            <saa:MomenttiKooste>Edellä 1 momentissa tarkoitetut määrät vastaavat sitä kansaneläkeindeksin pistelukua, jonka mukaan vuoden 2019 tammikuussa maksettavina olleiden kansaneläkkeiden suuruus on laskettu.</saa:MomenttiKooste>
          </saa:Pykala>
        </saa:Luku>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä laki tulee voimaan 1 päivänä tammikuuta 2020.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Lakia sovelletaan etuuteen, jota maksetaan lain voimaantulon jälkeiseltä ajalta.</saa:MomenttiKooste>
        </saa:Pykala>
