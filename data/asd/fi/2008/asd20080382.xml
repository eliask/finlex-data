  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>maa-ainesten ottamisesta annetun valtioneuvoston asetuksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvoston päätöksen mukaisesti, joka on tehty ympäristöministeriön esittelystä,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>lisätään</saa:SaadosKursiiviKooste> maa-ainesten ottamisesta 24 päivänä marraskuuta 2005 annettuun valtioneuvoston asetukseen (926/2005) uusi 2 a § seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 a §">
          <saa:PykalaTunnusKooste>2 a §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Kaivannaisjätteen jätehuoltosuunnitelma</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Maa-aineslain 5 a §:ssä tarkoitettuun kaivannaisjätteen jätehuoltosuunnitelmaan sovelletaan sen tavoitteiden ja sisällön osalta, mitä kaivannaisjätteen jätehuoltosuunnitelmasta säädetään ympäristönsuojelulain (86/2000) nojalla.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 13 päivänä kesäkuuta 2008.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
