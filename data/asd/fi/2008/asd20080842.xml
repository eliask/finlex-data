  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>äitiysavustuksesta ja adoptiotuesta annetun asetuksen 3 §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvoston päätöksen mukaisesti, joka on tehty sosiaali- ja terveysministeriön esittelystä,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> äitiysavustuksesta ja adoptiotuesta 31 päivänä lokakuuta 2002 annetun asetuksen (885/2002) 3 § seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Valtion varoista suoritettava tuki kansainvälisestä lapseksiottamisesta aiheutuneisiin kustannuksiin <saa:SaadosKursiiviKooste>(adoptiotuki)</saa:SaadosKursiiviKooste> on kohdemaasta riippuen seuraava:</saa:MomenttiKooste>
          <tau:table>
            <tau:tgroup COLS="2" COLSEP="0" ROWSEP="0" ALIGN="left" SECURITY="u">
              <tau:colspec COLNUM="1" COLNAME="1" COLWIDTH=""/>
              <tau:colspec COLNUM="2" COLNAME="2" COLWIDTH=""/>
              <tau:tbody VALIGN="TOP">
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Kohdemaa </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>Tuki</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Viro </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
