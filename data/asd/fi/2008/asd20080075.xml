  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>75</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>2008</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>poliisiasetuksen 1 §:n muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>poliisiasetuksen 1 §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvoston päätöksen mukaisesti, joka on tehty sisäasiainministeriön esittelystä,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> 8 päivänä syyskuuta 1995 annetun poliisiasetuksen (1112/1995) 1 §:n 2 momentin 1 kohdan f alakohta ja 3 momentti, sellaisina kuin ne ovat asetuksessa 181/2001, seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:SaadosValiotsikkoKooste>Poliisimiehet</saa:SaadosValiotsikkoKooste>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <sis:Katkoviiva/>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Poliisimiehiä ovat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>1) päällystöön kuuluvat:</saa:MomenttiAlakohtaKooste>
              <sis:Katkoviiva/>
              <saa:MomenttiAlakohtaKooste>
f) Poliisiammattikorkeakoulussa rehtori, ylikomisario, komisario ja poliisipäällystön tutkinnon A-osan, poliisipäällystön virkatutkinnon tai poliisipäällystön tutkinnon suorittanut opettaja;
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
          </saa:KohdatMomentti>
          <saa:MomenttiKooste>Edellä 1 momentissa tarkoitettuja poliisimiehiä ovat myös valtion hätäkeskuslaitoksen ja Hallinnon tietotekniikkakeskuksen poliisimiehet.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä maaliskuuta 2008.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Ennen asetuksen voimaantuloa voidaan ryhtyä asetuksen täytäntöönpanon edellyttämiin toimenpiteisiin.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
