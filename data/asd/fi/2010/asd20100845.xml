  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>845</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>2010</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>maitokiintiöistä annetun maa- ja metsätalousministeriön asetuksen 2 ja 14 §:n muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Maa- ja metsätalousministeriön asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>maitokiintiöistä annetun maa- ja metsätalousministeriön asetuksen 2 ja 14 §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Maa- ja metsätalousministeriön päätöksen mukaisesti</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> maitokiintiöistä annetun maa- ja metsätalousministeriön asetuksen (715/2009) 2 §:n 1 momentin 8 kohta ja 14 §:n 2 momentti seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Määritelmät</saa:SaadosOtsikkoKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKohtaKooste>8) toimivaltaisella viranomaisella sitä elinkeino-, liikenne- ja ympäristökeskusta, jonka toimialueella maatilan talouskeskus sijaitsee. Jos maatilalla ei ole talouskeskusta, toimivaltainen viranomainen on se elinkeino-, liikenne- ja ympäristökeskus, jonka alueella pääosa maatilan pelloista sijaitsee.</saa:MomenttiKohtaKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="14 §">
          <saa:PykalaTunnusKooste>14 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Toimivaltaisen viranomaisen tehtävät</saa:SaadosOtsikkoKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Tässä asetuksessa toimivaltaiselle viranomaiselle säädetyt tehtävät hoitaa Ahvenanmaalla Ahvenanmaan valtionvirasto.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 6 päivänä lokakuuta 2010. Ennen asetuksen voimaantuloa voidaan ryhtyä asetuksen täytäntöönpanon edellyttämiin toimenpiteisiin.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
