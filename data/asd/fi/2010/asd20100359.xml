    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>sairausvakuutuslain muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>HE 34/2010</sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
