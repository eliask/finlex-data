  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>eläinyksiköistä eräissä maatalouden tuissa annetun valtioneuvoston asetuksen 2 §:n ja liitteen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvoston päätöksen mukaisesti</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> eläinyksiköistä eräissä maatalouden tuissa annetun valtioneuvoston asetuksen (45/2015) 2 §:n 13 ja 14 kohta sekä liite, sellaisena kuin niistä on 2 §:n 13 kohta asetuksessa 1521/2015 ja liite asetuksessa 917/2016, seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Määritelmät</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Tässä asetuksessa tarkoitetaan:</saa:MomenttiJohdantoKooste>
            <sis:Katkoviiva/>
            <saa:MomenttiKohtaKooste>13) kilillä vähintään kahden kuukauden ja enintään vuoden ikäistä vuohta, ei kuitenkaan kääpiövuohta;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>14) karitsalla vähintään kahden kuukauden ja enintään vuoden kuukauden ikäistä lammasta;</saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 8 päivänä maaliskuuta 2017. </saa:MomenttiKooste>
          <saa:MomenttiKooste>Sitä sovelletaan kuitenkin 11 päivästä tammikuuta 2017 alkaen määräytyviin maatalouden tukiin.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
