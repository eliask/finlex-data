  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>Tullin työjärjestyksen 9 ja 11 §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Tullin pääjohtajan päätöksen mukaisesti</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> Tullin työjärjestyksen (57/2013) 9 §:n 2 momentin 6 kohta ja 11 §, sellaisina kuin ne ovat päätöksessä 1593/2015, ja</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>lisätään</saa:SaadosKursiiviKooste> 9 §:n 2 momenttiin siitä päätöksellä 1593/2015 kumotun 7 kohdan tilalle uusi 7 kohta seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="9 §">
          <saa:PykalaTunnusKooste>9 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Hallinto-osasto</saa:SaadosOtsikkoKooste>
          <sis:Katkoviiva/>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Osasto vastaa:</saa:MomenttiJohdantoKooste>
            <sis:Katkoviiva/>
            <saa:MomenttiKohtaKooste>6) Tullimuseon toiminnasta;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>7) organisaatioturvallisuudesta ja valmiusasioiden yhteensovittamisesta.</saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="11 §">
          <saa:PykalaTunnusKooste>11 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Esikunta</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Esikunta hoitaa oikeudellista asiantuntemusta vaativia erityistehtäviä ja sovittaa yhteen kansainvälisiä asioita. Euroopan unionin tietosuoja-asetuksessa (EU) 2016/679 tarkoitettu tietosuojavastaava toimii esikunnassa.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 12 päivänä joulukuuta 2017.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
