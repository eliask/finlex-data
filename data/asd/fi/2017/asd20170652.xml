  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>652</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>2017</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>kulttuuriesineiden maastaviennin rajoittamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>kulttuuriesineiden maastaviennin rajoittamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvoston päätöksen mukaisesti säädetään kulttuuriesineiden maastaviennin rajoittamisesta annetun lain (933/2016) 6 §:n 1 momentin ja 9 §:n 1 momentin nojalla:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Vientilupalomakkeen sisältö</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Museoviraston vahvistaman vientilupaa koskevan lomakkeen tulee sisältää seuraavat tiedot:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>1) hakijan nimi ja yhteystiedot;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>2) kulttuuriesineen omistajan nimi ja yhteystiedot;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>3) maa, johon kulttuuriesine on tarkoitus viedä;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>4) kulttuuriesineen valmistusaika ja -paikka, valmistusmateriaali, tekijä tai valmistaja sekä kulttuuriesineen historia ja Suomessa oloaika;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>5) kuuluuko kulttuuriesine ulkomaille muuttavan henkilön koti-irtaimistoon;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>6) onko ulkomailla asuva henkilö saanut kulttuuriesineen perintönä tai osituksen nojalla;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>7) haetaanko lupaa määräajaksi ja hakijan esitys kulttuuriesineen Suomeen palauttamisajankohdaksi;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>8) onko kulttuuriesineelle annettu väliaikainen maastavientikielto.</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Vientilupahakemukseen liitettävät tiedot</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Vientilupaa koskevaan hakemukseen on liitettävä:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>1) valtuutus kulttuuriesineen maastavientiin, jos vientilupaa hakee muu kuin kulttuuriesineen omistaja;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>2) valokuva kulttuuriesineestä.</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:MomenttiKooste>Edellä 1 momentissa säädetyn lisäksi lupahakemukseen on liitettävä perinnönjakokirja tai ositussopimus, jos vientilupaa haetaan kulttuuriesineiden maastaviennin rajoittamisesta annetun lain (933/2016) 8 §:n 3 momentin 2 kohdan perusteella.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Voimaantulo</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä lokakuuta 2017.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
