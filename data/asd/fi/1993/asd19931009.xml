  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>1009</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1993</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>rakennushallinnon maksullisista suoritteista</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtiovarainministeriön päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>rakennushallinnon maksullisista suoritteista</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtiovarainministeriö on valtion maksuperustelain (150/92) 8 §:n nojalla päättänyt:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Valtion maksuperustelain 7 §:ssä tarkoitettuja suoritteita, jotka rakennushallinto hinnoittelee liiketaloudellisin perustein, ovat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1) rakennuttamis- ja rakentamispalvelut sekä rakennustöiden valvonta;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
2) siivouspalvelut;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
3) kiinteistönhoitopalvelut;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
4) asiantuntijapalvelut;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
5) vahtimestari-, puhelunvälitys- ja muut vastaavat palvelut; sekä
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
6) muut toimeksiantoon perustuvat palvelut.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä tammikuuta 1994. Ennen tämän päätöksen voimaantuloa voidaan ryhtyä sen täytäntöönpanon edellyttämiin toimenpiteisiin.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
