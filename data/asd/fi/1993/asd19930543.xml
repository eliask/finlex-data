  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>543</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1993</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>eräiden metallirahojen lakkauttamisesta laillisina maksuvälineinä</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtiovarainministeriön päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>eräiden metallirahojen lakkauttamisesta laillisina maksuvälineinä</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtiovarainministeriö on 16 päivänä huhtikuuta 1993 annetun rahalain (358/93) 14 §:n 1 momentin nojalla päättänyt:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tammikuun 1 päivästä 1994 lukien seuraavat metallirahat lakkaavat olemasta laillisia maksuvälineitä:</saa:MomenttiKooste>
          <saa:MomenttiKooste>vuonna 1962 ja sitä ennen liikkeeseen lasketut markka- ja pennimääräiset metallirahat,</saa:MomenttiKooste>
          <saa:MomenttiKooste>vuosina 1963-1990 liikkeeseen lasketut alumiinipronssiset 50 pennin metallirahat,</saa:MomenttiKooste>
          <saa:MomenttiKooste>vuosina 1963-1990 liikkeeseen lasketut alumiinipronssiset 20 pennin metallirahat,</saa:MomenttiKooste>
          <saa:MomenttiKooste>vuosina 1963-1982 liikkeeseen lasketut alumiinipronssiset 10 pennin metallirahat,</saa:MomenttiKooste>
          <saa:MomenttiKooste>vuosina 1983-1990 liikkeeseen lasketut alumiiniset 10 pennin metallirahat,</saa:MomenttiKooste>
          <saa:MomenttiKooste>vuosina 1963-1977 liikkeeseen lasketut kupariset 5 pennin metallirahat,</saa:MomenttiKooste>
          <saa:MomenttiKooste>vuosina 1977-1990 liikkeeseen lasketut alumiiniset 5 pennin metallirahat,</saa:MomenttiKooste>
          <saa:MomenttiKooste>vuosina 1963-1969 liikkeeseen lasketut kupariset 1 pennin metallirahat,</saa:MomenttiKooste>
          <saa:MomenttiKooste>vuosina 1969-1979 liikkeeseen lasketut alumiiniset 1 pennin metallirahat.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä tammikuuta 1994.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
