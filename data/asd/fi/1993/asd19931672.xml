  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>1672</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1993</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>maa- ja metsätalousministeriön hallinnonalan eräiden virkojen perustamisesta ja lakkauttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>maa- ja metsätalousministeriön hallinnonalan eräiden virkojen perustamisesta ja lakkauttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvosto on maa- ja metsätalousministeriön esittelystä 24 päivänä lokakuuta 1986 annetun valtion virkamieslain (755/86) 6 §:n 1 momentin, sellaisena kuin se on 3 päivänä huhtikuuta 1992 annetussa laissa (306/92), sekä metsähallituksesta 17 päivänä joulukuuta 1993 annetun lain (1169/93) 12 §:n nojalla päättänyt:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Vuoden 1994 talousarvion mukaisesti perustetaan ja lakkautetaan jäljempänä mainitut sopimuspalkkaiset virat 1 päivästä tammikuuta 1994 lukien.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Metsähallitus</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Perustetaan </saa:SaadosKursiiviKooste>yksi S 33 sopimuspalkkaluokkaan kuuluva toimitusjohtajan virka.</saa:MomenttiKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Lakkautetaan </saa:SaadosKursiiviKooste>yksi S 33 sopimuspalkkaluokkaan kuuluva pääjohtajan virka.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Kasvinjalostuslaitos</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste><saa:SaadosKursiiviKooste>Perustetaan </saa:SaadosKursiiviKooste>yksi S 27 sopimuspalkkaluokkaan kuuluva johtajan virka.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä tammikuuta 1994.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
