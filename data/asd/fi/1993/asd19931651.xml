                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>muiden 1 §:ssä tarkoitettujen vakuutus- ja eläkelaitosten osalta</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>2 000 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>6)</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>vakuutuksenvälittäjän rekisteröintiä koskeva päätös</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>1 000 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>7)</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>rekisteriote ja todistus</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>200 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>8)</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>muu hakijan pyynnöstä annettava lupa ja päätös</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>2 000 mk.</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
              </tau:tbody>
