  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Sosiaali- ja terveysministeriön päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>vakuutustarkastuksen maksullisista suoritteista</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Sosiaali- ja terveysministeriö on 21 päivänä helmikuuta 1992 annetun valtion maksuperustelain (150/92) 8 §:n ja vakuutusyhtiön toimilupamaksusta 22 päivänä joulukuuta 1993 annetun lain (1421/93) 3 §:n nojalla päättänyt:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Sosiaali- ja terveysministeriö perii suomalaisia vakuutusyhtiöitä, ensivakuutusliikettä Suomessa harjoittavia ulkomaisia vakuutusyrityksiä, vakuutusyhdistyksiä, vakuutuskassoja, eläkesäätiöitä ja vakuutuksenvälittäjiä sekä liikennevakuutuskeskusta, potilasvakuutusyhdistystä ja vakuutuskassojen yhdistystä koskevista vakuutustarkastuksen julkisoikeudellisista suoritteista tämän päätöksen 2 §:n mukaiset kiinteät maksut. Sanottuja maksuja ei kuitenkaan peritä lakisääteisen tapaturmavakuutuksen ehtojen vahvistamista koskevista päätöksistä eikä lakisääteisen työeläkevakuutuksen ehtojen ja perusteiden vahvistamista koskevista päätöksistä lukuunottamatta vakuutusmaksun ja vastuuvelan laskuperusteiden vahvistamispäätöksiä.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Vakuutustarkastuksen julkisoikeudellisista suoritteista peritään seuraavat maksut:</saa:MomenttiKooste>
          <tau:table>
            <tau:tgroup cols="3" colsep="0" rowsep="0" align="left">
              <tau:colspec colnum="1" colname="1" colwidth=""/>
              <tau:colspec colnum="2" colname="2" colwidth=""/>
              <tau:colspec colnum="3" colname="3" colwidth=""/>
              <tau:tbody valign="top">
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>1)</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>vakuutusyhtiön toimilupaa koskeva päätös:</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>uuden toimiluvan antamista koskeva päätös</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>30 000 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>voimassa olevan toimiluvan muuttamista koskeva päätös</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>15 000 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>2)</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>yhtiöjärjestyksen ja yhdistysjärjestyksen vahvistamista tai muuttamista koskeva päätös</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>2 000 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>3)</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>sääntöjen vahvistamista tai muuttamista koskeva päätös:</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>eläkesäätiöiden, eläkekassojen, vakuutuskassojen yhdistyksen ja potilasvakuutusyhdistyksen osalta</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>2 000 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>muiden vakuutuskassojen kuin eläkekassojen osalta</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>1 000 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>4)</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>sulautumista, jakautumista, yhteisömuodon muuttamista, vakuutuskannan luovuttamista ja vastuun siirtämistä koskeva päätös:</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>vakuutusyhtiöiden osalta</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>10 000 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>vakuutusyhdistysten, eläkesäätiöiden ja eläkekassojen osalta</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>5 000 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>muiden vakuutuskassojen kuin eläkekassojen osalta</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>2 500 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>5)</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>vakuutusteknisten perusteiden vahvistamista koskeva päätös:</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>muiden vakuutuskassojen kuin eläkekassojen osalta</sis:KappaleKooste>
                  </tau:entry>
