        <sis1:ViiteTeksti>HE 180/93</sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
        <sis1:ViiteTeksti>LiVM 13/93</sis1:ViiteTeksti>
      </met:Muu2Viite>
