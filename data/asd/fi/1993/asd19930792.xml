        <saa:SaadosNimekeKooste>Autorekisterikeskuksen suoritteiden maksuista annetun valtioneuvoston päätöksen6 §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvosto on liikenneministeriön esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muuttanut</saa:SaadosKursiiviKooste> Autorekisterikeskuksen suoritteiden maksuista 30 päivänä joulukuuta 1992 annetun valtioneuvoston päätöksen (1704/92) 6 §:n 1 momentin seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="6 §">
          <saa:PykalaTunnusKooste>6 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Rekisteriote</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Asiakkaalle postitse toimitettavasta rekisteriotteesta peritään:</saa:MomenttiKooste>
          <tau:table>
            <tau:tgroup cols="2" colsep="1" rowsep="1">
              <tau:tbody>
                <tau:row>
                  <tau:entry namest="1" nameend="2">
                    <sis:KappaleKooste align="left" valign="top"/>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry namest="1" nameend="2">
                    <sis:KappaleKooste align="left" valign="top">1) jos rekisteri-ilmoitus on jätetty ajoneuvojen</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry namest="1" nameend="2">
                    <sis:KappaleKooste align="left" valign="top">rekisteröinnistä annetussa asetuksessa</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry namest="1" nameend="2">
                    <sis:KappaleKooste align="left" valign="top">(1703/92) säädetyllä tavalla</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>postitoimipaikkaan</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>60 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry namest="1" nameend="2">
                    <sis:KappaleKooste align="left" valign="top">2) jos rekisteriote toimitetaan suorarekisteröinnissä</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry namest="1" nameend="2">
                    <sis:KappaleKooste align="left" valign="top">jätetyn rekisteri-ilmoituksen perusteella,</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry namest="1" nameend="2">
                    <sis:KappaleKooste align="left" valign="top">suorarekisteröinti-ilmoituksen tekijältä</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>jälkikäteen</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>40 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>3) muussa tapauksessa</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>60 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry namest="1" nameend="2">
                    <sis:KappaleKooste align="left" valign="top"/>
                  </tau:entry>
                </tau:row>
