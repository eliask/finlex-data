    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>Euroopan parlamentin ja neuvoston direktiivi 2003/15/EY (32003L0066); EUVL N:o L 66, 11.3.2003, s. 26</sis1:ViiteTeksti>
      </met:Muu1Viite>
    </met:AsiakirjaViitteet>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Työ- ja elinkeinoministeriön asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>kosmeettisten valmisteiden eläinkoekielloista</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Työ- ja elinkeinoministeriön päätöksen mukaisesti säädetään kosmeettisista valmisteista 21 päivänä tammikuuta 2005 annetun lain (22/2005) 7 ja 8 §:n nojalla:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Eläinkokeiden käyttö kosmeettisten valmisteiden testauksessa</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Kosmeettisista valmisteista annetun lain 7 §:n 2 momentissa tarkoitettu kielto kosmeettisten ainesosien ja ainesosien yhdistelmien testaamisesta eläinkokeella tulee voimaan 11 päivänä maaliskuuta 2009.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Eläinkokeilla testattujen kosmeettisten valmisteiden markkinoille saattaminen</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Kosmeettisista valmisteista annetun lain 8 §:ssä tarkoitettu kielto eläinkokeilla testattujen kosmeettisten valmisteiden markkinoille saattamisesta tulee voimaan 11 päivänä maaliskuuta 2009.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Voimaantulo</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 9 päivänä maaliskuuta 2009.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
