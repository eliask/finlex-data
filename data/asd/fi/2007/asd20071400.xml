  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>1400</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>2007</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>käräjäoikeuksien tuomiopiireistä annetun valtioneuvoston asetuksen 2 §:n muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>käräjäoikeuksien tuomiopiireistä annetun valtioneuvoston asetuksen 2 §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvoston päätöksen mukaisesti, joka on tehty oikeusministeriön esittelystä,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>kumotaan</saa:SaadosKursiiviKooste> käräjäoikeuksien tuomiopiireistä 7 päivänä joulukuuta 2006 annetun valtioneuvoston asetuksen (1110/2006) 2 §:n Heinolan, Imatran ja Loviisan käräjäoikeuksia koskevat kohdat sekä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> 2 §:n Lahden, Lappeenrannan, Mikkelin ja Porvoon käräjäoikeuksia koskevat kohdat seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Kunnat kuuluvat käräjäoikeuksien tuomiopiireihin seuraavasti:</saa:MomenttiJohdantoKooste>
            <sis:Katkoviiva/>
            <saa:MomenttiKohtaKooste><saa:SaadosKursiiviKooste>Lahden käräjäoikeus</saa:SaadosKursiiviKooste>: Artjärvi, Asikkala, Hartola, Heinola, Hollola, Hämeenkoski, Kärkölä, Lahti, Nastola, Orimattila, Padasjoki, Pukkila ja Sysmä;

</saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
            <saa:MomenttiKohtaKooste><saa:SaadosKursiiviKooste>Lappeenrannan käräjäoikeus</saa:SaadosKursiiviKooste>: Imatra, Joutseno, Lappeenranta, Lemi, Luumäki, Parikkala, Rautjärvi, Ruokolahti, Savitaipale, Suomenniemi, Taipalsaari ja Ylämaa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste><saa:SaadosKursiiviKooste>Mikkelin käräjäoikeus</saa:SaadosKursiiviKooste>: Hirvensalmi, Joroinen, Juva, Mikkeli, Mäntyharju, Pertunmaa, Puumala, Rantasalmi, Ristiina ja Sulkava;

</saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
            <saa:MomenttiKohtaKooste><saa:SaadosKursiiviKooste>Porvoon käräjäoikeus</saa:SaadosKursiiviKooste>: Askola, Lapinjärvi, Liljendal, Loviisa, Myrskylä, Pernaja, Pornainen, Porvoo, Pyhtää, Ruotsinpyhtää ja Sipoo;

</saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä lokakuuta 2008.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
