  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>1106</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>2007</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>maatilatalouden kehittämisrahastosta annetun asetuksen muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>maatilatalouden kehittämisrahastosta annetun asetuksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvoston päätöksen mukaisesti, joka on tehty maa- ja metsätalousministeriön esittelystä,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>kumotaan</saa:SaadosKursiiviKooste> maatilatalouden kehittämisrahastosta 29 päivänä joulukuuta 1995 annetun valtioneuvoston asetuksen (1754/1995) 4 §, sekä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> 2 §:n 2 ja 3 momentti, sellaisena kuin niistä on 2 §:n 2 momentti asetuksessa 985/1999, seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Johtokunnassa on pysyvinä asiantuntijoina kolme maaseutuelinkeinojen rahoituslain (329/1999) mukaisia lainoja myöntävien luottolaitosten edustajaa sekä enintään kolme maa- ja metsätalousministeriön, Maaseutuviraston tai työvoima- ja elinkeinokeskusten maaseutuosastojen virkamiestä.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Johtokunta valitsee yhden tai useamman sihteerin, joiden tulee olla maa- ja metsätalousministeriön tai Maaseutuviraston virkamiehiä.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 10 päivänä joulukuuta 2007.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
