  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>1681</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1991</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>ammatillisen koulutuksen järjestämisestä väliaikaisesti annetun valtioneuvoston päätöksen muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>ammatillisen koulutuksen järjestämisestä väliaikaisesti annetun valtioneuvoston päätöksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvosto on opetusministeriön esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muuttanut</saa:SaadosKursiiviKooste> ammatillisen koulutuksen järjestämisestä väliaikaisesti 18 päivänä elokuuta 1988 annetun valtioneuvoston päätöksen (761/88) 3 §:n 1 momentin, sellaisena kuin se on 30 päivänä marraskuuta 1989 annetussa valtioneuvoston päätöksessä (1066/89), seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Koulutus voidaan aloittaa lääninhallituksen luvalla sen määräämänä ajankohtana. Päätös, joka koskee toiseen oppilaitosmuotoon kuuluvaa ammatillista peruskoulutusta tai koulutusastetta, jota asianomaisessa oppilaitoksessa ei ole, on kuitenkin saatettava opetusministeriön vahvistettavaksi.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä tammikuuta 1992.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Ennen tämän päätöksen voimaantuloa voidaan ryhtyä sen täytäntöönpanon edellyttämiin toimenpiteisiin.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
