              <tau:colspec colnum="2" colname="2" colwidth=""/>
              <tau:tbody valign="top">
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>1) ansioja liikennelentäjän lupakirjaan ja kelpuutuksiin liittyvät tarkastuslennot</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>800 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>2) yksityislentäjän lupakirjaan ja kelpuutuksiin liittyvät tarkastuslennot</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>700 mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
