  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>528</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1991</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>pellon metsitysavustuksista vuonna 1991</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Maa- ja metsätalousministeriön päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>pellon metsitysavustuksista vuonna 1991</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Maa- ja metsätalousministeriö on 10 päivänä tammikuuta 1991 pellon metsityspalkkioista vuonna 1991 annetun valtioneuvoston päätöksen (24/91) 8§:n nojalla päättänyt:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Avustus omatoimiseen metsittämiseen </saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Vuonna 1991 hyväksyttyjen metsittämishakemusten perusteella viljelijälle saadaan korvata pellon omatoimisesta metsittämisestä vuonna 1991 aiheutuvat kustannukset seuraavasti:</saa:MomenttiKooste>
          <tau:table>
            <tau:tgroup cols="2" colsep="0" rowsep="0" align="left">
              <tau:colspec colnum="1" colname="1" colwidth=""/>
              <tau:colspec colnum="2" colname="2" colwidth=""/>
              <tau:tbody valign="top">
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>Avustus markkaa/hehtaari</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>1.Luontainen metsittäminen </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>2 200</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>2. Tavallisen kivennäismaapellon metsittäminen viljelemällä </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>4 500</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>3. Suopellon ja muun vaikeasti metsitettävän pellon metsittäminen viljelemällä </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>6 000 </sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
              </tau:tbody>
            </tau:tgroup>
          </tau:table>
          <saa:MomenttiKooste>Jos omatoiminen metsittäminen tehdään viljelijän metsälautakunnalla tai metsänhoitoyhdistyksellä teettämän suunnitelman mukaan, saadaan edellä 1 kohdassa tarkoitettua määrää korottaa 500 markalla sekä 2 ja 3 kohdassa tarkoitettuja määriä 700 markalla hehtaarilta.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Voimaantulo</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 20 päivänä maaliskuuta 1991.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
