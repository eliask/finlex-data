  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>luonnontieteellisen perustutkinnon järjestämisestä annetun asetuksen 11 a§:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Opetusministerin esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> luonnontieteellisen perustutkinnon järjestämisestä 29 päivänä kesäkuuta 1979 annetun asetuksen (602/79) 11 a§, sellaisena kuin se on 14 päivänä kesäkuuta 1985 annetussa asetuksessa (493/85), seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="11 a §">
          <saa:PykalaTunnusKooste>11 a §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Tampereen yliopistossa on aineenopettajien koulutuksessa luonnontieteellisellä koulutusalalla seuraavat opetettavien aineiden yhdistelmät:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1)matematiikka ja tietotekniikka; sekä
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
2)matematiikka ja fysiikka.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä elokuuta 1991.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Ennen tämän asetuksen voimaantuloa voidaan ryhtyä sen täytäntöönpanon edellyttämiin toimenpiteisiin.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
