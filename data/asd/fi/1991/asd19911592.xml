  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>1592</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1991</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>eräiden korkeakoulujen professorin ja apulaisprofessorin virkojen opetusalojen muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>eräiden korkeakoulujen professorin ja apulaisprofessorin virkojen opetusalojen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Opetusministerin esittelystä säädetään 24 päivänä lokakuuta 1986 annetun valtion virkamieslain (755/86) 7 §:n nojalla:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Jyväskylän yliopiston palkkausluokkaan A 28 kuuluva liikuntahygienian ja kansanterveyden professorin virka muutetaan samaan palkkausluokkaan kuuluvaksi gerontologian ja kansanterveyden professorin viraksi.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Kuopion yliopiston palkkausluokkaan A 28 kuuluva fysiologisen kemian professorin virka muutetaan samaan palkkausluokkaan kuuluvaksi lääketieteellisen biokemian professorin viraksi.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Teknillisen korkeakoulun palkkausluokkaan A 28 kuuluva metalliopin, erityisesti metallien muokkauksen ja lämpökäsittelyn professorin virka muutetaan samaan palkkausluokkaan kuuluvaksi metallija materiaaliopin, erityisesti materiaalien muokkauksen ja lämpökäsittelyn professorin viraksi.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Oulun yliopiston palkkausluokkaan A 26 kuuluva rakennetekniikan apulaisprofessorin virka muutetaan samaan palkkausluokkaan kuuluvaksi talonrakentamistekniikan apulaisprofessorin viraksi.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="5 §">
          <saa:PykalaTunnusKooste>5 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Helsingin kauppakorkeakoulun palkkausluokkaan A 26 kuuluva liiketaloustieteen apulaisprofessorin virka opetusalana markkinointi, erityisesti kansainvälinen markkinointi, muutetaan samaan palkkausluokkaan kuuluvaksi kansainvälisen markkinoinnin apulaisprofessorin viraksi.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="6 §">
          <saa:PykalaTunnusKooste>6 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä tammikuuta 1992.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
