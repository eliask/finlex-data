  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>599</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1991</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>eräiden tilojen ja alueiden siirtämisestä Tohmajärven kunnasta Pyhäselän kuntaan</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>eräiden tilojen ja alueiden siirtämisestä Tohmajärven kunnasta Pyhäselän kuntaan</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvosto on kuntajaosta 21 päivänä tammikuuta 1977 annetun lain (73/77) 14§:n 1 momentin nojalla sisäasiainministeriön esittelystä päättänyt:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tohmajärven kunnasta siirretään Pyhäselän kuntaan seuraavat tilat ja alueet:</saa:MomenttiKooste>
          <saa:MomenttiKooste>Onkamon kylästä kokonaan tilat Jääskensaari 52:3, Saarenpää 52:9, Vatukko 52:13, Uittoniemi 52:14, Särkiniemi 52:15, Pykäläsaari 52:17, Pöllösaari 52:19, Repoharju 52:20, Onkiniemi 52:21, Sumpinniemi 52:23, Pikkupöllö 52:24, Koivusaari 52:26, Sulkuniemi 52:27, Rauhala 52:30, Reposaari 52:31 ja Liisanluoto 110 sekä Onkamo Sintsi jakokunnan yhteisestä vesialueesta Reposaaren ympärillä oleva erillinen palsta sillä olevine omistussuhteiltaan selvittämättömine, rekisterikartalle tunnuksin D<sis1:YlaindeksiTeksti>16</sis1:YlaindeksiTeksti> ja D<sis1:YlaindeksiTeksti>17</sis1:YlaindeksiTeksti> merkittyine saarineen.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Siirrot eivät aiheuta muutosta kuntien kielelliseen jaotukseen.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
            <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä tammikuuta 1992.</saa:MomenttiKooste>
          </saa:Pykala>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
