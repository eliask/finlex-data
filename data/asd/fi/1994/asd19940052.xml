  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>52</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1994</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>kuluttajavalituslautakunnan jaostoissa käsiteltävien asioiden jakoperusteista annetun valtioneuvoston päätöksen muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>kuluttajavalituslautakunnan jaostoissa käsiteltävien asioiden jakoperusteista annetun valtioneuvoston päätöksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvosto on kauppa- ja teollisuusministeriön esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muuttanut</saa:SaadosKursiiviKooste> kuluttajavalituslautakunnan jaostoissa käsiteltävien asioiden jakoperusteista 30 päivänä elokuuta 1978 annetun valtioneuvoston päätöksen (659/78) 1 §:n 1 momentin ja 2 §:n 9 momentin, sellaisena kuin ne ovat 26 päivänä syyskuuta 1985 annetussa valtioneuvoston päätöksessä (772/85), sekä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>lisännyt</saa:SaadosKursiiviKooste> mainittuun 2 §:ään uuden 10 momentin seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Kuluttajavalituslautakunta toimii jakautuneena kymmeneen jaostoon.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Jaosto IX käsittelee asiat, jotka koskevat vakuutuksia ja terveydenhoitopalveluksia.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Jaosto X käsittelee asiat, jotka koskevat muita kuin edellä mainittuja palveluksia.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 26 päivänä tammikuuta 1994.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
