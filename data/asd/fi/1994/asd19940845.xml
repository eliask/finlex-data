  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Laki">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Laki</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>kuluttajansuojasta kiinteistönvälityksessä annetun lain 15 §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Eduskunnan päätöksen mukaisesti</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> kuluttajansuojasta kiinteistönvälityksessä 15 päivänä heinäkuuta 1988 annetun lain (686/88) 15 §:n 1 momentti seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="15  §">
          <saa:PykalaTunnusKooste>15  §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Käsirahan vastaanottaminen</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Vastaanottaessaan ostotarjouksen välitettävänä olevasta kohteesta saa kiinteistönvälittäjä ottaa tarjouksen tekijältä toimeksiantajan lukuun käsirahan, jos tästä on toimeksiantosopimuksessa sovittu. Sen jälkeen kun välittäjä on vastaanottanut käsirahan, hän ei saa ottaa toista käsirahaa tai tarjousta ennen kuin ensiksi mainittu käsiraha on palautettu tarjouksen tekijälle tai ennen kuin on käynyt selväksi, että käsiraha jää toimeksiantajan hyväksi.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä laki tulee voimaan 1 päivänä syyskuuta 1995.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
