  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>1471</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1994</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>alkoholi- ja alkoholijuomaverosta</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>HE 237/94</sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
        <sis1:ViiteTeksti>VaVM 90/94</sis1:ViiteTeksti>
      </met:Muu2Viite>
      <met:Muu3Viite>
        <sis1:ViiteTeksti>Neuvoston direktiivi 92/83/ETY; EYVL N:o L 316, 31.10.1992, s. 21, 92/84/ETY; EYVL N:o L 316, 31.10.1992, s. 29</sis1:ViiteTeksti>
      </met:Muu3Viite>
