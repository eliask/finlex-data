  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Laki">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Laki</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>yhteistoiminnasta valtion virastoissa ja laitoksissa annetun lain 15 a §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Eduskunnan päätöksen mukaisesti</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> yhteistoiminnasta valtion virastoissa ja laitoksissa 1 päivänä heinäkuuta 1988 annetun lain (651/88) 15 a §:n 1 momentti,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste>sellaisena kuin se on 14 päivänä joulukuuta 1990 annetussa laissa (1102/90), seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="15 a  §">
          <saa:PykalaTunnusKooste>15 a  §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Hyvitys</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Milloin 7 §:n 1―5 tai 10 kohdassa tarkoitettu asia on tahallisesti tai ilmeisestä huolimattomuudesta  ratkaistu  noudattamatta,  mitä 8, 8 a tai 9 §:ssä säädetään, ja virkamies tai työntekijä on asian yhteydessä osa-aikaistettu taikka lomautettu tai irtisanottu, on tällä oikeus saada työnantajalta hyvityksenä enintään 20 kuukauden palkka.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä laki tulee voimaan 15 päivänä joulukuuta 1994.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
