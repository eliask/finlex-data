  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>veropiireistä annetun valtioneuvoston päätöksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvosto on 12 päivänä joulukuuta 1958 annetun verotuslain (482/58) 5 §:n nojalla sellaisena kuin se on 19 päivänä marraskuuta 1993 annetussa laissa (963/93), valtiovarainministeriön esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>kumonnut</saa:SaadosKursiiviKooste> veropiireistä 25 päivänä helmikuuta 1960 annetun valtioneuvoston päätöksen (122/60) Pielaveden veropiiriä koskevan kohdan, ja</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muuttanut</saa:SaadosKursiiviKooste> Siilinjärven veropiiriä koskevan kohdan, seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <te>Kuopion lääni:</te>
        <sis:Katkoviiva/>
        <te><saa:SaadosKursiiviKooste>Siilinjärven veropiiri</saa:SaadosKursiiviKooste> : Keiteleen, Maaningan, Pielaveden ja Siilinjärven kunnat;</te>
        <sis:Katkoviiva/>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä tammikuuta 1995.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
