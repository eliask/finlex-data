    <met:Nimeke>
      <met1:NimekeTeksti>maataloustuotteiden hinnanerokorvauksista annetun lain muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Laki">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Laki</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>maataloustuotteiden hinnanerokorvauksista annetun lain muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Eduskunnan päätöksen mukaisesti</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> maataloustuotteiden hinnanerokorvauksista 30 päivänä joulukuuta 1993 annetun lain (1521/93) 3 §:n 2 momentti ja 4 §:n 3 momentti sekä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>lisätään</saa:SaadosKursiiviKooste> 8 §:ään uusi 3 momentti ja 17 §:ään uusi 3 momentti seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Maataloustuotteet ja välivalmisteet, joista voidaan maksaa hinnanerokorvausta</saa:SaadosOtsikkoKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Hinnanerokorvausta voidaan maksaa seuraavista välivalmisteista 4 §:ssä tarkoitettuihin tuotteisiin sisältyvinä:</saa:MomenttiKooste>
          <tau:table>
            <tau:tgroup cols="2" colsep="1" rowsep="1">
              <tau:tbody>
                <tau:row>
                  <tau:entry namest="1" nameend="2">
                    <sis:KappaleKooste align="left" valign="top"/>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Tullitariffin ryhmä/nimike</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>Tuote</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>11.08</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>Tärkkelys</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>15.07-15.15:stä ja 15.16</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>Kasviöljyt, muut kuin raa'at</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>15.17</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>Margariini</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>17.</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>Sokeri (paitsi kemiallisesti</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>puhdas maltoosi), glukoosi,</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>glukoosisiirappi ja ihmis-</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>ravinnoksi soveltuva melassi</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry namest="1" nameend="2">
                    <sis:KappaleKooste align="left" valign="top"/>
                  </tau:entry>
                </tau:row>
              </tau:tbody>
            </tau:tgroup>
          </tau:table>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Tuotteet, joihin sisältyvistä maataloustuotteista ja välivalmisteista voidaan maksaa hinnanerokorvausta</saa:SaadosOtsikkoKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Jos maasta viety tuote on osoitettu valmistetuksi raaka-aineesta, josta on maahantuonnin yhteydessä kannettu tulli, tuontimaksu, elintarviketuotteiden valmistevero, ravintorasvavero, sokerivero tai rehujen rasva- ja valkuaisvero, hinnanerokorvauksena voidaan suorittaa määrä, joka vastaa sanotusta raaka-aineesta kannettua veroa tai maksua, vähennettynä tuonnin perusteella tehtävän arvonlisäveron alkutuotevähennyksen määrällä tai maahantuojalle maksettavan arvonlisäveron palautuksen määrällä (<saa:SaadosKursiiviKooste>takaisinmaksu</saa:SaadosKursiiviKooste>).</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="8 §">
          <saa:PykalaTunnusKooste>8 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Määräkiintiöt ja rajoitukset</saa:SaadosOtsikkoKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Mitä edellä 1 momentissa on säädetty maataloustuotteiden hinnanerokorvauksista, koskee soveltuvin osin myös tämän lain 2 §:n 2 momentissa tarkoitettuja hinnanalennuskorvauksia.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="17 §">
          <saa:PykalaTunnusKooste>17 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Oikaisu, takaisinperintä, valvonta ja salassapitovelvollisuus</saa:SaadosOtsikkoKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Mitä edellä 1 ja 2 momentissa on säädetty hinnanerokorvauksista tai jäljempänä 18 §:n 1-4 momentissa on säädetty tarkastuksesta, koskee soveltuvin osin myös hinnanalennuskorvauksia.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä laki tulee voimaan 20 päivänä heinäkuuta 1994.</saa:MomenttiKooste>
        </saa:Pykala>
