      <met1:NimekeTeksti>teleliikenteen verottamisesta annetun lain muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>HE 159/94</sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
