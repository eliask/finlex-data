    <met:Nimeke>
      <met1:NimekeTeksti>syöpäsairauden vaaraa aiheuttavista tekijöistä annetun työministeriön päätöksen 1 §:n ja liitteen muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Sosiaali- ja terveysministeriön asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>syöpäsairauden vaaraa aiheuttavista tekijöistä annetun työministeriön päätöksen 1 §:n ja liitteen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste> Sosiaali- ja terveysministeriön päätöksen mukaisesti</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> syöpäsairauden vaaraa aiheuttavista tekijöistä annetun työministeriön päätöksen (838/1993) 1 § ja liitteen B osan 3 ryhmän otsikko sekä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>lisätään</saa:SaadosKursiiviKooste> päätösen liitteen B osan 3 ryhmään uusia syöpäsairauden vaaraa aiheuttavia aineita seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Työnantajan on työhön liittyvän syöpävaaran torjunnasta annetun valtioneuvoston asetuksen (716/2000) 3 §:n mukaisesti luetteloitava tämän päätöksen liitteessä mainituille syöpäsairauden vaaraa aiheuttavile työmenetelmille ja aineille altistuvat työntekijät.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä tammikuuta 2001.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
