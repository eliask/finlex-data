  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>koneiden turvallisuudesta annetun valtioneuvoston päätöksen 15 §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvoston päätöksen mukaisesti, joka on tehty sosiaali- ja terveysministeriön esittelystä,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> koneiden turvallisuudesta 21 päivänä joulukuuta 1994 annetun valtioneuvoston päätöksen (1314/1994) 15 §:n 4 momentti seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="15 §">
          <saa:PykalaTunnusKooste>15 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Valvontaviranomaiset</saa:SaadosOtsikkoKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Milloin koneeseen sovelletaan sähköturvallisuuslakia (410/1996) ja sen nojalla annettuja säännöksiä, tämän päätöksen noudattamista valvoo osaltaan sähköturvallisuuslaissa tarkoitettu sähköturvallisuusviranomainen.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä lokakuuta 2000.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
