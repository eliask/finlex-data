                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Päätöksen nimike</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>MMM:n määräyskokoelman n:o </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>antopäivä </sis:KappaleKooste>
                  </tau:entry>
