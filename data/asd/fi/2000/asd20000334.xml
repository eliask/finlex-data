  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Ilmoitus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Maa- ja metsätalousministeriön ilmoitus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>eräistä päätöksistä</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Maa- ja metsätalousministeriö ilmoittaa Suomen säädöskokoelmasta 17 päivänä lokakuuta 1980 annetun asetuksen (696/1980) 2 b §:n nojalla, sellaisena kuin se on 22 päivänä joulukuuta 1993 annetussa asetuksessa (1364/1993):</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala">
          <saa:MomenttiKooste>Maa- ja metsätalousministeriö on antanut seuraavan päätöksen:</saa:MomenttiKooste>
          <tau:table>
            <tau:tgroup COLS="4" COLSEP="0" ROWSEP="0" ALIGN="left" SECURITY="u">
              <tau:colspec COLNUM="1" COLNAME="1" COLWIDTH=""/>
