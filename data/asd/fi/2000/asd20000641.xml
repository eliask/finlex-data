  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>641</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>2000</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>passintarkastuspaikoista annetun asetuksen muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>passintarkastuspaikoista annetun asetuksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvoston päätöksen mukaisesti, joka on tehty sisäasiainministeriön esittelystä,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> passintarkastuspaikoista 27 päivänä toukokuuta 1992 annetun asetuksen (461/1992) 3 §:n 2 momentti, 6 §:n 2 momentti ja 7 §,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste>sellaisina kuin ne ovat 6 §:n 2 momentti asetuksessa 403/1993 ja 3 §:n 2 momentti sekä 7 § asetuksessa 1530/1995, ja</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>lisätään </saa:SaadosKursiiviKooste>asetukseen uusi 2 a § seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 a §">
          <saa:PykalaTunnusKooste>2 a §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Lentopaikat</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Varsinainen passintarkastuspaikka yksinomaan helikopterilentoliikenteelle on Helsinki―Hernesaari.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Passintarkastuspaikat Suomen ja Ruotsin välisellä rajalla</saa:SaadosOtsikkoKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Lisäksi Suomen ja Ruotsin kansalaisilla, joilla ei ole mukanaan tuontirajoitusten alaista tavaraa eikä muuta kuin sellaista henkilökohtaiseen käyttöön tarkoitettua tavaraa, jota on oikeus verottomana tuoda maahan, on oikeus maahantuloon ja maastalähtöön Tornion kaupungin eteläpuolella Hellälän ja merirajan päätepisteen välisellä rajaosuudella.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="6 §">
          <saa:PykalaTunnusKooste>6 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Satamat</saa:SaadosOtsikkoKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Huvialusten passintarkastuspaikkoja ovat satamista ainoastaan Kaskinen, Kemi, Maarianhamina, Nuijamaa, Pietarsaari, Pori, Rauma ja Uusikaupunki.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="7 §">
          <saa:PykalaTunnusKooste>7 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Meri- ja rajavartioasemat</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Huvialusten passintarkastuspaikkoja 6 §:n 2 momentissa mainittujen satamien lisäksi ovat Bågaskärin, Enskärin, Glosholmenin, Haapasaaren, Hangon, Hiittisten, Jussarön, Kalajoen, Kokkolan, Kotkan, Kummelgrundin, Kökarin, Maarianhaminan, Mäntyluodon, Nauvon, Orrengrundin, Pirttisaaren, Porkkalan, Raahen, Röytän, Storklubbin, Suomenlinnan, Susiluodon, Valassaarten, Vallgrundin ja Virpiniemen merivartioasemat sekä Santion ja Utön passintarkastuspaikat.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 5 päivänä heinäkuuta 2000.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
