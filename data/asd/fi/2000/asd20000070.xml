  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>70</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>2000</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>käräjäoikeuksien tuomiopiireistä annetun valtioneuvoston päätöksen 2 §:n muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>käräjäoikeuksien tuomiopiireistä annetun valtioneuvoston päätöksen 2 §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvosto on oikeusministeriön esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>kumonnut</saa:SaadosKursiiviKooste> käräjäoikeuksien tuomiopiireistä 19 päivänä joulukuuta 1991 annetun valtioneuvoston päätöksen (1623/1991) 2 §:n Iitin ja Juvan käräjäoikeuksia koskevat kohdat sekä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muuttanut</saa:SaadosKursiiviKooste> 2 §:n Kouvolan ja Mikkelin käräjäoikeuksia koskevat kohdat seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Kunnat kuuluvat käräjäoikeuksien tuomiopiireihin seuraavasti:</saa:MomenttiJohdantoKooste>
            <sis:Katkoviiva/>
            <saa:MomenttiKohtaKooste>
              <saa:SaadosKursiiviKooste>Kouvolan käräjäoikeus:</saa:SaadosKursiiviKooste>
            </saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
Anjalankoski, Elimäki, Iitti, Jaala, Kouvola, Kuusankoski ja Valkeala.

</saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
            <saa:MomenttiKohtaKooste>
              <saa:SaadosKursiiviKooste>Mikkelin käräjäoikeus:</saa:SaadosKursiiviKooste>
            </saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
Anttola, Hirvensalmi, Joroinen, Juva, Mikkeli, Mikkelin maalaiskunta, Puumala, Rantasalmi, Ristiina ja Sulkava.

</saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä kesäkuuta 2000.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Toimenpiteisiin päätöksen täytäntöön panemiseksi saadaan ryhtyä jo ennen sen voimaantuloa.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
