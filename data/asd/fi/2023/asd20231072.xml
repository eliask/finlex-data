                  <tau:colspec colnum="1" colname="1" colwidth="50*"/>
                  <tau:colspec colnum="2" colname="2" colwidth="50*"/>
                  <tau:tbody>
                    <tau:row>
                      <tau:entry>
                        <sis:KappaleKooste>1) henkilökohtaiset pienriistaluvat alle 18-vuotiaalle sisältäen kanalintujen metsästysoikeuden</sis:KappaleKooste>
