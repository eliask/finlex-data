<?xml version="1.0" encoding="utf-8"?>
<vah:VahvistettavaLaki xmlns:vah="http://www.vn.fi/skeemat/vahvistettavalaki/2010/04/27" xmlns:sdk="http://www.finlex.fi/skeemat/edita/sdk/2010/04/27" xmlns:saa="http://www.vn.fi/skeemat/saadoskooste/2010/04/27" xmlns:saa1="http://www.vn.fi/skeemat/saadoselementit/2010/04/27" xmlns:met="http://www.vn.fi/skeemat/metatietokooste/2010/04/27" xmlns:met1="http://www.vn.fi/skeemat/metatietoelementit/2010/04/27" xmlns:asi="http://www.vn.fi/skeemat/asiakirjakooste/2010/04/27" xmlns:asi1="http://www.vn.fi/skeemat/asiakirjaelementit/2010/04/27" xmlns:org="http://www.vn.fi/skeemat/organisaatiokooste/2010/02/15" xmlns:org1="http://www.vn.fi/skeemat/organisaatioelementit/2010/02/15" xmlns:tau="http://www.vn.fi/skeemat/taulukkokooste/2010/04/27" xmlns:sis="http://www.vn.fi/skeemat/sisaltokooste/2010/04/27" xmlns:sis1="http://www.vn.fi/skeemat/sisaltoelementit/2010/04/27" met1:kieliKoodi="fi" met1:julkisuusKoodi="julkinen" met1:tilaKoodi="Valmis" met1:identifiointiTunnus="0000000000000000" met1:versioTeksti="1.0" met1:asiakirjatyyppiNimi="Vahvistettava laki" met1:laadintaPvm="2100-01-01" met1:rakennemaarittelyNimi="vahvistettavalaki.xsd">
  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>179</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1990</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>poliisin eräiden virkojen perustamisesta ja lakkauttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>poliisin eräiden virkojen perustamisesta ja lakkauttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Sisäasiainministerin esittelystä säädetään 24 päivänä lokakuuta 1986 annetun valtion virkamieslain (755/86) 6§:n 1 ja 4 momentin sekä 10§:n 1 momentin nojalla:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Eduskunnan myönnettyä tarkoitukseen tarvittavat varat perustetaan ja lakkautetaan seuraavat sopimuspalkkaiset ja peruspalkkaiset virat:</saa:MomenttiKooste>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Paikallispoliisi:</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Perustetaan 1 päivästä maaliskuuta 1990:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
yksi palkkausluokkaan A 12 kuuluva vahtimestarin virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi palkkausluokkaan A 11 kuuluva haastemiehen virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi palkkausluokkaan A 11 kuuluva vartijavahtimestarin virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi palkkausluokkaan A 11 kuuluva toimistosihteerin virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
kaksi palkkausluokkaan A10 kuuluvaa nuoremman konstaapelin virkaa; ja
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
kaksi palkkausluokkaan A 8 kuuluvaa toimistovirkailijan virkaa.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Perustetaan 1 päivästä syyskuuta 1990:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
kaksi palkkausluokkaan A 24 kuuluvaa rikosylikomisarion virkaa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
viisitoista palkkausluokkaan A 20 kuuluvaa rikoskomisarion virkaa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
kaksi palkkausluokkaan A 20 kuuluvaa komisarion virkaa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
kymmenen palkkausluokkaan A 17 kuuluvaa rikosylikonstaapelin virkaa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
kolme palkkausluokkaan A 17 kuuluvaa ylikonstaapelin virkaa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
kuusi palkkausluokkaan A 15 kuuluvaa vanhemman rikoskonstaapelin virkaa; ja
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
neljä palkkausluokkaan A 14 kuuluvaa vanhemman konstaapelin virkaa.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Perustetaan 1 päivästä marraskuuta 1990:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
yksi palkkausluokkaan A 26 kuuluva nimismiehen virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi palkkausluokkaan A 26 kuuluva osastopäällikön virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi palkkausluokkaan A 24 kuuluva apulaisnimismiehen virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi palkkausluokkaan A 14 kuuluva osastosihteerin virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
kaksi palkkausluokkaan A 12 kuuluvaa toimistosihteerin virkaa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi palkkausluokkaan A 11 kuuluva haastemiehen virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
neljä palkkausluokkaan A 11 kuuluvaa ulosottoapulaisen virkaa; ja
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
kaksi palkkausluokkaan A 8 kuuluvaa toimistovirkailijan virkaa.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Lakkautetaan 1 päivästä marraskuuta 1990:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
yksi palkkausluokkaan A 25 kuuluva nimismiehen virka; ja
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi palkkausluokkaan A 23 kuuluva poliisimestarin virka.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Keskusrikospoliisi:</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Perustetaan 1 päivästä maaliskuuta 1990:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
kaksitoista palkkausluokkaan A 16 kuuluvaa vanhemman rikoskonstaapelin virkaa.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Perustetaan 1 päivästä huhtikuuta 1990:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
kaksi sopimuspalkkaluokkaan S 25 kuuluvaa ATK-päällikön virkaa.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Perustetaan 1 päivästä syyskuuta 1990:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
yksi palkkausluokkaan A 25 kuuluva laboratorionjohtajan virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
kaksi palkkausluokkaan A 24 kuuluvaa rikoskemistin virkaa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
kaksi palkkausluokkaan A 24 kuuluvaa rikosylikomisarion virkaa; ja
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi palkkausluokkaan A 17 kuuluva tutkimusavustajan virka.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Lakkautetaan 1 päivästä maaliskuuta 1990:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
kaksitoista palkkausluokkaan A 13 kuuluvaa nuoremman konstaapelin virkaa.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Suojelupoliisi:</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Perustetaan 1 päivästä syyskuuta 1990:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
yksi sopimuspalkkaluokkaan S 25 kuuluva ylitarkastajan virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi palkkausluokkaan A 18 kuuluva ylietsivän virka; ja
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi palkkausluokkaan A 15 kuuluva etsivän virka.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Poliisiopisto:</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Perustetaan 1 päivästä syyskuuta 1990:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
yksi palkkausluokkaan A 23 kuuluva kirjastonhoitajan virka.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:MomenttiKooste>
            <saa:SaadosKursiiviKooste>Poliisikoulu:</saa:SaadosKursiiviKooste>
          </saa:MomenttiKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Perustetaan 1 päivästä syyskuuta 1990:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
kaksi palkkausluokkaan A 24 kuuluvaa opettajan virkaa.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä maaliskuuta 1990.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tällä asetuksella perustetut virat voidaan täyttää ensimmäisellä kerralla niitä haettavaksi julistamatta.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Toimenpiteisiin tällä asetuksella perustettujen virkojen täyttämiseksi saadaan ryhtyä jo ennen sitä ajankohtaa, josta lukien virat on perustettu.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
  <asi:AllekirjoitusOsa>
    <asi:PaivaysKooste>Helsingissä 16 päivänä helmikuuta 1990</asi:PaivaysKooste>
    <asi:Allekirjoittaja>
      <org:Henkilo>
        <org1:AsemaTeksti>Tasavallan Presidentti </org1:AsemaTeksti>
        <org1:SukuNimi>Mauno Koivisto</org1:SukuNimi>
      </org:Henkilo>
    </asi:Allekirjoittaja>
    <asi:Allekirjoittaja>
      <org:Henkilo>
        <org1:AsemaTeksti>Sisäasiainministeri </org1:AsemaTeksti>
        <org1:SukuNimi>Jarmo Rantanen</org1:SukuNimi>
      </org:Henkilo>
    </asi:Allekirjoittaja>
  </asi:AllekirjoitusOsa>
</vah:VahvistettavaLaki>
