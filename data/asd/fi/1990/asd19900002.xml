  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>2</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1990</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>valtioneuvoston kanslian eräiden virkojen perustamisesta ja lakkauttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>valtioneuvoston kanslian eräiden virkojen perustamisesta ja lakkauttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Pääministerin esittelystä säädetään 24 päivänä lokakuuta 1986 annetun valtion virkamieslain (755/86) 6§:n 1 ja 4 momentin nojalla:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Eduskunnan myönnettyä tarkoitukseen tarvittavat varat perustetaan ja lakkautetaan jäljempänä mainitut peruspalkkaiset ja sopimuspalkkaiset virat.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 § </saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Perustetaan 1 päivästä maaliskuuta 1990:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
yksi S 33 palkkausluokkaan kuuluva valtiosihteerin virka; ja
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi S 32 palkkausluokkaan kuuluva alivaltiosihteerin virka.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Lakkautetaan 1 päivästä maaliskuuta 1990:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
yksi S 33 palkkausluokkaan kuuluva kansliapäällikön virka; ja
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 31 palkkausluokkaan kuuluva hallitusneuvoksen, osastopäällikkönä virka.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 § </saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Perustetaan yksi S 24 palkkausluokkaan kuuluva ylitarkastajan virka.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 § </saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Perustetaan 1 päivästä syyskuuta 1990:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
yksi A 21 palkkausluokkaan kuuluva operaattorin virka; ja
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
kaksi A 20 palkkausluokkaan kuuluvaa kielenkääntäjän virkaa.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="5 §">
          <saa:PykalaTunnusKooste>5 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 10 päivänä tammikuuta 1990.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Toimenpiteisiin tällä asetuksella perustetun viran täyttämiseksi saadaan ryhtyä jo ennen sitä ajankohtaa, josta lukien virka on perustettu.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
  <asi:AllekirjoitusOsa>
    <asi:PaivaysKooste>Helsingissä 5 päivänä tammikuuta 1990</asi:PaivaysKooste>
    <asi:Allekirjoittaja>
      <org:Henkilo>
        <org1:AsemaTeksti>Tasavallan Presidentti </org1:AsemaTeksti>
        <org1:SukuNimi>Mauno Koivisto</org1:SukuNimi>
      </org:Henkilo>
    </asi:Allekirjoittaja>
    <asi:Allekirjoittaja>
      <org:Henkilo>
        <org1:AsemaTeksti>Pääministeri </org1:AsemaTeksti>
        <org1:SukuNimi>Harri Holkeri</org1:SukuNimi>
      </org:Henkilo>
    </asi:Allekirjoittaja>
  </asi:AllekirjoitusOsa>
</vah:VahvistettavaLaki>
