  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>opintotukiasetuksen 1 §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Opetusministerin esittelystä muutetaan 14 päivänä helmikuuta 1972 annetun opintotukiasetuksen (287/72) 1 §, sellaisena kuin se on 10 päivänä heinäkuuta 1979 annetussa asetuksessa (616/79), näin kuuluvaksi:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Opintotukilaissa tarkoitettuja opintotuen myöntämisestä päättäviä opintotukiviranomaisia ovat valtion opintotukikeskus sekä seuraavissa oppilaitoksissa toimivat opintotukilautakunnat:</saa:MomenttiKooste>
          <saa:MomenttiKooste>Helsingin yliopisto,</saa:MomenttiKooste>
          <saa:MomenttiKooste>Jyväskylän yliopisto,</saa:MomenttiKooste>
          <saa:MomenttiKooste>Oulun yliopisto,</saa:MomenttiKooste>
          <saa:MomenttiKooste>Joensuun yliopisto,</saa:MomenttiKooste>
          <saa:MomenttiKooste>Kuopion yliopisto,</saa:MomenttiKooste>
          <saa:MomenttiKooste>Turun yliopisto,</saa:MomenttiKooste>
          <saa:MomenttiKooste>Tampereen yliopisto,</saa:MomenttiKooste>
          <saa:MomenttiKooste>Åbo Akademi,</saa:MomenttiKooste>
          <saa:MomenttiKooste>Vaasan korkeakoulu,</saa:MomenttiKooste>
          <saa:MomenttiKooste>Lapin korkeakoulu,</saa:MomenttiKooste>
          <saa:MomenttiKooste>Eläinlääketieteellinen korkeakoulu,</saa:MomenttiKooste>
          <saa:MomenttiKooste>Teknillinen korkeakoulu,</saa:MomenttiKooste>
          <saa:MomenttiKooste>Tampereen teknillinen korkeakoulu,</saa:MomenttiKooste>
          <saa:MomenttiKooste>Lappeenrannan teknillinen korkeakoulu,</saa:MomenttiKooste>
          <saa:MomenttiKooste>Helsingin kauppakorkeakoulu,</saa:MomenttiKooste>
          <saa:MomenttiKooste>Svenska handelshögskolan,</saa:MomenttiKooste>
          <saa:MomenttiKooste>Turun kauppakorkeakoulu,</saa:MomenttiKooste>
          <saa:MomenttiKooste>Taideteollinen korkeakoulu,</saa:MomenttiKooste>
          <saa:MomenttiKooste>Sibelius-Akatemia ja</saa:MomenttiKooste>
          <saa:MomenttiKooste>Teatterikorkeakoulu.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä kesäkuuta 1990.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Ennen asetuksen voimaantuloa voidaan ryhtyä sen täytäntöönpanon edellyttämiin toimenpiteisiin.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
