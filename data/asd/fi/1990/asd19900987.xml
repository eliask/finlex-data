<?xml version="1.0" encoding="utf-8"?>
<vah:VahvistettavaLaki xmlns:vah="http://www.vn.fi/skeemat/vahvistettavalaki/2010/04/27" xmlns:sdk="http://www.finlex.fi/skeemat/edita/sdk/2010/04/27" xmlns:saa="http://www.vn.fi/skeemat/saadoskooste/2010/04/27" xmlns:saa1="http://www.vn.fi/skeemat/saadoselementit/2010/04/27" xmlns:met="http://www.vn.fi/skeemat/metatietokooste/2010/04/27" xmlns:met1="http://www.vn.fi/skeemat/metatietoelementit/2010/04/27" xmlns:asi="http://www.vn.fi/skeemat/asiakirjakooste/2010/04/27" xmlns:asi1="http://www.vn.fi/skeemat/asiakirjaelementit/2010/04/27" xmlns:org="http://www.vn.fi/skeemat/organisaatiokooste/2010/02/15" xmlns:org1="http://www.vn.fi/skeemat/organisaatioelementit/2010/02/15" xmlns:tau="http://www.vn.fi/skeemat/taulukkokooste/2010/04/27" xmlns:sis="http://www.vn.fi/skeemat/sisaltokooste/2010/04/27" xmlns:sis1="http://www.vn.fi/skeemat/sisaltoelementit/2010/04/27" met1:kieliKoodi="fi" met1:julkisuusKoodi="julkinen" met1:tilaKoodi="Valmis" met1:identifiointiTunnus="0000000000000000" met1:versioTeksti="1.0" met1:asiakirjatyyppiNimi="Vahvistettava laki" met1:laadintaPvm="2100-01-01" met1:rakennemaarittelyNimi="vahvistettavalaki.xsd">
  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>987</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1990</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>vankeinhoitolaitoksen eräiden virkojen perustamisesta ja lakkauttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>vankeinhoitolaitoksen eräiden virkojen perustamisesta ja lakkauttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Oikeusministerin esittelystä säädetään 24 päivänä lokakuuta 1986 annetun valtion virkamieslain (755/86) 6§:n 1 ja 4 momentin ja 10§:n 1 momentin nojalla, sellaisena kuin niistä 6§:n 4 momentti on 3 päivänä elokuuta 1990 annetussa laissa (673/90):</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 § </saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Perustetaan:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>
                <saa:SaadosKursiiviKooste>vankeinhoitolaitoksen yhteiset</saa:SaadosKursiiviKooste>
              </saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 17 palkkausluokkaan kuuluva ylityönjohtajan virka;
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 15 palkkausluokkaan kuuluva työnjohtajan virka;
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 15 palkkausluokkaan kuuluva ohjaajan virka;
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 15 palkkausluokkaan kuuluva virastosihteerin virka;
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
viisi A 11 palkkausluokkaan kuuluvaa vanhemman vartijan virkaa;
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
kuusi A 10 palkkausluokkaan kuuluvaa vanhemman vartijan virkaa;
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
kaksi A 10 palkkausluokkaan kuuluvaa toimistosihteerin virkaa; ja
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
kolme A 9 palkkausluokkaan kuuluvaa toimistosihteerin virkaa.
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Lakkautetaan:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>
                <saa:SaadosKursiiviKooste>Turun keskusvankila</saa:SaadosKursiiviKooste>
              </saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 8 palkkausluokkaan kuuluva toimistovirkailijan virka;
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>
                <saa:SaadosKursiiviKooste>Sukevan keskusvankila</saa:SaadosKursiiviKooste>
              </saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 8 palkkausluokkaan kuuluva toimistovirkailijan virka;
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>
                <saa:SaadosKursiiviKooste>Pelson keskusvankila</saa:SaadosKursiiviKooste>
              </saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 8 palkkausluokkaan kuuluva toimistovirkailijan virka;
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>
                <saa:SaadosKursiiviKooste>Hämeenlinnan keskus- ja lääninvankila</saa:SaadosKursiiviKooste>
              </saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
kolme A 9 palkkausluokkaan kuuluvaa nuoremman vartijan virkaa;
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>
                <saa:SaadosKursiiviKooste>Keravan nuorisovankila</saa:SaadosKursiiviKooste>
              </saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 9 palkkausluokkaan kuuluva nuoremman vartijan virka;
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 9 palkkausluokkaan kuuluva toimistovirkailijan virka;
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>
                <saa:SaadosKursiiviKooste>Vaasan lääninvankila</saa:SaadosKursiiviKooste>
              </saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 8 palkkausluokkaan kuuluva toimistovirkailijan virka;
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>
                <saa:SaadosKursiiviKooste>Pohjois-Karjalan lääninvankila</saa:SaadosKursiiviKooste>
              </saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 12 palkkausluokkaan kuuluva työmestarin virka;
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>
                <saa:SaadosKursiiviKooste>vankeinhoitolaitoksen yhteiset</saa:SaadosKursiiviKooste>
              </saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 16 palkkausluokkaan kuuluva varastopäällikön virka;
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yksi A 12 palkkausluokkaan kuuluva työmestarin virka; ja
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
yhdeksän A 9 palkkausluokkaan kuuluvaa nuoremman vartijan virkaa.
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämän asetuksen 1§:ssä mainitut vanhemman vartijan ja toimistosihteerin virat voidaan ensimmäisellä kerralla täyttää niitä haettavaksi julistamatta ja vanhemman vartijan virat sen estämättä, mitä niiden kelpoisuusvaatimuksista on säädetty.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä joulukuuta 1990.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
  <asi:AllekirjoitusOsa>
    <asi:PaivaysKooste>Helsingissä 16 päivänä marraskuuta 1990</asi:PaivaysKooste>
    <asi:Allekirjoittaja>
      <org:Henkilo>
        <org1:AsemaTeksti>Tasavallan Presidentti </org1:AsemaTeksti>
        <org1:SukuNimi>Mauno Koivisto</org1:SukuNimi>
      </org:Henkilo>
    </asi:Allekirjoittaja>
    <asi:Allekirjoittaja>
      <org:Henkilo>
        <org1:AsemaTeksti>Oikeusministeri </org1:AsemaTeksti>
        <org1:SukuNimi>Tarja Halonen</org1:SukuNimi>
      </org:Henkilo>
    </asi:Allekirjoittaja>
  </asi:AllekirjoitusOsa>
</vah:VahvistettavaLaki>
