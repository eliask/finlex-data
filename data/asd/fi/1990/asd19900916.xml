  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>916</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1990</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>veropiireistä annetun valtioneuvoston päätöksen muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>veropiireistä annetun valtioneuvoston päätöksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvosto on valtiovarainministeriön esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>kumonnut</saa:SaadosKursiiviKooste> veropiireistä 25 päivänä helmikuuta 1960 annetun valtioneuvoston päätöksen (122/60) Taipalsaaren veropiiriä, Pieksämäen maalaiskunnan veropiiriä ja Rovaniemen maalaiskunnan veropiiriä koskevat kohdat,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste>sellaisina kuin niistä ovat Taipalsaaren veropiiriä koskeva kohta 15 joulukuuta 1988 annetussa valtioneuvoston päätöksessä (1094/88) ja Rovaniemen maalaiskunnan veropiiriä koskeva kohta 27 helmikuuta 1964 annetussa valtioneuvoston päätöksessä (84/64), sekä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muuttanut</saa:SaadosKursiiviKooste> Lappeenrannan veropiiriä, Pieksämäen kaupungin veropiiriä ja Rovaniemen kaupungin veropiiriä koskevat kohdat,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste>sellaisina kuin niistä ovat Pieksämäen kaupungin veropiiriä koskeva kohta 29 joulukuuta 1961 annetussa valtioneuvoston päätöksessä (599/61) ja Rovaniemen kaupungin veropiiriä koskeva kohta mainitussa 27 helmikuuta 1964 annetussa valtioneuvoston päätöksessä, seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <te>
          <pl>Kymen lääni:</pl>
        </te>
        <sis:Katkoviiva/>
        <te><saa:SaadosKursiiviKooste>Lappeenrannan veropiiri:</saa:SaadosKursiiviKooste> Lappeenrannan kaupunki, Lemin kunta, Taipalsaaren kunta ja Ylämaan kunta; sekä</te>
        <sis:Katkoviiva/>
        <te>
          <pl>Mikkelin lääni:</pl>
        </te>
        <sis:Katkoviiva/>
        <te><saa:SaadosKursiiviKooste>Pieksämäen veropiiri:</saa:SaadosKursiiviKooste> Pieksämäen kaupunki, Haukivuoren kunta, Jäppilän kunta, Pieksämäen maalaiskunta ja Virtasalmen kunta;</te>
        <sis:Katkoviiva/>
        <te>
          <pl>Lapin lääni:</pl>
        </te>
        <sis:Katkoviiva/>
        <te><saa:SaadosKursiiviKooste>Rovaniemen veropiiri:</saa:SaadosKursiiviKooste> Rovaniemen kaupunki ja Rovaniemen maalaiskunta;</te>
        <sis:Katkoviiva/>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä tammikuuta 1991.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
  <asi:AllekirjoitusOsa>
