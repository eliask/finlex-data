        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Laatuhinnoittelu</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Maidonjalostamon on muutettava tuottajalle maksettavaa maidon hintaa laadun perusteella seuraavasti:</saa:MomenttiKooste>
          <tau:table>
