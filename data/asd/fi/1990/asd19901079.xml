    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:MuuSaados>
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Kuluttajaviraston päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>kulutustavaran yksikköhinnan ilmoittamisesta markkinoinnissa annetun elinkeinohallituksen päätöksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:SisaltoMuuSaados>
        <saa:SaadosKappaleKooste>Kuluttajavirasto on muuttanut kulutustavaran yksikköhinnan ilmoittamisesta markkinoinnissa 5 päivänä huhtikuuta 1990 annetun elinkeinohallituksen päätöksen (342/90) liitteen kohdan 13. näin kuuluvaksi:</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste>
          <saa:SaadosKursiiviKooste>Liite</saa:SaadosKursiiviKooste>
        </saa:SaadosKappaleKooste>
        <saa:SaadosValiotsikkoKooste>YKSIKKÖHINNAN ILMOITTAMISVELVOLLISUUDEN ALAAN KUULUVAT KULUTUSTAVARAT JA KULUTUSTAVARARYHMÄT </saa:SaadosValiotsikkoKooste>
        <tau:table>
          <tau:tgroup cols="3" colsep="1" rowsep="1">
            <tau:tbody>
              <tau:row>
                <tau:entry>
                  <sis:KappaleKooste colname="1">Tavara tai tavararyhmä</sis:KappaleKooste>
                </tau:entry>
                <tau:entry>
                  <sis:KappaleKooste colname="2">Muut ilmoittamista koskevat määräykset</sis:KappaleKooste>
                </tau:entry>
                <tau:entry>
                  <sis:KappaleKooste colname="3">Mittayksikkö, jota kohden</sis:KappaleKooste>
                </tau:entry>
              </tau:row>
              <tau:row>
                <tau:entry>
                  <sis:KappaleKooste colname="1"/>
                </tau:entry>
                <tau:entry>
                  <sis:KappaleKooste colname="2">Pakkauskoot, joiden osalta yksikkö-</sis:KappaleKooste>
                </tau:entry>
                <tau:entry>
                  <sis:KappaleKooste colname="3">yksikköhinta ilmoitetaan</sis:KappaleKooste>
                </tau:entry>
              </tau:row>
              <tau:row>
                <tau:entry>
                  <sis:KappaleKooste colname="1"/>
                </tau:entry>
                <tau:entry>
                  <sis:KappaleKooste colname="2">hintaa ei tarvitse ilmoittaa</sis:KappaleKooste>
                </tau:entry>
                <tau:entry>
                  <sis:KappaleKooste colname="3"/>
                </tau:entry>
              </tau:row>
            </tau:tbody>
          </tau:tgroup>
        </tau:table>
