  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>564</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1987</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>eräiden tilojen, tilanosan ja alueiden siirtämisestä Kokemäen kaupungista ja Nakkilan kunnasta Harjavallan kaupunkiin</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Sisäasiainministeriön päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>eräiden tilojen, tilanosan ja alueiden siirtämisestä Kokemäen kaupungista ja Nakkilan kunnasta Harjavallan kaupunkiin</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Sisäasiainministeriö on kuntajaosta 21 päivänä tammikuuta 1977 annetun lain (73/77) nojalla päättänyt:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Kokemäen kaupungista siirretään Harjavallan kaupunkiin seuraavat tilat, tilanosa ja alueet:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1)Kuurolan kylästä kokonaan tilat Niittymäki 3:7 ja Visti 3:16;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
2)Viikarin yksinäistalosta osa tilasta (takametsäpalsta) Alamäki 1:9; sekä
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
3)edellä mainittujen tilojen ja tilanosan alueilla olevat yleiset ja yhteiset alueet.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Nakkilan kunnasta siirretään Harjavallan kaupunkiin seuraava tila ja alueet:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
Arantilan kylästä kokonaan tila Kellarmaa 2:29 sekä tilan alueella olevat yleiset ja yhteiset alueet.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Siirrot eivät aiheuta muutosta kuntien kielelliseen jaotukseen.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä tammikuuta 1988.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
  <asi:AllekirjoitusOsa>
