  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>512</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1987</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>kunnallisen pysäköinninvalvojan virkapuvusta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Sisäasiainministeriön päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>kunnallisen pysäköinninvalvojan virkapuvusta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Sisäasiainministeriö on pysäköintivirhemaksusta 5 päivänä elokuuta 1970 annetun asetuksen (545/70) 3§:n nojalla päättänyt:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Soveltamisala </saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Kunnallisella pysäköinninvalvojalla tarkoitetaan tässä päätöksessä kunnallista pysäköinninvalvojaa ja hänen apulaistaan.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Pysäköintivirhemaksusta annetun asetuksen 3§:ssä tarkoitettuna kunnallisen pysäköinninvalvojan virkapukuna käytettävän asun on oltava tämän päätöksen mukainen.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Pusero </saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Kangas: tummanpunainen pukukangas.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Kuosi: väljähkö pusero, jossa piilonepparit ja vyötärössä joustonauha.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Kaulus: pystykaulus.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Hihat: istutetut, kapealla kalvosimella varustetut hihat.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Taskut: vinot upotetut taskut.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Hame </saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Kangas: sama kuin puserossa.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Kuosi: kolmesta kaistaleesta valmistettu suora hame, jossa takahalkio ja vyötärönauha, edessä upotetut, kaarevasti leikatut taskut.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Päällystakki </saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Kangas: sama kuin puserossa tai tummanpunainen paksu pukukangas. </saa:MomenttiKooste>
          <saa:MomenttiKooste>Kuosi: suora takki, jossa piilonepparit ja irrotettava huppu. Takki on pituudeltaan joko reiden tai pohkeen tasalle ulottuva.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Kaulus: paitakaulus.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Hihat: istutetut hihat, joissa kapeat kalvosimet.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Taskut: kannelliset päälle ommellut taskut.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="5 §">
          <saa:PykalaTunnusKooste>5 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Paidat </saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Paita: pitkä- tai lyhythihainen puuvillapaita, jossa valko-musta pystyraidoitus ja rintataskut.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Poolopaita: musta korkeakauluksinen villaneulepaita.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="6 §">
          <saa:PykalaTunnusKooste>6 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Housut </saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Kangas: sama kuin puserossa tai päällystakissa.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Kuosi: suorat housut, joissa vyötärölaskokset.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Taskut: edessä kaarevasti leikatut upotetut taskut.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="7 §">
          <saa:PykalaTunnusKooste>7 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Päähineet </saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Lippalakki: tummanpunaisesta pukukankaasta valmistettu päältä tasainen lakki, jossa kaareva lippa.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Turkislakki: mustasta lammasturkiksesta valmistettu lieriönmuotoinen päähine.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="8 §">
          <saa:PykalaTunnusKooste>8 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Muut asusteet </saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Huivi: paidan kankaasta valmistettu kaulaan solmittava huivi.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Kasvosuojus: mustasta neuleesta valmistettu kasvosuojus.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Jalkineet: mustat kävelykengät, talvisaappaat tai kumisaappaat.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="9 §">
          <saa:PykalaTunnusKooste>9 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Miesten asua koskevat poikkeukset </saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Miespuolisen pysäköinninvalvojan asu poikkeaa edellä määrätystä seuraavasti:</saa:MomenttiKooste>
          <saa:MomenttiKooste>Pusero: olkapoletit.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Päällystakki: kuten naisten lyhyt päällystakki mutta ilman huppua, rintatasku ja olkapoletit.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Paita: olkapoletit, yksi rintatasku.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Housut: mustaa pukukangasta, vyölenkit ja takatasku.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Lippalakki: lippa ja lakin reunus samaa kangasta kuin miesten housut.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Turkislakki: mustasta paksusta pukukankaasta valmistettu, päältä kapeahko lakki, jossa korvallisille laskettava musta lammasturkisreunus.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Solmio: valko-musta-raitainen tai musta kangassolmio.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="10 §">
          <saa:PykalaTunnusKooste>10 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Voimaantulo </saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä kesäkuuta 1987. </saa:MomenttiKooste>
          <saa:MomenttiKooste>Tällä päätöksellä kumotaan sisäasiainministeriön 16 päivänä helmikuuta 1971 kunnallisen pysäköinninvalvojan ja hänen apulaisensa virkapuvun kuosin vahvistamisesta antama päätös sellaisena kuin se on osittain muutettuna 15 päivänä marraskuuta 1972 annetulla päätöksellä.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Edellä 2 momentissa tarkoitetun päätöksen mukaista asua saadaan käyttää vuoden 1989 loppuun.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
