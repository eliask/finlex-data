                  <tau:entry>
                    <sis:KappaleKooste> A 24-A 27 </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>49 100 </sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>A 17-A 23 </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>39 800 </sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
