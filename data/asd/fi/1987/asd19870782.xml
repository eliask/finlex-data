  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>ulkomaanedustuksen virkamiesten paikalliskorotuksesta, varustautumiskorvauksesta ja asettautumiskustannusten korvauksesta annetun valtioneuvoston päätöksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvosto on ulkoasiainministeriön esittelystä muuttanut ulkomaanedustuksen virkamiesten paikalliskorotuksesta, varustautumiskorvauksesta ja asettautumiskustannusten korvauksesta 10 päivänä huhtikuuta 1980 annetun valtioneuvoston päätöksen (359/80) 2 ja 3§:n sekä 4§:ssä tarkoitetun päätöksen liitteenä oleva taulukko 1, sellaisena kuin se on tämän päätöksen muuttamisesta 16 päivänä lokakuuta 1986 ja 12 päivänä maaliskuuta 1987 annetuissa valtioneuvoston päätöksissä (741/86 ja 262/87), seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Varustautumiskorvauksen määrät ovat seuraavat:</saa:MomenttiKooste>
          <tau:table>
            <tau:tgroup cols="2" colsep="0" rowsep="0" align="left">
              <tau:colspec colnum="1" colname="1" colwidth=""/>
              <tau:colspec colnum="2" colname="2" colwidth=""/>
              <tau:tbody valign="top">
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Suurlähetystön, lähetystön ja näihin rinnastettavan pysyvän edustuston sekä lähetetyn pääkonsulinviraston päälliköksi määrätty virkamies </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>67 900 </sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Muut virkamiehet palkkausluokittain ja virkanimittäin:</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                </tau:row>
