  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>181</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1987</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>hankevientiin liittyvään tarjoustoimintaan myönnettävien lainojen yleisistä ehdoista annetun valtioneuvoston päätöksen muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>hankevientiin liittyvään tarjoustoimintaan myönnettävien lainojen yleisistä ehdoista annetun valtioneuvoston päätöksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvosto on kauppa- ja teollisuusministeriön esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muuttanut </saa:SaadosKursiiviKooste>hankevientiin liittyvään tarjoustoimintaan myönnettävien lainojen yleisistä ehdoista 25 päivänä elokuuta 1977 annetun valtioneuvoston päätöksen (650/77) 6§:n sekä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>lisännyt </saa:SaadosKursiiviKooste>11§:ään uuden 2 momentin, jolloin nykyinen 2 momentti siirtyy 3 momentiksi, seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="6 §">
          <saa:PykalaTunnusKooste>6 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Lainasta peritään korkoa, joka vastaa kulloinkin voimassa olevaa Suomen Pankin peruskorkoa, kuitenkin vähintään seitsemän prosenttia.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="11 §">
          <saa:PykalaTunnusKooste>11 § </saa:PykalaTunnusKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Takaisin maksettavaksi määrätylle lainapääomalle on maksettava 16 prosentin vuotuinen korko siitä päivästä, jolloin laina tai sen osa on nostettu. Mikäli palautusta tai korkoa ei ole määrättyyn eräpäivään mennessä maksettu, on sille maksettava vuotuista viivästyskorkoa 16 prosenttia eräpäivästä lukien.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 25 päivänä helmikuuta 1987. Sitä sovelletaan sen jälkeen myönnettäviin hankevientilainoihin.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
