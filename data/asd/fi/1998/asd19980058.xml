  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>opetus- ja kirjastotoimen perustamishankkeiden yksikköhinnoista annetun valtioneuvoston päätöksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvosto on opetusministeriön esittelystä päättänyt</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste> muuttaa</saa:SaadosKursiiviKooste> opetus- ja kirjastotoimen perustamishankkeiden yksikköhinnoista 11 päivänä helmikuuta 1993 annetun valtioneuvoston päätöksen (165/1993) 2 §:n, 4 §:n 1 momentin ja 6 §:n, sellaisina kuin ne ovat 13 pävänä helmikuuta 1997 annetussa valtioneuvoston päätöksessä (141/1997), seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Kalleusluokkaan 100 kuuluvan toimitilan yksikköhinta hyötyalan neliömetriä kohden on 8 010 markkaa. Muihin kalleusluokkiin kuuluvan toimitilan yksikköhinta saadaan kertomalla kalleusluokkaan 100 kuuluvan toimitilan yksikköhinta toimitilan kalleusluokalla ja jakamalla tulo luvulla 100.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämän päätöksen 2 ja 3 §:n mukaisesti laskettua yksikköhintaa korotetaan varustamisen perusteella seuraavasti:</saa:MomenttiKooste>
          <tau:table>
            <tau:tgroup cols="2" colsep="0" rowsep="0" align="left">
              <tau:colspec colnum="1" colname="1" colwidth=""/>
              <tau:colspec colnum="2" colname="2" colwidth=""/>
              <tau:tbody valign="top">
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste/>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>mk</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>a) peruskoulun ala-aste </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>650</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>b) peruskoulun yläaste, lukio ja iltalukio </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>870</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>c) ammattioppilaitos </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>2 090</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>d) yleinen kirjasto </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>1 330</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
              </tau:tbody>
            </tau:tgroup>
          </tau:table>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="6 §">
          <saa:PykalaTunnusKooste>6 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Edellä 2 ja 4 §:ssä tarkoitetut yksikköhinnat vastaavat lokakuun 1997 rakennuskustannusindeksin (1995 = 100) pisteluvun 102,4 kustannustasoa.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 13 päivänä helmikuuta 1998 ja sitä sovelletaan perustamishankkeisiin, joille myönnetään valtionosuutta vuonna 1998.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
