  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>372</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1998</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>valmismatkasta annettavista tiedoista annetun asetuksen 5 ja 7 §:n muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>Neuvoston direktiivi 90/314/ETY; EYVL N:o L 158, 23.6.1990, s. 59</sis1:ViiteTeksti>
      </met:Muu1Viite>
    </met:AsiakirjaViitteet>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>valmismatkasta annettavista tiedoista annetun asetuksen 5 ja 7 §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Oikeusministerin esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> valmismatkasta annettavista tiedoista 28 päivänä marraskuuta 1994 annetun asetuksen (1085/1994) 5 §:n 8 kohta sekä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>lisätään</saa:SaadosKursiiviKooste> 7 §:ään uusi 3 momentti seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="5 §">
          <saa:PykalaTunnusKooste>5 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Esitteessä mainitusta matkasta on annettava ainakin seuraavat tiedot:</saa:MomenttiJohdantoKooste>
            <sis:Katkoviiva/>
            <saa:MomenttiKohtaKooste>
8) niiden Euroopan talousalueeseen kuuluvien valtioiden kansalaisiin sovellettavat passi- ja viisumimääräykset, joissa esitettä jaetaan, passi- ja viisumiasiakirjojen hankkimiseen yleensä tarvittava aika sekä matkaa ja oleskelua koskevat terveysmääräykset;

</saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="7 §">
          <saa:PykalaTunnusKooste>7 §</saa:PykalaTunnusKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Matkanjärjestäjän on ennen sopimuksen päättämistä annettava matkustajalle tiedot häneen sovellettavista passi- ja viisumimääräyksistä sekä passi- ja viisumiasiakirjojen hankkimiseen yleensä tarvittavasta ajasta, jos matkustaja on Euroopan talousalueeseen kuuluvan valtion kansalainen eikä tietoja ole annettu esitteessä tai esitettä ei ole laadittu.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä heinäkuuta 1998.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
