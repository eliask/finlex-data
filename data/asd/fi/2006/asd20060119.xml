                    <sis:KappaleKooste>192 euroa </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>222 euroa </sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
