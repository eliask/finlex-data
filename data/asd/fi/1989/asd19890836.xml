  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>836</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1989</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>työministeriön eräistä virkajärjestelyistä</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>työministeriön eräistä virkajärjestelyistä</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Työministerin esittelystä säädetään 24 päivänä lokakuuta 1986 annetun valtion virkamieslain (755/86) 6§:n 1 ja 4 momentin sekä 10§:n 1 momentin nojalla:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 § </saa:PykalaTunnusKooste>
          <ete>Perustetaan:</ete>
          <saa:MomenttiKooste>yksi A 25 palkkausluokkaan kuuluva vanhempi hallitussihteerin virka;</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 24 palkkausluokkaan kuuluva tiedotuspäällikön virka;</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 14 palkkausluokkaan kuuluva toimialasihteerin virka;</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 12 palkkausluokkaan kuuluva toimialasihteerin virka;</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 12 palkkausluokkaan kuuluva operaattorin virka; sekä</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 11 palkkausluokkaan kuuluva virastomestarin virka.</saa:MomenttiKooste>
          <ete>Lakkautetaan:</ete>
          <saa:MomenttiKooste>yksi A 24 palkkausluokkaan kuuluva nuorempi hallitussihteerin virka;</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 21 palkkausluokkaan kuuluva esittelijän virka;</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 11 palkkausluokkaan kuuluva toimistosihteerin virka;</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 9 palkkausluokkaan kuuluva virastomestarin virka;</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 8 palkkausluokkaan kuuluva toimistovirkailijan virka;</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 7 palkkausluokkaan kuuluva julkaisusihteerin virka; sekä</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 4 palkkausluokkaan kuuluva toimistovirkailijan virka.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä joulukuuta 1989.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tällä asetuksella perustettu virka voidaan täyttää ensimmäisellä kerralla virkaa haettavaksi julistamatta.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Toimenpiteisiin tällä asetuksella perustetun viran täyttämiseksi saa ryhtyä jo ennen sitä ajankohtaa, josta lukien virka on perustettu, ei kuitenkaan ennen kuin eduskunta on käsitellyt asian.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
