  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>911</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1989</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>merenkulkulaitoksen eräiden virkojen perustamisesta ja lakkauttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>merenkulkulaitoksen eräiden virkojen perustamisesta ja lakkauttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Liikenneministerin esittelystä säädetään 24 päivänä lokakuuta 1986 annetun valtion virkamieslain (755/86) 6§:n 1 ja 4 momentin sekä 10§:n 1 momentin nojalla:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Luotsipiirit </saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Lakkautetaan:</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 23 palkkausluokkaan kuuluva luotsipiiripäällikön virka (Päijänteen luotsipiiri);</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 18 palkkausluokkaan kuuluva luotsivanhimman virka (Vaasan luotsiasema);</saa:MomenttiKooste>
          <saa:MomenttiKooste>kolme A 17 palkkausluokkaan kuuluvaa luotsin virkaa (Maarianhaminan, Hangon ja Ajoksen luotsiasema);</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 15 palkkausluokkaan kuuluva luotsin virka (Lohmin luotsiasema); ja</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 10 palkkausluokkaan kuuluva luotsin virka (Juojärven luotsiasema).</saa:MomenttiKooste>
          <saa:MomenttiKooste>Perustetaan:</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 21 palkkausluokkaan kuuluva väylätarkastajan virka (Saimaan luotsipiiri);</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 20 palkkausluokkaan kuuluva konetarkastajan virka (Saimaan luotsipiiri);</saa:MomenttiKooste>
          <saa:MomenttiKooste>neljä A 13 palkkausluokkaan kuuluvaa sisävesiluotsin virkaa (Saimaan luotsipiiri);</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 10 palkkausluokkaan kuuluva varastomestarin virka (Saimaan luotsipiiri); ja</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 10 palkkausluokkaan kuuluva väylämestarin virka (Saimaan luotsipiiri).</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 § </saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Merenkulkulaitoksen alukset </saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Lakkautetaan:</saa:MomenttiKooste>
          <saa:MomenttiKooste>yksi A 10 palkkausluokkaan kuuluva konemestarin 3 l. virka.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä tammikuuta 1990.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Tällä asetuksella perustetut virat saadaan täyttää ensimmäisellä kerralla niitä haettavaksi julistamatta.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
