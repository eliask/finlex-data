<?xml version="1.0" encoding="utf-8"?>
<vah:VahvistettavaLaki xmlns:vah="http://www.vn.fi/skeemat/vahvistettavalaki/2010/04/27" xmlns:sdk="http://www.finlex.fi/skeemat/edita/sdk/2010/04/27" xmlns:saa="http://www.vn.fi/skeemat/saadoskooste/2010/04/27" xmlns:saa1="http://www.vn.fi/skeemat/saadoselementit/2010/04/27" xmlns:met="http://www.vn.fi/skeemat/metatietokooste/2010/04/27" xmlns:met1="http://www.vn.fi/skeemat/metatietoelementit/2010/04/27" xmlns:asi="http://www.vn.fi/skeemat/asiakirjakooste/2010/04/27" xmlns:asi1="http://www.vn.fi/skeemat/asiakirjaelementit/2010/04/27" xmlns:org="http://www.vn.fi/skeemat/organisaatiokooste/2010/02/15" xmlns:org1="http://www.vn.fi/skeemat/organisaatioelementit/2010/02/15" xmlns:tau="http://www.vn.fi/skeemat/taulukkokooste/2010/04/27" xmlns:sis="http://www.vn.fi/skeemat/sisaltokooste/2010/04/27" xmlns:sis1="http://www.vn.fi/skeemat/sisaltoelementit/2010/04/27" met1:kieliKoodi="fi" met1:julkisuusKoodi="julkinen" met1:tilaKoodi="Valmis" met1:identifiointiTunnus="0000000000000000" met1:versioTeksti="1.0" met1:asiakirjatyyppiNimi="Vahvistettava laki" met1:laadintaPvm="2100-01-01" met1:rakennemaarittelyNimi="vahvistettavalaki.xsd">
  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>206</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1989</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>ilmailuhallinnosta annetun asetuksen muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>ilmailuhallinnosta annetun asetuksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Liikenneministerin esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan </saa:SaadosKursiiviKooste>ilmailuhallinnosta 28 päivänä tammikuuta 1972 annetun asetuksen (113/72) 7-9§ ja 36§:n 1 momentin 8 kohta,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste>näistä 7§ sellaisena kuin se on 13 päivänä helmikuuta 1987 annetussa asetuksessa (121/87) ja 36§:n 1 momentin 8 kohta sellaisena kuin se on 19 päivänä helmikuuta 1988 annetussa asetuksessa (172/88), sekä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>lisätään </saa:SaadosKursiiviKooste>asetukseen uusi 7 a§ seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="7 §">
          <saa:PykalaTunnusKooste>7 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Lennonvarmistusosastossa on järjestelmäsuunnittelutoimisto, lennonjohtotoimisto sekä viesti- ja sähkötoimisto.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="7 a §">
          <saa:PykalaTunnusKooste>7 a §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Järjestelmäsuunnittelutoimisto käsittelee asiat, jotka koskevat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1) ilmaliikenteen suunnittelua ja tutkimusta lennonvarmistuspalvelujen kannalta sekä
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
2) lennonjohto- ja lennonvarmistusjärjestelmien kehittämistä.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="8 §">
          <saa:PykalaTunnusKooste>8 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Lennonjohtotoimisto käsittelee asiat, jotka koskevat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1) ilmaliikennepalvelua;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
2)ilmaliikenteen etsintä- ja pelastuspalvelua;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
3) ilmailutiedotus- ja ilmailukarttapalvelua;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
4) ilmailun tarvitsemaa sääpalvelua;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
5) lennonvarmistuksen tietoliikennettä; sekä
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
6) lentosääntöjä.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="9§">
          <saa:PykalaTunnusKooste>9§</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Viesti- ja sähkötoimisto käsittelee asiat, jotka koskevat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1) lennonvarmistus-, navigaatio- ja telejärjestelmien teknistä suunnittelua, hankintaa ja toimivuutta;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
2) lennonvarmistuspalvelun tietoliikennetekniikkaa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
3) ilmailuhallinnon valaistus- ja sähköjärjestelmiä;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
4) ilmailuhallinnon sähkölaitoksia ja sähköenergian hankintaa ja jakelua;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
5) lennonvarmistus-, navigaatio- ja telejärjestelmien asennuksia, korjauksia ja huoltoa; sekä
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
6) ilmailuhallinnon elektroniikkahuoltojen toimintaa ja kehittämistä.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="36 §">
          <saa:PykalaTunnusKooste>36 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Kelpoisuusvaatimuksena ilmailuhallinnon virkoihin on:</saa:MomenttiJohdantoKooste>
            <sis:Katkoviiva/>
            <saa:MomenttiKohtaKooste>
8)järjestelmäsuunnittelutoimiston ja lennonjohtotoimiston toimistopäälliköllä virkaan soveltuva korkeakoulututkinto sekä perehtyneisyys toimiston toimialaan kuuluviin tehtäviin;

</saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä huhtikuuta 1989.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Ennen tämän asetuksen voimaantuloa voidaan ryhtyä sen täytäntöönpanon edellyttämiin toimenpiteisiin.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
