  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>Jyväskylän yliopistosta annetun asetuksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Opetusministerin esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> Jyväskylän yliopistosta 7 päivänä lokakuuta 1988 annetun asetuksen (856/88) 28§:n 3 momentti sekä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>lisätään </saa:SaadosKursiiviKooste>54§:ään uusi 1 momentti, jolloin nykyinen 1 ja 2 momentti siirtyvät 2 ja 3 momentiksi, seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="28 §">
          <saa:PykalaTunnusKooste>28 § </saa:PykalaTunnusKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Kasvatustieteiden tutkimuslaitoksen johtajalla ja korkeakoulujen kielikeskuksen johtajalla on professorin arvonimi.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="54 §">
          <saa:PykalaTunnusKooste>54 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Korkeakoulujen kielikeskuksen johtajan virka voidaan täyttää toistaiseksi tai määräajaksi, enintään viideksi vuodeksi kerrallaan.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 15 päivänä joulukuuta 1989.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
