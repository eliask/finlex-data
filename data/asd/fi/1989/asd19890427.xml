  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>427</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1989</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>työvoimaopistosta annetun asetuksen muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>työvoimaopistosta annetun asetuksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Työvoimaministerin esittelystä muutetaan työvoimaopistosta 21 päivänä joulukuuta 1973 annetun asetuksen (930/73) 1§:n 1 kohta, 2§:n 1 momentti, 4 ja 8§, 10§:n 1 momentti ja 13§,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste>näistä 10§:n 1 momentti sellaisena kuin se on 29 päivänä huhtikuuta 1988 annetussa asetuksessa (398/88), näin kuuluviksi:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Työvoimaopiston tehtävänä on:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1)antaa työministeriön hallinnonalan palvelujen ja tehtävien toimeenpanoon, johtamiseen sekä niitä koskevaan yleiseen hallintoon liittyvää koulutusta;

</saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Työvoimaopisto on työministeriön alainen.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Työvoimaopiston johtokuntaan kuuluvat puheenjohtaja, varapuheenjohtaja ja viisi muuta jäsentä, jotka samoin kuin jäsenten henkilökohtaiset varamiehet työministeriö määrää kolmeksi vuodeksi kerrallaan.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Työministeriö vahvistaa johtokunnan jäsenten palkkiot.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="8 §">
          <saa:PykalaTunnusKooste>8 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Rehtorin ollessa estyneenä hänen tehtäviään hoitaa johtokunnan määräämä henkilö.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="10 §">
          <saa:PykalaTunnusKooste>10 § </saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Rehtorin nimittää työministeriö.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="13 §">
          <saa:PykalaTunnusKooste>13 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Työministeriö vahvistaa työvoimaopiston työjärjestyksen, jossa annetaan tarkemmat määräykset työvoimaopiston toiminnasta.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä kesä&gt;kuuta 1989.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
