  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>1345</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1989</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>maidon kuljetusavustuksista annetun valtioneuvoston päätöksen 9 ja 12§:n muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>maidon kuljetusavustuksista annetun valtioneuvoston päätöksen 9 ja 12§:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvosto on maa- ja metsätalousministeriön esittelystä muuttanut 19 päivänä tammikuuta 1984 maidon kuljetusavustuksista antamansa päätöksen (48/84) 9 ja 12§:n, sellaisena kuin 12§ on 26 päivänä helmikuuta 1987 annetussa valtioneuvoston päätöksessä (221/87), näin kuuluviksi:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="9 §">
          <saa:PykalaTunnusKooste>9 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Meijerin, jolle maidon kuljetusavustusta suoritetaan, on noudatettava tuottajamaidon hinnoittelussa maa- ja metsätalousministeriön maataloustuloasetuksen (1081/89) 2 ja 13§:n nojalla määräämiä perusteita.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="12 §">
          <saa:PykalaTunnusKooste>12 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tarkempia määräyksiä tämän päätöksen täytäntöönpanomenettelystä antaa maatilahallitus.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä tammikuuta 1990.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
