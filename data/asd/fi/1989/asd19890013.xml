  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>13</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1989</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>Varastokirjaston eräiden virkojen perustamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>Varastokirjaston eräiden virkojen perustamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Opetusministerin esittelystä säädetään 24 päivänä lokakuuta 1986 annetun valtion virkamieslain (755/86) 6§:n nojalla:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Eduskunnan myönnettyä tarkoitukseen tarvittavat varat perustetaan Varastokirjastoon jäljempänä mainitut peruspalkkaiset virat.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Perustetaan 1 päivästä maaliskuuta 1989 lukien:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
yksi A 25 palkkausluokkaan kuuluva kirjastonjohtajan virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 23 palkkausluokkaan kuuluva toimistopäällikön virka; ja
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 15 palkkausluokkaan kuuluva osastosihteerin virka.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Perustetaan 1 päivästä elokuuta 1989 lukien:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
yksi A 21 palkkausluokkaan kuuluva kirjastonhoitajan virka;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
kaksi A 15 palkkausluokkaan kuuluvaa kirjastosihteerin virkaa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 12 palkkausluokkaan kuuluva virastomestarin virka; ja
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
yksi A 10 palkkausluokkaan kuuluva kirjastoavustajan virka.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä maaliskuuta 1989.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Toimenpiteisiin perustettujen virkojen täyttämiseksi saadaan ryhtyä jo ennen sitä ajankohtaa, josta lukien virka on perustettu.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Varastokirjastoon 1 päivästä maaliskuuta 1989 perustettujen virkojen väliaikaisesta hoitamisesta 1 päivään toukokuuta 1989 ja niiden haettavaksi julistamisesta päättää opetusministeriö.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
