  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>rajavartiolaitoksesta annetun asetuksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Sisäasiainministerin esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan </saa:SaadosKursiiviKooste>rajavartiolaitoksesta 9 päivänä tammikuuta 1976 annetun asetuksen (2/76) 13§:n 1 kohta, sellaisena kuin se on 23 päivänä helmikuuta 1979 annetussa asetuksessa (218/79), ja</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>lisätään </saa:SaadosKursiiviKooste>asetukseen uusi 19 a§ seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="13 §">
          <saa:PykalaTunnusKooste>13 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Rajaosasto käsittelee asiat, jotka koskevat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1) valtakunnan rajaa ja sen vartioimista sekä rajanylittämislupia;

</saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="19 a §">
          <saa:PykalaTunnusKooste>19 a §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Rajaosaston rajatoimiston toimistopäällikkö ratkaisee asiat, jotka koskevat ulkomaalaislain (400/83) 3§:n 2 momentissa ja passilain (642/86) 11§:n 1 momentissa tarkoitettuja lupia.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä tammikuuta 1990.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
