  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>277</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1989</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>metalliraha-asetuksen muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>metalliraha-asetuksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtiovarainministeriön toimialaan kuuluvia asioita käsittelemään määrätyn ministerin esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>kumotaan </saa:SaadosKursiiviKooste>28 päivänä joulukuuta 1979 annetun metalliraha-asetuksen (1026/79) 3 ja 15§ sekä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan </saa:SaadosKursiiviKooste>1 ja 2§, 4§:n 1 momentti, 8§, 10§:n 1 momentti sekä 13 ja 14§,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste>sellaisina kuin niistä ovat 2§, 4§:n 1 momentti ja 8§ 24 päivänä kesäkuuta 1982 annetussa asetuksessa (494/82), seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Metallirahoja ovat viiden ja yhden markan sekä viidenkymmenen ja kymmenen pennin rahat.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Viiden markan raha on alumiinipronssia, jossa on 93 prosenttia painosta kuparia, 5 prosenttia alumiinia ja 2 prosenttia nikkeliä.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Yhden markan, viidenkymmenen pennin ja kymmenen pennin rahat ovat nikkelikuparia, jossa on 75 prosenttia painosta kuparia ja 25 prosenttia nikkeliä.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Metallirahojen halkaisijat millimetreinä ja painot grammoina sekä sallitut mittapoikkeamat ovat seuraavat:</saa:MomenttiKooste>
          <tau:table>
            <tau:tgroup cols="3" colsep="0" rowsep="0" align="left">
              <tau:colspec colnum="1" colname="1" colwidth=""/>
              <tau:colspec colnum="2" colname="2" colwidth=""/>
              <tau:colspec colnum="3" colname="3" colwidth=""/>
              <tau:tbody valign="top">
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste> </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>halkaisija mm</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>paino g</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>5 markkaa </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>26,3- 0,10</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>8,2 ± 0,35 </sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>1 markka </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>24,0- 0,10</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>6,1 ± 0,30 </sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>50 penniä </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>19,7±0,05</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>3,3±0,25 </sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>10 penniä </sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>16,3±0,05</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
                    <sis:KappaleKooste>1,8±0,20</sis:KappaleKooste>
                  </tau:entry>
                </tau:row>
              </tau:tbody>
            </tau:tgroup>
          </tau:table>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="8 §">
          <saa:PykalaTunnusKooste>8 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Viidenkymmenen pennin rahan lieriöpinta on uritettu. Kymmenen pennin rahan lieriöpinta on sileä.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="10 §">
          <saa:PykalaTunnusKooste>10 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Metallirahojen leimat ovat 11-14§:ssä olevien selitelmien ja piirrosten mukaiset.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="13 §">
          <saa:PykalaTunnusKooste>13 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Viidenkymmenen pennin rahan tunnuspuolella on allekkain karhun kuva, tekstit ''SUOMI'' ja ''FINLAND'' sekä vuosiluku. Arvopuolella on numero ''50'', sen alla oikealla allekkain tekstit ''PENNIÄ'' ja ''PENNI'' sekä vasemmalla ja alhaalla karhunsammalkuvioita.</saa:MomenttiKooste>
          <sis:Kuva>
            <sis:KuvaAsemointi ID="0158"/>
          </sis:Kuva>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="14 §">
          <saa:PykalaTunnusKooste>14 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Kymmenen pennin rahan tunnuspuolella on vasemmalla kaksi kielon lehteä ja kukintoa. Oikealla on allekkain tekstit ''SUOMI'' ja ''FINLAND'' sekä vuosiluku. Arvopuolella on oikealla allekkain numero ''10'', tekstit ''PENNIÄ'' ja ''PENNI'' sekä vasemmalla mehiläiskennokuvio.</saa:MomenttiKooste>
          <sis:Kuva>
            <sis:KuvaAsemointi ID="0159"/>
          </sis:Kuva>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä marraskuuta 1990.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Ennen asetuksen voimaantuloa voidaan ryhtyä sen täytäntöönpanon edellyttämiin toimiin.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Tällä asetuksella kumottavien ja muutettavien säännösten mukaiset viidenkymmenen, kahdenkymmenen, kymmenen ja viiden pennin rahat sekä metalliraha-asetuksen alkuperäisten säännösten mukainen kymmenen pennin raha pysyvät edelleen laillisina maksuvälineinä.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
