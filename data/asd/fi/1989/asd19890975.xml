  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Laki">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Laki</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>muutoksenhausta tuomioistuimen virkaa haettaessa</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Eduskunnan päätöksen mukaisesti säädetään:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Määräaika valitettaessa virkaehdotuksesta tuomioistuimen virkaan on kolmekymmentä päivää siitä päivästä, jona virkaehdotus tehtiin, jollei jonkin viran osalta ole erikseen muuta säädetty.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Hovioikeuden ratkaisuun, joka koskee nimittämistä hovioikeuden viskaalin virkaan, saa hakea muutosta korkeimmalta oikeudelta valittamalla. Määräaika valituksen tekemiseen on kolmekymmentä päivää siitä päivästä, jona hovioikeuden ratkaisu tehtiin.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä laki tulee voimaan 1 päivänä joulu&gt;kuuta 1989.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
