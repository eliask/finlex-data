  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Ulkoministeriön asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>ulkoministeriön työjärjestyksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Ulkoministeriön päätöksen mukaisesti</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> ulkoministeriön työjärjestyksen (550/2008) 25 §, sellaisena kuin se on asetuksessa 1043/2016, seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="25 §">
          <saa:PykalaTunnusKooste>25 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Afrikan ja Lähi-idän osaston organisaatio</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Afrikan ja Lähi-idän osastolla on seuraavat yksiköt:</saa:MomenttiKooste>
          <saa:MomenttiKooste>1) Lähi-idän yksikkö;</saa:MomenttiKooste>
          <saa:MomenttiKooste>2) Pohjoisen Afrikan yksikkö;</saa:MomenttiKooste>
          <saa:MomenttiKooste>3) Afrikan sarven ja itäisen Afrikan yksikkö; ja</saa:MomenttiKooste>
          <saa:MomenttiKooste>4) Eteläisen ja läntisen Afrikan yksikkö.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä tammikuuta 2019.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
