    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>ajoneuvolain muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>HE 267/2004</sis1:ViiteTeksti>
      </met:Muu1Viite>
