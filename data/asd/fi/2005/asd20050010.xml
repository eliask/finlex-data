  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Kauppa- ja teollisuusministeriön asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>tiettyjen epoksijohdannaisten käytöstä elintarvikkeen kanssa kosketuksiin joutuvissa materiaaleissa ja tarvikkeissa annetun direktiivin 2002/16/EY muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Kauppa- ja teollisuusministeriön päätöksen mukaisesti säädetään 17 päivänä maaliskuuta 1995 annetun elintarvikelain (361/1995) 48 §:n nojalla:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Asetuksen tarkoitus</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Tällä asetuksella pannaan täytäntöön 29 päivänä tammikuuta 2004 annettu Euroopan yhteisöjen komission direktiivi 2004/13/EY tiettyjen epoksijohdannaisten käytöstä elintarvikkeen kanssa kosketuksiin joutuvissa materiaaleissa ja tarvikkeissa annetun direktiivin 2002/16/EY muuttamisesta.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Voimaantulo</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 29 päivänä tammikuuta 2005.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
