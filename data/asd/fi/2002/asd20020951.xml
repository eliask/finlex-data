  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>951</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>2002</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>puolustusministeriön työjärjestyksen muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Puolustusministeriön asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>puolustusministeriön työjärjestyksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Puolustusministeriön päätöksen mukaisesti</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>kumotaan</saa:SaadosKursiiviKooste> 14 päivänä helmikuuta 2002 annetun puolustusministeriön työjärjestyksen (115/2002) 39 §, sekä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> 12 ja 31 § seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="12 §">
          <saa:PykalaTunnusKooste>12 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Resurssipoliittinen osasto</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Resurssipoliittisessa osastossa on henkilöstöyksikkö, materiaaliyksikkö, talousyksikkö sekä yhdyskunta- ja ympäristöyksikkö.</saa:MomenttiKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Osasto käsittelee asiat, jotka koskevat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1) hallinnonalan tulosohjausta ja taloussuunnittelua;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
2) hallinnonalan talousarvion valmistelua ja täytäntöönpanoa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
3) talousseurannan toteuttamista;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
4) hallinnonalan henkilöstö- ja työnantajapolitiikan linjausten tuottamista;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
5) henkilöstövoimavarojen ohjausta ja hallinnonalan muita laaja-alaisia henkilöstöasioita;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
6) hallinnonalan materiaalipolitiikkaa ja hankeohjausta sekä puolustusmateriaaliyhteistyötä;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
7) puolustustarvikkeiden vientivalvontaa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
8) virka- ja työvoima-apua;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
9) yhdyskunta- ja ympäristöpolitiikan tuottamista ja ohjausta;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
10) kiinteistöhallintoa.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="31 §">
          <saa:PykalaTunnusKooste>31 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Resurssipoliittisen osaston osastopäällikkö</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Resurssipoliittisen osaston osastopäällikkö ratkaisee sen lisäksi, mitä 28 §:ssä säädetään, asiat jotka koskevat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1) hallinnonalan talousarvion toimeenpanoa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
2) hallinnonalan käytössä olevia kiinteistöjä;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
3) sotilasilma-alustyypin ja taistelualuksen hylkäämistä;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
4) käytöstä poistetun irtaimen omaisuuden myymistä, jos omaisuuden arvo on enintään 250.000 euroa, sekä käytöstä poistetun omaisuuden lahjoittamista;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
5) puolustusvoimien virka- ja työvoima-apua.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä joulukuuta 2002.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Tällä asetuksella kumottavassa 39 §:ssä säädetyn kiinteistötalouden vastuualueen tehtävät siirretään resurssipoliittisen osaston yhdyskunta- ja ympäristöyksikölle.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Ennen tämän asetuksen voimaantuloa voidaan ryhtyä sen täytäntöönpanon edellyttämiin toimenpiteisiin.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
