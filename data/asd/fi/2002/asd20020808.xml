  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>808</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>2002</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>ulkoasiainministeriön työjärjestyksen muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Ulkoasiainministeriön asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>ulkoasiainministeriön työjärjestyksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Ulkoasiainministeriön päätöksen mukaisesti</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> 27 päivänä kesäkuuta 2000 annetun ulkoasiainministeriön työjärjestyksen (623/2000) 36 §:n 3 momentti ja 39 §:n 3 momentti seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="36 §">
          <saa:PykalaTunnusKooste>36 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Tiedotuksen ja kansalaistoiminnan linjan tehtävät yksiköittäin</saa:SaadosOtsikkoKooste>
          <sis:Katkoviiva/>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Kansalaisjärjestöyksikkö:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1) hanke- ja tiedotustuki suomalaisille ja kansainvälisille kansalaisjärjestöille;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
2) suhteet Kehitysyhteistyön palvelukeskukseen (KEPA);
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
3) EU:n kansalaisjärjestötoiminta; sekä
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
4) suhteet ihmisoikeus-, ympäristö-, vammais- ja muihin säätiöihin ja rahastoihin.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="39 §">
          <saa:PykalaTunnusKooste>39 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Itälinjan alueet ja tehtävät</saa:SaadosOtsikkoKooste>
          <sis:Katkoviiva/>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Lähialueyhteistyön yksiköllä on seuraavat tehtävät:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1) kahdenvälinen ja monenkeskinen lähialueyhteistyö;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
2) EBRD ja muut yhteisrahoitusjärjestelyt;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
3) EU:n rahoitusohjelmat; PHARE ja TACIS; sekä
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
4) hanketuki suomalaisille ja kansainvälisille kansalaisjärjestöille.

</saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä lokakuuta 2002.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
