<vah:VahvistettavaLaki xmlns:vah="http://www.vn.fi/skeemat/vahvistettavalaki/2010/04/27" xmlns:sdk="http://www.finlex.fi/skeemat/edita/sdk/2010/04/27" xmlns:saa="http://www.vn.fi/skeemat/saadoskooste/2010/04/27" xmlns:saa1="http://www.vn.fi/skeemat/saadoselementit/2010/04/27" xmlns:met="http://www.vn.fi/skeemat/metatietokooste/2010/04/27" xmlns:met1="http://www.vn.fi/skeemat/metatietoelementit/2010/04/27" xmlns:asi="http://www.vn.fi/skeemat/asiakirjakooste/2010/04/27" xmlns:asi1="http://www.vn.fi/skeemat/asiakirjaelementit/2010/04/27" xmlns:org="http://www.vn.fi/skeemat/organisaatiokooste/2010/02/15" xmlns:org1="http://www.vn.fi/skeemat/organisaatioelementit/2010/02/15" xmlns:tau="http://www.vn.fi/skeemat/taulukkokooste/2010/04/27" xmlns:sis="http://www.vn.fi/skeemat/sisaltokooste/2010/04/27" xmlns:sis1="http://www.vn.fi/skeemat/sisaltoelementit/2010/04/27" met1:kieliKoodi="fi" met1:julkisuusKoodi="julkinen" met1:tilaKoodi="Valmis" met1:identifiointiTunnus="0000000000000000" met1:versioTeksti="1.0" met1:asiakirjatyyppiNimi="Vahvistettava laki" met1:laadintaPvm="2002-08-19" met1:rakennemaarittelyNimi="vahvistettavalaki.xsd">
  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>705</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>2002</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>rikoslain 44 luvun 16 §:n 1 momentissa tarkoitetuista dopingaineista</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>rikoslain 44 luvun 16 §:n 1 momentissa tarkoitetuista dopingaineista</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvoston päätöksen mukaisesti, joka on tehty oikeusministeriön esittelystä, säädetään 19 päivänä joulukuuta 1889 annetun rikoslain (39/1889) 44 luvun 16 §:n 2 momentin nojalla, sellaisena kuin se on laissa 400/2002:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Dopingaineina pidettävät aineet</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Rikoslain (39/1889) 44 luvun 16 §:n 1 momentissa tarkoitettuina dopingaineina pidetään seuraavia aineita:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>1) synteettiset anaboliset steroidit ja niiden johdannaiset:</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
androsteenidioli
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
androsteenidioni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
bolasteroni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
boldenoni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
calusteroni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
danatsoli
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
dehydroepiandrosteroni (DHEA)
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
dehydrokloorimetyylitestosteroni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
dihydrotestosteroni (DHT)
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
drostanoloni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
fluoksimesteroni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
formeboloni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
gestrinoni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
klosteboli
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
mestanoloni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
mesteroloni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
metandienoni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
metandrioli
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
metenoloni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
metyylitestosteroni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
miboleroni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
nandroloni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
19-norandrosteenidioli
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
19-norandrosteenidioni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
noretandroloni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
oksaboloni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
oksandroloni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
oksimesteroni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
oksimetoloni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
quinbolone
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
stanatsoli
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
trenboloni sekä
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
tämän luettelon aineiden johdannaiset, esterit ja suolat;
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>2) testosteroni ja sen johdannaiset:</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
testosteroniesterit
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
testosteroniundekanoaatti-esteri;
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
3) kasvuhormoni; ja
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
              <saa:MomenttiAlakohtaKooste>4) kemialliset aineet, jotka lisäävät testosteronin, sen johdannaisten tai kasvuhormonin tuotantoa ihmiskehossa:</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
follikkelia stimuloiva hormoni (FSH, follitropiini alfa, follitropiini beta)
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
gonadotropiini
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
insuliinin kaltainen kasvutekijä (IGF-1)
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
istukkahormoni (HCG)
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
klomifeeni
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
luteinisoiva hormoni (LH)
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
menotropiini
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
syklofeniili
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
tamoksifeeni sekä
</saa:MomenttiAlakohtaKooste>
              <saa:MomenttiAlakohtaKooste>
tämän luettelon aineiden suolat ja johdannaiset.
</saa:MomenttiAlakohtaKooste>
            </saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Voimaantulo</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä syyskuuta 2002.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
