  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>Yhdistyneiden Kansakuntien turvallisuusneuvoston Libyaa koskeviin päätöksiin perustuvien velvoitusten täyttämisestä annetun asetuksen soveltamisen väliaikaisesta keskeyttämisestä</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Ulkoasiainministerin esittelystä säädetään:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Yhdistyneiden Kansakuntien turvallisuusneuvoston Libyaa koskevaan päätökseen perustuvien velvoitusten täyttämisestä 16 päivänä huhtikuuta 1992 annetun asetuksen (336/1992), sellaisena kuin se on muutettuna 26 päivänä marraskuuta 1993 annetulla ase- tuksella (1041/1993), soveltaminen keskeytetään väliaikaisesti.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 28 päivänä huhtikuuta 1999.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
