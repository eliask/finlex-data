    <asi:SisaltoLiite>
      <sis:KappaleKooste>
        <sis1:KursiiviTeksti>Liite</sis1:KursiiviTeksti>
      </sis:KappaleKooste>
      <sis1:ValiotsikkoTeksti>Kuluttajaviraston julkisoikeudelliset suoritteet</sis1:ValiotsikkoTeksti>
      <sis:KappaleKooste>Hakemuksesta tehdyt päätökset ovat maksullisia myös silloin, kun hakemus hylätään. Maksut eivät sisällä laskutuksesta aiheutuvia kustannuksia, jotka peritään erikseen.</sis:KappaleKooste>
      <sis:KappaleKooste>Maksut ovat seuraavat:</sis:KappaleKooste>
      <tau:table>
        <tau:tgroup cols="2" colsep="0" rowsep="0" align="left">
          <tau:colspec colnum="1" colname="1" colwidth="50*"/>
          <tau:colspec colnum="2" colname="2" colwidth="50*"/>
          <tau:tbody valign="top">
            <tau:row>
              <tau:entry>
                <sis:KappaleKooste>1) valmismatkaliikkeen harjoittajan rekisteröintiä koskeva päätös</sis:KappaleKooste>
              </tau:entry>
              <tau:entry>
                <sis:KappaleKooste>150,00 €</sis:KappaleKooste>
              </tau:entry>
            </tau:row>
            <tau:row>
              <tau:entry>
                <sis:KappaleKooste>2) valmismatkaliikerekisteriin rekisteröityjen vuotuiset valvontamaksut:</sis:KappaleKooste>
              </tau:entry>
              <tau:entry>
                <sis:KappaleKooste/>
              </tau:entry>
            </tau:row>
            <tau:row>
              <tau:entry>
                <sis:KappaleKooste>- maksuluokka I (laskennallinen vakuus enintään 10 000 €, vakuudesta vapautetut ja välittäjät)</sis:KappaleKooste>
              </tau:entry>
              <tau:entry>
                <sis:KappaleKooste>250,00 €/vuosi</sis:KappaleKooste>
              </tau:entry>
            </tau:row>
            <tau:row>
              <tau:entry>
                <sis:KappaleKooste>- maksuluokka II (laskennallinen vakuus 10 001 €—100 000 €)</sis:KappaleKooste>
              </tau:entry>
              <tau:entry>
                <sis:KappaleKooste>500,00 €/vuosi</sis:KappaleKooste>
              </tau:entry>
            </tau:row>
            <tau:row>
              <tau:entry>
                <sis:KappaleKooste>- maksuluokka III (laskennallinen vakuus 100 001 €—1 000 000 €)</sis:KappaleKooste>
              </tau:entry>
              <tau:entry>
                <sis:KappaleKooste>990,00 €/vuosi</sis:KappaleKooste>
              </tau:entry>
            </tau:row>
            <tau:row>
              <tau:entry>
                <sis:KappaleKooste>- maksuluokka IV (laskennallinen vakuus yli 1 000 000 €)</sis:KappaleKooste>
              </tau:entry>
              <tau:entry>
                <sis:KappaleKooste>1 960,00 €/vuosi</sis:KappaleKooste>
              </tau:entry>
            </tau:row>
            <tau:row>
              <tau:entry>
                <sis:KappaleKooste>3) ote valmismatkaliikerekisteristä</sis:KappaleKooste>
              </tau:entry>
