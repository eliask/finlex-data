  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>137</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1976</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>nuorista rikoksentekijöistä annetun lain muuttamisesta </met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Laki">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Laki</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>nuorista rikoksentekijöistä annetun lain muuttamisesta </saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Eduskunnan päätöksen mukaisesti muutetaan nuorista rikoksentekijöistä 31 päivänä toukokuuta 1940 annetun lain (262/40) 7 ja 13 § seuraavasti: </saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="7§">
          <saa:PykalaTunnusKooste>7§</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Mitä ehdollisesta rangaistuksesta annetun lain 2 §:ssä on säädetty, ei estä sellaisen rangaistuksen käyttämistä nuoren rikoksentekijän tekemästä rikoksesta. </saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="13§">
          <saa:PykalaTunnusKooste>13§</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Ehdollisen rangaistuksen täytäntöönpanosta on säädetty ehdollisesta rangaistuksesta annetussa laissa. </saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä laki tuke voimaan 1 päivän huhtikuuta 1976. </saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
