<vah:VahvistettavaLaki xmlns:vah="http://www.vn.fi/skeemat/vahvistettavalaki/2010/04/27" xmlns:sdk="http://www.finlex.fi/skeemat/edita/sdk/2010/04/27" xmlns:saa="http://www.vn.fi/skeemat/saadoskooste/2010/04/27" xmlns:saa1="http://www.vn.fi/skeemat/saadoselementit/2010/04/27" xmlns:met="http://www.vn.fi/skeemat/metatietokooste/2010/04/27" xmlns:met1="http://www.vn.fi/skeemat/metatietoelementit/2010/04/27" xmlns:asi="http://www.vn.fi/skeemat/asiakirjakooste/2010/04/27" xmlns:asi1="http://www.vn.fi/skeemat/asiakirjaelementit/2010/04/27" xmlns:org="http://www.vn.fi/skeemat/organisaatiokooste/2010/02/15" xmlns:org1="http://www.vn.fi/skeemat/organisaatioelementit/2010/02/15" xmlns:tau="http://www.vn.fi/skeemat/taulukkokooste/2010/04/27" xmlns:sis="http://www.vn.fi/skeemat/sisaltokooste/2010/04/27" xmlns:sis1="http://www.vn.fi/skeemat/sisaltoelementit/2010/04/27" met1:kieliKoodi="fi" met1:julkisuusKoodi="julkinen" met1:tilaKoodi="Valmis" met1:identifiointiTunnus="0000000000000000" met1:versioTeksti="1.0" met1:asiakirjatyyppiNimi="Vahvistettava laki" met1:laadintaPvm="2100-01-01" met1:rakennemaarittelyNimi="vahvistettavalaki.xsd">
  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>798</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>2024</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>rikosseuraamuskeskuksen yksiköistä ja vankiloiden kaksikielisistä osastoista</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Oikeusministeriön asetus </saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>rikosseuraamuskeskuksen yksiköistä ja vankiloiden kaksikielisistä osastoista</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Oikeusministeriön päätöksen mukaisesti säädetään Rikosseuraamuslaitoksesta annetun lain (221/2022) 3 §:n 3 momentin ja kielilain (423/2003) 40 §:n 2 momentin nojalla:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Helsingin rikosseuraamuskeskuksen yksiköt</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Helsingin rikosseuraamuskeskuksen yksiköt ovat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>1) Helsingin kaupungissa sijaitseva Helsingin vankila;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>2) Helsingin kaupungissa sijaitseva Suomenlinnan vankila; sekä</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>3) Helsingin yhdyskuntaseuraamustoimisto.</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:MomenttiKooste>Helsingin vankilassa ja Suomenlinnan vankilassa on kaksikielinen osasto.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Uudenmaan rikosseuraamuskeskuksen yksiköt</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Uudenmaan rikosseuraamuskeskuksen yksiköt ovat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>1) Tuusulan kunnassa sijaitseva Jokelan vankila;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>2) Keravan kaupungissa sijaitseva Keravan vankila;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>3) Vantaan kaupungissa sijaitseva Vantaan vankila; sekä</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>4) Uudenmaan yhdyskuntaseuraamustoimisto.</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Lounais-Suomen rikosseuraamuskeskuksen yksiköt</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Lounais-Suomen rikosseuraamuskeskuksen yksiköt ovat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>1) Huittisten kaupungissa sijaitseva Huittisten vankila;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>2) Auran kunnassa sijaitseva Käyrän vankila;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>3) Säkylän kunnassa sijaitseva Köyliön vankila;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>4) Turun kaupungissa sijaitseva Turun vankila; sekä</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>5) Lounais-Suomen yhdyskuntaseuraamustoimisto.</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:MomenttiKooste>Turun vankilassa ja Käyrän vankilassa on kaksikielinen osasto.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="4 §">
          <saa:PykalaTunnusKooste>4 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Hämeen ja Pirkanmaan rikosseuraamuskeskuksen yksiköt</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Hämeen ja Pirkanmaan rikosseuraamuskeskuksen yksiköt ovat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>1) Hämeenlinnan kaupungissa sijaitseva Hämeenlinnan vankila;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>2) Akaan kaupungissa sijaitseva Kylmäkosken vankila;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>3) Riihimäen kaupungissa sijaitseva Riihimäen vankila;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>4) Hämeenlinnan kaupungissa sijaitseva Vanajan ja Ojoisten vankila; sekä</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>5) Hämeen ja Pirkanmaan yhdyskuntaseuraamustoimisto.</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="5 §">
          <saa:PykalaTunnusKooste>5 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Itä-Suomen rikosseuraamuskeskuksen yksiköt</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Itä-Suomen rikosseuraamuskeskuksen yksiköt ovat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>1) Juuan kunnassa sijaitseva Juuan vankila;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>2) Mikkelin kaupungissa sijaitseva Mikkelin vankila;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>3) Pieksämäen kaupungissa sijaitseva Naarajärven vankila;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>4) Joensuun kaupungissa sijaitseva Pyhäselän vankila;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>5) Sulkavan kunnassa sijaitseva Sulkavan vankila; sekä</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>6) Itä-Suomen yhdyskuntaseuraamustoimisto.</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="6 §">
          <saa:PykalaTunnusKooste>6 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Väli-Suomen rikosseuraamuskeskuksen yksiköt</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Väli-Suomen rikosseuraamuskeskuksen yksiköt ovat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>1) Jyväskylän kaupungissa sijaitseva Jyväskylän vankila;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>2) Kuopion kaupungissa sijaitseva Kuopion vankila;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>3) Vaasan kaupungissa sijaitseva Vaasan vankila;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>4) Mänttä-Vilppulan kaupungissa sijaitseva Vilppulan vankila; sekä</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>5) Väli-Suomen yhdyskuntaseuraamustoimisto.</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:MomenttiKooste>Vaasan vankilassa on kaksikielinen osasto.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="7 §">
          <saa:PykalaTunnusKooste>7 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Pohjois-Suomen rikosseuraamuskeskuksen yksiköt</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Pohjois-Suomen rikosseuraamuskeskuksen yksiköt ovat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>1) Siikalatvan kunnassa sijaitseva Kestilän vankila;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>2) Oulun kaupungissa sijaitseva Oulun vankila;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>3) Vaalan kunnassa sijaitseva Pelson vankila;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>4) Sonkajärven kunnassa sijaitseva Sukevan vankila;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>5) Ylitornion kunnassa sijaitseva Ylitornion vankila; sekä</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>6) Pohjois-Suomen yhdyskuntaseuraamustoimisto.</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="8 §">
          <saa:PykalaTunnusKooste>8 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Voimaantulo</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä tammikuuta 2025.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Tällä asetuksella kumotaan rikosseuraamuskeskuksen yksiköistä ja vankiloiden kaksikielisistä osastoista annettu oikeusministeriön asetus (797/2022).</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
