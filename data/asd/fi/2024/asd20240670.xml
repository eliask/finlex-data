<vah:VahvistettavaLaki xmlns:vah="http://www.vn.fi/skeemat/vahvistettavalaki/2010/04/27" xmlns:sdk="http://www.finlex.fi/skeemat/edita/sdk/2010/04/27" xmlns:saa="http://www.vn.fi/skeemat/saadoskooste/2010/04/27" xmlns:saa1="http://www.vn.fi/skeemat/saadoselementit/2010/04/27" xmlns:met="http://www.vn.fi/skeemat/metatietokooste/2010/04/27" xmlns:met1="http://www.vn.fi/skeemat/metatietoelementit/2010/04/27" xmlns:asi="http://www.vn.fi/skeemat/asiakirjakooste/2010/04/27" xmlns:asi1="http://www.vn.fi/skeemat/asiakirjaelementit/2010/04/27" xmlns:org="http://www.vn.fi/skeemat/organisaatiokooste/2010/02/15" xmlns:org1="http://www.vn.fi/skeemat/organisaatioelementit/2010/02/15" xmlns:tau="http://www.vn.fi/skeemat/taulukkokooste/2010/04/27" xmlns:sis="http://www.vn.fi/skeemat/sisaltokooste/2010/04/27" xmlns:sis1="http://www.vn.fi/skeemat/sisaltoelementit/2010/04/27" met1:kieliKoodi="fi" met1:julkisuusKoodi="julkinen" met1:tilaKoodi="Valmis" met1:identifiointiTunnus="0000000000000000" met1:versioTeksti="1.0" met1:asiakirjatyyppiNimi="Vahvistettava laki" met1:laadintaPvm="2100-01-01" met1:rakennemaarittelyNimi="vahvistettavalaki.xsd">
  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>670</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>2024</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>työ- ja elinkeinoministeriön työjärjestyksestä annetun työ- ja elinkeinoministeriön asetuksen muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Työ- ja elinkeinoministeriön asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>työ- ja elinkeinoministeriön työjärjestyksestä annetun työ- ja elinkeinoministeriön asetuksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Työ- ja elinkeinoministeriön päätöksen mukaisesti</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>kumotaan</saa:SaadosKursiiviKooste> työ- ja elinkeinoministeriön työjärjestyksestä annetun työ- ja elinkeinoministeriön asetuksen (826/2020) 17 §:n 11 kohta, sellaisena kuin se on asetuksessa 924/2021, ja</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> 17 §:n 5 ja 10 kohta, 20 §:n 15 kohta, 21 §:n 2 kohta ja 23 §, sellaisina kuin niistä ovat 17 §:n 5 kohta asetuksessa 924/2021 sekä 17 §:n 10 kohta ja 21 §:n 2 kohta asetuksessa 964/2022, seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="17 §">
          <saa:PykalaTunnusKooste>17 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Alueet ja kasvupalvelut -osasto</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Alueet ja kasvupalvelut -osasto käsittelee seuraavat asiat:</saa:MomenttiJohdantoKooste>
            <sis:Katkoviiva/>
            <saa:MomenttiKohtaKooste>5) elinkeino-, liikenne- ja ympäristökeskusten ohjauksen sekä niiden kehittämis- ja hallintokeskuksen (KEHA-keskus) toimialaohjauksen koordinointi ja tulossopimusprosessin johtaminen;</saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
            <saa:MomenttiKohtaKooste>10) julkisten työvoima- ja yrityspalvelujen palvelujärjestelmän ohjaus ja kehittäminen (mukaan lukien työllisyyden kuntakokeilut); </saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="20 §">
          <saa:PykalaTunnusKooste>20 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Työllisyys ja toimivat markkinat -osasto</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Työllisyys ja toimivat markkinat -osasto käsittelee seuraavat asiat:</saa:MomenttiJohdantoKooste>
            <sis:Katkoviiva/>
            <saa:MomenttiKohtaKooste>15) ulkomaalaisten yritysostojen seuranta- ja vahvistusprosessit;</saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="21 §">
          <saa:PykalaTunnusKooste>21 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Työvoiman maahanmuutto- ja kotoutumisyksikkö</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Työvoiman maahanmuutto- ja kotoutumisyksikkö käsittelee seuraavat asiat:</saa:MomenttiJohdantoKooste>
            <sis:Katkoviiva/>
            <saa:MomenttiKohtaKooste>2) osaajien houkuttelu ja kansainvälinen rekrytointi (mukaan lukien EURES);</saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="23 §">
          <saa:PykalaTunnusKooste>23 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Viestintäyksikkö</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Viestintäyksikkö käsittelee seuraavat asiat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>1) ministeriön ulkoinen viestintä;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>2) ministeriön työyhteisöviestintä;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>3) verkkoviestintä;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>4) visuaalinen viestintä;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>5) hallinnonalan viestintäyhteistyö.</saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä joulukuuta 2024.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
