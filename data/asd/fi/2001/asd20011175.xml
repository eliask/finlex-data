    <met:Nimeke>
      <met1:NimekeTeksti>eräiden opintotukilaissa (28/1972) tarkoitettujen määrien vahvistamisesta annetun valtioneuvoston asetuksen 1 ja 2 §:n muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>eräiden opintotukilaissa (28/1972) tarkoitettujen määrien vahvistamisesta annetun valtioneuvoston asetuksen 1 ja 2 §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvoston päätöksen mukaisesti, joka on tehty opetusministeriön esittelystä,</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> eräiden opintotukilaissa (28/1972) tarkoitettujen määrien vahvistamisesta 14 päivänä joulukuuta 2000 annetun valtioneuvoston asetuksen (1132/2000) 1 ja 2 § seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Opintolainan enimmäiskorko</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Opintolainan enimmäiskorko on 5,53 prosenttia.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Valtion maksama korkotuki sekä lainansaajan korko-osuus</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Valtion luottolaitoksille korkotuen alaisista opintolainoista maksaman korkotuen määrä on 1,86 prosenttia.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Lainansaajan korko-osuus on 3,67 prosenttia.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä tammikuuta 2002.</saa:MomenttiKooste>
          <saa:MomenttiKooste>Ennen asetuksen voimaantuloa voidaan ryhtyä sen täytäntöönpanon edellyttämiin toimenpiteisiin.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
