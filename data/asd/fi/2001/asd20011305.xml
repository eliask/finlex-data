<vah:VahvistettavaLaki xmlns:vah="http://www.vn.fi/skeemat/vahvistettavalaki/2010/04/27" xmlns:sdk="http://www.finlex.fi/skeemat/edita/sdk/2010/04/27" xmlns:saa="http://www.vn.fi/skeemat/saadoskooste/2010/04/27" xmlns:saa1="http://www.vn.fi/skeemat/saadoselementit/2010/04/27" xmlns:met="http://www.vn.fi/skeemat/metatietokooste/2010/04/27" xmlns:met1="http://www.vn.fi/skeemat/metatietoelementit/2010/04/27" xmlns:asi="http://www.vn.fi/skeemat/asiakirjakooste/2010/04/27" xmlns:asi1="http://www.vn.fi/skeemat/asiakirjaelementit/2010/04/27" xmlns:org="http://www.vn.fi/skeemat/organisaatiokooste/2010/02/15" xmlns:org1="http://www.vn.fi/skeemat/organisaatioelementit/2010/02/15" xmlns:tau="http://www.vn.fi/skeemat/taulukkokooste/2010/04/27" xmlns:sis="http://www.vn.fi/skeemat/sisaltokooste/2010/04/27" xmlns:sis1="http://www.vn.fi/skeemat/sisaltoelementit/2010/04/27" met1:kieliKoodi="fi" met1:julkisuusKoodi="julkinen" met1:tilaKoodi="Valmis" met1:identifiointiTunnus="0000000000000000" met1:versioTeksti="1.0" met1:asiakirjatyyppiNimi="Vahvistettava laki" met1:laadintaPvm="2001-12-20" met1:rakennemaarittelyNimi="vahvistettavalaki.xsd">
  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>1305</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>2001</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>liikenne- ja viestintäministeriön työjärjestyksen muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Liikenne- ja viestintäministeriön asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>liikenne- ja viestintäministeriön työjärjestyksen muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Liikenne- ja viestintäministeriön päätöksen mukaisesti</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste>muutetaan</saa:SaadosKursiiviKooste> 22 päivänä elokuuta 2000 annetun liikenne- ja viestintäministeriön työjärjestyksen (774/2000) 17, 20 ja 21 § seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="17 §">
          <saa:PykalaTunnusKooste>17 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Liikenneväyläyksikkö</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Liikenneväyläyksikössä käsitellään asiat, jotka koskevat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1) liikennejärjestelmien suunnittelua ja kehittämistä;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
2) liikenneverkkojen ylläpitoa ja kehittämistä sekä yleiseurooppalaisten liikenneverkkojen suunnitelmia ja rahoitusta;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
3) Tiehallinnon, Ratahallintokeskuksen sekä Merenkulkulaitoksen väylänpidon tulosohjausta;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
4) Tiehallinnon hallintoa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
5) liikenneväylien suunnittelun ja rakentamisen ohjausta sekä niitä koskevia suunnitelmia;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
6) liikenneväyliin liittyviä ympäristön arviointia sekä ympäristökysymyksiä;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
7) liikenteeseen liittyvää maankäyttöä, kaavoitusta ja aluepolitiikkaa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
8) liikenneväylänpidon ja liikenneväylien yhteiskuntataloudellisia arviointimenetelmiä;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
9) kiinteän omaisuuden hankintaa, luovutusta, vaihtoa ja vuokralleantoa sekä erityisten oikeuksien lunastamista ja talonrakennushankkeita;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
10) työ- ja työllisyysohjelmia;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
11) yksityisiä teitä ja yksityisiä lentokenttiä; sekä
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
12) liikennemuseohankkeiden ja eräiden liikennealan yhteisöjen valtionapupäätöksiä.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="20 §">
          <saa:PykalaTunnusKooste>20 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Rautatie- ja ilmailuyksikkö</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Rautatie- ja ilmailuyksikössä käsitellään asiat, jotka koskevat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1) rautatieliikenteen ja ilmailun lainsäädäntöä sekä elinkeino- ja lupapolitiikkaa ja rautatieliikenteen ja ilmailun yleistä kehittämistä;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
2) rautatieliikenteen ja ilmailun turvallisuutta;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
3) Ilmailulaitoksen viranomaistehtäviä; sekä
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
4) Ratahallintokeskusta ja sen tulosohjausta, mikäli asia ei kuulu muulle yksikölle.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="21 §">
          <saa:PykalaTunnusKooste>21 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Merenkulkuyksikkö</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Merenkulkuyksikössä käsitellään asiat, jotka koskevat:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1) merenkulun ja muun vesiliikenteen elinkeinopolitiikkaa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
2) merenkulun ja muun vesiliikenteen turvallisuutta;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
3) merikuljetuksiin liittyviä oikeudellisia vastuukysymyksiä;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
4) Merentutkimuslaitosta ja sen tulosohjausta;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
5) Merenkulkulaitosta ja sen tulosohjausta vesiväylien rakentamista ja ylläpitoa lukuun ottamatta;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
6) Saimaan kanavan hoitokuntaa;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
7) aluksista aiheutuvan vesien pilaantumisen estämistä;
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
8) muita merenkulkuun ja muuhun vesiliikenteeseen liittyviä ympäristökysymyksiä lukuun ottamatta vaarallisten aineiden meri- ja sisävesikuljetusta kappaletavarana koskevia kuljetusmääräyksiä; sekä
</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
9) Ilmatieteen laitosta ja sen tulosohjausta.
</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä tammikuuta 2002.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
