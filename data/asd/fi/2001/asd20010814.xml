                </tau:entry>
                <tau:entry>
                  <sis:KappaleKooste/>
                </tau:entry>
                <tau:entry>
                  <sis:KappaleKooste>10 000</sis:KappaleKooste>
                </tau:entry>
              </tau:row>
              <tau:row>
