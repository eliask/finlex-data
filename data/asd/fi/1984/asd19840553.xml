  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>rakennusasetuksen 51 ja 123 §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Ympäristöministerin esittelystä lisätään 26 päivänä kesäkuuta 1959 annetun rakennusasetuksen (266/59)51 §:ään uusi 5 momentti, jolloin nykyinen 5 momentti siirtyy 6 momentiksi, ja 123 §:ään uusi 3 momentti, jolloin nykyinen 3 momentti siirtyy 4 momentiksi, seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="51 §">
          <saa:PykalaTunnusKooste>51 §</saa:PykalaTunnusKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Jos rakennuslupaa haetaan alueelle, joka sisältyy valtioneuvoston hyväksymään luonnonsuojelulaissa (71/23) tarkoitettuja suojelualueita koskevaan ohjelmaan, on asiasta pyydettävä lääninhallituksen lausunto. Lausunto on annettava viivyttelemättä.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="123 §">
          <saa:PykalaTunnusKooste>123 §</saa:PykalaTunnusKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Jos rakennuslupaa haetaan alueelle, joka sisältyy valtioneuvoston hyväksymään luonnonsuojelulaissa (71/23) tarkoitettuja suojelualueita koskevaan ohjelmaan, on asiasta pyydettävä lääninhallituksen lausunto. Lausunto on annettava viivyttelemättä.</saa:MomenttiKooste>
          <sis:Katkoviiva/>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä syyskuuta 1984.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
