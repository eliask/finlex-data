      <met1:NimekeTeksti>pakkokeinolain 10 luvun muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>HE 141/2012</sis1:ViiteTeksti>
      </met:Muu1Viite>
      <met:Muu2Viite>
        <sis1:ViiteTeksti>LaVM 8/2013</sis1:ViiteTeksti>
      </met:Muu2Viite>
      <met:Muu3Viite>
