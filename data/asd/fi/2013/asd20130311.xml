  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>311</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>2013</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>puolustustarvikkeiden vientiä koskevan lupaharkinnan yleisistä edellytyksistä</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Valtioneuvoston asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>puolustustarvikkeiden vientiä koskevan lupaharkinnan yleisistä edellytyksistä</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Valtioneuvoston päätöksen mukaisesti säädetään puolustustarvikkeiden viennistä annetun lain (282/2012) 9 §:n 3 momentin nojalla:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Puolustustarvikkeiden vienti ilman maakohtaista ulko- ja turvallisuuspoliittista harkintaa</saa:SaadosOtsikkoKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Käsiteltäessä hakemusta, joka koskee puolustustarvikkeiden viennistä annetun lain (282/2012) 2 luvussa tarkoitettua lupaa puolustustarvikkeiden viennille, siirrolle, välitykselle tai kauttakuljetukselle Suomesta, luvan myöntämisessä ei sovelleta ulko- ja turvallisuuspoliittista harkintaa, jos kyse on seuraavasta valtiosta:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>1) Euroopan unionin jäsenvaltio;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>2) Euroopan talousalueen jäsenvaltio; taikka</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>3) Sveitsi, Australia, Japani, Kanada, Uusi-Seelanti tai Yhdysvallat.</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:SaadosOtsikkoKooste>Voimaantulo</saa:SaadosOtsikkoKooste>
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 30 päivänä huhtikuuta 2013.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
