    </asi:Allekirjoittaja>
  </asi:AllekirjoitusOsa>
  <asi:LiiteOsa>
    <asi:SisaltoLiite>
      <sis:KappaleKooste>
        <sis1:KursiiviTeksti>Liite 1 </sis1:KursiiviTeksti>
      </sis:KappaleKooste>
      <sis:KappaleKooste>
        <sis1:LihavaTeksti>Materiaalit ja osat, joihin ei sovelleta 5 §:n 1 momenttia, sekä eräiden materiaalien ja osien merkitseminen 5 §:n 2 momentin mukaisesti </sis1:LihavaTeksti>
      </sis:KappaleKooste>
      <sis:KappaleKooste>
        <sis1:LihavaTeksti>A. Lyijy seosaineena </sis1:LihavaTeksti>
      </sis:KappaleKooste>
      <tau:table>
        <tau:tgroup cols="3" colsep="0" rowsep="0" align="left">
          <tau:colspec colnum="1" colname="1" colwidth="33*"/>
          <tau:colspec colnum="2" colname="2" colwidth="33*"/>
          <tau:colspec colnum="3" colname="3" colwidth="33*"/>
          <tau:tbody valign="top">
            <tau:row>
              <tau:entry>
