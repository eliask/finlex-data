  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>868</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1997</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>elintarvikevirastosta annetun asetuksen 12 ja 13 §:n muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>elintarvikevirastosta annetun asetuksen 12 ja 13 §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Kauppa- ja teollisuusministeriön toimialaan kuuluvia asioita käsittelemään määrätyn ministerin esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste> muutetaan</saa:SaadosKursiiviKooste> elintarvikevirastosta 16 päivänä maaliskuuta 1990 annetun asetuksen (278/1990) 12 §:n 2 momentti ja 13 § seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="12 §">
          <saa:PykalaTunnusKooste>12 §</saa:PykalaTunnusKooste>
          <sis:Katkoviiva/>
          <saa:MomenttiKooste>Johtajalta ja apulaisjohtajalta vaaditaan lisäksi ylempi korkeakoulututkinto, käytännössä osoitettua johtamistaitoa ja hyvä perehtyneisyys viraston toimialaan kuuluviin tehtäviin. Lisäksi johtajalta vaaditaan johtamiskokemusta.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="13 §">
          <saa:PykalaTunnusKooste>13 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Johtajan nimittää tasavallan presidentti valtioneuvoston esityksestä. Apulaisjohtajan nimittää viraston johtokunta. Muun henkilöstön nimittää tai ottaa johtaja.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 15 päivänä syyskuuta 1997.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
  <asi:AllekirjoitusOsa>
