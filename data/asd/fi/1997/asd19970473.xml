  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>473</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1997</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>toimiluvista ja teletoimintailmoituksista</met1:NimekeTeksti>
    </met:Nimeke>
    <met:AsiakirjaViitteet>
      <met:Muu1Viite>
        <sis1:ViiteTeksti>Parlamentin ja neuvoston direktiivi 97/13/EY; EYVL N:o L 117, 7.5.1997, s. 15</sis1:ViiteTeksti>
      </met:Muu1Viite>
    </met:AsiakirjaViitteet>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Päätös">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Liikenneministeriön päätös</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>toimiluvista ja teletoimintailmoituksista</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Liikenneministeriö on 30 päivänä huhtikuuta 1997 annetun telemarkkinalain (396/1997) 6 §:n ja 7 §:n 1 momentin nojalla päättänyt:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Telemarkkinalain (396/1997) 7 §:ssä tarkoitetun asianomaiselle ministeriölle osoitettavan toimilupahakemuksen tulee sisältää seuraavat tiedot:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1) hakijan yhteystiedot;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
2) kaupparekisterin ote tai vastaava selvitys hakijasta;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
3) hakijayhteisön omistussuhteet;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
4) selvitys siitä, mihin säännösten ja määräysten mukaiseen matkaviestinverkkoon lupaa haetaan;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
5) televerkkopalveluiden kuvaus ja toiminnan rajaukset;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
6) toimialue;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
7) verkostokuvaus tai -suunnitelma;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
8) yhteenliittämiskuvaus tai -suunnitelma;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
9) arvio toiminnan vaatimista investoinneista ja niiden rahoituksesta; sekä</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
10) arvio liiketoiminnan kehityksestä viiden vuoden kuluessa toiminnan aloittamisesta tai hakemuksen jättämisestä.</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:MomenttiKooste>Toimilupahakemus tehdään ministeriöstä saatavalle lomakkeelle, jossa tarkemmin eritellään 1 momentin mukaisesti ilmoitettavat tiedot.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="2 §">
          <saa:PykalaTunnusKooste>2 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Telemarkkinalain 6 §:ssä tarkoitetun asianomaiselle ministeriölle osoitettavan teletoimintailmoituksen tulee sisältää seuraavat tiedot:</saa:MomenttiJohdantoKooste>
            <saa:MomenttiKohtaKooste>
1) ilmoittajan yhteystiedot;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
2) kaupparekisterin ote tai vastaava selvitys ilmoittajasta;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
3) ilmoittajayhteisön omistussuhteet;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
4) tarjottavien palveluiden erittely ja kuvaus sisältäen toiminnan rajaukset;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
5) toimialue;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
6) verkostokuvaus tai -suunnitelma;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
7) yhteenliittämiskuvaus tai -suunnitelma;</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
8) arvio toiminnan vaatimista investoinneista ja niiden rahoituksesta; sekä</saa:MomenttiKohtaKooste>
            <saa:MomenttiKohtaKooste>
9) arvio liiketoiminnan kehityksestä viiden vuoden kuluesssa toiminnan aloittamisesta tai ilmoituksen jättämisestä.</saa:MomenttiKohtaKooste>
          </saa:KohdatMomentti>
          <saa:MomenttiKooste>Teletoimintailmoitus tehdään ministeriöstä saatavalle lomakkeella, jossa tarkemmin eritellään 1 momentin mukaisesti ilmoitettavat tiedot.</saa:MomenttiKooste>
        </saa:Pykala>
        <saa:Pykala saa1:pykalaLuokitusKoodi="VoimaantuloPykala" saa1:identifiointiTunnus="3 §">
          <saa:PykalaTunnusKooste>3 §</saa:PykalaTunnusKooste>
          <saa:MomenttiKooste>Tämä päätös tulee voimaan 1 päivänä kesäkuuta 1997.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
