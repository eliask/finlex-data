              <tau:colspec colnum="2" colname="col2" colwidth="11520*"/>
              <tau:tbody valign="top">
                <tau:row>
                  <tau:entry>
                    <sis:KappaleKooste>Lainavuosi</sis:KappaleKooste>
                  </tau:entry>
                  <tau:entry>
