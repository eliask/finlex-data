  <asi:IdentifiointiOsa>
    <met1:AsiakirjatyyppiNimi>Vahvistettava laki</met1:AsiakirjatyyppiNimi>
    <asi:EduskuntaTunniste>
      <met1:AsiakirjatyyppiKoodi>SDK</met1:AsiakirjatyyppiKoodi>
      <asi1:AsiakirjaNroTeksti>591</asi1:AsiakirjaNroTeksti>
      <asi1:ValtiopaivavuosiTeksti>1997</asi1:ValtiopaivavuosiTeksti>
    </asi:EduskuntaTunniste>
    <met:Nimeke>
      <met1:NimekeTeksti>konkurssiasioista tehtävistä ilmoituksista annetun asetuksen 1 §:n muuttamisesta</met1:NimekeTeksti>
    </met:Nimeke>
  </asi:IdentifiointiOsa>
  <saa:SaadosOsa>
    <saa:Saados met1:kieliKoodi="fi" saa1:saadostyyppiNimi="Asetus">
      <saa:SaadosNimeke>
        <saa:SaadostyyppiKooste>Asetus</saa:SaadostyyppiKooste>
        <saa:SaadosNimekeKooste>konkurssiasioista tehtävistä ilmoituksista annetun asetuksen 1 §:n muuttamisesta</saa:SaadosNimekeKooste>
      </saa:SaadosNimeke>
      <saa:Johtolause>
        <saa:SaadosKappaleKooste>Oikeusministerin esittelystä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste> muutetaan</saa:SaadosKursiiviKooste> konkurssiasioista tehtävistä ilmoituksista 4 päivänä marraskuuta 1994 annetun asetuksen (959/1994) 1 §:n 2 momentti sekä</saa:SaadosKappaleKooste>
        <saa:SaadosKappaleKooste><saa:SaadosKursiiviKooste> lisätään</saa:SaadosKursiiviKooste> 1 §:n 1 momenttiin uusi 5 a kohta seuraavasti:</saa:SaadosKappaleKooste>
      </saa:Johtolause>
      <saa:Pykalisto>
        <saa:Pykala saa1:pykalaLuokitusKoodi="Pykala" saa1:identifiointiTunnus="1 §">
          <saa:PykalaTunnusKooste>1 §</saa:PykalaTunnusKooste>
          <saa:KohdatMomentti>
            <saa:MomenttiJohdantoKooste>Pesänhoitajan on viipymättä ilmoitettava konkurssin alkamisesta:</saa:MomenttiJohdantoKooste>
            <sis:Katkoviiva/>
            <saa:MomenttiKohtaKooste>
5 a) selvitysyhteisölle, jossa velallisen arvopaperi-, johdannais- tai valuuttakauppoja selvitetään;

</saa:MomenttiKohtaKooste>
            <sis:Katkoviiva/>
          </saa:KohdatMomentti>
          <saa:MomenttiKooste>Pesänhoitajan on viipymättä ilmoitettava 1 momentissa tarkoitetulle viranomaiselle, rekisterinpitäjälle tai selvitysyhteisölle myös, jos konkurssi raukeaa tai jos konkurssiasian käsittely muusta syystä päättyy muuhun kuin tuomioon.</saa:MomenttiKooste>
        </saa:Pykala>
        <sis:Palstaviiva/>
        <saa:Pykala saa1:identifiointiTunnus="Voimaantulo" saa1:pykalaLuokitusKoodi="VoimaantuloSaannos">
          <saa:MomenttiKooste>Tämä asetus tulee voimaan 1 päivänä heinäkuuta 1997.</saa:MomenttiKooste>
        </saa:Pykala>
      </saa:Pykalisto>
    </saa:Saados>
  </saa:SaadosOsa>
