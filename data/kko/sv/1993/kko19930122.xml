  <dcterms:description>
    <p>
      <strong>Extraordinärt ändringssökande</strong>
      <span>Återställande av fatalietid</span>
    </p>
  </dcterms:description>
  <finlex:registerNumber>H 93/52</finlex:registerNumber>
  <dcterms:date pvm="19930810">1993-08-10</dcterms:date>
