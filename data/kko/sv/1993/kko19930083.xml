  <dcterms:description>
    <p>
      <strong>Rättegångskostnader</strong>
    </p>
    <p>
      <strong>Konkurs</strong>
    </p>
  </dcterms:description>
  <finlex:registerNumber>S 91/1117</finlex:registerNumber>
  <dcterms:date pvm="19921006">1992-10-06</dcterms:date>
