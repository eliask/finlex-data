  <dcterms:title>KKO:2004:25</dcterms:title>
  <dcterms:description>
    <p>
      <strong>Villkorligt fängelse</strong>
      <span>Förordnande om verkställighet av              villkorligt fängelse</span>
    </p>
    <p>
      <strong>Gemensamt straff</strong>
      <span>Bestämmande av gemensamt fängelsestraff</span>
    </p>
    <p>
      <strong>Extraordinärt ändringssökande</strong>
      <span>Återbrytande av dom i              brottmål</span>
    </p>
  </dcterms:description>
  <finlex:registerNumber>H2003/190</finlex:registerNumber>
  <dcterms:date pvm="20040128">2004-01-28</dcterms:date>
