  <dcterms:title>KKO:2008:78</dcterms:title>
  <dcterms:description>
    <p>
      <strong>Våldsamt motstånd mot tjänsteman</strong>
    </p>
    <p>
      <strong>Utövning av offentlig makt</strong>
    </p>
  </dcterms:description>
  <finlex:registerNumber>R2007/1029</finlex:registerNumber>
  <dcterms:date pvm="20080410">2008-04-10</dcterms:date>
