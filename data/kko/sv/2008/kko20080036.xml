  <dcterms:description>
    <p>
      <strong>Domstols behörighet</strong>
      <span>Domstols- eller förvaltningstvistemål</span>
    </p>
  </dcterms:description>
  <finlex:registerNumber>S2006/918-919</finlex:registerNumber>
