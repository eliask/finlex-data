  <dcterms:title>KKO:2002:74</dcterms:title>
  <dcterms:description>
    <p>
      <strong>Skadestånd</strong>
      <span>Skadestånd i avtalsförhållande</span>
    </p>
    <p>
      <strong>Preskription av fordran</strong>
    </p>
  </dcterms:description>
  <finlex:registerNumber>S2001/144</finlex:registerNumber>
  <dcterms:date pvm="20020429">2002-04-29</dcterms:date>
