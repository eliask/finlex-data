    <p>
      <strong>Våldsamt motstånd mot tjänsteman</strong>
    </p>
  </dcterms:description>
  <finlex:registerNumber>R 94/52</finlex:registerNumber>
  <dcterms:date pvm="19940518">1994-05-18</dcterms:date>
