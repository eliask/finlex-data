    <p>
      <strong>Allmän väg</strong>
      <span>Reglering av enskilda vägar</span>
      <span>Väglag</span>
    </p>
  </dcterms:description>
  <finlex:registerNumber>M 93/266</finlex:registerNumber>
  <dcterms:date pvm="19940222">1994-02-22</dcterms:date>
