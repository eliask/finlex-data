  <dcterms:title>KKO:1984-II-114</dcterms:title>
  <dcterms:description>
    <p>
      <strong>Återförvisning av mål</strong>
    </p>
    <p>
      <strong>Återförvisning på grund av rättegångsfel</strong>
      <span>Motivering av dom</span>
    </p>
  </dcterms:description>
  <finlex:registerNumber>R 83/308</finlex:registerNumber>
  <dcterms:date pvm="19840213">1984-02-13</dcterms:date>
