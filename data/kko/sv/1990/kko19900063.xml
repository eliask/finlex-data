  <dcterms:title>KKO:1990:63</dcterms:title>
  <dcterms:description>
    <p>
      <strong>Domstols behörighet</strong>
      <span>Allmän domstol eller sjörättsdomstol</span>
    </p>
  </dcterms:description>
  <finlex:registerNumber>R 89/451</finlex:registerNumber>
  <dcterms:date pvm="19891218">1989-12-18</dcterms:date>
