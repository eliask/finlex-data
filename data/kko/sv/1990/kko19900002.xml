    <p>
      <strong>Olycksfallsersättning</strong>
      <span>Arbetstagares olycksfallsersättning</span>
    </p>
  </dcterms:description>
  <finlex:registerNumber>VA 89/63</finlex:registerNumber>
  <dcterms:date pvm="19890829">1989-08-29</dcterms:date>
