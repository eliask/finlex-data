  <dcterms:title>KKO:1998:131</dcterms:title>
  <dcterms:description>
    <p>
      <strong>Kränkning av säkerhetsrätt</strong>
    </p>
    <p>
      <strong>Pantsättning</strong>
    </p>
  </dcterms:description>
  <finlex:registerNumber>R 97/953</finlex:registerNumber>
  <dcterms:date pvm="19980819">1998-08-19</dcterms:date>
